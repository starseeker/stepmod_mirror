    <module name="mesh_function"/>
    <module name="mesh_topology"/>
    <module name="non_manifold_surface"/>
    <module name="product_analysis"/>
  </ballot_package>

  <!-- record of the CVS tags that have been created for managing this
       ballot package -->
  <cvs_tags>
    <cvs_tag tag="xxx">
      <description>

      </description>
    </cvs_tag>
  </cvs_tags>
