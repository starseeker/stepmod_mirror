  <description>ap209 analysis ballot package</description>

  <contacts>
    <projlead ref="xx"/>
    <editor ref="xx"/>
  </contacts>

  <ballot_package name="">
    <description>ap209 analysis ballot package</description>
    <module name="analysis"/>
    <module name="analysis_shape"/>
