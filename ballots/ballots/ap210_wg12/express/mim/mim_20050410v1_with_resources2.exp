       'LAYERED_INTERCONNECT_MODULE_DESIGN_MIM.' +
       'SHAPE_ASPECT_RELATIONSHIP.RELATED_SHAPE_ASPECT') |
       sar\shape_aspect_relationship.name = 'instantiated feature') |
       ('LAYERED_INTERCONNECT_MODULE_DESIGN_MIM.' +
       'PRINTED_PART_TEMPLATE_TERMINAL' IN
