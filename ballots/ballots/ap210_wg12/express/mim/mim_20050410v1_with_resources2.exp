     ) OR ( SIZEOF ( QUERY ( pdr <*
     get_property_definition_representations (
     SELF.related_shape_aspect. of_shape.definition ) | ( SIZEOF
     ( QUERY ( i <* pdr. used_representation.items | ( i.name=
     'row index' ) AND ( 'AUTOMOTIVE_DESIGN.'+
     'MEASURE_REPRESENTATION_ITEM' IN TYPEOF ( i ) ) AND (
     'AUTOMOTIVE_DESIGN.'+ 'COUNT_MEASURE' IN TYPEOF (
     i\measure_with_unit. value_component ) ) ) ) =1 ) ) ) =1 )
     ;
 WR8: NOT ( 'AUTOMOTIVE_DESIGN.'+ 'RECTANGULAR_PATTERN' IN
     TYPEOF ( SELF.relating_shape_aspect. of_shape.definition )
     ) OR ( SIZEOF ( QUERY ( pdr <*
     get_property_definition_representations (
     SELF.related_shape_aspect. of_shape.definition ) | ( SIZEOF
     ( QUERY ( i <* pdr. used_representation.items | ( i.name=
     'column index' ) AND ( 'AUTOMOTIVE_DESIGN.'+
     'MEASURE_REPRESENTATION_ITEM' IN TYPEOF ( i ) ) AND (
     'AUTOMOTIVE_DESIGN.'+ 'COUNT_MEASURE' IN TYPEOF (
     i\measure_with_unit. value_component ) ) ) ) =1 ) ) ) =1 )
     ;
END_ENTITY;

ENTITY person_and_organization_address
