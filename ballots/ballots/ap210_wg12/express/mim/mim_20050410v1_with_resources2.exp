ENTITY moments_of_inertia_representation
  SUBTYPE OF (representation);
WHERE
 WR1: ( SIZEOF ( SELF.items ) =1 ) AND ( SIZEOF ( QUERY ( i <*
     SELF.items | ( 'AUTOMOTIVE_DESIGN.'+
     'COMPOUND_REPRESENTATION_ITEM' IN TYPEOF ( i ) ) AND (
     i.name = 'moments of inertia matrix' ) ) ) =1 ) ;
 WR2: SIZEOF ( QUERY ( i <* SELF.items | ( 'AUTOMOTIVE_DESIGN.'+
     'COMPOUND_REPRESENTATION_ITEM' IN TYPEOF ( i ) ) AND (
     SIZEOF ( i\compound_representation_item. item_element ) =3
     ) AND ( 'AUTOMOTIVE_DESIGN.'+ 'LIST_REPRESENTATION_ITEM' IN
     TYPEOF ( i\compound_representation_item. item_element ) )
     AND value_range_aggregate_rep_item (
     i\compound_representation_item. item_element ) ) ) =1;
END_ENTITY;

