     item_correlation ( SELF.items , ['ACTION' ,
     'ORGANIZATIONAL_PROJECT' , 'EFFECTIVITY'] ) ;
END_ENTITY;

ENTITY applied_document_reference
  SUBTYPE OF (document_reference);
