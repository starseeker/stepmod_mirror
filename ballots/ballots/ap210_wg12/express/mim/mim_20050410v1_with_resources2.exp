     USEDIN ( pdr ,
     'AUTOMOTIVE_DESIGN.APPLIED_EFFECTIVITY_ASSIGNMENT.ITEMS' )
     ) =0 ) ) ) =0;
END_RULE;

RULE product_requires_category FOR
    (product,
     product_related_product_category);
WHERE
 WR1: SIZEOF ( QUERY ( p <* product | SIZEOF ( USEDIN ( p ,
     'AUTOMOTIVE_DESIGN.PRODUCT_RELATED_PRODUCT_CATEGORY.PRODUCTS'
     ) ) =0 ) ) =0;
END_RULE;

RULE product_requires_person_organization FOR
    (product);
WHERE
 WR1: SIZEOF ( QUERY ( prod <* product | ( SIZEOF ( QUERY ( prpc
     <* USEDIN ( prod ,
     'AUTOMOTIVE_DESIGN.PRODUCT_RELATED_PRODUCT_CATEGORY.PRODUCTS'
     ) | prpc. name IN ['part' , 'tool' , 'raw material' ] ) )
