END_RULE;

RULE plib_property_reference_requires_version FOR
    (externally_defined_general_property);
WHERE
 WR1: SIZEOF ( QUERY ( edgp <*
     externally_defined_general_property | (
     'AUTOMOTIVE_DESIGN.'+ 'KNOWN_SOURCE' IN TYPEOF ( edgp.
     source ) ) AND ( SIZEOF ( QUERY ( edir <* USEDIN ( edgp ,
     'AUTOMOTIVE_DESIGN.'+'APPLIED_EXTERNAL_IDENTIFICATION_ASSIGNMENT.'+
     'ITEMS' ) | ( edir. role.name ='version' ) ) ) <>1 ) ) )
