     applied_effectivity_assignment.items | NOT (
     'AUTOMOTIVE_DESIGN.'+
     'PRODUCT_CONCEPT_FEATURE_CATEGORY_USAGE' IN TYPEOF ( i ) )
     ) ) =0 ) ;
