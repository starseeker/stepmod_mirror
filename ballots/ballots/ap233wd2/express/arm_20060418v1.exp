ENTITY Risk_consequence;
  name : STRING;
  description : OPTIONAL STRING;
END_ENTITY;

ENTITY Risk_containment
  SUBTYPE OF (Risk_treatment);
END_ENTITY;

ENTITY Risk_context;
  name : STRING;
  description : OPTIONAL STRING;
