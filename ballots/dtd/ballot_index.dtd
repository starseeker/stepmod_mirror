<!ELEMENT ballot_index (ballot_package+)>
<!ATTLIST ballot_index
	name CDATA #REQUIRED
	wg.number.ballot_package CDATA #REQUIRED
	wg.number.ballot_comment CDATA #REQUIRED
	description CDATA #REQUIRED
>

<!ELEMENT ballot_package (module+)>
<!ATTLIST ballot_package
	name CDATA #REQUIRED
	id CDATA #IMPLIED
>

<!ELEMENT module EMPTY>
<!ATTLIST module
	name CDATA #REQUIRED
>
