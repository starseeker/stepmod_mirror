      <entity id="arm-person_in_organization">
	<description>the identification of an individual human being in an organization.
	  <note>The mechanism for guaranteeing the uniqueness of person id is outside the scope of ISO 10303.</note>
	</description>
	<entity_id>Person_in_organization</entity_id>
	<explicit_attr>
	  <description>the unique identifier of the person.</description>
	  <attribute_id id="arm-person_in_organization.id">id</attribute_id>
	  <string/>
	</explicit_attr>
	<explicit_attr>
	  <attribute_id
			id="arm-person_in_organization.id_org">id_org</attribute_id>
	  <entity_ref refid="arm-organization"/>
	</explicit_attr>
	<explicit_attr>
	  <attribute_id id="arm-person_in_organization.last_name">last_name</attribute_id>
	  <optional/>
	  <string/>
	</explicit_attr>
	<explicit_attr>
	  <attribute_id id="arm-person_in_organization.first_name">first_name</attribute_id>
	  <optional/>
	  <string/>
	</explicit_attr>
	<explicit_attr>
	  <attribute_id id="arm-person_in_organization.middle_names">middle_names</attribute_id>
	  <optional/>
	  <list_type>
	    <bound_spec>
	      <lower_bound>
		<expression>1</expression>
	      </lower_bound>
	      <upper_bound>
		<expression>?</expression>
	      </upper_bound>
	    </bound_spec>
	    <string/>
	  </list_type>
	</explicit_attr>
	<explicit_attr>
	  <attribute_id id="arm-person_in_organization.prefix_titles">prefix_titles</attribute_id>
	  <optional/>
	  <list_type>
	    <bound_spec>
	      <lower_bound>
		<expression>1</expression>
	      </lower_bound>
	      <upper_bound>
		<expression>?</expression>
	      </upper_bound>
	    </bound_spec>
	    <string/>
	  </list_type>
	</explicit_attr>
	<explicit_attr>
	  <attribute_id id="arm-person_in_organization.suffix_titles">suffix_titles</attribute_id>
	  <optional/>
	  <list_type>
	    <bound_spec>
	      <lower_bound>
		<expression>1</expression>
	      </lower_bound>
	      <upper_bound>
		<expression>?</expression>
	      </upper_bound>
	    </bound_spec>
	    <string/>
	  </list_type>
	</explicit_attr>
	<explicit_attr>
	  <description>the personal address of the person.</description>
	  <attribute_id id="arm-person_in_organization.address">address</attribute_id>
	  <optional/>
	  <entity_ref refid="arm-address"/>
	</explicit_attr>
	<unique_rule>
	  <label>person_is_unique</label>
	  <attribute_ref refid="arm-person_in_organization.id"/>
	  <attribute_ref refid="arm-person_in_organization.id_org"/>
	</unique_rule>
      </entity>
