SUPERTYPE OF ( ONEOF (General_requirement_version_relationship, Requirement_sequence_relationship, Requirement_tracing_relationship, Requirement_collection_relationship, Requirement_decomposition_relationship));
  description : OPTIONAL STRING; 
  related     : SET [1:?] OF Requirement_version; 
  relating    : Requirement_version; 
END_ENTITY;

ENTITY Retention_period;  -- GMI1
