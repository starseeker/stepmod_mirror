ENTITY Transformation ABSTRACT  -- SBC3
SUPERTYPE OF ( ONEOF (Transformation_2d, Transformation_3d));
  name : OPTIONAL STRING; 
END_ENTITY;

ENTITY Transformation_2d  -- SBC3
SUPERTYPE OF ( ONEOF (Explicit_transformation_2d, Implicit_transformation_2d))
SUBTYPE OF ( Transformation );
END_ENTITY;

ENTITY Transformation_3d  -- SBC3
SUPERTYPE OF ( ONEOF (Explicit_transformation_3d, Implicit_transformation_3d))
SUBTYPE OF ( Transformation );
END_ENTITY;

ENTITY Unit;  -- C1
  kind     : OPTIONAL STRING; 
  name     : STRING; 
  prefix   : OPTIONAL prefix; 
  quantity : OPTIONAL quantity; 
END_ENTITY;

ENTITY Value_limit  -- C1
SUBTYPE OF ( Value_with_unit );
  limit     : NUMBER; 
  qualifier : limit_qualifier; 
END_ENTITY;

ENTITY Value_list  -- C1
SUBTYPE OF ( Property_value );
  values : LIST [1:?] OF Property_value; 
END_ENTITY;

ENTITY Value_range  -- C1
SUBTYPE OF ( Value_with_unit );
  lower_limit : NUMBER; 
  upper_limit : NUMBER; 
END_ENTITY;

ENTITY Value_with_unit ABSTRACT  -- C1
SUPERTYPE OF ( ONEOF (Numerical_value, Value_limit, Value_range))
SUBTYPE OF ( Property_value );
  significant_digits : OPTIONAL INTEGER; 
  unit               : Unit; 
END_ENTITY;

ENTITY Work_order;  -- AWM2
  additional_ids : OPTIONAL SET [1:?] OF Id_with_context; 
