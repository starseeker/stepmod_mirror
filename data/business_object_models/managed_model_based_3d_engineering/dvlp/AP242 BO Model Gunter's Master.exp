END_ENTITY;

ENTITY ExternalCsgShapeRepresentation
 SUBTYPE OF(ExternalGeometricModel);
END_ENTITY;

ENTITY Curve
 SUPERTYPE OF (ONEOF (Pcurve, SurfaceCurve, TrimmedCurve, CompositeCurveOnSurface))
