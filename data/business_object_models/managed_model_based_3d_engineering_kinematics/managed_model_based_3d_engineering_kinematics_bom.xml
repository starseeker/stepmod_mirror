<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$">
   <application name="JSDAI" owner="LKSoft" source="managed_model_based_3d_engineering_kinematics_bom schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="managed_model_based_3d_engineering_kinematics_bom">
      <interface kind="use" schema="managed_model_based_3d_engineering_bom"/>
      <type name="ActuatedDirection">
         <enumeration items="bidirectional positive_only negative_only not_actuated"/>
      </type>
      <type name="ExternalCurveOrSurfaceSelect">
         <select selectitems="ExternalCurve ExternalSurface"/>
      </type>
      <type name="ExternalTrimmedCurveOrSurfaceSelect">
         <select selectitems="ExternalTrimmedCurve ExternalRectangularTrimmedSurface"/>
      </type>
      <type name="HighOrderKinematicPairType">
         <enumeration items="linearFlexibleAndPlanarCurvePair pointOnSurfacePair pointOnPlanarCurvePair planarCurvePair rollingCurvePair slidingCurvePair rollingSurfacePair slidingSurfacePair"/>
      </type>
      <type name="InterpolationType">
         <enumeration items="undefinedInterpolationType discontinuousInterpolationType synchronous_interpolationType linearInterpolationType"/>
      </type>
      <type name="LowOrderKinematicPairType">
         <enumeration items="revolutePair cylindricalPair sphericalPair sphericalPair_with_pin planarPair unconstrainedPair fullyConstrainedPair prismaticPair universalPair"/>
      </type>
      <type name="LowOrderKinematicPairWithMotionCouplingType">
         <enumeration items="gearPair linearFlexibleAndPinionPair rackAndPinionPair screwPair"/>
      </type>
      <type name="MotionModel">
         <select selectitems="LinkMotionAlongPath InterpolatedConfigurationModel"/>
      </type>
      <type name="ParameterValue">
         <builtintype type="REAL"/>
      </type>
      <type name="PlaneAngleMeasure">
         <builtintype type="REAL"/>
      </type>
      <type name="RotationAboutAxes">
         <aggregate lower="1" type="LIST" upper="3"/>
         <typename name="PlaneAngleMeasure"/>
      </type>
      <type name="SimpleOrSpatialRotation">
         <select selectitems="RotationAboutAxes RotationAboutDirection"/>
      </type>
      <type name="TranslationAlongAxes">
         <aggregate lower="1" type="LIST" upper="3"/>
         <typename name="LengthMeasure"/>
      </type>
      <entity name="ExternalRectangularTrimmedSurface" supertypes="ExternalSurface"/>
      <entity name="ExternalTrimmedCurve" supertypes="ExternalCurve"/>
      <entity name="HighOrderKinematicPair" supertypes="KinematicPair">
         <explicit name="kind">
            <typename name="HighOrderKinematicPairType"/>
         </explicit>
         <explicit name="model1">
            <typename name="ExternalGeometricModel"/>
         </explicit>
         <explicit name="model2" optional="YES">
            <typename name="ExternalGeometricModel"/>
         </explicit>
         <explicit name="curveOrSurface1">
            <typename name="ExternalCurveOrSurfaceSelect"/>
         </explicit>
         <explicit name="curveOrSurface2" optional="YES">
            <typename name="ExternalCurveOrSurfaceSelect"/>
         </explicit>
         <explicit name="orientation" optional="YES">
            <builtintype type="BOOLEAN"/>
         </explicit>
         <explicit name="range1" optional="YES">
            <typename name="ExternalTrimmedCurveOrSurfaceSelect"/>
         </explicit>
         <explicit name="range2" optional="YES">
            <typename name="ExternalTrimmedCurveOrSurfaceSelect"/>
         </explicit>
         <explicit name="lowerLimitYaw" optional="YES">
            <typename name="PlaneAngleMeasure"/>
         </explicit>
         <explicit name="upperLimitYaw" optional="YES">
            <typename name="PlaneAngleMeasure"/>
         </explicit>
         <explicit name="lowerLimitPitch" optional="YES">
            <typename name="PlaneAngleMeasure"/>
         </explicit>
         <explicit name="upperLimitPitch" optional="YES">
            <typename name="PlaneAngleMeasure"/>
         </explicit>
         <explicit name="lowerLimitRoll" optional="YES">
            <typename name="PlaneAngleMeasure"/>
         </explicit>
         <explicit name="upperLimitRoll" optional="YES">
            <typename name="PlaneAngleMeasure"/>
         </explicit>
      </entity>
      <entity name="InterpolatedConfigurationModel">
         <explicit name="id">
            <typename name="IdentifierSelect"/>
         </explicit>
         <explicit name="name" optional="YES">
            <typename name="DescriptorSelect"/>
         </explicit>
         <explicit name="description" optional="YES">
            <typename name="DescriptorSelect"/>
         </explicit>
         <explicit name="versionId" optional="YES">
            <typename name="IdentifierSelect"/>
         </explicit>
         <explicit name="definedIn">
            <typename name="GeometricCoordinateSpace"/>
         </explicit>
         <explicit name="parameterUnit">
            <typename name="Unit"/>
         </explicit>
         <explicit name="synchronisedSequences">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="InterpolatedConfigurationSequence"/>
         </explicit>
      </entity>
      <entity name="InterpolatedConfigurationSegment">
         <explicit name="state">
            <typename name="MechanismState"/>
         </explicit>
         <explicit name="t_parameter">
            <typename name="ParameterValue"/>
         </explicit>
         <explicit name="interpolation">
            <typename name="InterpolationType"/>
         </explicit>
      </entity>
      <entity name="InterpolatedConfigurationSequence">
         <explicit name="segments">
            <aggregate lower="2" type="LIST" upper="?"/>
            <typename name="InterpolatedConfigurationSegment"/>
         </explicit>
      </entity>
      <entity name="KinematicLink">
         <explicit name="id">
            <typename name="IdentifierSelect"/>
         </explicit>
         <explicit name="name" optional="YES">
            <typename name="DescriptorSelect"/>
         </explicit>
         <explicit name="description" optional="YES">
            <typename name="DescriptorSelect"/>
         </explicit>
         <explicit name="versionId" optional="YES">
            <typename name="IdentifierSelect"/>
         </explicit>
         <explicit name="definedIn">
            <typename name="GeometricCoordinateSpace"/>
         </explicit>
         <explicit name="ofModel" optional="YES">
            <typename name="ExternalGeometricModel"/>
         </explicit>
         <explicit name="paitFrames">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="CoordinateSystem"/>
         </explicit>
      </entity>
      <entity name="KinematicLinkToPartOccurrenceAssociation">
         <explicit name="associatedLink">
            <typename name="KinematicLink"/>
         </explicit>
         <explicit name="associatedTo">
            <typename name="AssemblyOccurrenceRelationship"/>
         </explicit>
      </entity>
      <entity name="KinematicMechanismAssociation">
         <explicit name="associatedMechanism">
            <typename name="Mechanism"/>
         </explicit>
         <explicit name="associatedTo">
            <typename name="AssemblyDefinition"/>
         </explicit>
         <explicit name="baseLink">
            <typename name="KinematicLink"/>
         </explicit>
         <unique label="ur1">
            <unique.attribute attribute="associatedTo"/>
         </unique>
      </entity>
      <entity abstract.supertype="YES" name="KinematicPair" super.expression="ONEOF (LowOrderKinematicPair, LowOrderKinematicPairWithMotionCoupling, HighOrderKinematicPair)">
         <explicit name="link1">
            <typename name="KinematicLink"/>
         </explicit>
         <explicit name="link2">
            <typename name="KinematicLink"/>
         </explicit>
         <explicit name="pairFrame1">
            <typename name="CoordinateSystem"/>
         </explicit>
         <explicit name="pairFrame2">
            <typename name="CoordinateSystem"/>
         </explicit>
         <explicit name="tx" optional="YES">
            <typename name="ActuatedDirection"/>
         </explicit>
         <explicit name="ty" optional="YES">
            <typename name="ActuatedDirection"/>
         </explicit>
         <explicit name="tz" optional="YES">
            <typename name="ActuatedDirection"/>
         </explicit>
         <explicit name="rx" optional="YES">
            <typename name="ActuatedDirection"/>
         </explicit>
         <explicit name="ry" optional="YES">
            <typename name="ActuatedDirection"/>
         </explicit>
         <explicit name="rz" optional="YES">
            <typename name="ActuatedDirection"/>
         </explicit>
      </entity>
      <entity name="KinematicPairValue">
         <explicit name="appliesToPair">
            <typename name="KinematicPair"/>
         </explicit>
         <explicit name="actualTranslation" optional="YES">
            <typename name="TranslationAlongAxes"/>
         </explicit>
         <explicit name="actualRotation" optional="YES">
            <typename name="RotationAboutAxes"/>
         </explicit>
         <explicit name="actualPointOnCurve1" optional="YES">
            <typename name="ExternalPointOnCurve"/>
         </explicit>
         <explicit name="actualPointOnCurve2" optional="YES">
            <typename name="ExternalPointOnCurve"/>
         </explicit>
         <explicit name="actualPointOnSurface1" optional="YES">
            <typename name="ExternalPointOnSurface"/>
         </explicit>
         <explicit name="actualPointOnSurface2" optional="YES">
            <typename name="ExternalPointOnSurface"/>
         </explicit>
      </entity>
      <entity name="LowOrderKinematicPair" supertypes="KinematicPair">
         <explicit name="kind">
            <typename name="LowOrderKinematicPairType"/>
         </explicit>
         <explicit name="lowerLimitActualTranslationX" optional="YES">
            <typename name="LengthMeasure"/>
         </explicit>
         <explicit name="upperLimitActualTranslationX" optional="YES">
            <typename name="LengthMeasure"/>
         </explicit>
         <explicit name="lowerLimitActualTranslationY" optional="YES">
            <typename name="LengthMeasure"/>
         </explicit>
         <explicit name="upperLimitActualTranslationY" optional="YES">
            <typename name="LengthMeasure"/>
         </explicit>
         <explicit name="lowerLimitActualTranslationZ" optional="YES">
            <typename name="LengthMeasure"/>
         </explicit>
         <explicit name="upperLimitActualTranslationZ" optional="YES">
            <typename name="LengthMeasure"/>
         </explicit>
         <explicit name="lowerLimitActualRotationX" optional="YES">
            <typename name="PlaneAngleMeasure"/>
         </explicit>
         <explicit name="upperLimitActualRotationX" optional="YES">
            <typename name="PlaneAngleMeasure"/>
         </explicit>
         <explicit name="lowerLimitActualRotationY" optional="YES">
            <typename name="PlaneAngleMeasure"/>
         </explicit>
         <explicit name="upperLimitActualRotationY" optional="YES">
            <typename name="PlaneAngleMeasure"/>
         </explicit>
         <explicit name="lowerLimitActualRotationZ" optional="YES">
            <typename name="PlaneAngleMeasure"/>
         </explicit>
         <explicit name="upperLimitActualRotationZ" optional="YES">
            <typename name="PlaneAngleMeasure"/>
         </explicit>
      </entity>
      <entity name="LowOrderKinematicPairWithMotionCoupling" supertypes="KinematicPair">
         <explicit name="kind">
            <typename name="LowOrderKinematicPairWithMotionCouplingType"/>
         </explicit>
         <explicit name="pitch" optional="YES">
            <typename name="LengthMeasure"/>
         </explicit>
         <explicit name="radius1" optional="YES">
            <typename name="LengthMeasure"/>
         </explicit>
         <explicit name="radius2" optional="YES">
            <typename name="LengthMeasure"/>
         </explicit>
         <explicit name="lowerLimitRackDisplacement" optional="YES">
            <typename name="LengthMeasure"/>
         </explicit>
         <explicit name="upperLimitRackDisplacement" optional="YES">
            <typename name="LengthMeasure"/>
         </explicit>
         <explicit name="lowerLimitActualRotation" optional="YES">
            <typename name="PlaneAngleMeasure"/>
         </explicit>
         <explicit name="upperLimitActualRotation" optional="YES">
            <typename name="PlaneAngleMeasure"/>
         </explicit>
         <explicit name="bevel" optional="YES">
            <typename name="PlaneAngleMeasure"/>
         </explicit>
         <explicit name="helical_angle" optional="YES">
            <typename name="PlaneAngleMeasure"/>
         </explicit>
         <explicit name="gear_ratio" optional="YES">
            <builtintype type="REAL"/>
         </explicit>
      </entity>
      <entity name="Mechanism">
         <explicit name="id">
            <typename name="IdentifierSelect"/>
         </explicit>
         <explicit name="name" optional="YES">
            <typename name="DescriptorSelect"/>
         </explicit>
         <explicit name="description" optional="YES">
            <typename name="DescriptorSelect"/>
         </explicit>
         <explicit name="versionId" optional="YES">
            <typename name="IdentifierSelect"/>
         </explicit>
         <explicit name="definedIn">
            <typename name="GeometricCoordinateSpace"/>
         </explicit>
         <explicit name="items">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="KinematicPair"/>
         </explicit>
      </entity>
      <entity name="MechanismState">
         <explicit name="id">
            <typename name="IdentifierSelect"/>
         </explicit>
         <explicit name="name" optional="YES">
            <typename name="DescriptorSelect"/>
         </explicit>
         <explicit name="description" optional="YES">
            <typename name="DescriptorSelect"/>
         </explicit>
         <explicit name="versionId" optional="YES">
            <typename name="IdentifierSelect"/>
         </explicit>
         <explicit name="representedMechanism">
            <typename name="Mechanism"/>
         </explicit>
         <explicit name="items">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="KinematicPairValue"/>
         </explicit>
      </entity>
      <entity name="MotionModelAssociation">
         <explicit name="associatedModel">
            <typename name="MotionModel"/>
         </explicit>
         <explicit name="associatedTo">
            <typename name="AssemblyDefinition"/>
         </explicit>
      </entity>
      <entity name="RotationAboutDirection" supertypes="DetailedGeometricModelElement">
         <explicit name="directionOfAxis">
            <typename name="Direction"/>
         </explicit>
         <explicit name="rotationAngle">
            <typename name="PlaneAngleMeasure"/>
         </explicit>
      </entity>
      <entity name="ToolAttachmentPointFrame" supertypes="CoordinateSystem"/>
      <entity name="ToolCentrePointFrame" supertypes="CoordinateSystem"/>
   </schema>
</express>
