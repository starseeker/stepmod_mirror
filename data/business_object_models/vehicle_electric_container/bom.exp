	value_ : ValueRange;
END_ENTITY;

(*PDM-merge*)
ENTITY ValueWithUnit
	ABSTRACT SUPERTYPE OF (ONEOF (NumericalValue, ValueRange));
	unitComponent : Unit;
--	id : xs:ID" use="required;
END_ENTITY;

ENTITY VariantCode;
	identification : STRING;
	codeType : OPTIONAL STRING;
	description : SET[0:?] OF AbstractLocalizedString;
INVERSE
	i1 : VariantCodeSpecification FOR variantCode;
END_ENTITY;

ENTITY VariantCodeSpecification
	SUBTYPE OF (Specification);
	variantCode : SET[0:?] OF VariantCode;
END_ENTITY;

ENTITY VariantConfiguration;
	identification : OPTIONAL STRING;
	description : SET[0:?] OF AbstractLocalizedString;
	logisticControlString : OPTIONAL STRING;
	logisticControlExpression : OPTIONAL STRING;
	configurationType : OPTIONAL STRING;
	configurationElements : SET[0:?] OF configurableElementSelect;
INVERSE
	i1 : VariantConfigurationSpecification FOR variantConfiguration;
END_ENTITY;

ENTITY VariantConfigurationSpecification
	SUBTYPE OF (Specification);
	variantConfiguration : SET[0:?] OF VariantConfiguration;
END_ENTITY;

ENTITY VariantGroup;
	identification : STRING;
	description : SET[0:?] OF AbstractLocalizedString;
	groupType : OPTIONAL STRING;
	variantCode : SET[0:?] OF VariantCode;
INVERSE
	i1 : VariantGroupSpecification FOR variantGroup;
END_ENTITY;

ENTITY VariantGroupSpecification
	SUBTYPE OF (Specification);
	variantGroup : SET[0:?] OF VariantGroup;
END_ENTITY;

ENTITY VecContent;
	vecVersion : STRING;
	generatingSystemName : OPTIONAL STRING;
	dateOfCreation : OPTIONAL dateTime;
	generatingSystemVersion : OPTIONAL STRING;
	compliantConformanceClass :  SET[0:?] OF ConformanceClass;
	contract : SET[0:?] OF Contract;
	copyrightInformation : SET[0:?] OF CopyrightInformation;
	documentVersion : SET[0:?] OF DocumentVersion;
	itemHistoryEntry : SET[0:?] OF ItemHistoryEntry;
	partVersion : SET[0:?] OF PartVersion;
	project : SET[0:?] OF Project;
	unit : SET[0:?] OF Unit;
END_ENTITY;

ENTITY WireElement;
	identification : STRING;
	wireElementSpecification : WireElementSpecification;
END_ENTITY;

ENTITY WireElementReference
	SUBTYPE OF (RoutableElement);
	identification : OPTIONAL STRING;
	connection : OPTIONAL Connection;
	referencedWireElement : WireElement;
	signal : OPTIONAL Signal;
	wireEnd : SET[0:?] OF WireEnd;
	wireLength : SET[0:?] OF WireLength;
END_ENTITY;

ENTITY WireElementSpecification
	SUBTYPE OF (Specification);
	type_ : SET[0:?] OF WireType;
	minBendRadiusDynamic : OPTIONAL NumericalValue;
	minBendRadiusStatic : OPTIONAL NumericalValue;
	outsideDiameter : OPTIONAL NumericalValue;
	suitedForDynamicUse : OPTIONAL BOOLEAN;
	impedance : OPTIONAL NumericalValue;
	size : OPTIONAL Size;
	conductorSpecification : OPTIONAL ConductorSpecification;
	fillerSpecification : OPTIONAL FillerSpecification;
	insulationSpecification : OPTIONAL InsulationSpecification;
	subWireElementSpecification : SET[0:?] OF WireElementSpecification;
	wireGroupSpecification : OPTIONAL WireGroupSpecification;
END_ENTITY;

ENTITY WireEnd;
	identification : OPTIONAL STRING;
	positionOnWire : REAL;
END_ENTITY;

ENTITY WireGroupSpecification
	SUBTYPE OF (Specification);
	groupType : STRING;
	lengthOfTwist : OPTIONAL NumericalValue;
END_ENTITY;

ENTITY WireGrouping;
	identification : STRING;
	relatedWireRole : SET[1:?] OF WireElementReference;
--	<xs:appinfo>There shall be at least 2 elements referenced.</xs:appinfo>
	wireGroupSpecification : WireGroupSpecification;
END_ENTITY;

ENTITY WireGroupingSpecification
	SUBTYPE OF (Specification);
	wireGrouping : WireGrouping;
END_ENTITY;

ENTITY WireLength;
	lengthType : STRING;
	lengthValue : NumericalValue;
END_ENTITY;

ENTITY WireMounting;
	mountedCavitySeal : OPTIONAL CavitySealRole;
	referencedWireEnd : SET[1:?] OF WireEnd;
	wireMountingDetail : SET[0:?] OF WireMountingDetail;
END_ENTITY;

ENTITY WireMountingDetail;
	contactedWireReception : WireReceptionReference;
	referencedWireEnd : SET[1:?] OF WireEnd;
END_ENTITY;

ENTITY WireProtectionRole
	SUBTYPE OF (Role);
	protectionLength : OPTIONAL NumericalValue;
	tapeOverlap : OPTIONAL STRING;
	tapingDirection : OPTIONAL TapingDirection;
	gradient : OPTIONAL ValueWithUnit;
	wireProtectionSpecification : WireProtectionSpecification;
END_ENTITY;

ENTITY WireProtectionSpecification
	SUPERTYPE OF ( ONEOF (FittingSpecification,  TapeSpecification, TubeSpecification, StripeSpecification))
	SUBTYPE OF (PartOrUsageRelatedSpecification);
	abrasionResistanceClass : SET[0:?] OF AbrasionResistanceClass;
	soundDampingClass : SET[0:?] OF SoundDampingClass;
END_ENTITY;

ENTITY WireReception;
	identification : STRING;
	placementPoint : OPTIONAL PlacementPoint;
	wireReceptionSpecification : OPTIONAL WireReceptionSpecification;
END_ENTITY;

ENTITY WireReceptionReference;
	identification : OPTIONAL STRING;
	wireReception : WireReception;
END_ENTITY;

ENTITY WireReceptionSpecification
	SUBTYPE OF (Specification);
	coreCrossSectionArea : OPTIONAL ValueRange;
	insolationDisplacementLength : OPTIONAL NumericalValue;
	multiContact : OPTIONAL BOOLEAN;
	wireReceptionType : OPTIONAL STRING;
	wireElementOutsideDiameter : OPTIONAL ValueRange;
	platingMaterial : SET[0:?] OF Material;
	sealable : OPTIONAL BOOLEAN;
END_ENTITY;

ENTITY WireRole
	SUBTYPE OF (Role);
	wireSpecification : WireSpecification;
	wireElementReference : SET[0:?] OF WireElementReference;
END_ENTITY;

ENTITY WireSpecification
	SUBTYPE OF (PartOrUsageRelatedSpecification);
	wireElementSpecification : WireElementSpecification;
	wireElement : WireElement;
END_ENTITY;

ENTITY WireType;
	type_ : STRING;
	referenceSystem : STRING;
--	id : xs:ID" use="required;
END_ENTITY;

(*PDM-merge*)
ENTITY Zone;
	identification : STRING;
	description : SET[0:?] OF AbstractLocalizedString;
	ambientTemperature : OPTIONAL TemperatureInformation;
	requiredRobustnessProperties : SET[0:?] OF RobustnessProperties;
	requiredSealingClass : SET[0:?] OF SealingClass;
	subZone : SET[0:?] OF Zone;
END_ENTITY;

