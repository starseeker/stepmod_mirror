SCHEMA VEC_modelSchema;
(*
<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:vec="http://www.prostep.org/ecad-if/vec/1.0.5/VEC_modelSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.prostep.org/ecad-if/vec/1.0.5/VEC_modelSchema" version="1.0.5;
	<!-- VEC XSD V1.0.5 2012-12-20: Compliant to VEC Modules V1.0.5

Change Log:
For issue details see the KBL-Forum https://track.prostep.com/browse/KBLFRM. To get access to the KBL Forum contact max.ungerer@prostep.com

VEC 1.0.5:
** Improvement
    * [KBLFRM-428] - Beziehung zwischen PartOrUsageRelatedSpecification & Role
    * [KBLFRM-444] - Cavity von der SlotSpecification zum Slot verschieben
    * [KBLFRM-445] - Erweiterung der TerminalRole um Terminal- & WireReceptionReferences
    * [KBLFRM-446] - SubUsageNode-Beziehung zwischen UsageNodes soll Composite sein.
    * [KBLFRM-451] - Für das Objekt "Creation" wird neben dem "Creator" auch ein "ResponsibleDesigner" benötigt
    * [KBLFRM-453] - Der "UsageNode" sollte einen "usageNodeType" erhalten
    * [KBLFRM-455] - Direkte Beziehung zwischen ComponentPort und ConnectorHousing.Cavity erforderlich
    * [KBLFRM-457] - VEC namespace (in Kleinbuchstaben)

** New Feature
    * [KBLFRM-288] - Modellierung von Bauteilen mit mehreren Bündelanschlüssen
    * [KBLFRM-408] - Use Case "Stecker oder Sonderkontakt mit mehreren Bündelanschlusspunkten"
    * [KBLFRM-431] - Referenzierung von identischen Innenleitern

VEC 1.0.4:
- No model changes
- Namespace corrected
- Ordering of Types: changed for VecContent and SimpleType
- TopologySpecification: ordering of elements changed

VEC 1.0.3:
** Bug
    * [KBLFRM-440] - Reihenfolge der Attribute im VEC 1.0.2 Schema in alphabetischer Reihenfolge
    * [KBLFRM-441] - Änderungen von NumericalValue -> double nicht vollständig erfolgt

VEC 1.0.2:
** Bug
    * [KBLFRM-352] - KILOGRAM in SIUnitNames inkorrekt. 
    * [KBLFRM-355] - CoreSpecification.coreMassInformationPerMetre ist inkonsistent modelliert. 
    * [KBLFRM-356] - CoreSpecification.coreResistancePerMetre ist inkonsistent modelliert. 
    * [KBLFRM-363] - Inkonsistes WireType / WireTypeReferenceSystem
    * [KBLFRM-369] - Sealing Class an der SlotSpecification und nicht am Slot
    * [KBLFRM-409] - Attribut Description der ModuleFamily & ModuleList besitzt den falschen Typ.
    * [KBLFRM-410] - WireElement.elementType ist verpflichtend.
    * [KBLFRM-411] - TerminalReception.GENDER verpflichtend?
    * [KBLFRM-430] - WireType außerhalb des WireElements

** Decision
    * [KBLFRM-405] - Behandlung von idrefs bei Verarbeitung von VEC-daten in mehreren Dateien, z.B. Units

	<xs:element name="VecContent : VecContent;
*)

TYPE configurableElementSelect = SELECT (
	buildingBlockPositioning2D,
	buildingBlockPositioning3D,
	componentNode,
	componentPort,
	connectionEnd,
	connectionGroup,
	contactPoint,
	dimension,
	geometryNode,
	geometrySegment,
	matingPoint,
	net,
	netGroup,
	networkNode,
	networkPort,
	occurrenceOrUsage,
	path,
	placement,
	routableElement,
	routing,
	topologyNode,
	topologySegment,
	usageNode,
	zone);
END_TYPE;	
	
TYPE date = STRING;
END_TYPE;

ENTITY AbrasionResistanceClass;
	classKey : STRING;
	referenceSystem : STRING;
--	id : xs:ID" use="required;
END_ENTITY;

ENTITY AliasIdentification;
	identificationValue : STRING;
	scope : OPTIONAL STRING;
	description : OPTIONAL LocalizedString;
--	id : xs:ID" use="required;
END_ENTITY;

ENTITY AntennaSpecification
	SUBTYPE OF (EEComponentSpecification);
	fMin : OPTIONAL NumericalValue;
	fMax : OPTIONAL NumericalValue;
	impedance : OPTIONAL NumericalValue;
END_ENTITY;

ENTITY Approval;
	identification : OPTIONAL STRING;
	description : SET[0:?] OF LocalizedString;
	companyInScope : OPTIONAL STRING;
	status : StatusOfApproval;
	levelOfApproval : OPTIONAL STRING;
	permission : SET[0:?] OF Permission;
END_ENTITY;

ENTITY BSplineCurve;
	degree : INTEGER;
	controlPoint : SET[0:?] OF CartesianPoint3D;
END_ENTITY;

ENTITY BatterySpecification
	SUBTYPE OF (EEComponentSpecification);
	u : OPTIONAL NumericalValue;
	i : OPTIONAL NumericalValue;
	iCool : OPTIONAL NumericalValue;
	capacity : OPTIONAL NumericalValue;
END_ENTITY;

ENTITY BoltMountedFixingSpecification
	SUBTYPE OF (FixingSpecification);
	boltType : OPTIONAL STRING;
	boltDiameter : OPTIONAL NumericalValue;
	boltHeight : OPTIONAL NumericalValue;
END_ENTITY;

ENTITY BuildingBlockPositioning2D;
	referenced2DBuildingBlock : BuildingBlockSpecification2D;
	centerPoint : OPTIONAL CartesianPoint2D;
END_ENTITY;

ENTITY BuildingBlockPositioning3D;
	referenced3DBuildingBlock : BuildingBlockSpecification3D;
	positioning : OPTIONAL Transformation3D;
END_ENTITY;

ENTITY BuildingBlockSpecification2D
	SUBTYPE OF (Specification);
	baseUnit : Unit;
	boundingBox : CartesianDimension;
	cartesianPoint : SET[0:?] OF CartesianPoint2D;
	geometryNode : SET[0:?] OF GeometryNode2D;
	geometrySegment : SET[0:?] OF GeometrySegment2D;
	placedElementViewItem : SET[0:?] OF OccurrenceOrUsageViewItem2D;
END_ENTITY;

ENTITY BuildingBlockSpecification3D
	SUBTYPE OF (Specification);
	zone : OPTIONAL Zone;
	baseUnit : Unit;
	cartesianPoint : SET[0:?] OF CartesianPoint3D;
	geometryNode : SET[0:?] OF GeometryNode3D;
	geometrySegment : SET[0:?] OF GeometrySegment3D;
	placedElementViewItem3D : SET[0:?] OF OccurrenceOrUsageViewItem3D;
END_ENTITY;

ENTITY CableLeadThrough;
	identification : OPTIONAL STRING;
	segmentDiameter : ValueRange;
END_ENTITY;

ENTITY CartesianDimension;
	height : REAL;
	width : REAL;
END_ENTITY;

ENTITY CartesianPoint2D;
	x : REAL;
	y : REAL;
	sheet : OPTIONAL SheetOrChapter;
END_ENTITY;

ENTITY CartesianPoint3D;
	x : REAL;
	y : REAL;
	z : REAL;
END_ENTITY;

ENTITY CartesianVector
	aBSTRACT SUPERTYPE OF (ONEOF (CartesianVector2D, CartesianVector3D));
--	id : xs:ID" use="required;
END_ENTITY;

ENTITY CartesianVector2D
	SUBTYPE OF (CartesianVector);
	x : REAL;
	y : REAL;
END_ENTITY;

ENTITY CartesianVector3D
	SUBTYPE OF (CartesianVector);
	x : REAL;
	y : REAL;
	z : REAL;
END_ENTITY;

ENTITY Cavity;
	available : OPTIONAL BOOLEAN;
	cavityNumber : OPTIONAL STRING;
	wireAddOn : OPTIONAL NumericalValue;
	cavitySpecification : OPTIONAL CavitySpecification;
END_ENTITY;

ENTITY CavityMapping;
	identificationA : STRING;
	identificationB : STRING;
--	id : xs:ID" use="required;
END_ENTITY;

ENTITY CavityMounting;
	equippedCavityRef : SET[1:?] OF CavityReference;
	replacedPlug : SET[0:?] OF CavityPlugRole;
	cavityMountingDetail : SET[0:?] OF CavityMountingDetail;
END_ENTITY;

ENTITY CavityMountingDetail;
	equippedCavityRef : CavityReference;
	terminalReceptionReference : TerminalReceptionReference;
END_ENTITY;

ENTITY CavityPlugRole
	SUBTYPE OF (Role);
	cavityPlugSpecification : CavityPlugSpecification;
	pluggedCavityRef : SET[0:?] OF CavityReference;
END_ENTITY;

ENTITY CavityPlugSpecification
	SUBTYPE OF (PartOrUsageRelatedSpecification);
	cavityDiameter : OPTIONAL ValueRange;
	hardness : OPTIONAL NumericalValue;
	sealingClass : SET[0:?] OF SealingClass;
END_ENTITY;

ENTITY CavityReference;
	identification : OPTIONAL STRING;
	componentPort : OPTIONAL ComponentPort;
	referencedCavity : Cavity;
END_ENTITY;

ENTITY CavitySealRole
	SUBTYPE OF (Role);
	cavitySealSpecification : CavitySealSpecification;
END_ENTITY;

ENTITY CavitySealSpecification
	SUBTYPE OF (PartOrUsageRelatedSpecification);
	cavityDiameter : OPTIONAL ValueRange;
	hardness : OPTIONAL NumericalValue;
	sealingClass : SET[0:?] OF SealingClass;
	wireElementOutsideDiameter : OPTIONAL ValueRange;
	wireReceptionType : OPTIONAL STRING;
END_ENTITY;

ENTITY CavitySpecification
	SUBTYPE OF (Specification);
	angle : LIST [0:2] OF NumericalValue;
	cavityDesign : OPTIONAL STRING;
	cavityType : OPTIONAL STRING;
	cavityDiameter : OPTIONAL NumericalValue;
	maxWireElementOutsideDiameter : OPTIONAL NumericalValue;
	nominalTerminalSize : OPTIONAL STRING;
	primaryLocking : OPTIONAL BOOLEAN;
	sealable : OPTIONAL BOOLEAN;
END_ENTITY;

ENTITY ChangeDescription;
	identification : OPTIONAL STRING;
	label : OPTIONAL STRING;
	description : SET[0:?] OF LocalizedString;
	approver : OPTIONAL Person;
	changeDate : OPTIONAL date;
	responsibleDesigner : Person;
	relatedChangeRequest : OPTIONAL STRING;
END_ENTITY;

ENTITY Coding;
	coding : STRING;
END_ENTITY;

ENTITY Color;
	key : STRING;
	referenceSystem : STRING;
	description : SET[0:?] OF LocalizedString;
--	id : xs:ID" use="required;
END_ENTITY;

ENTITY CompatibilitySpecification
	SUBTYPE OF (Specification);
	compatibilityStatement : SET[0:?] OF CompatibilityStatement;
END_ENTITY;

ENTITY CompatibilityStatement;
	hasCompatibility : BOOLEAN;
	cavity : SET[0:?] OF Cavity;
	cavityPlugSpecification : SET[0:?] OF CavityPlugSpecification;
	cavitySealSpecification : SET[0:?] OF CavitySealSpecification;
	cavitySpecification : SET[0:?] OF CavitySpecification;
	connectorHousingSpecification : SET[0:?] OF ConnectorHousingSpecification;
	slot : SET[0:?] OF Slot;
	terminalSpecification : SET[0:?] OF TerminalSpecification;
	wireSpecification : SET[0:?] OF WireSpecification;
END_ENTITY;

ENTITY ComponentNode;
	identification : STRING;
	abbreviation : OPTIONAL STRING;
	componentNodeType : OPTIONAL STRING;
	description : SET[0:?] OF LocalizedString;
	networkNode : OPTIONAL NetworkNode;
	realizedUsageNode : OPTIONAL UsageNode;
	childNode : SET[0:?] OF ComponentNode;
	componentPort : SET[0:?] OF ComponentPort;
END_ENTITY;

ENTITY ComponentPort;
	identification : STRING;
	signalDirection : OPTIONAL SignalDirection;
	description : OPTIONAL LocalizedString;
	networkPort : OPTIONAL NetworkPort;
	signal : OPTIONAL Signal;
END_ENTITY;

ENTITY CompositionSpecification
	SUBTYPE OF (Specification);
	component : SET[0:?] OF PartOccurrence;
END_ENTITY;

ENTITY ConformanceClass;
	identification : STRING;
	version : STRING;
	url : OPTIONAL STRING;
END_ENTITY;

ENTITY Connection
	SUBTYPE OF (RoutableElement);
	identification : STRING;
	description : SET[0:?] OF LocalizedString;
	installationInstruction : SET[0:?] OF Instruction ;
	net : OPTIONAL Net;
	signal : Signal;
	connectionEnd : SET[2:?] OF ConnectionEnd;
END_ENTITY;

ENTITY ConnectionEnd;
	identification : STRING;
	isExternalEnd : BOOLEAN;
	gender : OPTIONAL STRING;
	installationInstruction : SET[0:?] OF Instruction ;
	connectedComponentPort : ComponentPort;
END_ENTITY;

ENTITY ConnectionGroup;
	identification : OPTIONAL STRING;
	connectionGroupType : STRING;
	installationInstruction : SET[0:?] OF Instruction ;
	description : SET[0:?] OF LocalizedString;
	connection : SET[1:?] OF Connection;
--	<xs:appinfo>There shall be at least 2 elements referenced.</xs:appinfo>
	netGroup : OPTIONAL NetGroup;
	subGroup : SET[0:?] OF ConnectionGroup;
END_ENTITY;

ENTITY ConnectionSpecification
	SUBTYPE OF (Specification);
	componentNode : SET[0:?] OF ComponentNode;
	connection : SET[0:?] OF Connection;
	connectionGroup : SET[0:?] OF ConnectionGroup;
END_ENTITY;

ENTITY ConnectorHousingRole
	SUBTYPE OF (Role);
	sealState : OPTIONAL SealState;
	componentNode : OPTIONAL ComponentNode;
	connectorHousingSpecification : ConnectorHousingSpecification;
	segmentConnectionPointReference : SET[0:?] OF SegmentConnectionPointReference;
	slotReference : SET[0:?] OF SlotReference;
END_ENTITY;

ENTITY ConnectorHousingSpecification
	SUBTYPE OF (PartOrUsageRelatedSpecification);
	averageWireLengthAddOn : OPTIONAL NumericalValue;
	voltageRange : OPTIONAL ValueRange;
	coupleable : OPTIONAL BOOLEAN;
	segmentConnectionPoint : SET[0:?] OF SegmentConnectionPoint;
	slot : SET[0:?] OF Slot;
END_ENTITY;

ENTITY ContactPoint;
	identification : OPTIONAL STRING;
	mountedTerminal : OPTIONAL TerminalRole;
	cavityMounting : SET[0:?] OF CavityMounting;
	wireMounting : SET[0:?] OF WireMounting;
END_ENTITY;

ENTITY ContactSystem;
	contactResistance : OPTIONAL NumericalValue;
	joiningForce : OPTIONAL NumericalValue;
	firstTerminal : PartVersion;
	referencedCoreSpecification : CoreSpecification;
	secondTerminal : PartVersion;
END_ENTITY;

ENTITY ContactSystemSpecification
	SUBTYPE OF (Specification);
	contactSystem : SET[0:?] OF ContactSystem;
END_ENTITY;

ENTITY ContactingSpecification
	SUBTYPE OF (Specification);
	contactPoint : SET[0:?] OF ContactPoint;
END_ENTITY;

ENTITY Contract;
	companyName : STRING;
	contractRole : ContractRole;
END_ENTITY;

ENTITY CopyrightInformation;
	copyrightNote : LocalizedString;
	item : SET[1:?] OF ItemVersion;
END_ENTITY;

ENTITY CoreCurrentInformation;
	maxCurrent : NumericalValue;
	environmentTemperature : NumericalValue;
	voltage : NumericalValue;
--	id : xs:ID" use="required;
END_ENTITY;

ENTITY CoreSpecification
	SUBTYPE OF (Specification);
	crossSectionArea : OPTIONAL NumericalValue;
	massInformation : OPTIONAL MassInformation;
	material : SET[0:?] OF Material;
	outsideDiameter : OPTIONAL NumericalValue;
	resistance : OPTIONAL NumericalValue;
	structure : OPTIONAL STRING;
	type_ : OPTIONAL STRING;
	currentInformation : SET[0:?] OF CoreCurrentInformation;
	numberOfStrands : OPTIONAL NumericalValue;
	platingMaterial : SET[0:?] OF Material;
	strandCrossSectionArea : OPTIONAL NumericalValue;
	voltageRange : OPTIONAL NumericalValue;
END_ENTITY;

ENTITY CorrugatedPipeSpecification
	SUBTYPE OF (TubeSpecification);
	corrugationHeight : OPTIONAL NumericalValue;
	corrugationWidth : OPTIONAL NumericalValue;
	corrugationGradient : OPTIONAL NumericalValue;
END_ENTITY;

ENTITY Creation;
	creationDate : date;
	creator : OPTIONAL Person;
	responsibleDesigner : OPTIONAL Person;
END_ENTITY;

ENTITY CustomProperty
	aBSTRACT SUPERTYPE OF (ONEOF (NumericalValueProperty, SimpleValueProperty, ValueRangeProperty));
	propertyType : STRING;
--	id : xs:ID" use="required;
END_ENTITY;

ENTITY Dimension;
	dimensionValue : NumericalValue;
	dimensionLocation : Location;
	referenceLocation : Location;
	path : OPTIONAL Path;
	tolerance : OPTIONAL Tolerance;
END_ENTITY;

ENTITY DocumentBasedInstruction
	SUBTYPE OF (Instruction);
	referencedDocument : DocumentVersion;
	referencedSheetOrChapter : OPTIONAL SheetOrChapter;
END_ENTITY;

ENTITY DocumentVersion
	SUBTYPE OF (ItemVersion);
	documentNumber : STRING;
	documentType : OPTIONAL STRING;
	documentVersion : STRING;
	creatingSystem : OPTIONAL STRING;
	dataFormat : OPTIONAL STRING;
	fileName : OPTIONAL STRING;
	location : OPTIONAL STRING;
	numberOfSheets : OPTIONAL STRING;
	referencedPart : SET[0:?] OF PartVersion;
	relatedDocument : SET[0:?] OF DocumentVersion;
	itemEquivalence : SET[0:?] OF ItemEquivalence;
	sheetOrChapter : SET[0:?] OF SheetOrChapter;
	specification : SET[0:?] OF Specification;
END_ENTITY;

ENTITY EEComponentRole
	SUBTYPE OF (Role);
	eEComponentSpecification : EEComponentSpecification;
	componentNode : OPTIONAL ComponentNode;
	mountingCavity : SET[0:?] OF CavityReference;
	mountingHousing : SET[0:?] OF ConnectorHousingRole;
	mountingSlot : SET[0:?] OF SlotReference;
	housingComponentRef : SET[0:?] OF HousingComponentReference;
END_ENTITY;

ENTITY EEComponentSpecification
	SUPERTYPE OF (ONEOF (BatterySpecification, RelaySpecification, AntennaSpecification, FuseSpecification))
	SUBTYPE OF (PartOrUsageRelatedSpecification);
	housingComponent :  SET[0:?] OF HousingComponent;
END_ENTITY;

TYPE ExtendableElementSelect = SELECT (
		Approval, 
		BSplineCurve, 
		CableLeadThrough,
		CartesianDimension,
		CartesianPoint2D,
		CartesianPoint3D,
		Cavity,
		CavityMounting,
		CavityMountingDetail,
		CavityReference,
		ChangeDescription,
		Coding,
		CompatibilityStatement,
		configurableElementSelect,
		ConformanceClass,
		ContactSystem,
		Contract,
		CopyrightInformation,
		Creation,
		HousingComponent,
		Instruction,
		InternalTerminalConnection,
		ItemEquivalence,
		ItemHistoryEntry,
		ItemVersion,
		Location,
		Mapping,
		MatingDetail,
		ModuleFamily,
		ModuleList,
		OccurrenceOrUsageViewItem2D,
		OccurrenceOrUsageViewItem3D,
		Outlet,
		PathSegment,
		Permission,
		PinComponent,
		Project,
		Role,
		RoutingDestinationReference,
		SegmentConnectionPoint,
		SegmentCrossSectionArea,
		SegmentLength,
		SheetOrChapter,
		Signal,
		Slot,
		Specification,
		TerminalReception,
		Tolerance,
		Transformation2D,
		Transformation3D,
		UsageConstraint,
		VariantCode,
		VariantConfiguration,
		VariantGroup,
		VecContent,
		WireElement,
		WireEnd,
		WireGrouping,
		WireLength,
		WireMounting,
		WireMountingDetail,
		WireReception);
END_TYPE;

ENTITY ExtendableElement;
	target : ExtendableElementSelect;
	customProperty : SET[0:?] OF CustomProperty;
--	id : xs:ID" use="required;
END_ENTITY;

ENTITY FileBasedInstruction
	SUBTYPE OF (Instruction);
	fileName : STRING;
	lastModified : OPTIONAL date;
	dataFormat : OPTIONAL STRING;
	location : OPTIONAL STRING;
END_ENTITY;

ENTITY FittingSpecification
	SUBTYPE OF (WireProtectionSpecification);
	form : OPTIONAL STRING;
	outlet : SET[0:?] OF Outlet;
END_ENTITY;

ENTITY FixingRole
	SUBTYPE OF (Role);
	fixingSpecification : FixingSpecification;
END_ENTITY;

ENTITY FixingSpecification
	SUPERTYPE OF ( ONEOF (HoleMountedFixingSpecification, BoltMountedFixingSpecification))
	SUBTYPE OF (PartOrUsageRelatedSpecification);
	offset : OPTIONAL NumericalValue;
	segmentDiameter : OPTIONAL ValueRange;
END_ENTITY;

ENTITY FuseSpecification
	SUBTYPE OF (EEComponentSpecification);
	fuseType : OPTIONAL STRING;
	iMax : OPTIONAL NumericalValue;
END_ENTITY;

ENTITY GeneralTechnicalPartSpecification
	SUBTYPE OF (PartOrUsageRelatedSpecification);
	colorInformation : OPTIONAL Color;
	massInformation : SET[0:?] OF MassInformation ;
	materialInformation : SET[0:?] OF Material;
	robustnessProperties : SET[0:?] OF RobustnessProperties;
	temperatureInformation : SET[0:?] OF TemperatureInformation;
	partRelation : SET[0:?] OF PartRelation;
END_ENTITY;

ENTITY GeometryNode
	aBSTRACT SUPERTYPE OF ( ONEOF (GeometryNode2D, GeometryNode3D));
	identification : STRING;
	aliasId : SET[0:?] OF AliasIdentification;
	referenceNode : OPTIONAL TopologyNode;
END_ENTITY;

ENTITY GeometryNode2D
	SUBTYPE OF (GeometryNode);
	cartesianPoint : CartesianPoint2D;
END_ENTITY;

ENTITY GeometryNode3D
	SUBTYPE OF (GeometryNode);
	cartesianPoint : CartesianPoint3D;
END_ENTITY;

ENTITY GeometrySegment
	aBSTRACT SUPERTYPE OF (ONEOF (GeometrySegment2D, GeometrySegment3D));
	identification : STRING;
	aliasId : SET[0:?] OF AliasIdentification;
	referenceSegment : OPTIONAL TopologySegment;
END_ENTITY;

ENTITY GeometrySegment2D
	SUBTYPE OF (GeometrySegment);
	startVector : CartesianVector2D;
	endVector : CartesianVector2D;
	endNode : GeometryNode2D;
	startNode : GeometryNode2D;
	pathSegment : SET[0:?] OF PathSegment;
END_ENTITY;

ENTITY GeometrySegment3D
	SUBTYPE OF (GeometrySegment);
	startVector : CartesianVector3D;
	endVector : CartesianVector3D;
	endNode : GeometryNode3D;
	startNode : GeometryNode3D;
	bSplineCurve : SET[0:?] OF BSplineCurve;
END_ENTITY;

ENTITY GrommetRole
	SUBTYPE OF (Role);
	grommetSpecification : GrommetSpecification;
END_ENTITY;

ENTITY GrommetSpecification
	SUBTYPE OF (PartOrUsageRelatedSpecification);
	hardness : OPTIONAL NumericalValue;
	holeDiameter : OPTIONAL NumericalValue;
	plateThickness : OPTIONAL ValueRange;
	grommetType : OPTIONAL STRING;
	cableLeadThrough : SET[0:?] OF CableLeadThrough;
END_ENTITY;

ENTITY HarnessDrawingSpecification2D
	SUBTYPE OF (Specification);
	buildingBlockPositionings : BuildingBlockPositioning2D;
END_ENTITY;

ENTITY HarnessGeometrySpecification3D
	SUBTYPE OF (Specification);
	type_ : GeometryType;
	buildingBlockPositionings : SET[0:?] OF BuildingBlockPositioning3D;
END_ENTITY;

ENTITY HoleMountedFixingSpecification
	SUBTYPE OF (FixingSpecification);
	holeDiameter : OPTIONAL NumericalValue;
	plateThickness : OPTIONAL ValueRange;
END_ENTITY;

ENTITY HousingComponent;
	identification : STRING;
	housingSpecification : OPTIONAL ConnectorHousingSpecification;
	pinComponent : SET[0:?] OF PinComponent;
END_ENTITY;

ENTITY HousingComponentReference;
	identification : OPTIONAL STRING;
	housingComponent : HousingComponent;
	pinComponentRef : SET[0:?] OF PinComponentReference;
--	id : xs:ID" use="required;
END_ENTITY;

ENTITY Instruction
	aBSTRACT SUPERTYPE OF (ONEOF (TextBasedInstruction, DocumentBasedInstruction, FileBasedInstruction));
END_ENTITY;

ENTITY InsulationSpecification
	SUBTYPE OF (Specification);
	baseColor : SET[0:?] OF Color;
	firstIdentificationColor : SET[0:?] OF Color;
	secondIdentificationColor : SET[0:?] OF Color;
	labelIdentificationColor : SET[0:?] OF Color;
	labelIdentificationType : OPTIONAL STRING;
	labelIdentificationValue : OPTIONAL STRING;
	material : SET[0:?] OF Material;
	thickness : OPTIONAL NumericalValue;
END_ENTITY;

ENTITY InternalTerminalConnection;
	identification : STRING;
	terminalReception : SET[0:?] OF TerminalReception;
	wireReception : SET[0:?] OF WireReception;
END_ENTITY;

ENTITY ItemEquivalence;
	companyName : STRING;
	item : SET[1:?] OF ItemVersion;
--	<xs:appinfo>There shall be at least 2 elements referenced.</xs:appinfo>
END_ENTITY;

ENTITY ItemHistoryEntry;
	type_ : HistoryEntryType;
	predecessorVersion : ItemVersion;
	successorVersion : ItemVersion;
END_ENTITY;

ENTITY ItemVersion
	aBSTRACT SUPERTYPE OF (ONEOF (PartVersion, DocumentVersion));
	abbreviation : OPTIONAL STRING;
	description : SET[0:?] OF LocalizedString;
	companyName : STRING;
	contract : SET[0:?] OF Contract;
	approval : SET[0:?] OF Approval ;
	changeDescription : SET[0:?] OF ChangeDescription ;
	creation : OPTIONAL Creation;
END_ENTITY;

ENTITY LocalizedString;
	languageCode : LanguageCode;
	value_ : STRING;
--	id : xs:ID" use="required;
END_ENTITY;

ENTITY Location
	aBSTRACT SUPERTYPE Of (ONEOF (SegmentLocation, NodeLocation));
	identification : STRING;
	matchingId : OPTIONAL STRING;
END_ENTITY;

ENTITY Mapping;
	a : PartVersion;
	b : PartVersion;
--	SlotMapping : SlotSET[0:?] OF Mapping ;
END_ENTITY;

ENTITY MappingSpecification
	SUBTYPE OF (Specification);
	mapping : SET[0:?] OF Mapping ;
END_ENTITY;

ENTITY MassInformation;
	determinationType : OPTIONAL ValueDetermination;
	value_ : NumericalValue;
--	id : xs:ID" use="required;
END_ENTITY;

ENTITY Material;
	key : STRING;
	referenceSystem : STRING;
