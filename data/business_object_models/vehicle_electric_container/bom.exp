	tapeOverlap : OPTIONAL STRING;
	tapingDirection : OPTIONAL TapingDirection;
	gradient : OPTIONAL ValueWithUnit;
	wireProtectionSpecification : WireProtectionSpecification;
END_ENTITY;

ENTITY WireProtectionSpecification
	SUPERTYPE OF ( ONEOF (FittingSpecification,  TapeSpecification, TubeSpecification, StripeSpecification))
	SUBTYPE OF (PartOrUsageRelatedSpecification);
	abrasionResistanceClass : SET[0:?] OF AbrasionResistanceClass;
	soundDampingClass : SET[0:?] OF SoundDampingClass;
END_ENTITY;

ENTITY WireReception;
	identification : STRING;
	wireReceptionSpecification : OPTIONAL WireReceptionSpecification;
END_ENTITY;

ENTITY WireReceptionReference
	SUBTYPE OF (RoutingDestinationReference);
	identification : OPTIONAL STRING;
	wireReception : WireReception;
END_ENTITY;

ENTITY WireReceptionSpecification
	SUBTYPE OF (Specification);
	coreCrossSectionArea : OPTIONAL ValueRange;
	insolationDisplacementLength : OPTIONAL NumericalValue;
	multiContact : OPTIONAL BOOLEAN;
	wireReceptionType : OPTIONAL STRING;
	wireElementOutsideDiameter : OPTIONAL ValueRange;
	platingMaterial : SET[0:?] OF Material;
	sealable : OPTIONAL BOOLEAN;
END_ENTITY;

ENTITY WireRole
	SUBTYPE OF (Role);
	wireSpecification : WireSpecification;
	wireElementReference : SET[0:?] OF WireElementReference;
END_ENTITY;

ENTITY WireSpecification
	SUBTYPE OF (PartOrUsageRelatedSpecification);
	wireElementSpecification : WireElementSpecification;
	wireElement : WireElement;
END_ENTITY;

ENTITY WireType;
	type_ : STRING;
	referenceSystem : STRING;
--	id : xs:ID" use="required;
END_ENTITY;

ENTITY Zone;
	identification : STRING;
	description : OPTIONAL LocalizedString;
	subZone : SET[0:?] OF Zone;
END_ENTITY;

