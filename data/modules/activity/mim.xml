<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.9 2002/02/25 12:10:19 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $"
  description.file="mim_descriptions.xml">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.9"
    source="plcs_mim_ge13v59v5.exp"/>

  <schema name="activity_mim">
    <interface
      kind="use"
      schema="action_schema">
    </interface>

    <interface
      kind="use"
      schema="approval_mim">
    </interface>

    <interface
      kind="use"
      schema="configuration_management_schema">
    </interface>

    <interface
      kind="use"
      schema="contract_mim">
    </interface>

    <interface
      kind="use"
      schema="date_time_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="person_organization_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="product_definition_schema">
    </interface>

    <interface
      kind="use"
      schema="product_identification_mim">
    </interface>

    <type name="action_approval_item">
      <select
        basedon="approval_item"
        selectitems="action">
      </select>

    </type>
    <type name="action_contract_item">
      <select
        basedon="contract_item"
        selectitems="action">
      </select>

    </type>
    <type name="action_date_and_time_item">
      <select
        basedon="date_and_time_item"
        selectitems="action">
      </select>

    </type>
    <type name="action_date_item">
      <select
        basedon="date_item"
        selectitems="action">
      </select>

    </type>
    <type name="action_item">
      <select
        extensible="YES"
        selectitems="configuration_effectivity product_definition product_definition_formation product_definition_relationship">
      </select>

    </type>
    <type name="action_organization_item">
      <select
        basedon="organization_item"
        selectitems="action">
      </select>

    </type>
    <type name="action_person_and_organization_item">
      <select
        basedon="person_and_organization_item"
        selectitems="action">
      </select>

    </type>
    <entity
      name="applied_action_assignment"
      supertypes="action_assignment">
      <explicit
        name="items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="action_item"/>
      </explicit>
    </entity>

  </schema>

</express>