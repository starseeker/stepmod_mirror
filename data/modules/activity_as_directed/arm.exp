{ iso standard 10303 part (11) version (4) }

SCHEMA Activity_as_directed_arm;

USE FROM Activity_arm;

USE FROM Work_order_arm;

ENTITY Directed_activity
SUBTYPE OF (Activity);
  directive : Work_order;
END_ENTITY;

END_SCHEMA;

SCHEMA Work_order_arm;

ENTITY Work_order;
END_ENTITY;

END_SCHEMA;

SCHEMA Activity_arm;

ENTITY Activity;
END_ENTITY;

END_SCHEMA;
