SCHEMA Activity_as_realized_arm;

USE FROM Activity_arm;

ENTITY Activity_actual
SUBTYPE OF (Activity);
END_ENTITY;

ENTITY Activity_happening
SUBTYPE OF (Activity_relationship);
  SELF\Activity_relationship.related RENAMED actual : Activity_actual;
  SELF\Activity_relationship.relating RENAMED predicted : Activity;
END_ENTITY;

END_SCHEMA;

