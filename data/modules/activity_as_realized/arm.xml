<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.12 2002/06/06 07:52:56 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.12"
    source="../data/modules/Activity_as_realized/arm.exp"/>

  <schema name="activity">
    <interface
      kind="use"
      schema="activity_as_realized">
    </interface>

    <entity
      name="activity">
    </entity>

    <entity
      name="activity_relationship">
    </entity>

  </schema>

  <schema name="activity_as_realized">
    <interface
      kind="use"
      schema="activity">
    </interface>

    <entity
      name="activity_actual"
      supertypes="activity">
    </entity>

    <entity
      name="activity_happening"
      supertypes="activity_relationship">
      <explicit
        name="actual">
        <typename
          name="activity_actual"/>
      </explicit>
      <explicit
        name="predicted">
        <typename
          name="activity"/>
      </explicit>
    </entity>

  </schema>

</express>
