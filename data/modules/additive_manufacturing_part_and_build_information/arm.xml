<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?><!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="">
   <application name="JSDAI" owner="LKSoft" source="additive_manufacturing_part_and_build_information_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Additive_manufacturing_part_and_build_information_arm">
      <interface kind="use" schema="Construction_geometry_arm"/>
      <interface kind="use" schema="Elemental_geometric_shape_arm"/>
      <interface kind="use" schema="Foundation_representation_arm"/>
      <interface kind="use" schema="Item_definition_structure_arm"/>
      <interface kind="use" schema="Part_shape_arm"/>
      <interface kind="use" schema="Part_view_definition_arm"/>
      <interface kind="use" schema="Process_plan_arm"/>
      <interface kind="use" schema="Property_assignment_arm"/>
      <interface kind="use" schema="Value_with_unit_extension_arm"/>
      <type name="ampubi_constructive_element_select">
         <select basedon="constructive_element_select" genericentity="YES" selectitems="Direction"/>
      </type>
      <type name="ampubi_initial_application_domain_enumeration">
         <enumeration basedon="initial_application_domain_enumeration" items="additive_manufacturing"/>
      </type>
      <entity name="Additive_manufacturing_build_direction_element" supertypes="Additive_manufacturing_orientation_element">
         <explicit name="identified_item">
            <typename name="Direction"/>
            <redeclaration entity-ref="Shape_element"/>
         </explicit>
         <derived expression="'build orientation z'" name="element_name">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="Shape_element"/>
         </derived>
      </entity>
      <entity name="Additive_manufacturing_orientation_element" supertypes="Part_shape_element">
         <explicit name="associated_definition">
            <typename name="Additive_manufacturing_part_definition"/>
            <redeclaration entity-ref="Shape_element"/>
         </explicit>
      </entity>
      <entity name="Additive_manufacturing_part_definition" supertypes="Part_view_definition">
         <inverse attribute="relating_view" entity="Geometrical_relationship" name="design_relationship"/>
         <where expression="SELF\Product_view_definition.initial_context\Initial_view_definition_context.application_domain = ampubi_initial_application_domain_enumeration.additive_manufacturing" label="WR1"/>
      </entity>
      <entity name="Additive_manufacturing_property_assignment" supertypes="Assigned_property">
         <explicit name="described_element">
            <typename name="Additive_manufacturing_part_definition"/>
            <redeclaration entity-ref="Assigned_property"/>
         </explicit>
      </entity>
      <entity name="Additive_manufacturing_setup" supertypes="Product_occurrence">
         <explicit name="its_origin">
            <typename name="Axis_placement_3d"/>
         </explicit>
         <inverse attribute="relating_view" entity="Additive_manufacturing_workpiece_setup" name="its_workpiece_setup">
            <inverse.aggregate lower="1" type="SET" upper="?"/>
         </inverse>
         <inverse attribute="relating_view" entity="Additive_manufacturing_support_setup" name="its_support_setup">
            <inverse.aggregate lower="0" type="SET" upper="?"/>
         </inverse>
         <inverse attribute="relating_view" entity="Additive_manufacturing_setup_build_plate" name="its_build_plate_setup"/>
      </entity>
      <entity name="Additive_manufacturing_setup_build_plate" supertypes="Product_occurrence_definition_relationship">
         <explicit name="its_build_plate">
            <typename name="Build_platform_definition"/>
            <redeclaration entity-ref="Product_occurrence_definition_relationship" old_name="related_view"/>
         </explicit>
         <explicit name="relating_view">
            <typename name="Additive_manufacturing_setup"/>
            <redeclaration entity-ref="Product_occurrence_definition_relationship"/>
         </explicit>
         <inverse attribute="product_context" entity="Contextual_shape_representation" name="placement"/>
      </entity>
      <entity name="Additive_manufacturing_support_setup" supertypes="Product_occurrence_definition_relationship">
         <explicit name="its_support">
            <typename name="Additive_manufacturing_support_structure"/>
            <redeclaration entity-ref="Product_occurrence_definition_relationship" old_name="related_view"/>
         </explicit>
         <explicit name="relating_view">
            <typename name="Additive_manufacturing_setup"/>
            <redeclaration entity-ref="Product_occurrence_definition_relationship"/>
         </explicit>
         <inverse attribute="product_context" entity="Contextual_shape_representation" name="placement"/>
      </entity>
      <entity name="Additive_manufacturing_support_structure" supertypes="Part_view_definition">
         <where expression="SELF\Product_view_definition.initial_context\Initial_view_definition_context.application_domain = ampubi_initial_application_domain_enumeration.additive_manufacturing" label="WR1"/>
      </entity>
      <entity name="Additive_manufacturing_workpiece_setup" supertypes="Product_occurrence_definition_relationship">
         <explicit name="its_workpiece">
            <typename name="Additive_manufacturing_part_definition"/>
            <redeclaration entity-ref="Product_occurrence_definition_relationship" old_name="related_view"/>
         </explicit>
         <explicit name="relating_view">
            <typename name="Additive_manufacturing_setup"/>
            <redeclaration entity-ref="Product_occurrence_definition_relationship"/>
         </explicit>
         <inverse attribute="product_context" entity="Contextual_shape_representation" name="placement"/>
      </entity>
      <entity name="Additive_manufacturing_workplan" supertypes="Executable">
         <explicit name="its_setup">
            <typename name="Additive_manufacturing_setup"/>
         </explicit>
      </entity>
      <entity name="Additive_manufacturing_x_direction_element" supertypes="Additive_manufacturing_orientation_element">
         <explicit name="identified_item">
            <typename name="Direction"/>
            <redeclaration entity-ref="Shape_element"/>
         </explicit>
         <derived expression="'build orientation x'" name="element_name">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="Shape_element"/>
         </derived>
      </entity>
      <entity name="Build_platform_definition" supertypes="Product_view_definition">
         <explicit name="build_volume_representation">
            <typename name="Build_volume"/>
         </explicit>
         <explicit name="build_platform_representation" optional="YES">
            <typename name="Geometric_model"/>
         </explicit>
      </entity>
      <entity abstract.supertype="YES" name="Build_volume" super.expression="ONEOF (Rectangular_build_volume, Circular_build_volume)" supertypes="Geometric_model">
         <explicit name="its_origin">
            <typename name="Axis_placement_3d"/>
         </explicit>
      </entity>
      <entity name="Circular_build_volume" supertypes="Build_volume">
         <explicit name="diameter">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="height" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
      </entity>
      <entity name="Executable" supertypes="Process_operation_definition"/>
      <entity name="Rectangular_build_volume" supertypes="Build_volume">
         <explicit name="x">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="y">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="z" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
      </entity>
      <subtype.constraint entity="Part_view_definition" name="Additive_manufacturing_part_view_subtypes" super.expression="ONEOF (Additive_manufacturing_part_definition, Additive_manufacturing_support_structure)"/>
   </schema>
</express>
