<ext_description linkend="Additive_manufacturing_part_and_build_information_mim">
</ext_description><!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Additive_manufacturing_part_and_build_information_mim  Entity: additive_manufacturing_build_plate_relationship  
   <express_ref linkend="additive_manufacturing_part_and_build_information:mim:Additive_manufacturing_part_and_build_information_mim.additive_manufacturing_build_plate_relationship"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Additive_manufacturing_part_and_build_information_mim.additive_manufacturing_build_plate_relationship">
        An <b>additive_manufacturing_build_plate_relationship</b> is a type of
        <express_ref linkend="additive_manufacturing_part_and_build_information:mim:Additive_manufacturing_part_and_build_information_mim.additive_manufacturing_setup_relationship"/>  that specifies the usage of a build plate in the setup.
</ext_description><!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Additive_manufacturing_part_and_build_information_mim  Entity: additive_manufacturing_setup  
   <express_ref linkend="additive_manufacturing_part_and_build_information:mim:Additive_manufacturing_part_and_build_information_mim.additive_manufacturing_setup"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Additive_manufacturing_part_and_build_information_mim.additive_manufacturing_setup">
        An <b>additive_manufacturing_setup</b> is a type of
        <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product" /> that specifies the setup for one or more workpieces.
</ext_description><!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Additive_manufacturing_part_and_build_information_mim  Entity: additive_manufacturing_setup_relationship  
   <express_ref linkend="additive_manufacturing_part_and_build_information:mim:Additive_manufacturing_part_and_build_information_mim.additive_manufacturing_setup_relationship"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Additive_manufacturing_part_and_build_information_mim.additive_manufacturing_setup_relationship">
        An <b>additive_manufacturing_setup_relationship</b> is a type of
        <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_relationship" /> that specifies the usage of a workpiece or a build plate in the setup.
</ext_description><!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Additive_manufacturing_part_and_build_information_mim  Entity: additive_manufacturing_setup_workpiece_relationship  
   <express_ref linkend="additive_manufacturing_part_and_build_information:mim:Additive_manufacturing_part_and_build_information_mim.additive_manufacturing_setup_workpiece_relationship"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Additive_manufacturing_part_and_build_information_mim.additive_manufacturing_setup_workpiece_relationship">
        An <b>additive_manufacturing_setup_workpiece_relationship</b> is a type of
        <express_ref linkend="additive_manufacturing_part_and_build_information:mim:Additive_manufacturing_part_and_build_information_mim.additive_manufacturing_setup_relationship"/> that specifies the usage of a workpiece in the setup.
</ext_description><!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Additive_manufacturing_part_and_build_information_mim  Entity: machining_process_executable  
   <express_ref linkend="additive_manufacturing_part_and_build_information:mim:Additive_manufacturing_part_and_build_information_mim.machining_process_executable"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Additive_manufacturing_part_and_build_information_mim.machining_process_executable">
        A <b>machining_process_executable</b> is a type of
        <express_ref linkend="action_schema:ir_express:action_schema.action_method" /> that represents an executable step in a machining process.
</ext_description>
