<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.13"
    source="../data/modules/alias_identification/arm.exp"/>

  <schema name="Alias_identification_arm">
    <interface
      kind="use"
      schema="Identification_assignment_arm">
    </interface>

    <type name="alias_identification_item">
      <select
        extensible="YES"
        basedon="identification_item">
      </select>

    </type>
    <entity
      name="Alias_identification"
      supertypes="Identification_assignment">
      <explicit
        name="SELF\Identification_assignment.items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="alias_identification_item"/>
      </explicit>
      <derived
        name="SELF\Identification_assignment.role"
        expression="alias'">
        <builtintype
          type="STRING"/>
      </derived>
    </entity>

  </schema>

</express>
