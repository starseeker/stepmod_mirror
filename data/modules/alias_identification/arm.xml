<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: 2002/08/01 10:21:45 $"
  rcs.revision="$Revision: 1.16 $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.20"
    source="../data/modules/alias_identification/arm.exp"/>

  <schema name="Alias_identification_arm">
    <interface
      kind="use"
      schema="Identification_assignment_arm">
    </interface>

    <type name="alias_identification_item">
      <select
        extensible="YES"
        genericentity="YES">
      </select>

    </type>
    <entity
      name="Alias_identification"
      supertypes="Identification_assignment">
      <derived name="role" expression="'alias'">
	<!-- one of (typename builtintype) -->
        <builtintype type="STRING"/>
	<redeclaration entity-ref="Identification_assignment"/>
      </derived>
      <where
        label="WR1"
        expression="SIZEOF (QUERY( item &lt;* SELF\Identification_assignment.items | NOT ('ALIAS_IDENTIFICATION_ARM.ALIAS_IDENTIFICATION_ITEM' IN TYPEOF(item) ) )) = 0">
      </where>
    </entity>

  </schema>

</express>
