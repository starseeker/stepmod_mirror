<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/05/27 22:06:28 $"
  rcs.revision="$Revision: 1.8 $"
  descrtiption.file="arm_descriptions.xml">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.9"
    source="../data/modules/alias_identification/arm.exp"/>

  <schema name="Alias_identification_arm">
    <interface
      kind="use"
      schema="Identification_assignment_arm">
    </interface>

    <type name="alias_identification_item">
      <select
        extensible="YES"
        basedon="identification_item"
        selectitems="">
      </select>

    </type>
    <entity
      name="Alias_identification"
      supertypes="Identification_assignment">
      <where
        label="alias_must_have_scope"
        expression="(EXISTS(owner) OR EXISTS(role))">
      </where>
    </entity>

  </schema>

</express>
