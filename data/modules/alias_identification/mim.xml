<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/02/13 09:33:14 $"
  rcs.revision="$Revision: 1.4 $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.6"
    source="../data/modules/alias_identification/mim.exp"/>

  <schema name="Alias_identification_mim">
    <interface
      kind="use"
      schema="Identification_assignment_mim">

    <described.item item="applied_identification_assignment" kind="ENTITY">
      <description>The <b>identification_role</b> name value shall be 'alias'.</description>
    </described.item>
    </interface>

    <type name="alias_identification_item">
      <description>An <b>alias_identification_item</b> specifies those objects to which an alias for an <b>identification</b> may be assigned.</description>
      <select
        extensible="YES"
        basedon="identification_item">
      </select>
    </type>


  </schema>

</express>
