SCHEMA Altered_part_arm;

	USE FROM Altered_package_arm;	-- ISO/TS 10303-1601
	USE FROM Packaged_part_black_box_model_arm;	-- ISO/TS 10303-1710

  ENTITY Altered_packaged_part
    SUBTYPE OF (Packaged_part);
