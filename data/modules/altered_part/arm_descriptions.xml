<!-- +++++++++++ Altered_packaged_part +++++++++++ -->
<ext_description linkend="Altered_part_arm.Altered_packaged_part">
<p>
	An <b>Altered_packaged_part</b> is a type of 
	<express_ref linkend="Packaged_part_black_box_model:arm:Packaged_part_black_box_model_arm.Packaged_part"/>
	whose 
	<express_ref linkend="Packaged_part_black_box_model:arm:Packaged_part_black_box_model_arm.Packaged_part.used_package"/> 
	is an 
	<express_ref linkend="Altered_package:arm:Altered_package_arm.Altered_package"/>
	or that has a different implemented function than the <b>base_packaged_part</b>. 
	The material or shape or functional properties are different from the reference 
	<express_ref linkend="Packaged_part_black_box_model:arm:Packaged_part_black_box_model_arm.Packaged_part"/>. 
</p>
</ext_description>

<!-- base_packaged_part -->
<ext_description linkend="Altered_part_arm.Altered_packaged_part.base_packaged_part">
	specifies the original, unmodified 
	<express_ref linkend="Packaged_part_black_box_model:arm:Packaged_part_black_box_model_arm.Packaged_part"/> 
	for the <b>Altered_packaged_part</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Altered_part_arm.Altered_packaged_part.wr:WR1">
	The
	<express_ref linkend="Product_view_definition:arm:Product_view_definition_arm.Product_view_definition.name"/>
	shall not be populated.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Altered_part_arm.Altered_packaged_part.wr:WR2">
	either the 
	<express_ref linkend="Packaged_part_black_box_model:arm:Packaged_part_black_box_model_arm.Packaged_part.used_package"/>
	inherited from 
	<express_ref linkend="Packaged_part_black_box_model:arm:Packaged_part_black_box_model_arm.Packaged_part"/> 
	shall be of size one and shall only contain an 
	<express_ref linkend="Altered_package:arm:Altered_package_arm.Altered_package"/>  
	or the 
	<express_ref linkend="Packaged_part_black_box_model:arm:Packaged_part_black_box_model_arm.Packaged_part.implemented_function"/> 
	inherited from 
	<express_ref linkend="Packaged_part_black_box_model:arm:Packaged_part_black_box_model_arm.Packaged_part"/> 
	shall exist and shall not be the 
	<express_ref linkend="Packaged_part_black_box_model:arm:Packaged_part_black_box_model_arm.Packaged_part.implemented_function"/>  
	of the <b>base_packaged_part</b>.
</ext_description>

</ext_descriptions>

