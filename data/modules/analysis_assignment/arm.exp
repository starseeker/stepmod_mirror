SCHEMA Analysis_assignment_arm;

USE FROM Analysis_identification_arm;

TYPE analised_item = EXTENSIBLE GENERIC_ENTITY SELECT 
  (Product_version);
END_TYPE;

ENTITY Analysis_assignment;
  analysis : Analysis_version;
  applied_to : analised_item;
END_ENTITY;

END_SCHEMA;

