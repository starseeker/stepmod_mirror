<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<?xml-stylesheet type="text/xsl"
href="../../../xsl/express.xsl" ?>
<express
   language_version="2"
   rcs.date="$Date: 2005/07/04 09:06:08 $"
   rcs.revision="$Revision: 1.44 $"
   description.file="arm_descriptions.xml">
  <schema name="Analysis_assignment_arm">
    <interface
      kind="use"
      schema="Analysis_identification_arm">
    </interface>
    <entity name="Analysis_design_version_relationship"
      supertypes="Product_version_relationship"
    >
      <explicit
        name="analysis">
        <typename name="Analysis_version"/>
        <redeclaration
          entity-ref="Product_version_relationship"
          old_name="related_version"/>
      </explicit>
      <explicit
        name="design">
        <typename name="Product_version"/>
        <redeclaration
          entity-ref="Product_version_relationship"
          old_name="relating_version"/>
      </explicit>
    </entity>

  </schema>
</express>
