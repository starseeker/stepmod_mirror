<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<?xml-stylesheet type="text/xsl"
href="../../../xsl/express.xsl" ?>
<express
   language_version="2"
   rcs.date="$Date: 2005/07/04 09:06:08 $"
   rcs.revision="$Revision: 1.44 $"
   description.file="arm_descriptions.xml">
  <schema name="Analysis_characterized_arm">
    <interface
      kind="use"
      schema="Analysis_identification_arm">
    </interface>
    <interface
      kind="use"
      schema="Approval_arm">
    </interface>
    <interface
      kind="use"
      schema="Classification_assignment_arm">
    </interface>
    <interface
      kind="use"
      schema="Contract_arm">
    </interface>
    <interface
      kind="use"
      schema="Date_time_assignment_arm">
    </interface>
    <interface
      kind="use"
      schema="Effectivity_application_arm">
    </interface>
    <interface
      kind="use"
      schema="Identification_assignment_arm">
    </interface>
    <interface
      kind="use"
      schema="Person_organization_assignment_arm">
    </interface>
    <interface
      kind="use"
      schema="Security_classification_arm">
    </interface>
    <type name="analysis_approval_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="approval_item"
        selectitems="Analysis_discipline_definition Analysis_version"
      />
    </type>

    <type name="analysis_classification_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="classification_item"
        selectitems="Analysis"
      />
    </type>

    <type name="analysis_contract_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="contract_item"
        selectitems="Analysis_version"
      />
    </type>

    <type name="analysis_date_or_date_time_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="date_or_date_time_item"
        selectitems="Analysis_discipline_definition"
      />
    </type>

    <type name="analysis_effectivity_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="effectivity_item"
        selectitems="Analysis_status"
      />
    </type>

    <type name="analysis_identification_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="identification_item"
        selectitems="Analysis Analysis_discipline_definition Analysis_version"
      />
    </type>

    <type name="analysis_organization_or_person_in_organization_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="organization_or_person_in_organization_item"
        selectitems="Analysis Analysis_discipline_definition Analysis_version"
      />
    </type>

    <type name="analysis_security_classification_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="security_classification_item"
        selectitems="Analysis_version"
      />
    </type>

  </schema>
</express>
