<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<?xml-stylesheet type="text/xsl"
href="../../../xsl/express.xsl" ?>
<express
   language_version="2"
   rcs.date="$Date: 2005/07/04 09:06:08 $"
   rcs.revision="$Revision: 1.44 $"
   description.file="arm_descriptions.xml">
  <schema name="Analysis_identification_arm">
    <interface
      kind="use"
      schema="Product_version_relationship_arm">
    </interface>
    <interface
      kind="use"
      schema="Product_view_definition_arm">
    </interface>
    <entity name="Analysis"
      supertypes="Product"
    >
    </entity>

    <entity name="Analysis_discipline_definition"
      supertypes="Product_view_definition"
    >
      <explicit
        name="defined_version">
        <typename name="Analysis_version"/>
        <redeclaration
          entity-ref="Product_view_definition"/>
      </explicit>
    </entity>

    <entity name="Analysis_version"
      supertypes="Product_version"
    >
      <explicit
        name="of_product">
        <typename name="Analysis"/>
        <redeclaration
          entity-ref="Product_version"/>
      </explicit>
    </entity>

    <entity name="Analysis_status"
    >
      <explicit
        name="analysis">
        <typename name="Analysis_version"/>
      </explicit>
      <explicit
        name="status">
        <builtintype type="STRING"
        />
      </explicit>
    </entity>
  </schema>

</express>
