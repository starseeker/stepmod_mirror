<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: analytical_model_port
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Analytical_model_mim.analytical_model_port">
	An <b>analytical_model_port</b> is a type of 
	<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.property_definition"/>,
	<express_ref linkend="product_property_representation_schema:ir_express:product_property_representation_schema.property_definition_representation"/>
	and of
	<express_ref linkend="representation_schema:ir_express:representation_schema.representation"/>
	that implements the ARM concept of 
	<express_ref linkend="Analytical_model:arm:Analytical_model_arm.Analytical_model_port"/>.
	An <b>analytical_model_port</b> describes an interface point for an  
	<express_ref linkend="Analytical_model:mim:Analytical_model_mim.analytical_model_definition"/>.
</ext_description>

<!-- definition -->
<ext_description linkend="Analytical_model_mim.analytical_model_port.definition">
	an attribute inhertied from the
	<express_ref linkend="product_property_representation_schema:ir_express:product_property_representation_schema.property_definition_representation"/>
	shall be redeclared as the
	<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.property_definition"/>
	for the <b>analytical_model_port</b>.
</ext_description>

<!-- used_representation -->
