++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Analytical_model_mim.analytical_model_make_from_relationship.relating_product_definition">
	specifies a role of the
	<express_ref linkend="Analytical_model:mim:Analytical_model_mim.analytical_model_definition"/>
	for the <b>analytical_model_make_from_relationship</b>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
