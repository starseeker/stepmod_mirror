<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?><!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="">
   <application name="JSDAI" owner="LKSoft" source="annotated_3d_model_equivalence_criteria_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Annotated_3d_model_equivalence_criteria_arm">
      <interface kind="use" schema="Foundation_representation_arm"/>
      <interface kind="use" schema="Product_data_equivalence_definition_arm"/>
      <interface kind="use" schema="Qualified_measure_arm"/>
      <interface kind="reference" schema="Support_resource_arm">
         <interfaced.item name="bag_to_set"/>
      </interface>
      <type name="a3m_accuracy_type_name">
         <enumeration extensible="YES"/>
      </type>
      <type name="a3m_detected_difference_type_name">
         <enumeration extensible="YES"/>
      </type>
      <type name="a3m_element_type_name">
         <enumeration extensible="YES"/>
      </type>
      <type name="a3m_equivalence_assessment_specification_select">
         <select selectitems="A3m_equivalence_assessment_by_logical_test A3m_equivalence_assessment_by_numerical_test"/>
      </type>
      <type name="a3m_equivalence_value_limit_type_select">
         <select selectitems="A3m_equivalence_value_range A3m_equivalence_value_limit"/>
      </type>
      <type name="a3m_measured_data_type_name">
         <enumeration extensible="YES"/>
      </type>
      <type name="a3mc_used_equivalence_representation_select">
         <select basedon="used_equivalence_representation_select" extensible="YES" genericentity="YES" selectitems="A3m_equivalence_criteria_representation"/>
      </type>
      <type name="accuracy_associated_target_select">
         <select extensible="YES" selectitems="A3m_equivalence_criteria_representation A3m_equivalence_criterion"/>
      </type>
      <type name="detailed_report_style_type">
         <enumeration items="inspected_element defective_element"/>
      </type>
      <type name="inspected_equivalence_element_select">
         <select extensible="YES" genericentity="YES"/>
      </type>
      <type name="preferred_report_ordering">
         <enumeration items="in_notable_order in_detected_order"/>
      </type>
      <type name="representative_value_type">
         <enumeration items="average_value max_value min_value min_max_value"/>
      </type>
      <type name="summary_report_style_type">
         <enumeration items="conclusion number_of_defects number_of_inspections"/>
      </type>
      <entity name="A3m_equivalence_accuracy_association">
         <explicit name="id">
            <typename name="identifier"/>
         </explicit>
         <explicit name="name">
            <typename name="label"/>
         </explicit>
         <explicit name="description" optional="YES">
            <typename name="text"/>
         </explicit>
         <explicit name="specific_accuracy">
            <typename name="A3m_equivalence_inspection_accuracy"/>
         </explicit>
         <explicit name="equivalence_target">
            <typename name="accuracy_associated_target_select"/>
         </explicit>
      </entity>
      <entity name="A3m_equivalence_assessment_by_logical_test"/>
      <entity name="A3m_equivalence_assessment_by_numerical_test">
         <explicit name="threshold">
            <typename name="a3m_equivalence_value_limit_type_select"/>
         </explicit>
      </entity>
      <entity name="A3m_equivalence_criteria_representation" supertypes="Representation">
         <where expression="SIZEOF( QUERY( q &lt;* SELF\Representation.items|&#10;            'ANNOTATED_3D_MODEL_EQUIVALENCE_CRITERIA_ARM.' +&#10;            'A3M_EQUIVALENCE_CRITERION' IN TYPEOF(q))) &gt; 0" label="WR1"/>
      </entity>
      <entity abstract.supertype="YES" name="A3m_equivalence_criterion" super.expression="A3m_equivalence_criterion_with_specified_elements" supertypes="Representation_item">
         <explicit name="assessment_specification">
            <typename name="a3m_equivalence_assessment_specification_select"/>
         </explicit>
         <explicit name="comparing_element_types">
            <aggregate lower="1" type="LIST" upper="?"/>
            <typename name="a3m_element_type_name"/>
         </explicit>
         <explicit name="compared_element_types">
            <aggregate lower="1" type="LIST" upper="?"/>
            <typename name="a3m_element_type_name"/>
         </explicit>
         <explicit name="measured_data_type">
            <typename name="a3m_measured_data_type_name"/>
         </explicit>
         <explicit name="detected_difference_types">
            <aggregate lower="0" type="LIST" upper="?"/>
            <typename name="a3m_detected_difference_type_name"/>
         </explicit>
         <explicit name="accuracy_types">
            <aggregate lower="0" type="LIST" upper="?"/>
            <typename name="a3m_accuracy_type_name"/>
         </explicit>
         <where expression="SIZEOF(QUERY(q &lt;* bag_to_set( USEDIN(SELF,&#10;            'FOUNDATION_REPRESENTATION_ARM.REPRESENTATION.ITEMS'))&#10;            |'ANNOTATED_3D_MODEL_EQUIVALENCE_INSPECTION_RESULT_ARM.'+&#10;             'A3M_EQUIVALENCE_INSPECTION_RESULT_REPRESENTATION'&#10;               IN TYPEOF(q)))=0" label="WR1"/>
      </entity>
      <entity name="A3m_equivalence_criterion_with_specified_elements" supertypes="A3m_equivalence_criterion">
         <explicit name="comparing_specified_elements">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="inspected_equivalence_element_select"/>
         </explicit>
         <explicit name="compared_specified_elements">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="inspected_equivalence_element_select"/>
         </explicit>
      </entity>
      <entity name="A3m_equivalence_inspection_accuracy">
         <explicit name="description">
            <typename name="text"/>
         </explicit>
         <explicit name="range">
            <typename name="a3m_equivalence_value_limit_type_select"/>
         </explicit>
      </entity>
      <entity abstract.supertype="YES" name="A3m_equivalence_inspection_requirement_with_values" supertypes="Representation_item">
         <explicit name="applied_values">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Numerical_item_with_unit"/>
         </explicit>
         <where expression="SIZEOF(QUERY(q &lt;* bag_to_set( USEDIN(SELF,&#10;            'FOUNDATION_REPRESENTATION_ARM.REPRESENTATION.ITEMS'))&#10;            |'ANNOTATED_3D_MODEL_EQUIVALENCE_INSPECTION_RESULT_ARM.'+&#10;             'A3M_EQUIVALENCE_INSPECTION_RESULT_REPRESENTATION'&#10;               IN TYPEOF(q)))=0" label="WR1"/>
      </entity>
      <entity name="A3m_equivalence_lower_value_limit" supertypes="A3m_equivalence_value_limit">
         <where expression="SELF\Qualified_representation_item.qualifiers[1]\Type_qualifier.name = 'minimum'" label="WR1"/>
      </entity>
      <entity name="A3m_equivalence_report_request">
         <explicit name="id">
            <typename name="identifier"/>
         </explicit>
         <explicit name="name">
            <typename name="label"/>
         </explicit>
         <explicit name="description" optional="YES">
            <typename name="text"/>
         </explicit>
         <explicit name="criterion_of_report_request">
            <typename name="A3m_equivalence_criterion"/>
         </explicit>
      </entity>
      <entity name="A3m_equivalence_upper_value_limit" supertypes="A3m_equivalence_value_limit">
         <where expression="SELF\Qualified_representation_item.qualifiers[1]\Type_qualifier.name = 'maximum'" label="WR1"/>
      </entity>
      <entity abstract.supertype="YES" name="A3m_equivalence_value_limit" super.expression="ONEOF (A3m_equivalence_upper_value_limit, A3m_equivalence_lower_value_limit)" supertypes="Numerical_item_with_unit Qualified_representation_item">
         <explicit name="qualifiers">
            <aggregate lower="1" type="SET" upper="1"/>
            <typename name="Pre_defined_type_qualifier"/>
            <redeclaration entity-ref="Qualified_representation_item"/>
         </explicit>
         <where expression="SELF\Qualified_representation_item.qualifiers[1]\Type_qualifier.name IN ['minimum', 'maximum']" label="WR1"/>
      </entity>
      <entity name="A3m_equivalence_value_range" supertypes="Measure_item">
         <explicit name="lower_limit">
            <typename name="Numerical_item_with_unit"/>
         </explicit>
         <explicit name="upper_limit">
            <typename name="Numerical_item_with_unit"/>
         </explicit>
      </entity>
      <entity name="Detailed_equivalence_report_request" supertypes="A3m_equivalence_report_request">
         <explicit name="report_request_type">
            <typename name="detailed_report_style_type"/>
         </explicit>
         <explicit name="report_sequence">
            <typename name="preferred_report_ordering"/>
         </explicit>
      </entity>
      <entity name="Detailed_equivalence_report_request_with_number_of_instances" supertypes="Detailed_equivalence_report_request">
         <explicit name="number_of_instances">
            <builtintype type="INTEGER"/>
         </explicit>
         <derived expression="inspected_element" name="report_request_type">
            <typename name="detailed_report_style_type"/>
            <redeclaration entity-ref="Detailed_equivalence_report_request"/>
         </derived>
      </entity>
      <entity name="Summary_report_request_with_number_of_instances" supertypes="A3m_equivalence_report_request">
         <explicit name="report_request_type">
            <aggregate lower="1" type="SET" upper="3"/>
            <typename name="summary_report_style_type"/>
         </explicit>
      </entity>
      <entity name="Summary_report_request_with_representative_value" supertypes="A3m_equivalence_report_request">
         <explicit name="report_request_type">
            <typename name="representative_value_type"/>
         </explicit>
      </entity>
   </schema>
</express>
