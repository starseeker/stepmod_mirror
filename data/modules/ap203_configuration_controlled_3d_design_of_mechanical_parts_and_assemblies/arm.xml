<rule name="addresses_are_assigned" appliesto="Address">
<algorithm>
</algorithm>
<where label="WR1" expression="SIZEOF(QUERY(a &lt;* Address | SIZEOF(USEDIN(a, a + '.ADDRESS_ASSIGNMENT.ASSIGNED_ADDRESS')) = 0)) = 0">
</where>
</rule>
<rule name="Person_requires_person_and_organization" appliesto="Person">
<algorithm>
</algorithm>
<where label="WR1" expression="SIZEOF(QUERY(p &lt;* Person | SIZEOF(USEDIN(p, schema_name + '.PERSON_IN_ORGANIZATION.CONCERNED_PERSON')) = 0)) = 0">
</where>
</rule>
<rule name="Shape_requires_product_view" appliesto="Item_shape">
<algorithm>
</algorithm>
<where label="WR1" expression="SIZEOF(QUERY(is &lt;* Item_shape |  NOT (schema_name + '.PRODUCT_VIEW_DEFINITION' IN TYPEOF(is.described_element)))) = 0">
</where>
</rule>
