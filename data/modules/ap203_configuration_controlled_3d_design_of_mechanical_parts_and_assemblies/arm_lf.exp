	WR1 : NOT('AP203_CONFIGURATION_CONTROLLED_3D_DESIGN_OF_MECHANICAL_PARTS_AND_ASSEMBLIES_ARM_LF.'+'DRAUGHTING_MODEL' IN TYPEOF(rep_2))OR 
