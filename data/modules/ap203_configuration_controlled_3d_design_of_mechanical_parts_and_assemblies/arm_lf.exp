  ENTITY Draughting_model_item_association;
    name            : STRING;
    definition      : Shape_element;
    identified_item : draughting_model_item_association_select;
    used_model      : Draughting_model;
    WHERE 
	  WR1: (NOT('AP203_CONFIGURATION_CONTROLLED_3D_DESIGN_OF_MECHANICAL_PARTS_AND_ASSEMBLIES_ARM_LF.'+'DATUM_TARGET_CALLOUT' IN TYPEOF(identified_item)) OR
	       ('AP203_CONFIGURATION_CONTROLLED_3D_DESIGN_OF_MECHANICAL_PARTS_AND_ASSEMBLIES_ARM_LF.'+'DATUM_TARGET' IN TYPEOF(definition)));
	  WR2: (NOT('AP203_CONFIGURATION_CONTROLLED_3D_DESIGN_OF_MECHANICAL_PARTS_AND_ASSEMBLIES_ARM_LF.'+'GEOMETRIC_TOLERANCE_CALLOUT' IN TYPEOF(identified_item)) OR
	       ('AP203_CONFIGURATION_CONTROLLED_3D_DESIGN_OF_MECHANICAL_PARTS_AND_ASSEMBLIES_ARM_LF.'+'GEOMETRIC_TOLERANCE' IN TYPEOF(definition)));
	  -- WRs capturing callout <- GD&T type for N:1 links and       
	  WR3: (NOT('AP203_CONFIGURATION_CONTROLLED_3D_DESIGN_OF_MECHANICAL_PARTS_AND_ASSEMBLIES_ARM_LF.'+'SIZE_DIMENSION' IN TYPEOF(definition)) OR
	        (('AP203_CONFIGURATION_CONTROLLED_3D_DESIGN_OF_MECHANICAL_PARTS_AND_ASSEMBLIES_ARM_LF.'+'DIMENSION_CALLOUT' IN TYPEOF(identified_item)) OR
	        ('AP203_CONFIGURATION_CONTROLLED_3D_DESIGN_OF_MECHANICAL_PARTS_AND_ASSEMBLIES_ARM_LF.'+'DIMENSION_CURVE_DIRECTED_CALLOUT' IN TYPEOF(identified_item)) OR
	        ('AP203_CONFIGURATION_CONTROLLED_3D_DESIGN_OF_MECHANICAL_PARTS_AND_ASSEMBLIES_ARM_LF.'+'STRUCTURED_DIMENSION_CALLOUT' IN TYPEOF(identified_item))));
  END_ENTITY;

