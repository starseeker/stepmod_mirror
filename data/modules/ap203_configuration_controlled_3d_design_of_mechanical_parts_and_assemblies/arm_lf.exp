	(thing : GENERIC) : SET [0:?] OF Scalar_variable;
LOCAL

    accum : SET [0:?] OF Scalar_variable := [];

  END_LOCAL
;

  RETURN(local_vars_aux(thing, accum));
END_FUNCTION; -- local_vars_of

