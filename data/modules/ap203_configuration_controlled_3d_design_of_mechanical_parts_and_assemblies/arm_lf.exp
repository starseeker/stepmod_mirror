      WR3: (NOT (relation_type IN ['occurrence'])) OR 
	    ('AP203_CONFIGURATION_CONTROLLED_3D_DESIGN_OF_MECHANICAL_PARTS_AND_ASSEMBLIES_ARM_LF.PHYSICAL_ELEMENT_DEFINITION' IN TYPEOF(relating));
