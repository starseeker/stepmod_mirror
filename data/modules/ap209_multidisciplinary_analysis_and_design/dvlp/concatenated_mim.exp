       (srwp_i.name = 'width') OR
       (srwp_i.name = 'depth') OR
       (srwp_i.name = 'cross bar width') OR
       (srwp_i.name = 'cross bar depth') OR
       (srwp_i.name = 'first offset') OR
       (srwp_i.name = 'second offset') OR
