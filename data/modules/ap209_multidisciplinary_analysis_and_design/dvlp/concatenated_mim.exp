         'PRODUCT_PROPERTY_DEFINITION_SCHEMA.SHAPE_ASPECT_RELATIONSHIP.RELATED_SHAPE_ASPECT') | 
         (('AIC_MACHINING_FEATURE.FEATURE_COMPONENT_RELATIONSHIP') 
