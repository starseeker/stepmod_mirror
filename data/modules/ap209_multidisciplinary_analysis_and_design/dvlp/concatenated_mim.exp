    WR5: (NOT (SELF.description = 'complex')) OR 
         (SIZEOF(QUERY ( pd <* USEDIN(SELF,
         'PRODUCT_PROPERTY_DEFINITION_SCHEMA.PROPERTY_DEFINITION.DEFINITION') | 
         (NOT (SIZEOF(QUERY ( pdr <* USEDIN(pd,
