    SUBTYPE OF (shape_aspect);
      SELF\shape_aspect.of_shape : functional_unit;
    UNIQUE
      UR1: SELF\shape_aspect.name, of_shape;
    WHERE
      WR1: SELF\shape_aspect.product_definitional = FALSE;
  END_ENTITY;

