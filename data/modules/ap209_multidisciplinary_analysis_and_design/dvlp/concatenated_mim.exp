        ((('AIC_MACHINING_FEATURE.SLOT_END' 
        IN TYPEOF(fcr.relating_shape_aspect)) AND 
        (fcr.relating_shape_aspect.description IN ['loop'])) AND 
        ('AIC_MACHINING_FEATURE.SLOT' 
        IN TYPEOF(fcr.related_shape_aspect.of_shape.definition))) 
