<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="ap210_assembly_physical_design_arm schema_instance"/>
   <schema name="Ap210_assembly_physical_design_arm">
      <interface kind="use" schema="Altered_part_arm"/>
      <interface kind="use" schema="Design_product_data_management_arm"/>
      <interface kind="use" schema="Assembly_2d_shape_arm"/>
      <interface kind="use" schema="Assembly_3d_shape_arm"/>
      <interface kind="use" schema="Assembly_functional_interface_requirement_arm"/>
      <interface kind="use" schema="Assembly_module_with_cable_component_2d_arm"/>
      <interface kind="use" schema="Assembly_module_with_cable_component_3d_arm"/>
      <interface kind="use" schema="Assembly_module_with_macro_component_arm"/>
      <interface kind="use" schema="Chemical_substance_arm"/>
      <interface kind="use" schema="Design_specific_assignment_to_assembly_usage_view_arm"/>
      <interface kind="use" schema="Device_marking_arm"/>
      <interface kind="use" schema="Physical_unit_interconnect_definition_arm"/>
      <type name="ap210_apd_assembly_module_or_assembly_group_component">
         <select extensible="YES" genericentity="YES" basedon="assembly_module_or_assembly_group_component" selectitems="Assembly_group_component"/>
      </type>
      <subtype.constraint name="physical_component_interface_terminal_subtypes" entity="Physical_component_interface_terminal" super.expression="ONEOF (Packaged_connector_component_interface_terminal, Interconnect_component_interface_terminal)"/>
      <subtype.constraint name="packaged_part_subtypes" entity="Packaged_part" super.expression="(ONEOF (Minimally_defined_connector, Packaged_connector) ANDOR Altered_packaged_part)"/>
      <subtype.constraint name="ap210_apd_part_view_definition_subtypes" entity="Part_view_definition" super.expression="ONEOF (Part_usage_view, Part_design_view, Stratum, Physical_unit_interconnect_definition)"/>
      <subtype.constraint name="ap210_physical_component_subtypes" entity="Physical_component" super.expression="ONEOF (Assembly_module_component, Bare_die_component, Cable_component, Interconnect_module_component, Packaged_component, Routed_interconnect_component, Routed_physical_component)"/>
      <subtype.constraint name="ap210_part_usage_view_subtypes" entity="Part_usage_view" super.expression="ONEOF (Package, Bare_die, Cable_usage_view, Packaged_part, Assembly_module_usage_view, Interconnect_module_usage_view)"/>
   </schema>
</express>
