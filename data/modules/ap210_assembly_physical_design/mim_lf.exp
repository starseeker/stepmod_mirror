END_ENTITY;

ENTITY annotation_symbol
  SUBTYPE OF(mapped_item);
WHERE
  WR1: 'AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.SYMBOL_REPRESENTATION_MAP' IN
         TYPEOF (SELF\mapped_item.mapping_source);
  WR2: 'AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.SYMBOL_TARGET' IN
         TYPEOF (SELF\mapped_item.mapping_target);
  WR3: 'AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.GEOMETRIC_REPRESENTATION_ITEM' IN

         TYPEOF (SELF);
END_ENTITY;

ENTITY annotation_symbol_occurrence
  SUBTYPE OF (annotation_occurrence);
WHERE
  WR1: SIZEOF(
         ['AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.ANNOTATION_SYMBOL',
          'AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.DEFINED_SYMBOL'] *

         TYPEOF(SELF\styled_item.item)) > 0;
END_ENTITY;

ENTITY annotation_table
  SUBTYPE OF(annotation_symbol);
WHERE
  WR1: 'AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.TABLE_REPRESENTATION' IN

         TYPEOF (SELF\mapped_item.mapping_source.mapped_representation);
END_ENTITY;

ENTITY annotation_table_occurrence
  SUBTYPE OF (annotation_symbol_occurrence);
WHERE
  WR1: SIZEOF (
         ['AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.ANNOTATION_TABLE',
          'AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.DEFINED_TABLE'] *

         TYPEOF (SELF\styled_item.item)) > 0;
END_ENTITY;

ENTITY annotation_text
  SUBTYPE OF (mapped_item);
WHERE
  WR1: 'AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.AXIS2_PLACEMENT' IN
       TYPEOF( SELF\mapped_item.mapping_target);
  WR2: 'AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.TEXT_STRING_REPRESENTATION' IN
       TYPEOF( SELF\mapped_item.mapping_source.mapped_representation);
  WR3: 'AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.GEOMETRIC_REPRESENTATION_ITEM' IN

       TYPEOF( SELF);
END_ENTITY;

ENTITY annotation_text_character
  SUBTYPE OF (mapped_item);
