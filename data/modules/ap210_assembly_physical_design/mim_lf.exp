ENTITY annotation_occurrence
  SUPERTYPE OF (ONEOF (
              ANNOTATION_POINT_OCCURRENCE,
              ANNOTATION_CURVE_OCCURRENCE,
              ANNOTATION_FILL_AREA_OCCURRENCE,
              ANNOTATION_TEXT_OCCURRENCE,
              ANNOTATION_SYMBOL_OCCURRENCE))
