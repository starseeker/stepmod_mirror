   v_sense        : BOOLEAN;
 INVERSE
   using_surfaces : BAG[1:?] OF rectangular_composite_surface FOR segments;
 WHERE
   WR1: (NOT ('AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.CURVE_BOUNDED_SURFACE' 

                 IN TYPEOF(parent_surface)));
