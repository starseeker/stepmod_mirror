WHERE
  WR1: 'AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.CHARACTER_GLYPH_SYMBOL' IN
         TYPEOF (SELF\mapped_item.mapping_source.mapped_representation);
  WR2: 'AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.AXIS2_PLACEMENT' IN
         TYPEOF (SELF\mapped_item.mapping_target);
  WR3: 'AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.GEOMETRIC_REPRESENTATION_ITEM' IN

         TYPEOF (SELF);
