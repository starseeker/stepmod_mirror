ENTITY annotation_text_occurrence
  SUBTYPE OF (annotation_occurrence);
WHERE
  WR1: SIZEOF (
         ['AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.TEXT_LITERAL',
          'AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.ANNOTATION_TEXT',
          'AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.ANNOTATION_TEXT_CHARACTER',
          'AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.DEFINED_CHARACTER_GLYPH',
