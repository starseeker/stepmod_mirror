   WR1: u1 <> u2;
   WR2: v1 <> v2;
   WR3: (('AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.ELEMENTARY_SURFACE' IN TYPEOF(basis_surface))
       AND  (NOT ('AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.PLANE' IN TYPEOF(basis_surface)))) OR
      ('AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.SURFACE_OF_REVOLUTION' IN TYPEOF(basis_surface))
          OR (usense = (u2 > u1));
   WR4: (('AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.SPHERICAL_SURFACE' IN TYPEOF(basis_surface))
          OR
         ('AP210_ASSEMBLY_PHYSICAL_DESIGN_MIM_LF.TOROIDAL_SURFACE' IN TYPEOF(basis_surface)))

          OR (vsense = (v2 > v1));
 END_ENTITY;

