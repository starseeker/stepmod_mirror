           )) = 1);
      WR4 : NOT EXISTS(SELF\Representation.description);
      WR5 : ('AP210_ASSEMBLY_TECHNOLOGY_CONSTRAINTS_ARM_LF.PLUS_MINUS_TOLERANCE_CHARACTERISTIC' IN TYPEOF(SELF)) OR
        (SIZEOF(QUERY(it <* SELF\Representation.items |
          NOT(('AP210_ASSEMBLY_TECHNOLOGY_CONSTRAINTS_ARM_LF.VALUE_WITH_TOLERANCES' IN TYPEOF(it)) AND
             ('AP210_ASSEMBLY_TECHNOLOGY_CONSTRAINTS_ARM_LF.LENGTH_DATA_ELEMENT' IN TYPEOF(it\Value_with_tolerances.item_value)))
