FUNCTION get_value
	(input1 : STRING; np : SET [0:?] OF Representation_item) : Numerical_item_with_unit;
REPEAT i := 1 TO SIZEOF(np)BY 1;

  IF input1 = np[i]\Representation_item.name THEN
       RETURN(np[i]);

  END_IF;

END_REPEAT;

  RETURN(?);
END_FUNCTION; -- get_value

