    SUBTYPE OF(Tolerance_characteristic);
    WHERE
      WR1 : ('AP210_ASSEMBLY_TECHNOLOGY_CONSTRAINTS_ARM_LF.STATISTICAL_TOLERANCE_CHARACTERISTIC' IN TYPEOF(SELF)) OR
            ('AP210_ASSEMBLY_TECHNOLOGY_CONSTRAINTS_ARM_LF.SYMMETRICAL_TOLERANCE_CHARACTERISTIC' IN TYPEOF(SELF)) OR
            ('AP210_ASSEMBLY_TECHNOLOGY_CONSTRAINTS_ARM_LF.PLUS_MINUS_TOLERANCE_CHARACTERISTIC' IN TYPEOF(SELF)) OR
        (SIZEOF(QUERY(it <* SELF\Representation.items |
          NOT('AP210_ASSEMBLY_TECHNOLOGY_CONSTRAINTS_ARM_LF.LENGTH_DATA_ELEMENT' IN TYPEOF(it))
         )) = 0);
      WR2 : NOT('AP210_ASSEMBLY_TECHNOLOGY_CONSTRAINTS_ARM_LF.SYMMETRICAL_TOLERANCE_CHARACTERISTIC' IN TYPEOF(SELF)) OR
        (SIZEOF(QUERY(it <* SELF\Representation.items |
        ('AP210_ASSEMBLY_TECHNOLOGY_CONSTRAINTS_ARM_LF.LENGTH_DATA_ELEMENT' IN TYPEOF(it))
           )) = 1);
      WR3 : NOT('AP210_ASSEMBLY_TECHNOLOGY_CONSTRAINTS_ARM_LF.STATISTICAL_TOLERANCE_CHARACTERISTIC' IN TYPEOF(SELF)) OR
        (SIZEOF(QUERY(it <* SELF\Representation.items |
