  WR5: SIZEOF (['AP210_ASSEMBLY_TECHNOLOGY_CONSTRAINTS_ARM_LF.NEXT_ASSEMBLY_USAGE', 'AP210_ASSEMBLY_TECHNOLOGY_CONSTRAINTS_ARM_LF.COMPONENT_UPPER_LEVEL_IDENTIFICATION'] * TYPEOF(upper_assembly_relationship)) = 1;
