ENTITY product_concept_feature_category
	SUBTYPE OF (group);
WHERE
	WR1 : SIZEOF(QUERY(aga <* USEDIN(SELF, 'AP210_DEVICE_FUNCTIONAL_AND_PHYSICAL_CHARACTERIZATION_MIM_LF.' + 'GROUP_ASSIGNMENT.ASSIGNED_GROUP')|
    ('AP210_DEVICE_FUNCTIONAL_AND_PHYSICAL_CHARACTERIZATION_MIM_LF.'+ 'APPLIED_GROUP_ASSIGNMENT' IN  TYPEOF(aga))AND
   ((aga.role.name <> 'specification category member')OR 
  (SIZEOF(QUERY(i <* aga.items |('AP210_DEVICE_FUNCTIONAL_AND_PHYSICAL_CHARACTERIZATION_MIM_LF.'+ 'PRODUCT_CONCEPT_FEATURE' IN  TYPEOF(i))AND 
    NOT('AP210_DEVICE_FUNCTIONAL_AND_PHYSICAL_CHARACTERIZATION_MIM_LF.'+ 'CONDITIONAL_CONCEPT_FEATURE' IN TYPEOF(i))))<> SIZEOF(aga.items)))
     ))=0;
