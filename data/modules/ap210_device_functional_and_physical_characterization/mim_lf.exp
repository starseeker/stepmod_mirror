	(pd.frame_of_reference.name = 'part occurrence'));

WHERE
	WR1 : SIZEOF(QUERY(pd <* part_occurrences | 
	(NOT(pd.name IN 
['single instance', 'selected instance' ,'quantified instance', 'specified instance' ]))))= 0;
	WR2 : SIZEOF(QUERY(pd <* part_occurrences | 
 	(SIZEOF(QUERY(pdr <* USEDIN(pd ,
'AP210_DEVICE_FUNCTIONAL_AND_PHYSICAL_CHARACTERIZATION_MIM_LF.' + 'PRODUCT_DEFINITION_RELATIONSHIP.RELATED_PRODUCT_DEFINITION')| 
	pdr.name = 'definition usage'))<>1)AND  
	(SIZEOF(QUERY(cd <* USEDIN(pd, 'AP210_DEVICE_FUNCTIONAL_AND_PHYSICAL_CHARACTERIZATION_MIM_LF.' + 'CONFIGURATION_DESIGN.DESIGN')| 
	(cd.name = 'occurrence usage definition')AND 
	(NOT('AP210_DEVICE_FUNCTIONAL_AND_PHYSICAL_CHARACTERIZATION_MIM_LF.' + 'PRODUCT_IDENTIFICATION' IN TYPEOF(cd.configuration)))))<>1)))= 0;
	WR3 : SIZEOF(QUERY(pd <* part_occurrences |  
	(SIZEOF(QUERY(cd <* USEDIN(pd, 'AP210_DEVICE_FUNCTIONAL_AND_PHYSICAL_CHARACTERIZATION_MIM_LF.'+ 'PRODUCT_DEFINITION_RELATIONSHIP.RELATED_PRODUCT_DEFINITION')|
 	('AP210_DEVICE_FUNCTIONAL_AND_PHYSICAL_CHARACTERIZATION_MIM_LF.' +'PRODUCT_DEFINITION_USAGE' IN TYPEOF(cd))))= 0)AND
 	(SIZEOF(USEDIN(pd, 'AP210_DEVICE_FUNCTIONAL_AND_PHYSICAL_CHARACTERIZATION_MIM_LF.' + 'PRODUCT_DEFINITION_OCCURRENCE_RELATIONSHIP.OCCURRENCE'))= 0)))= 0;
	WR4 : SIZEOF(QUERY(pd <* part_occurrences | 
	(pd.name = 'selected instance')AND 
	NOT valid_selected_instance_representation(pd)))=0;
	WR5 : SIZEOF(QUERY(pd <* part_occurrences | 
 	(pd.name = 'quantified instance')AND  
	(SIZEOF(QUERY (ppd <* USEDIN(pd, 'AP210_DEVICE_FUNCTIONAL_AND_PHYSICAL_CHARACTERIZATION_MIM_LF.' +'PROPERTY_DEFINITION.DEFINITION')|
 	(ppd.name ='occurrence quantity')AND  
	(SIZEOF(QUERY(pdr <*USEDIN(ppd, 'AP210_DEVICE_FUNCTIONAL_AND_PHYSICAL_CHARACTERIZATION_MIM_LF.' +'PROPERTY_DEFINITION_REPRESENTATION.DEFINITION')|(
	pdr.used_representation.name = 'quantity')AND  
	(SIZEOF(pdr.used_representation.items)= 1)AND 
 	(SIZEOF(QUERY(i <* pdr.used_representation.items | 
	('AP210_DEVICE_FUNCTIONAL_AND_PHYSICAL_CHARACTERIZATION_MIM_LF.' + 'MEASURE_REPRESENTATION_ITEM' IN TYPEOF(i))AND
	(i.name = 'quantity measure')))= 1)))= 1)))= 0)))= 0;
	WR6 : SIZEOF(QUERY(pd <* part_occurrences | 
	(pd.name = 'specified instance')AND 
	(SIZEOF(QUERY(
pdor <* USEDIN(pd, 'AP210_DEVICE_FUNCTIONAL_AND_PHYSICAL_CHARACTERIZATION_MIM_LF.' +'PRODUCT_DEFINITION_OCCURRENCE_RELATIONSHIP.OCCURRENCE')| 
