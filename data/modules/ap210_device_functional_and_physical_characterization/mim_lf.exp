	WR1 : SIZEOF(QUERY(temp <* qualifiers |
             'AP210_DEVICE_FUNCTIONAL_AND_PHYSICAL_CHARACTERIZATION_MIM_LF.PRECISION_QUALIFIER'
             IN TYPEOF(temp)))< 2;
