    WHERE
      WR1 : start_vertex :<>: end_vertex;
      WR2 : SIZEOF(QUERY(ucur <* USEDIN(SELF,
            'PART_FEATURE_LOCATION_ARM.USAGE_CONCEPT_USAGE_RELATIONSHIP.' +
