*) 
SCHEMA Constructive_solid_geometry_2d_arm;
 
USE FROM Area_2d_arm;	-- ISO/TS 10303-1631

TYPE primitive_or_boolean_result_2d = SELECT (
    Primitive_2d, 
    Boolean_result_2d);
