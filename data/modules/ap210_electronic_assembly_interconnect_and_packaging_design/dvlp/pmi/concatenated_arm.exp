SUBTYPE_CONSTRAINT ks_axis_placement_3d_subtypes FOR Axis_placement_3d;
  ONEOF (Tool_attachment_point_frame,
         Tool_centre_point_frame);
END_SUBTYPE_CONSTRAINT;

