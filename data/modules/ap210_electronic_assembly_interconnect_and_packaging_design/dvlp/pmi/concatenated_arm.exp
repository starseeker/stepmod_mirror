      WR3 : NOT (SIZEOF(USEDIN(SELF, '')) > 1) OR 
                (SIZEOF(QUERY(ct <* USEDIN(SELF, '') | NOT ('ASSEMBLY_MODULE_WITH_MACRO_COMPONENT_ARM.ASSEMBLY_MODULE_MACRO_COMPONENT_JOIN_TERMINAL' IN TYPEOF(ct)))) = 1);
