  WR2: SIZEOF( QUERY( cgr_i <* SELF.items | SIZEOF(['AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.PLACEMENT', 'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.CURVE', 'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.EDGE', 'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.FACE', 'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.POINT', 'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.SURFACE', 'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.FACE_SURFACE', 'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.VERTEX_POINT'] * TYPEOF(cgr_i)) <> 1 )) = 0;
  WR3: SIZEOF( USEDIN( SELF, 'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.' + 'REPRESENTATION_RELATIONSHIP.REP_2') ) > 0;
  WR4: SIZEOF( USEDIN( SELF, 'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.' + 'REPRESENTATION_MAP.MAPPED_REPRESENTATION') ) = 0;
END_ENTITY;

ENTITY constructive_geometry_representation_relationship
  SUBTYPE OF (representation_relationship);
WHERE
  WR1: (SELF.rep_1.context_of_items :=: SELF.rep_2.context_of_items) AND ('AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.GEOMETRIC_REPRESENTATION_CONTEXT' IN TYPEOF(SELF.rep_1.context_of_items));
  WR2: 'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.CONSTRUCTIVE_GEOMETRY_REPRESENTATION' IN TYPEOF(SELF.rep_2);
