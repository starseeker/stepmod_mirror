ENTITY geometrically_bounded_2d_wireframe_representation
  SUBTYPE OF (shape_representation);
WHERE
  WR1: SELF.context_of_items\geometric_representation_context.
            coordinate_space_dimension = 2;
  WR2: SIZEOF (QUERY (item <* SELF.items |
         NOT (SIZEOF (TYPEOF (item) *
['AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.GEOMETRIC_CURVE_SET',           
'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.AXIS2_PLACEMENT_2D',
            'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.MAPPED_ITEM']) = 1)
       )) = 0;
  WR3: SIZEOF (QUERY (item <* SELF.items |
         SIZEOF (TYPEOF (item) *
          
['AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.GEOMETRIC_CURVE_SET',
            'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.MAPPED_ITEM']) = 1
       )) >= 1;
  WR4: SIZEOF (QUERY (mi <* QUERY (item <* SELF.items |
     ('AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.MAPPED_ITEM'
           IN TYPEOF (item))) |
         NOT ('AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.' +
              'GEOMETRICALLY_BOUNDED_2D_WIREFRAME_REPRESENTATION'
           IN TYPEOF
              (mi\mapped_item.mapping_source.mapped_representation))
       )) = 0;
  WR5: SIZEOF (QUERY (gcs <* QUERY (item <* SELF.items |
     ('AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.GEOMETRIC_CURVE_SET'
                IN TYPEOF (item))) |
         NOT (SIZEOF (QUERY (elem <* gcs\geometric_set.elements |
           NOT (SIZEOF (TYPEOF (elem) *
             ['AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.B_SPLINE_CURVE',
              'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.CIRCLE',
              'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.COMPOSITE_CURVE',
              'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.ELLIPSE',
              'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.OFFSET_CURVE_2D',
              'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.POINT',
              'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.POLYLINE',
              'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.TRIMMED_CURVE']) =
               1)
         )) = 0)
       )) = 0;
  WR6: SIZEOF (QUERY (gcs <* QUERY (item <* SELF.items |
     ('AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.GEOMETRIC_CURVE_SET'
                IN TYPEOF (item))) |
         NOT (SIZEOF (QUERY (crv <* 
           QUERY (elem <* gcs\geometric_set.elements |
              ('AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.CURVE'
                   IN TYPEOF (elem))) |
           NOT (valid_basis_curve_in_2d_wireframe
              (crv))
         )) = 0)
       )) = 0;
  WR7: SIZEOF (QUERY (gcs <* QUERY (item <* SELF.items |
           ('AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.GEOMETRIC_CURVE_SET'
                IN TYPEOF (item))) |
         NOT (SIZEOF (QUERY (pnt <*
           QUERY (elem <* gcs\geometric_set.elements |
             ('AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.POINT'
                  IN TYPEOF(elem))) |
             NOT (SIZEOF (TYPEOF (pnt) *
               ['AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.CARTESIAN_POINT',
                'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.POINT_ON_CURVE'])
          = 1)
         )) = 0)
       )) = 0;
  WR8: SIZEOF (QUERY (gcs <* QUERY (item <* SELF.items |
        
     ('AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.GEOMETRIC_CURVE_SET'
           IN TYPEOF (item))) |
         NOT (SIZEOF (QUERY (pl <* 
           QUERY (elem <* gcs\geometric_set.elements |
             ('AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.POLYLINE'
                  IN TYPEOF (elem))) |
             NOT (SIZEOF (pl\polyline.points) > 2)
         )) = 0)
       )) = 0;
END_ENTITY;

ENTITY geometrically_bounded_surface_shape_representation
  SUBTYPE OF (shape_representation);
WHERE
  WR1: SIZEOF(QUERY(it <* SELF.items | NOT (SIZEOF(['AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.GEOMETRIC_SET', 'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.MAPPED_ITEM', 'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.AXIS2_PLACEMENT_3D'] * TYPEOF(it)) = 1))) = 0;
  WR2: SIZEOF(QUERY(it <* SELF.items | SIZEOF(['AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.GEOMETRIC_SET', 'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.MAPPED_ITEM'] * TYPEOF(it)) = 1)) > 0;
  WR3: SIZEOF(QUERY(mi <* QUERY(it <* SELF.items | 'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.MAPPED_ITEM' IN TYPEOF(it)) | NOT (('AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.' + 'GEOMETRICALLY_BOUNDED_SURFACE_SHAPE_REPRESENTATION' IN TYPEOF(mi\mapped_item.mapping_source.mapped_representation)) AND (SIZEOF(QUERY(mr_it <* mi\mapped_item.mapping_source.mapped_representation.items | ('AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.GEOMETRIC_SET' IN TYPEOF(mr_it)))) > 0)))) = 0;
  WR4: SIZEOF(QUERY(gs <* QUERY(it <* SELF.items | 'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.GEOMETRIC_SET' IN TYPEOF(it)) | NOT (SIZEOF(QUERY(pnt <* QUERY(gsel <* gs\geometric_set.elements | 'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.POINT' IN TYPEOF(gsel)) | NOT (gbsf_check_point(pnt)))) = 0))) = 0;
  WR5: SIZEOF(QUERY(gs <* QUERY(it <* SELF.items | 'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.GEOMETRIC_SET' IN TYPEOF(it)) | NOT (SIZEOF(QUERY(cv <* QUERY(gsel <* gs\geometric_set.elements | 'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.CURVE' IN TYPEOF(gsel)) | NOT (gbsf_check_curve(cv)))) = 0))) = 0;
  WR6: SIZEOF(QUERY(gs <* QUERY(it <* SELF.items | 'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.GEOMETRIC_SET' IN TYPEOF(it)) | NOT (SIZEOF(QUERY(sf <* QUERY(gsel <* gs\geometric_set.elements | 'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.SURFACE' IN TYPEOF(gsel)) | NOT (gbsf_check_surface(sf)))) = 0))) = 0;
  WR7: SIZEOF(QUERY(gs <* QUERY(it <* SELF.items | 'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.GEOMETRIC_SET' IN TYPEOF(it)) | SIZEOF(QUERY(gsel <* gs\geometric_set.elements | 'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.SURFACE' IN TYPEOF(gsel))) > 0)) > 0;
END_ENTITY;

