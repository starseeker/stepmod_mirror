 FUNCTION associated_surface(arg : pcurve_or_surface) : surface;
   LOCAL
     surf : surface;
   END_LOCAL;
   
   IF 'AP210_LAMINATE_ASSEMBLY_DESIGN_MIM_LF.PCURVE' IN TYPEOF(arg) THEN
     surf := arg.basis_surface;
