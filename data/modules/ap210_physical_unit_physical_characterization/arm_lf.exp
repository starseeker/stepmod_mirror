    INVERSE
       composed_model : SET[1:1] OF Geometric_model FOR items;

 WHERE
       WR1 : (  ('AP210_PHYSICAL_UNIT_PHYSICAL_CHARACTERIZATION_ARM_LF.NON_FEATURE_SHAPE_MODEL' IN TYPEOF(associated_shape_definition)) AND 
                ('AP210_PHYSICAL_UNIT_PHYSICAL_CHARACTERIZATION_ARM_LF.NON_FEATURE_SHAPE_MODEL' IN TYPEOF(associating_usage_shape))) OR 
             (('AP210_PHYSICAL_UNIT_PHYSICAL_CHARACTERIZATION_ARM_LF.GEOMETRIC_MODEL' IN TYPEOF(associated_shape_definition)) AND 
              ('AP210_PHYSICAL_UNIT_PHYSICAL_CHARACTERIZATION_ARM_LF.GEOMETRIC_MODEL' IN TYPEOF(associating_usage_shape)));
       WR2 : NOT ( ('AP210_PHYSICAL_UNIT_PHYSICAL_CHARACTERIZATION_ARM_LF.GEOMETRIC_OPERATOR_TRANSFORMATION' IN
               TYPEOF(SELF)) AND ('AP210_PHYSICAL_UNIT_PHYSICAL_CHARACTERIZATION_ARM_LF.CARTESIAN_TRANSFORMATION_2D' IN 
