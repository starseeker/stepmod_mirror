             ('AP210_PHYSICAL_UNIT_PHYSICAL_CHARACTERIZATION_ARM_LF.NON_FEATURE_SHAPE_MODEL' IN TYPEOF(associating_usage_shape));  
       WR3 : associating_usage_shape = composed_model[1]; 
       WR4 : NOT EXISTS(SELF\Representation_item.name);
       WR5 : ('AP210_PHYSICAL_UNIT_PHYSICAL_CHARACTERIZATION_ARM_LF.GEOMETRIC_PLACEMENT' IN TYPEOF(SELF)) OR
