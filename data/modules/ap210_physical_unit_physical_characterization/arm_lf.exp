  ENTITY Length_tolerance_characteristic
    SUBTYPE OF(Tolerance_characteristic);
    WHERE
      WR1 : ('AP210_PHYSICAL_UNIT_PHYSICAL_CHARACTERIZATION_ARM_LF.STATISTICAL_TOLERANCE_CHARACTERISTIC' IN TYPEOF(SELF)) OR
            ('AP210_PHYSICAL_UNIT_PHYSICAL_CHARACTERIZATION_ARM_LF.SYMMETRICAL_TOLERANCE_CHARACTERISTIC' IN TYPEOF(SELF)) OR
            ('AP210_PHYSICAL_UNIT_PHYSICAL_CHARACTERIZATION_ARM_LF.PLUS_MINUS_TOLERANCE_CHARACTERISTIC' IN TYPEOF(SELF)) OR
        (SIZEOF(QUERY(it <* SELF\Representation.items |
          NOT('AP210_PHYSICAL_UNIT_PHYSICAL_CHARACTERIZATION_ARM_LF.LENGTH_DATA_ELEMENT' IN TYPEOF(it))
         )) = 0);
      WR2 : NOT('AP210_PHYSICAL_UNIT_PHYSICAL_CHARACTERIZATION_ARM_LF.SYMMETRICAL_TOLERANCE_CHARACTERISTIC' IN TYPEOF(SELF)) OR
        (SIZEOF(QUERY(it <* SELF\Representation.items |
        ('AP210_PHYSICAL_UNIT_PHYSICAL_CHARACTERIZATION_ARM_LF.LENGTH_DATA_ELEMENT' IN TYPEOF(it))
           )) = 1);
      WR3 : NOT('AP210_PHYSICAL_UNIT_PHYSICAL_CHARACTERIZATION_ARM_LF.STATISTICAL_TOLERANCE_CHARACTERISTIC' IN TYPEOF(SELF)) OR
        (SIZEOF(QUERY(it <* SELF\Representation.items |
        ('AP210_PHYSICAL_UNIT_PHYSICAL_CHARACTERIZATION_ARM_LF.LENGTH_DATA_ELEMENT' IN TYPEOF(it))
           )) = 1);
      WR4 : NOT EXISTS(SELF\Representation.description);
      WR5 : ('AP210_PHYSICAL_UNIT_PHYSICAL_CHARACTERIZATION_ARM_LF.PLUS_MINUS_TOLERANCE_CHARACTERISTIC' IN TYPEOF(SELF)) OR
        (SIZEOF(QUERY(it <* SELF\Representation.items |
          NOT(('AP210_PHYSICAL_UNIT_PHYSICAL_CHARACTERIZATION_ARM_LF.VALUE_WITH_TOLERANCES' IN TYPEOF(it)) AND
             ('AP210_PHYSICAL_UNIT_PHYSICAL_CHARACTERIZATION_ARM_LF.LENGTH_DATA_ELEMENT' IN TYPEOF(it\Value_with_tolerances.item_value)))
         )) = 0);
  END_ENTITY;

