      WR2: SIZEOF(QUERY( pv <* property_value |
                  NOT (SIZEOF(pv.property_value) = SIZEOF(property_value[1].property_value))
                  )) = 0;
      WR3: SIZEOF(USEDIN
                 (SELF,'AP210_PHYSICAL_UNIT_PHYSICAL_CHARACTERIZATION_ARM_LF.ASSIGNED_PROPERTY.DESCRIBED_ELEMENT')) = 0;
