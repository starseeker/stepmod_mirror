          NOT ('TESSELLATED_GEOMETRY_ARM.TESSELLATED_SHAPE_REPRESENTATION' IN
         TYPEOF(using_rep)))) = 0; 
 END_ENTITY;
 
  
 ENTITY Repositioned_tessellated_item
