  ENTITY Cubic_tessellated_connecting_edge
    SUBTYPE OF (Cubic_bezier_tessellated_edge);
    smooth : LOGICAL;
    face1 : Cubic_bezier_triangulated_face;
    face2 : Cubic_bezier_triangulated_face;
 END_ENTITY;

 ENTITY Tessellated_point_set
