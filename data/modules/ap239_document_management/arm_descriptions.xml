<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Ap239_document_management_arm  Type: ap239_dm_string_select 
   <express_ref linkend="ap239_document_management:arm:Ap239_document_management_arm.ap239_dm_string_select"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Ap239_document_management_arm.ap239_dm_string_select">
  <p>
    Using
    <express_ref linkend="multi_linguism:arm:Multi_linguism_arm.Language_indication"/>,
    a primary 
    <express_ref linkend="multi_linguism:arm:Multi_linguism_arm.Language"/>
    can be assigned to the entities in <b>ap239_dm_string_select</b>.
  </p>
  <p>
    Translations of the text attributes of the entities in
    <b>ap239_dm_string_select</b>
    into multiple 
    <express_ref linkend="multi_linguism:arm:Multi_linguism_arm.Language"/>s
    languages can be held using
    <express_ref linkend="multi_linguism:arm:Multi_linguism_arm.Attribute_translation_assignment"/>.
  </p>
</ext_description>

