SCHEMA Ap239_management_resource_information_arm; 

USE FROM Approval_arm;    -- ISO/TS 10303-1012

USE FROM Attribute_classification_arm;    -- ISO/CD-TS 10303-1246

USE FROM Certification_arm;    -- ISO/TS 10303-1044

USE FROM Classification_assignment_arm;    -- ISO/CD-TS 10303-1114

USE FROM Contract_arm;    -- ISO/TS 10303-1062

USE FROM Date_time_assignment_arm;    -- ISO/TS 10303-1014

USE FROM External_class_arm;    -- ISO/CD-TS 10303-1275

USE FROM Identification_assignment_arm;    -- ISO/TS 10303-1021

USE FROM Management_resource_information_arm;    -- ISO/CD-TS 10303-1288

USE FROM Multi_linguism_arm;    -- ISO/CD-TS 10303-1105

USE FROM Person_organization_arm;    -- ISO/TS 10303-1011

USE FROM Person_organization_assignment_arm;    -- ISO/TS 10303-1013

USE FROM Project_arm;    -- ISO/TS 10303-1061

USE FROM Security_classification_arm;    -- ISO/TS 10303-1015

USE FROM Set_theory_arm;    -- ISO/CD-TS 10303-1210


TYPE ap239_mri_classification_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON classification_item WITH 
   (Approval, 
    Approval_assignment, 
    Approval_relationship, 
    Approval_status, 
    Approving_person_organization, 
    Certification, 
    Certification_assignment, 
    Contract, 
    Contract_assignment, 
    Date_or_date_time_assignment, 
    Identification_assignment, 
    Language, 
    Organization, 
    Organization_or_person_in_organization_assignment, 
    Organization_relationship, 
    Person_in_organization, 
    Project, 
    Project_assignment, 
    Project_relationship, 
    Security_classification, 
    Security_classification_assignment);
END_TYPE; 

TYPE ap239_mri_classified_attribute_select = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON classified_attribute_select WITH 
   (Approval, 
    Approval_assignment, 
    Approval_relationship, 
    Approval_status, 
    Approving_person_organization, 
    Certification, 
    Certification_assignment, 
    Contract, 
    Date_or_date_time_assignment, 
    Identification_assignment, 
    Language, 
    Organization_or_person_in_organization_assignment, 
    Organization_relationship, 
    Person_in_organization, 
    Project_assignment, 
    Project_relationship, 
    Security_classification);
END_TYPE; 

END_SCHEMA;  -- Ap239_management_resource_information_arm
