<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Ap239_part_definition_information_arm  Type: ap239_padi_classified_attribute_select 
   <express_ref linkend="ap239_part_definition_information:arm:Ap239_part_definition_information_arm.ap239_padi_classified_attribute_select"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description
  linkend="Ap239_part_definition_information_arm.ap239_padi_classified_attribute_select">
  Using 
  <express_ref linkend="attribute_classification:arm:Attribute_classification_arm.Attribute_classification"/>,
  a <express_ref linkend="class:arm:Class_arm.Class"/>
  can be assigned to attributes on instances of the entities in
  <b>ap239_padi_classified_attribute_select</b> as follows:

   <ul>
    <li>
      <express_ref
        linkend="interface:arm:Interface_arm.Interface_connection"/>
	- the 'relation_type' attribute, inherited from the entity
      <express_ref linkend="Product_view_definition_relationship:arm:Product_view_definition_relationship_arm.View_definition_relationship"/>
      is being classified;
    </li>

  
    <li>
      <express_ref
        linkend="interface:arm:Interface_arm.Interface_connector_definition"/>
	- the 'name' attribute, inherited from the entity
      <express_ref linkend="Product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/>
      is being classified;
    </li>

  
    <li>
      <express_ref
        linkend="interface:arm:Interface_arm.Interface_specification_definition"/>
	- the 'name' attribute, inherited from the entity
      <express_ref linkend="Product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/>
      is being classified;
    </li>

  
    <li>
      <express_ref
        linkend="product_concept_identification:arm:Product_concept_identification_arm.Market"/>
	- the 'name' and 'market_segment_type' attributes on the entity
      <express_ref
        linkend="product_concept_identification:arm:Product_concept_identification_arm.Market"/>
      are being classified;
    </li>

  
    <li>
      <express_ref
        linkend="part_view_definition:arm:Part_view_definition_arm.Part_view_definition"/>
	- the 'name' attribute, inherited from the entity
      <express_ref linkend="Product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/>
      is being classified;
    </li>

  
    <li>
      <express_ref
        linkend="product_categorization:arm:Product_categorization_arm.Product_category"/>
	- the 'name' attribute on the entity
      <express_ref
        linkend="product_categorization:arm:Product_categorization_arm.Product_category"/>
      is being classified;
    </li>

  
    <li>
      <express_ref
        linkend="product_concept_identification:arm:Product_concept_identification_arm.Product_concept"/>
	- the 'name' attribute on the entity
      <express_ref
        linkend="product_concept_identification:arm:Product_concept_identification_arm.Product_concept"/>
      is being classified;
    </li>

  
    <li>
      <express_ref
        linkend="product_version_relationship:arm:Product_version_relationship_arm.Product_version_relationship"/>
	- the 'relation_type' attribute on the entity
      <express_ref
        linkend="product_version_relationship:arm:Product_version_relationship_arm.Product_version_relationship"/>
      is being classified;
    </li>

  
    <li>
      <express_ref
        linkend="requirement_identification_and_version:arm:Requirement_identification_and_version_arm.Requirement_version_relationship"/>
	- the 'relation_type' attribute inherited from the entity
      <express_ref
        linkend="product_version_relationship:arm:Product_version_relationship_arm.Product_version_relationship"/>
      is being classified;
    </li>

  
    <li>
      <express_ref
        linkend="requirement_view_definition:arm:Requirement_view_definition_arm.Requirement_view_definition"/>
	- the 'name' attribute, inherited from the entity
      <express_ref linkend="Product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/>
      is being classified;
    </li>

  
    <li>
      <express_ref
        linkend="product_view_definition:arm:Product_view_definition_arm.View_definition_context"/>
	- the 'application_domain' and 'life_cycle_stage' attributes on the entity
      <express_ref
        linkend="product_view_definition:arm:Product_view_definition_arm.View_definition_context"/>
      are being classified;
    </li>

  
    <li>
      <express_ref
        linkend="product_view_definition_relationship:arm:Product_view_definition_relationship_arm.View_definition_relationship"/>
	- the 'relation_type' attribute on the entity
      <express_ref
        linkend="product_view_definition_relationship:arm:Product_view_definition_relationship_arm.View_definition_relationship"/>
      is being classified;
    </li>
  </ul>

</ext_description>

