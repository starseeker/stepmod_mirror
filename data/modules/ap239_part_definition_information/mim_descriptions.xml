  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Ap239_part_definition_information_mim.characterized_applied_organization_assignment">
  A 
  <b>characterized_applied_organization_assignment</b>
  is a type of 
   <express_ref 
     linkend="product_property_definition_schema:ir_express:product_property_definition_schema.characterized_object"/>
   and 
   <express_ref 
     linkend="person_organization_assignment:mim:Person_organization_assignment_mim.applied_organization_assignment"/>
     that serves to assign 
   <express_ref 
     linkend="product_property_definition_schema:ir_express:product_property_definition_schema.property_definition"/>
   instances to 
   <express_ref 
     linkend="person_organization_assignment:mim:Person_organization_assignment_mim.applied_organization_assignment"/>
     instances. 
</ext_description>

