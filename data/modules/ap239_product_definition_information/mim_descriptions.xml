<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Ap239_product_definition_information_mim  Rule: restrict_product_category_for_product 
   <express_ref linkend="ap239_product_definition_information:mim:Ap239_product_definition_information_mim.restrict_product_category_for_product"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Ap239_product_definition_information_mim.restrict_product_category_for_product">
  The <b>restrict_product_category_for_product</b> rule enforces the
  existence of a
  <express_ref
    linkend="product_definition_schema:ir_express:product_definition_schema.product_related_product_category"/> 
  with a name of either 
  'breakdown element',
  'breakdown',
  'conceptual product',
  'document',
  'envelope',
  'functional breakdown',
  'functionality',
  'hybrid breakdown',
  'hybrid breakdown',
  'hybrid element',
  'interface connector',
  'interface specification',
  'message',
  'part',
  'physical breakdown',
  'raw material',
  'requirement',
  'slot',
  'system breakdown',
  'system element',
  'tool',
  'zone breakdown', or 
  'zone element',
  for each 
  <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product"/>. 
</ext_description>


<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Ap239_product_definition_information_mim  Entity: restrict_product_category_for_product  Where: WR1 
   <express_ref linkend="ap239_product_definition_information:mim:Ap239_product_definition_information_mim.restrict_product_category_for_product.wr:WR1"/>

  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Ap239_product_definition_information_mim.restrict_product_category_for_product.wr:WR1">
  Each instance of 
  <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product"/>
  shall be referenced by a 
  <express_ref
    linkend="product_definition_schema:ir_express:product_definition_schema.product_related_product_category"/>
  with a name of either 
  'breakdown element',
  'breakdown',
  'conceptual product',
  'document',
  'envelope',
  'functional breakdown',
  'functionality',
  'hybrid breakdown',
  'hybrid breakdown',
  'hybrid element',
  'interface connector',
  'interface specification',
  'message',
  'part',
  'physical breakdown',
  'raw material',
  'requirement',
  'slot',
  'system breakdown',
  'system element',
  'tool',
  'zone breakdown', or
  'zone element'.
</ext_description>



