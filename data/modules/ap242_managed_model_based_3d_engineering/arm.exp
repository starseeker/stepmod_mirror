TYPE ap242_condition_item = SELECT BASED_ON condition_item WITH (
    Activity, -- Activity, 
    Applied_activity_assignment, -- ActivityAssignment,
    Activity_method, -- ActivityMethod,
    Activity_method_assignment, -- ActivityMethodAssignment,
    Activity_method_relationship, -- ActivityMethodRelationship,
    Address_assignment, -- AddressAssignment, 
    Approval, -- Approval, 
    Approval_assignment, -- ApprovalAssignment, 
    -- Breakdown_element_view_relationship ??, -- BreakdownElementViewRelationship,
    -- Breakdown_version_assignment ??, -- BreakdownVersionAssignment,
    Certification_assignment, -- CertificationAssignment, 
    Condition_assignment, -- ConditionAssignment, 
    Contract_assignment, -- ContractAssignment, 
    -- NO CORRESPONDING ARM, -- DateAndPersonAssignment, 
    Date_or_date_time_assignment, -- DateTimeAssignment,
    Document_assignment, -- DocumentAssignment, 
    Document_definition_relationship, -- DocumentDefinitionRelationship, 
    Effectivity, -- Effectivity, 
    Effectivity_assignment, -- EffectivityAssignment, 
    Event_assignment, -- EventAssignment, 
    Frozen_assignment, -- FrozenAssignment, 
    Product_design_view_to_individual, -- IndividualPartViewRelationship, -- or in 442 View_definition_relationship ??
    Applied_information_usage_right, -- InformationUsageRightAssignment, 
    Material_property_association, -- ?? MaterialPropertyAssignment, 
    -- PROPERTIES ARE ORGANIZED DIFFERENTLY, -- ModelPropertyAssignment, 
    Organization_or_person_in_organization_assignment, -- OrganizationOrPersonInOrganizationAssignment, 
    Part_definition_relationship, -- PartViewRelationship, 
    Process_operation_resource_assignment, -- ProcessOperationResourceAssignment, 
    Project_assignment, -- ProjectAssignment,
    -- PROPERTIES ARE ORGANIZED DIFFERENTLY, -- PropertyDefinitionAssignment, 
    -- PROPERTIES ARE ORGANIZED DIFFERENTLY, -- PropertyValueAssignment, 
    Requirement_assignment, -- RequirementAssignment, 
    Requirement_view_definition_relationship, -- RequirementViewRelationship, 
    Security_classification_assignment, -- SecurityClassificationAssignment, 
    Class_specification_association, -- SpecificationAssignment, 
    Class_category_association, -- SpecificationCategoryAssignment, 
    Class_condition_association, -- ?? SpecificationConditionAssignment, 
    Class_inclusion_association, -- SpecificationInclusionAssignment, 
    Time_interval_assignment -- TimeIntervalAssignment, 
    -- NO CORRESPONDING ARM, -- WorkOrderAssignment, 
    -- NO CORRESPONDING ARM  -- WorkRequestAssignment
    );
END_TYPE;

