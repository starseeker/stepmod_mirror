<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Ap242_managed_model_based_3d_engineering_arm  Rule: addresses_are_assigned 
   <express_ref linkend="ap242_managed_model_based_3d_engineering:arm:Ap242_managed_model_based_3d_engineering_arm.addresses_are_assigned"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
  <ext_description linkend="Ap242_managed_model_based_3d_engineering_arm.addresses_are_assigned"> 
    The <b>addresses_are_assigned</b> rule constrains any instance of 
    <express_ref linkend="person_organization:arm:Person_organization_arm.Address"/> 
    to be used for at least an 
    <express_ref linkend="person_organization:arm:Person_organization_arm.Address_assignment"/>. 
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Ap242_managed_model_based_3d_engineering_arm  Entity: addresses_are_assigned  Where: WR1 
   <express_ref linkend="ap242_managed_model_based_3d_engineering:arm:Ap242_managed_model_based_3d_engineering_arm.addresses_are_assigned.wr:WR1"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Ap242_managed_model_based_3d_engineering_arm.addresses_are_assigned.wr:WR1"> 
  There shall not be any instance of 
  <express_ref linkend="person_organization:arm:Person_organization_arm.Address"/> 
  that is not used by at least an 
  <express_ref linkend="person_organization:arm:Person_organization_arm.Address_assignment"/>.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Ap242_managed_model_based_3d_engineering_arm  Rule: Person_requires_person_and_organization 
   <express_ref linkend="ap242_managed_model_based_3d_engineering:arm:Ap242_managed_model_based_3d_engineering_arm.Person_requires_person_and_organization"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Ap242_managed_model_based_3d_engineering_arm.person_requires_person_and_organization"> 
  The <b>person_requires_person_and_organization</b> rule constrains any instance of <express_ref linkend="person_organization:arm:Person_organization_arm.Person"/>
  to be referred to by at least a <express_ref linkend="person_organization:arm:Person_organization_arm.Person_in_organization"/>. 
  <note>This involves that persons are only considered in the context of their organizations.
  </note>
</ext_description>

<ext_description linkend="Ap242_managed_model_based_3d_engineering_arm.person_requires_person_and_organization.wr:WR1"> 
  There shall not be any instance of 
  <express_ref linkend="person_organization:arm:Person_organization_arm.Person"/>
  not referred to by at least a 
  <express_ref linkend="person_organization:arm:Person_organization_arm.Person_in_organization"/>.  
</ext_description>

<!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->

  <ext_description linkend="Ap242_managed_model_based_3d_engineering_arm.styled_element_constraint"> 
    The <b>styled_element_constraint</b> rule constrains any instance of <express_ref linkend="appearance_assignment_arm:arm:Appearance_assignment_arm.Styled_element"/>
    that is not also an instance of <express_ref linkend="vertex_colours_for_tessellated_geometry_arm:arm:Vertex_colours_for_tessellated_geometry_arm.Styled_tessellated_item"/> to provide the <b>appearance</b> attribute.    
  </ext_description>
  
  <ext_description linkend="Ap242_managed_model_based_3d_engineering_arm.person_requires_person_and_organization.wr:WR1"> 
    Each instance of <express_ref linkend="appearance_assignment_arm:arm:Appearance_assignment_arm.Styled_element"/> shall satisfy the description of this constraint.  
  </ext_description>
  
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Configuration_control_3d_design_arm  Rule: Shape_requires_product_view 
   <express_ref linkend="ap242_managed_model_based_3d_engineering:arm:Ap242_managed_model_based_3d_engineering_arm.Shape_requires_product_view"/>

  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
  <ext_description linkend="Ap242_managed_model_based_3d_engineering_arm.shape_requires_product_view"> 
  The <b>shape_requires_product_view</b> constrains instances of <express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Item_shape"/>

 to only be associated with instances of <express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/>

 or of its subtypes.

</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Configuration_control_3d_design_arm  Entity: Shape_requires_product_view  Where: WR1 
   <express_ref linkend="ap242_managed_model_based_3d_engineering:arm:Ap242_managed_model_based_3d_engineering_arm.Shape_requires_product_view.wr:WR1"/>


  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Ap242_managed_model_based_3d_engineering_arm.shape_requires_product_view.wr:WR1"> There shall not be any instance of <express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Item_shape"/>

 not referred to by an instance of <express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/>

 or of one of its subtypes.

</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Configuration_control_3d_design_arm  Subtype constraint: exclusive_product_versions_203cc2  
   <express_ref linkend="ap242_managed_model_based_3d_engineering:arm:Ap242_managed_model_based_3d_engineering_arm.exclusive_product_versions_203cc2"/>

  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
  <ext_description linkend="Ap242_managed_model_based_3d_engineering_arm.exclusive_product_versions_203cc2"> The <b>exclusive_product_versions</b> constraint specifies that instances of <express_ref linkend="product_version:arm:Product_version_arm.Product_version"/>

 shall not be simultaneously an instance of two of the types <express_ref linkend="document_and_version_identification:arm:Document_and_version_identification_arm.Document_version"/>, and <express_ref linkend="part_and_version_identification:arm:Part_and_version_identification_arm.Part_version"/>. 

</ext_description>

<ext_description linkend="Ap242_managed_model_based_3d_engineering_arm.specific_product_occurrence_definition_relationship"> 
  The <b>specific_product_occurrence_definition_relationship</b> rule constrains any instance of 
  <express_ref linkend="assembly_structure:arm:Assembly_structure_arm.Product_occurrence_definition_relationship"/> 
  to relates a
  <express_ref linkend="product_occurrence:arm:Product_occurrence_arm.Product_occurrence"/>
  to a
  <express_ref linkend="part_view_definition:arm:Part_view_definition_arm.Part_view_definition"/>.  
</ext_description>

<ext_description linkend="Ap242_managed_model_based_3d_engineering_arm.specific_product_occurrence_definition_relationship.wr:WR1">
  Attribute
  <express_ref linkend="assembly_structure:arm:Assembly_structure_arm.Product_occurrence_definition_relationship.relating_view"/>
  of 
  <express_ref linkend="assembly_structure:arm:Assembly_structure_arm.Product_occurrence_definition_relationship"/>
  shall have a value of type 
  <express_ref linkend="part_view_definition:arm:Part_view_definition_arm.Part_view_definition"/>.
</ext_description>

<ext_description linkend="Ap242_managed_model_based_3d_engineering_arm.specific_product_occurrence_definition_relationship.wr:WR2"> 
  Attribute
  <express_ref linkend="assembly_structure:arm:Assembly_structure_arm.Product_occurrence_definition_relationship.related_view"/>
  of 
  <express_ref linkend="assembly_structure:arm:Assembly_structure_arm.Product_occurrence_definition_relationship"/>
  shall have a value of type 
  <express_ref linkend="product_occurrence:arm:Product_occurrence_arm.Product_occurrence"/>.
</ext_description>



