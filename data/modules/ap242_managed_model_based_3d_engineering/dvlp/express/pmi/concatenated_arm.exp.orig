  END_TYPE; 

  TYPE breakdown_item = EXTENSIBLE SELECT
    (Breakdown_element_definition, 
     Breakdown_element_usage);
  END_TYPE; 

  TYPE product_item = EXTENSIBLE SELECT
    (Product_view_definition, 
     View_definition_usage);
