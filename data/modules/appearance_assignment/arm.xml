<?xml version="1.0" encoding="utf-8"?><?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express
  SYSTEM "../../../dtd/express.dtd">
<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="">
   <application name="JSDAI" owner="LKSoft" source="appearance_assignment_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Appearance_assignment_arm">
      <interface kind="use" schema="Elemental_geometric_shape_arm"/>
      <interface kind="reference" schema="Support_resource_arm">
         <interfaced.item name="bag_to_set"/>
      </interface>
      <type name="appearance_context">
         <select extensible="YES" genericentity="YES" selectitems="Representation"/>
      </type>
      <type name="appearance_select">
         <select extensible="YES" genericentity="YES"/>
      </type>
      <type name="context_dependent_appearance_assignment_set">
         <aggregate lower="2" type="SET" upper="?"/>
         <typename name="Context_dependent_appearance_assignment"/>
      </type>
      <type name="overriding_style_context">
         <select extensible="YES" genericentity="YES" selectitems="Detailed_geometric_model_element Styled_model"/>
      </type>
      <type name="presented_model_select">
         <select extensible="YES" genericentity="YES" selectitems="Styled_model"/>
      </type>
      <type name="styled_element_appearance">
         <select selectitems="Appearance_assignment context_dependent_appearance_assignment_set"/>
      </type>
      <entity name="Appearance_assignment">
         <explicit name="appearance_components">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="appearance_select"/>
         </explicit>
      </entity>
      <entity name="Context_dependent_appearance_assignment" supertypes="Appearance_assignment">
         <explicit name="context_definition">
            <typename name="appearance_context"/>
         </explicit>
      </entity>
      <entity name="Context_dependent_over_riding_styled_element" supertypes="Over_riding_styled_element">
         <explicit name="context_definition">
            <aggregate lower="1" type="LIST" upper="?"/>
            <typename name="overriding_style_context"/>
         </explicit>
      </entity>
      <entity name="Over_riding_styled_element" supertypes="Styled_element">
         <explicit name="over_ridden_element">
            <typename name="Styled_element"/>
         </explicit>
      </entity>
      <entity name="Planar_box" supertypes="Detailed_geometric_model_element">
         <explicit name="size_in_x">
            <typename name="length_measure"/>
         </explicit>
         <explicit name="size_in_y">
            <typename name="length_measure"/>
         </explicit>
         <explicit name="placement">
            <typename name="Axis_placement"/>
         </explicit>
      </entity>
      <entity name="Styled_element" supertypes="Representation_item">
         <explicit name="appearance">
            <typename name="styled_element_appearance"/>
         </explicit>
         <explicit name="element">
            <typename name="Detailed_geometric_model_element"/>
         </explicit>
         <explicit name="invisible">
            <builtintype type="BOOLEAN"/>
         </explicit>
      </entity>
      <entity abstract.supertype="YES" name="Styled_model" supertypes="Representation">
         <explicit name="version_id" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="context_of_items">
            <typename name="Geometric_coordinate_space"/>
            <redeclaration entity-ref="Representation"/>
         </explicit>
         <derived expression="bag_to_set(QUERY (r &lt;* (USEDIN(SELF\Representation.context_of_items,&#xA;&#x9; &#x9;'FOUNDATION_REPRESENTATION_ARM.' + &#xA;&#x9; &#x9;'REPRESENTATION.' + 'CONTEXT_OF_ITEMS')) | &#xA;&#x9; &#x9;'ELEMENTAL_GEOMETRIC_SHAPE_ARM.GEOMETRIC_MODEL' IN TYPEOF(r)))" name="styled_geometry">
            <aggregate lower="0" type="SET" upper="?"/>
            <typename name="Geometric_model"/>
         </derived>
      </entity>
      <entity name="Styled_model_replication" supertypes="Representation_item">
         <explicit name="replicated_model">
            <typename name="Styled_model"/>
         </explicit>
         <explicit name="source">
            <typename name="Axis_placement"/>
         </explicit>
         <explicit name="target">
            <typename name="geometric_mapping_target"/>
         </explicit>
      </entity>
      <subtype.constraint entity="Representation_item" name="aa_representation_item_subtypes" super.expression="ONEOF (Styled_element, Styled_model_replication)"/>
   </schema>
</express>