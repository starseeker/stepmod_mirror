<?xml version="1.0"?>
<!-- $Id: mim.xml,v 1.12 2002/05/27 22:16:00 darla Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/05/27 22:16:00 $"
  rcs.revision="$Revision: 1.12 $"
  description.file="mim_descriptions.xml">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.9"
    source="../data/modules/approval/mim.exp"/>

  <schema name="Approval_mim">
    <interface
      kind="use"
      schema="Date_time_mim">
    </interface>

    <interface
      kind="use"
      schema="Person_organization_mim">
    </interface>

    <interface
      kind="use"
      schema="approval_schema">
      <interfaced.item
        name="approval"/>
      <interfaced.item
        name="approval_date_time"/>
      <interfaced.item
        name="approval_person_organization"/>
      <interfaced.item
        name="approval_relationship"/>
      <interfaced.item
        name="approval_role"/>
      <interfaced.item
        name="approval_status"/>
    </interface>

    <interface
      kind="use"
      schema="management_resources_schema">
      <interfaced.item
        name="approval_assignment"/>
    </interface>

    <type name="approval_item">
      <select
        extensible="YES">
      </select>

    </type>
    <entity
      name="applied_approval_assignment"
      supertypes="approval_assignment">
      <explicit
        name="items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="approval_item"/>
      </explicit>
    </entity>
  </schema>

</express>
