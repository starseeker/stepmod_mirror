<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$">
   <application name="JSDAI" owner="LKSoft" source="area_2d_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Area_2d_arm">
      <interface kind="use" schema="Extended_basic_geometry_arm"/>
      <interface kind="use" schema="Value_with_unit_extension_arm"/>
      <entity name="Circular_area" supertypes="Primitive_2d">
         <explicit name="centre">
            <typename name="Cartesian_point"/>
         </explicit>
         <explicit name="radius">
            <typename name="Length_data_element"/>
         </explicit>
      </entity>
      <entity name="Elliptic_area" supertypes="Primitive_2d">
         <explicit name="position">
            <typename name="Axis_placement_2d"/>
         </explicit>
         <explicit name="semi_axis_1">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="semi_axis_2">
            <typename name="Length_data_element"/>
         </explicit>
      </entity>
      <entity name="Polygonal_area" supertypes="Primitive_2d">
         <explicit name="bounds">
            <aggregate lower="3" type="LIST" unique="YES" upper="?"/>
            <typename name="Cartesian_point"/>
         </explicit>
      </entity>
      <entity abstract.supertype="YES" name="Primitive_2d" super.expression="ONEOF (Circular_area, Elliptic_area, Rectangular_area, Polygonal_area)" supertypes="Detailed_geometric_model_element"/>
      <entity name="Rectangular_area" supertypes="Primitive_2d">
         <explicit name="position">
            <typename name="Axis_placement_2d"/>
         </explicit>
         <explicit name="x">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="y">
            <typename name="Length_data_element"/>
         </explicit>
      </entity>
   </schema>
</express>
