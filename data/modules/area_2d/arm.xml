<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:07" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="area_2d_arm schema_instance"/>
   <schema name="AREA_2D_ARM">
      <interface kind="use" schema="EXTENDED_ELEMENTAL_GEOMETRIC_SHAPE_ARM"/>
      <interface kind="use" schema="VALUE_WITH_UNIT_EXTENSION_ARM"/>
      <entity name="circular_area" supertypes="primitive_2d">
         <explicit name="centre">
            <typename name="cartesian_point"/>
         </explicit>
         <explicit name="radius">
            <typename name="length_data_element"/>
         </explicit>
      </entity>
      <entity name="elliptic_area" supertypes="primitive_2d">
         <explicit name="position">
            <typename name="axis_placement_2d"/>
         </explicit>
         <explicit name="semi_axis_1">
            <typename name="length_data_element"/>
         </explicit>
         <explicit name="semi_axis_2">
            <typename name="length_data_element"/>
         </explicit>
      </entity>
      <entity name="polygonal_area" supertypes="primitive_2d">
         <explicit name="bounds">
            <aggregate type="LIST" unique="YES" lower="3" upper="?"/>
            <typename name="cartesian_point"/>
         </explicit>
      </entity>
      <entity name="primitive_2d" abstract.supertype="YES" supertypes="detailed_geometric_model_element" super.expression="ONEOF (circular_area, elliptic_area, rectangular_area, polygonal_area)"/>
      <entity name="rectangular_area" supertypes="primitive_2d">
         <explicit name="position">
            <typename name="axis_placement_2d"/>
         </explicit>
         <explicit name="x">
            <typename name="length_data_element"/>
         </explicit>
         <explicit name="y">
            <typename name="length_data_element"/>
         </explicit>
      </entity>
   </schema>
</express>
