<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$">
   <application name="JSDAI" owner="LKSoft" source="assembly_2d_shape_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Assembly_2d_shape_arm">
      <interface kind="use" schema="Assembly_technology_arm"/>
      <interface kind="use" schema="Physical_unit_2d_design_view_arm"/>
      <entity name="Bond_assembly_2d_position">
         <explicit name="bond">
            <typename name="Assembled_with_bonding"/>
         </explicit>
         <explicit name="assembly_model">
            <typename name="Physical_unit_planar_shape_model"/>
         </explicit>
         <explicit name="bond_model">
            <typename name="Default_assembly_bond_shape_model"/>
         </explicit>
         <explicit name="bond_definition_placement">
            <typename name="Axis_placement_2d"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="bond"/>
         </unique>
      </entity>
      <entity name="Component_2d_edge_location" supertypes="Component_2d_location">
         <explicit name="reference_terminal_assembly_joint">
            <typename name="Assembly_joint"/>
         </explicit>
         <explicit name="mounting_surface_assembly_joint">
            <typename name="Assembly_joint"/>
         </explicit>
         <where expression="reference_terminal_assembly_joint &lt;&gt; mounting_surface_assembly_joint" label="WR1"/>
         <where expression="SELF\Component_2d_location.substrate_location = FALSE" label="WR2"/>
         <where expression="SIZEOF(['ASSEMBLY_MODULE_WITH_INTERCONNECT_COMPONENT_ARM.INTERCONNECT_MODULE_COMPONENT_TERMINAL',&#10;                    'ASSEMBLY_MODULE_WITH_INTERCONNECT_COMPONENT_ARM.INTERCONNECT_MODULE_COMPONENT_SURFACE_FEATURE'] * TYPEOF(mounting_surface_assembly_joint.assembly_feature_1)) &gt;= 1" label="WR3"/>
         <where expression="'PACKAGE_ARM.PRIMARY_REFERENCE_TERMINAL' IN TYPEOF(reference_terminal_assembly_joint\assembly_joint.assembly_feature_1)" label="WR4"/>
         <where expression="NOT EXISTS(SELF\Representation_relationship.description)" label="WR5"/>
      </entity>
      <entity name="Component_2d_stacked_location" supertypes="Component_2d_location">
         <explicit name="mounting_joint">
            <typename name="Assembly_joint"/>
         </explicit>
         <where expression="SIZEOF(['ASSEMBLY_MODULE_WITH_INTERCONNECT_COMPONENT_ARM.INTERCONNECT_MODULE_COMPONENT_TERMINAL',&#10;                    'ASSEMBLY_MODULE_WITH_INTERCONNECT_COMPONENT_ARM.INTERCONNECT_MODULE_COMPONENT_SURFACE_FEATURE'] * TYPEOF(mounting_joint.assembly_feature_1)) = 0" label="WR1"/>
         <where expression="SIZEOF(['ASSEMBLY_MODULE_WITH_INTERCONNECT_COMPONENT_ARM.INTERCONNECT_MODULE_COMPONENT_TERMINAL',&#10;                    'ASSEMBLY_MODULE_WITH_INTERCONNECT_COMPONENT_ARM.INTERCONNECT_MODULE_COMPONENT_SURFACE_FEATURE'] * TYPEOF(mounting_joint.assembly_feature_2)) = 0" label="WR2"/>
         <where expression="SELF\Component_2d_location.substrate_location = FALSE" label="WR3"/>
         <where expression="NOT EXISTS(SELF\Representation_relationship.description)" label="WR4"/>
      </entity>
      <entity name="Component_2d_surface_location" supertypes="Component_2d_location">
         <explicit name="mounting_surface_assembly_joint">
            <typename name="Assembly_joint"/>
         </explicit>
         <where expression="SELF\Component_2d_location.substrate_location = FALSE" label="WR1"/>
         <where expression="SIZEOF(['ASSEMBLY_MODULE_WITH_INTERCONNECT_COMPONENT_ARM.INTERCONNECT_MODULE_COMPONENT_TERMINAL',&#10;                    'ASSEMBLY_MODULE_WITH_INTERCONNECT_COMPONENT_ARM.INTERCONNECT_MODULE_COMPONENT_SURFACE_FEATURE'] * TYPEOF(mounting_surface_assembly_joint.assembly_feature_1)) &gt;= 1" label="WR2"/>
         <where expression="NOT EXISTS(SELF\Representation_relationship.description)" label="WR3"/>
      </entity>
      <subtype.constraint entity="Component_2d_location" name="a2ds_component_2d_location_subtypes" super.expression="ONEOF (Component_2d_edge_location, Component_2d_stacked_location, Component_2d_surface_location)"/>
   </schema>
</express>
