<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-01-21T11:05:53" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="assembly_2d_shape_arm schema_instance"/>
   <schema name="Assembly_2d_shape_arm">
      <interface kind="use" schema="Physical_unit_2d_design_view_arm"/>
      <interface kind="use" schema="Assembly_technology_arm"/>
      <entity name="Component_2d_edge_location" supertypes="Component_2d_location">
         <explicit name="reference_terminal_assembly_joint">
            <typename name="Assembly_joint"/>
         </explicit>
         <explicit name="mounting_surface_assembly_joint">
            <typename name="Assembly_joint"/>
         </explicit>
         <where label="WR1" expression=" reference_terminal_assembly_joint &lt;&gt; mounting_surface_assembly_joint "/>
         <where label="WR2" expression=" SELF \ Component_2d_location . substrate_location = FALSE "/>
         <where label="WR3" expression=" SIZEOF ( [ 'ASSEMBLY_2D_SHAPE_ARM.INTERCONNECT_MODULE_COMPONENT_TERMINAL' , 'ASSEMBLY_2D_SHAPE_ARM.INTERCONNECT_MODULE_COMPONENT_SURFACE_FEATURE' ] * TYPEOF ( mounting_surface_assembly_joint . assembly_features_1 ) ) &gt;= 1 "/>
      </entity>
      <entity name="Component_2d_stacked_location" supertypes="Component_2d_location">
         <explicit name="mounting_joint">
            <typename name="Assembly_joint"/>
         </explicit>
         <where label="WR1" expression=" SIZEOF ( [ 'ASSEMBLY_2D_SHAPE_ARM.INTERCONNECT_MODULE_COMPONENT_TERMINAL' , 'ASSEMBLY_2D_SHAPE_ARM.INTERCONNECT_MODULE_COMPONENT_SURFACE_FEATURE' ] * TYPEOF ( mounting_joint . assembly_features_1 ) ) = 0 "/>
         <where label="WR2" expression=" SIZEOF ( [ 'ASSEMBLY_2D_SHAPE_ARM.INTERCONNECT_MODULE_COMPONENT_TERMINAL' , 'ASSEMBLY_2D_SHAPE_ARM.INTERCONNECT_MODULE_COMPONENT_SURFACE_FEATURE' ] * TYPEOF ( mounting_joint . assembly_features_2 ) ) = 0 "/>
         <where label="WR3" expression=" SELF \ Component_2d_location . substrate_location = FALSE "/>
      </entity>
      <entity name="Component_2d_surface_location" supertypes="Component_2d_location">
         <explicit name="mounting_surface_assembly_joint">
            <typename name="Assembly_joint"/>
         </explicit>
         <where label="WR1" expression=" SELF \ Component_2d_location . substrate_location = FALSE "/>
         <where label="WR2" expression=" SIZEOF ( [ 'ASSEMBLY_2D_SHAPE_ARM.INTERCONNECT_MODULE_COMPONENT_TERMINAL' , 'ASSEMBLY_2D_SHAPE_ARM.INTERCONNECT_MODULE_COMPONENT_SURFACE_FEATURE' ] * TYPEOF ( mounting_surface_assembly_joint . assembly_features_1 ) ) &gt;= 1 "/>
      </entity>
      <subtype.constraint name="a2ds_component_2d_location_subtypes" entity="Component_2d_location" super.expression="ONEOF (Component_2d_edge_location, Component_2d_stacked_location, Component_2d_surface_location)"/>
   </schema>
</express>
