<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: Assembly_2d_shape_arm
   <express_ref linkend="assembly_2d_shape:arm:Assembly_2d_shape_arm"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Assembly_2d_shape_arm">
</ext_description>

<!-- +++++++++++ Bond_assembly_2d_position +++++++++++ -->
<ext_description linkend="Assembly_2d_shape_arm.Bond_assembly_2d_position">
	A <b>Bond_assembly_2d_position</b> is a means to provide information about the shape and position of a bond in an assembly in two-dimensional space.
</ext_description>

<!-- bond -->
<ext_description linkend="Assembly_2d_shape_arm.Bond_assembly_2d_position.bond">
	specifies the role of the <express_ref linkend="Assembly_technology:arm:Assembly_technology_arm.Assembled_with_bonding"/> for the <b>Bond_assembly_2d_position</b>.  The <b>bond</b> is the joint that is created in the assembly.
</ext_description>

<!-- assembly_model -->
<ext_description linkend="Assembly_2d_shape_arm.Bond_assembly_2d_position.assembly_model">
	specifies the role of the <express_ref linkend="Physical_unit_2d_shape:arm:Physical_unit_2d_shape_arm.Physical_unit_planar_shape_model"/> 
	for the <b>Bond_assembly_2d_position</b>.  The <b>assembly_model</b> is the planar shape of the bond. 
</ext_description>

<!-- bond_model -->
<ext_description linkend="Assembly_2d_shape_arm.Bond_assembly_2d_position.bond_model">
	specifies the role of the <express_ref linkend="Assembly_technology:arm:Assembly_technology_arm.Default_assembly_bond_shape_model"/> 
	for the <b>Bond_assembly_2d_position</b>.  The <b>bond_model</b> is the default shape of the bond. 
</ext_description>

<!-- bond_definition_placement -->
<ext_description linkend="Assembly_2d_shape_arm.Bond_assembly_2d_position.bond_definition_placement"> 
	specifies the role of the <express_ref linkend="Extended_basic_geometry:arm:Extended_basic_geometry_arm.Axis_placement_2d"/> for the <b>Bond_assembly_2d_position</b>.  The <b>bond_definition_placement</b> is the position of the bond in two-dimensional space. 
</ext_description>

<!-- UR1 -->
<ext_description linkend="Assembly_2d_shape_arm.Bond_assembly_2d_position.ur:UR1">
<p>
	The <b>bond</b> shall be unique within a population of <b>Bond_assembly_2d_position</b>.
</p>
</ext_description>

<!-- +++++++++++ Component_2d_edge_location +++++++++++ -->
<ext_description linkend="Assembly_2d_shape_arm.Component_2d_edge_location">
<p>
	A <b>Component_2d_edge_location</b> is a type of 
	<express_ref linkend="Physical_unit_2d_design_view:arm:Physical_unit_2d_design_view_arm.Component_2d_location"/>.
	The <b>Component_2d_edge_location</b> places components on the edge of a substrate in order to maintain contact with both the top and bottom 
	circuitry.
</p>
	<note>
	<p>
		<express_ref linkend="Bare_die:arm:Bare_die_arm.Connection_zone_bare_die_interface_plane_relationship"/> 
		and 
		<express_ref linkend="Assembly_technology:arm:Assembly_technology_arm.Connection_zone_based_assembly_joint"/> 
		may be used to provide more details if needed by a receiving organization.
	 </p>
	</note>
</ext_description>

<!-- mounting_surface_assembly_joint -->
<ext_description linkend="Assembly_2d_shape_arm.Component_2d_edge_location.mounting_surface_assembly_joint">
	specifies one role of the 
	<express_ref linkend="Assembly_technology:arm:Assembly_technology_arm.Assembly_joint"/> 
	for the <b>Component_2d_edge_location</b>.
</ext_description>


<!-- reference_terminal_assembly_joint -->
<ext_description linkend="Assembly_2d_shape_arm.Component_2d_edge_location.reference_terminal_assembly_joint">
	specifies one role of the 
	<express_ref linkend="Assembly_technology:arm:Assembly_technology_arm.Assembly_joint"/> 
	for the <b>Component_2d_edge_location</b>.  The <b>reference_terminal_assembly_joint</b> joins the 
	<express_ref linkend="Package:arm:Package_arm.Primary_reference_terminal"/> 
	of the component being located to the associated mating component feature.
	The surface that the mating component feature is associated with is part of the orientation 
	description for the <b>Component_2d_edge_location</b>.
</ext_description>


<!-- WR1 -->
<ext_description linkend="Assembly_2d_shape_arm.Component_2d_edge_location.wr:WR1">
<p>
	<b>reference_terminal_assembly_joint</b> shall not be <b>mounting_surface_assembly_joint</b>.
</p>
</ext_description>


<!-- WR2 -->
<ext_description linkend="Assembly_2d_shape_arm.Component_2d_edge_location.wr:WR2">
	The <express_ref linkend="Physical_unit_2d_design_view:arm:Physical_unit_2d_design_view_arm.Component_2d_location.substrate_location"/>  
	inherited from the 
	<express_ref linkend="Physical_unit_2d_design_view:arm:Physical_unit_2d_design_view_arm.Component_2d_location"/> 
	shall be "False". 
</ext_description>


<!-- WR3 -->
<ext_description linkend="Assembly_2d_shape_arm.Component_2d_edge_location.wr:WR3">
<p>
	<express_ref linkend="assembly_technology:arm:Assembly_technology_arm.Assembly_joint.assembly_feature_1"/> 
	of <b>mounting_surface_assembly_joint</b>
	shall only be a 
	<express_ref linkend="assembly_module_with_interconnect_component:arm:Assembly_module_with_interconnect_component_arm.Interconnect_module_component_terminal"/>
	or a 
	<express_ref linkend="assembly_module_with_interconnect_component:arm:Assembly_module_with_interconnect_component_arm.Interconnect_module_component_surface_feature"/>.
</p>
</ext_description>


<!-- WR4 -->
<ext_description linkend="Assembly_2d_shape_arm.Component_2d_edge_location.wr:WR4">
The <b>reference_terminal_assembly_joint</b> shall reference an entity instance of type
<express_ref linkend="package:arm:Package_arm.Primary_reference_terminal"/> as
<express_ref linkend="assembly_technology:arm:Assembly_technology_arm.Assembly_joint.assembly_feature_1"/>.
</ext_description>

<!-- WR5 -->
<ext_description linkend="Assembly_2d_shape_arm.Component_2d_edge_location.wr:WR5">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation_relationship.description"/>
	shall not be populated.
</ext_description>

<!-- +++++++++++ Component_2d_stacked_location +++++++++++ -->
<ext_description linkend="Assembly_2d_shape_arm.Component_2d_stacked_location">
<p>
	A <b>Component_2d_stacked_location</b> is a type of 
	<express_ref linkend="Physical_unit_2d_design_view:arm:Physical_unit_2d_design_view_arm.Component_2d_location"/> 
	where the components are vertically stacked.  
	The stack reference direction shall be parallel to the normal vector of the primary surface of the interconnect.
</p>
	<note number="1">
		The reference direction is independent of whether the stack is based on the primary or secondary surface of the interconnect.
	</note>
	<note number="2">
		The reference direction is opposite to that of the Z axis of the interconnect internal layer stackup.
	</note>
	<note number="3">
		Applications should ensure that stacked components maintain a consistent direction in the stack.
	</note>
	<example>
		The arrangement of stacked memory chips may be represented using <b>Component_2d_stacked_location</b>.
	</example>
</ext_description>

<!-- mounting_joint -->
<ext_description linkend="Assembly_2d_shape_arm.Component_2d_stacked_location.mounting_joint">
	specifies the role of the 
	<express_ref linkend="Assembly_technology:arm:Assembly_technology_arm.Assembly_joint"/> 
	for the <b>Component_2d_stacked_location</b>.  The joint provides a mechanism
	for traversing the vertical stack unambiguously. 
	The stack direction vector origin shall be represented by the feature specified by the 
	<express_ref linkend="assembly_technology:arm:Assembly_technology_arm.Assembly_joint.assembly_feature_1"/> 
	attribute of the <b>mounting_joint</b>. 
	The stack direction vector destination shall be represented by the feature specified by the 
	<express_ref linkend="assembly_technology:arm:Assembly_technology_arm.Assembly_joint.assembly_feature_2"/> 
	attribute of the <b>mounting_joint</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Assembly_2d_shape_arm.Component_2d_stacked_location.wr:WR1">
<p>
	<express_ref linkend="assembly_technology:arm:Assembly_technology_arm.Assembly_joint.assembly_feature_1"/> 
	of <b>mounting_joint</b>
	shall not be a 
	<express_ref linkend="assembly_module_with_interconnect_component:arm:Assembly_module_with_interconnect_component_arm.Interconnect_module_component_terminal"/>
	nor a 
	<express_ref linkend="assembly_module_with_interconnect_component:arm:Assembly_module_with_interconnect_component_arm.Interconnect_module_component_surface_feature"/>.
</p>
</ext_description>

<!-- WR2 -->
<ext_description linkend="Assembly_2d_shape_arm.Component_2d_stacked_location.wr:WR2">
<p>
	<express_ref linkend="assembly_technology:arm:Assembly_technology_arm.Assembly_joint.assembly_feature_2"/> 
	of <b>mounting_joint</b>
	shall not be a 
	<express_ref linkend="assembly_module_with_interconnect_component:arm:Assembly_module_with_interconnect_component_arm.Interconnect_module_component_terminal"/>
	nor a 
	<express_ref linkend="assembly_module_with_interconnect_component:arm:Assembly_module_with_interconnect_component_arm.Interconnect_module_component_surface_feature"/>.
</p>
</ext_description>

<!-- WR3 -->
<ext_description linkend="Assembly_2d_shape_arm.Component_2d_stacked_location.wr:WR3">
	The <express_ref linkend="Physical_unit_2d_design_view:arm:Physical_unit_2d_design_view_arm.Component_2d_location.substrate_location"/>  
	inherited from the 
	<express_ref linkend="Physical_unit_2d_design_view:arm:Physical_unit_2d_design_view_arm.Component_2d_location"/> 
	shall be "False".
</ext_description>

<!-- WR4 -->
<ext_description linkend="Assembly_2d_shape_arm.Component_2d_stacked_location.wr:WR4">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation_relationship.description"/> 
	shall not be populated.
</ext_description>

<!-- +++++++++++ Component_2d_surface_location +++++++++++ -->
<ext_description linkend="Assembly_2d_shape_arm.Component_2d_surface_location">
<p>
	A <b>Component_2d_surface_location</b> is a type of 
	<express_ref linkend="Physical_unit_2d_design_view:arm:Physical_unit_2d_design_view_arm.Component_2d_location"/>.
	The <b>Component_2d_surface_location</b> places components on the external surface of a substrate.
</p>
	<note>
	<p>
	<express_ref linkend="Bare_die:arm:Bare_die_arm.Connection_zone_bare_die_interface_plane_relationship"/> 
	and 
	<express_ref linkend="Assembly_technology:arm:Assembly_technology_arm.Connection_zone_based_assembly_joint"/> 
	may be used to provide more details if needed by a receiving organization.
	</p>
	</note>
</ext_description>

<!-- mounting_surface_assembly_joint -->
<ext_description linkend="Assembly_2d_shape_arm.Component_2d_surface_location.mounting_surface_assembly_joint">
	specifies the 
	<express_ref linkend="Assembly_technology:arm:Assembly_technology_arm.Assembly_joint"/> 
	that joins the 
	<express_ref linkend="Assembly_module_design:arm:Assembly_module_design_arm.Component_mounting_feature"/> 
	of the component being located to the associated mating component feature.
	The surface the mating component feature is associated with is part of the orientation description for the <b>Component_2d_surface_location</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Assembly_2d_shape_arm.Component_2d_surface_location.wr:WR1">
	The <express_ref linkend="Physical_unit_2d_design_view:arm:Physical_unit_2d_design_view_arm.Component_2d_location.substrate_location"/> 
	inherited from the 
	<express_ref linkend="Physical_unit_2d_design_view:arm:Physical_unit_2d_design_view_arm.Component_2d_location"/> 
	shall be "False".
</ext_description>

<!-- WR2 -->
<ext_description linkend="Assembly_2d_shape_arm.Component_2d_surface_location.wr:WR2">
<p>
	<express_ref linkend="assembly_technology:arm:Assembly_technology_arm.Assembly_joint.assembly_feature_1"/> of <b>mounting_surface_assembly_joint</b>
	shall only be a 
	<express_ref linkend="assembly_module_with_interconnect_component:arm:Assembly_module_with_interconnect_component_arm.Interconnect_module_component_terminal"/>
	or a 
	<express_ref linkend="assembly_module_with_interconnect_component:arm:Assembly_module_with_interconnect_component_arm.Interconnect_module_component_surface_feature"/>.
</p>
</ext_description>

<!-- WR3 -->
<ext_description linkend="Assembly_2d_shape_arm.Component_2d_surface_location.wr:WR3">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation_relationship.description"/> 
	shall not be populated.
</ext_description>

</ext_descriptions>

