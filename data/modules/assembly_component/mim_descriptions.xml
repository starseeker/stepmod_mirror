<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: Assembly_component_mim
   <express_ref linkend="assembly_component:mim:Assembly_component_mim"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Assembly_component_mim">
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: assembly_component
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Assembly_component_mim.assembly_component">
	An <b>assembly_component</b> is a type of 
	<express_ref linkend="assembly_component:mim:Assembly_component_mim.component_definition"/> 
	and of 
	<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.product_definition_shape"/>
	that implements the ARM concept of
	<express_ref linkend="Assembly_component:arm:Assembly_component_arm.Assembly_component"/>.
</ext_description>

<!-- definition -->
<ext_description linkend="Assembly_component_mim.assembly_component.definition">
	an attribute inherited from the
	<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.property_definition"/>
	shall be redeclared as the
	<express_ref linkend="assembly_component:mim:Assembly_component_mim.assembly_component"/>
	for the <b>assembly_component</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Assembly_component_mim.assembly_component.wr:WR1">
	The 
	<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.property_definition.description"/>
	shall not be specified for the <b>assembly_component</b>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: component_definition
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Assembly_component_mim.component_definition"><p>
A <b>component_definition</b> is a type of 
<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition"/> 
and a type of
<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_relationship"/>
that represents a product
that is used in an assembly.  The <b>component_definition</b> entity implements
the ARM concepts of:
</p><ul>
<li> <express_ref linkend="Component_grouping:arm:Component_grouping_arm.Array_placement_group"/>;</li>
<li> <express_ref linkend="Assembly_component:arm:Assembly_component_arm.Assembly_component"/>;</li>
<li> <express_ref linkend="Component_grouping:arm:Component_grouping_arm.Assembly_group_component"/>;</li>
<li> <express_ref linkend="Assembly_module_with_subassembly:arm:Assembly_module_with_subassembly_arm.Assembly_module_component"/>;</li>
<li> <express_ref linkend="Assembly_module_with_macro_component:arm:Assembly_module_with_macro_component_arm.Assembly_module_macro_component"/>;</li>
<li> <express_ref linkend="assembly_module_with_interconnect_component:arm:Assembly_module_with_interconnect_component_arm.Bare_die_component"/>;</li>
<li> <express_ref linkend="cable:arm:Cable_arm.Cable_component"/>;</li>
<li> <express_ref linkend="Assembly_module_with_interconnect_component:arm:Assembly_module_with_interconnect_component_arm.Interconnect_module_component"/>;</li>
<li> <express_ref linkend="Interconnect_module_with_macros:arm:Interconnect_module_with_macros_arm.Interconnect_module_macro_component"/>;</li>
<li> <express_ref linkend="Interface_component:arm:Interface_component_arm.Interface_component"/>;</li>
<li> <express_ref linkend="Assembly_component:arm:Assembly_component_arm.Physical_component"/>;</li>
<li> <express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Laminate_component"/>;</li>
<li> <express_ref linkend="assembly_module_with_interconnect_component:arm:Assembly_module_with_interconnect_component_arm.Routed_interconnect_component"/>;</li>
<li> <express_ref linkend="assembly_module_with_interconnect_component:arm:Assembly_module_with_interconnect_component_arm.Routed_physical_component"/>;</li>
<li> <express_ref linkend="assembly_component:arm:Assembly_component_arm.Thermal_component"/>.</li>
</ul>
</ext_description>

<!-- related_product_definition -->
<ext_description linkend="Assembly_component_mim.component_definition.related_product_definition">
	an attribute inherited from the
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_relationship"/>
	shall be redeclared as the
	<express_ref linkend="assembly_component:mim:Assembly_component_mim.component_definition"/>
	for the <b>component_definition</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Assembly_component_mim.component_definition.wr:WR1">
	The 
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_relationship.description">product_definition_relationship.description</express_ref>
	shall not be specified for the <b>component_definition</b>.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Assembly_component_mim.component_definition.wr:WR2">
	The 
	<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.property_definition.description">property_definition.description</express_ref>
	shall not be specified for the <b>component_definition</b>.
</ext_description>

<!-- WR3 -->
<ext_description linkend="Assembly_component_mim.component_definition.wr:WR3">
	The
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_relationship.id">product_definition_relationship.id</express_ref>
	shall be equal to the
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition.id">product_definition.id</express_ref>.	
</ext_description>

<!-- WR4 -->
<ext_description linkend="Assembly_component_mim.component_definition.wr:WR4">
	The
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_relationship.name">product_definition_relationship.name</express_ref>
	shall be 'definition usage'.
</ext_description>

<!-- WR5 -->
<ext_description linkend="Assembly_component_mim.component_definition.wr:WR5">
	<p>
	    The <b>component_definition</b> shall not be a 
	    <express_ref linkend="product_structure_schema:ir_express:product_structure_schema.product_definition_usage">product_definition_usage</express_ref>.
	</p>
</ext_description>


<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: physical_component
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Assembly_component_mim.physical_component">
  A <b>physical_component</b> is a type of 
  <express_ref linkend="assembly_component:mim:Assembly_component_mim.assembly_component"/> 
  that implements the ARM concept of
  <express_ref linkend="assembly_component:arm:Assembly_component_arm.Physical_component"/>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: thermal_component
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Assembly_component_mim.thermal_component">
<p>
  A <b>thermal_component</b> is a type of 
  <express_ref linkend="assembly_component:mim:Assembly_component_mim.assembly_component"/> 
  that implements the ARM concept of
  <express_ref linkend="assembly_component:arm:Assembly_component_arm.Thermal_component"/>.
</p>
</ext_description>

</ext_descriptions>
