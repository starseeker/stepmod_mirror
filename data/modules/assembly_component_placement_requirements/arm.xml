<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2004-12-02T11:57:01" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="assembly_component_placement_requirements_arm schema_instance"/>
   <schema name="Assembly_component_placement_requirements_arm">
      <interface kind="use" schema="Physical_unit_design_view_arm"/>
      <interface kind="use" schema="Requirement_decomposition_arm"/>
      <interface kind="use" schema="Extended_geometric_tolerance_arm"/>
      <interface kind="use" schema="Non_feature_shape_element_arm"/>
      <interface kind="use" schema="Assembly_module_design_arm"/>
      <interface kind="use" schema="Assembly_module_with_interconnect_component_arm"/>
      <type name="restriction_basis_item">
         <select extensible="YES" genericentity="YES"/>
      </type>
      <entity name="Component_group_assignment">
         <explicit name="assigned_component">
            <typename name="Assembly_component"/>
         </explicit>
         <explicit name="associated_group">
            <typename name="Placement_group"/>
         </explicit>
      </entity>
      <entity name="Component_placement_restriction_assignment">
         <explicit name="minimum_mounting_clearance" optional="YES">
            <typename name="Datum_based_length_measure"/>
         </explicit>
         <explicit name="maximum_mounting_clearance" optional="YES">
            <typename name="Datum_based_length_measure"/>
         </explicit>
         <explicit name="components_permitted">
            <builtintype type="BOOLEAN"/>
         </explicit>
         <explicit name="requirement">
            <typename name="Item_restricted_requirement_occurrence"/>
         </explicit>
         <explicit name="volume" optional="YES">
            <typename name="Mounting_restriction_volume"/>
         </explicit>
         <explicit name="area" optional="YES">
            <typename name="Mounting_restriction_area"/>
         </explicit>
         <explicit name="maximum_positive_component_height" optional="YES">
            <typename name="Datum_based_length_measure"/>
         </explicit>
         <explicit name="maximum_negative_component_height" optional="YES">
            <typename name="Datum_based_length_measure"/>
         </explicit>
         <where label="WR1" expression=" EXISTS ( area ) OR EXISTS ( volume ) "/>
      </entity>
      <entity name="Item_restricted_requirement_occurrence" supertypes="Requirement_definition_property">
         <explicit name="basis">
            <typename name="restriction_basis_item"/>
         </explicit>
      </entity>
      <entity name="Mounting_restriction_area" supertypes="Non_feature_shape_element">
         <explicit name="scope">
            <typename name="Assembly_module"/>
            <redeclaration entity-ref="Non_feature_shape_element"/>
         </explicit>
         <explicit name="mounting_surface">
            <typename name="Interconnect_module_component_surface_feature"/>
         </explicit>
         <where label="WR1" expression=" 'AP210_ARM.CONSTRUCTIVE_GEOMETRY' IN TYPEOF ( SELF \ non_feature_shape_element . element_shape ) "/>
      </entity>
      <entity name="Mounting_restriction_volume" supertypes="Non_feature_shape_element">
         <explicit name="scope">
            <typename name="Assembly_module"/>
            <redeclaration entity-ref="Non_feature_shape_element"/>
         </explicit>
         <explicit name="mounting_surface">
            <typename name="Interconnect_module_component_surface_feature"/>
         </explicit>
         <inverse name="volume" entity="Non_feature_shape_definition" attribute="associated_element"/>
         <where label="WR1" expression=" 'AP210_ARM.THREE_DIMENSIONAL_SHAPE' IN TYPEOF ( volume ) "/>
      </entity>
      <entity name="Placement_group">
         <explicit name="design_specific_usage">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="Placement_group_area_assignment">
         <explicit name="area">
            <typename name="Mounting_restriction_area"/>
         </explicit>
         <explicit name="placed_group">
            <typename name="Placement_group"/>
         </explicit>
      </entity>
      <entity name="Placement_group_volume_assignment">
         <explicit name="volume">
            <typename name="Mounting_restriction_volume"/>
         </explicit>
         <explicit name="placed_group">
            <typename name="Placement_group"/>
         </explicit>
      </entity>
   </schema>
</express>
