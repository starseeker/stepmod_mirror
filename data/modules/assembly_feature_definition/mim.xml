<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<?xml-stylesheet type="text/xsl"
href="../../../xsl/express.xsl" ?>

<express
   language_version="2"
   description.file="mim_descriptions.xml"
   rcs.date="$Date: 2003/07/23 15:37:15 $"
   rcs.revision="$Revision: 1.36 $">

<!--
<application name="GraphicalEXPRESS" owner="Eurostep" url="www.eurostep.com" version="" source="" />
-->

<schema name="Assembly_feature_definition_mim">

<interface kind="use" schema="Product_property_feature_definition_mim">
<interfaced.item name="product_property_instanced_feature" />
</interface>

<interface kind="use" schema="product_property_definition_schema">
<interfaced.item name="shape_aspect_relationship" />
</interface>

<interface kind="use" schema="representation_schema">
<interfaced.item name="representation_relationship_with_transformation" />
</interface>

<interface kind="use" schema="assembly_feature_relationship_schema">
<interfaced.item name="shape_aspect_relationship_representation_association" />
<interfaced.item name="representing_relationship" />
</interface>


<entity name="instanced_assembly_feature" supertypes="product_property_instanced_feature">
<where
 label="WR1"
 expression="'PRODUCT_DEFINITION' IN TYPEOF (SELF\shape_aspect.of_shape.definition)">
</where>
</entity>

<entity name="instanced_assembly_feature_relationship" supertypes="shape_aspect_relationship">

<explicit
        name="relating_shape_aspect">
        <typename
          name="instanaced_assemlby_feature"/>
</explicit>
<explicit
        name="related_shape_aspect">
        <typename
          name="instanaced_assemlby_feature"/>
</explicit>

<where
 label="WR1"
expression="SELF\shape_aspect_relationship.relating_shape_aspect :=\: SELF\shape_aspect_relationship.related_shape_aspect">
</where>
<where
 label="WR2"
expression=" SELF\shape_aspect_relationship.relating_shape_aspect.of_shape.definition :=\: SELF\shape_aspect_relationship.related_shape_aspect.of_shape.definition">
</where>

</entity>

<entity name="instanced_assembly_feature_relationship_representation_association" supertypes="shape_aspect_relationship_representation_association">
<explicit
  name="represented_shape_aspect_relationship">
        <typename
          name="instanced_assembly_feature_relationship"/>
</explicit>
<explicit name="representing_representation_relationship">
        <typename
          name="representing_relationship"/>
</explicit>

<derived
            name="definition"          expression="SELF\shape_aspect_relationship_representation_association.represented_shape_aspect_relationship ">
            <typename
              name="instanced_assembly_feature_relationship"/>
</derived>

<derived
name="used_representation"
expression=" SELF\shape_aspect_relationship_representation_association.representing_representation_relationship">
            <typename
              name="representing_relationship"/>
</derived>

</entity>

</schema>
</express>
