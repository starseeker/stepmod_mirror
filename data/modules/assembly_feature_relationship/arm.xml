<?xml version="1.0"?>
<?xml-stylesheet 
  type="text/xsl" 
  href="../../../xsl/express.xsl"
?> <!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express language_version="2" rcs.date="$Date: 2003/08/14 22:40:06 $" rcs.revision="$Revision: 1.11 $"
  description.file="arm_descriptions.xml"
>
<application name="GraphicalEXPRESS" owner="Eurostep" url="www.eurostep.com" version="1.3" source="assembly_feature_relationship_arm_v005.vsd" />
<schema name="Assembly_feature_relationship_arm">

<interface kind="use" schema="Contextual_shape_positioning_arm"/>
      <interface kind="use" schema="Foundation_representation_arm"/>
<!--       <interface kind="use" schema="Kinematic_motion_representation_arm"/>
      <interface kind="use" schema="Kinematic_structure_arm"/>
  -->    
      <interface kind="use" schema="Shape_property_assignment_arm"/>
     
<entity
              name="Constrained_kinematic_motion_representation"
              supertypes="Geometric_relationship_with_placement_transformation">
              <explicit
                name="rep_1">
                <typename
                  name="Representative_geometric_model"/>
                <redeclaration
                  entity-ref="Representation_relationship"/>
                </explicit>
                <explicit
                  name="rep_2">
                  <typename
                    name="Representative_geometric_model"/>
                  <redeclaration
                    entity-ref="Representation_relationship"/>
                  </explicit>
       <where           label="WR1" expresson="('ELEMENTAL_GEOMETRIC_SHAPE_ARM.AXIS_PLACEMENT_3D' IN TYPEOF
     (SELF\Geometric_relationship_with_placement_transformation.
     transformation.source)) AND
     ('ELEMENTAL_GEOMETRIC_SHAPE_ARM.AXIS_PLACEMENT_3D' IN TYPEOF
     (SELF\Geometric_relationship_with_placement_transformation.
     transformation.target))">
          </where>
       </entity>
 
     <entity
      name="Free_kinematic_motion_representation"           supertypes="Geometric_model_relationship_with_transformation">
           <explicit
             name="rep_1">
             <typename
               name="Representative_geometric_model"/>
             <redeclaration
               entity-ref="Representation_relationship"/>
             </explicit>
             <explicit
               name="rep_2">
               <typename
                 name="Representative_geometric_model"/>
               <redeclaration
                 entity-ref="Representation_relationship"/>
               </explicit>
               <explicit
                 name="motion">
                 <typename
                   name="Kinematic_path"/>
               </explicit>
        </entity>  
    
   <entity
         name="Representative_geometric_model"
         supertypes="Geometric_model">
         
    </entity>
   
  <entity
      name="Shape_element_relationship_representation_association">
      <explicit
        name="represented_shape_element_relationship">
        <typename
          name="Shape_element_relationship"/>
      </explicit>
      <explicit
        name="representing_representation_relationship">
        <typename
          name="Representation_relationship"/>
      </explicit>
      <where
        label="WR1"
        expression="('REPRESENTATIVE_GEOMETRIC_MODEL' IN TYPEOF(representing_representation_relationship\ representation_relationship.rep_1)) AND('REPRESENTATIVE_GEOMETRIC_MODEL' IN TYPEOF(representing_representation_relationship\ representation_relationship.rep_2))">
      </where>
     
    </entity>
 
   
   
   </schema>
</express>
