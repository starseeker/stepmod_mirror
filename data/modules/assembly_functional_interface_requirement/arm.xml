<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1635">
   <application name="JSDAI" owner="LKSoft" source="assembly_functional_interface_requirement_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Assembly_functional_interface_requirement_arm">
      <interface kind="use" schema="Altered_part_arm"/>
      <interface kind="reference" schema="Specification_document_arm">
         <interfaced.item name="get_document_definition"/>
      </interface>
      <type name="afir_documented_element_select">
         <select basedon="documented_element_select" extensible="YES" genericentity="YES" selectitems="Protocol_physical_layer_definition"/>
      </type>
      <entity name="Minimally_defined_connector" supertypes="Packaged_part">
         <explicit name="used_package">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Package_external_reference"/>
            <redeclaration entity-ref="Packaged_part"/>
         </explicit>
         <inverse attribute="associated_definition" entity="Shape_element" name="terminal">
            <inverse.aggregate lower="1" type="SET" upper="?"/>
         </inverse>
         <where expression="SIZEOF(SELF\Packaged_part.access_mechanisms) = 0" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Packaged_part.implemented_function)" label="WR2"/>
         <where expression="NOT ('ALTERED_PART_ARM.ALTERED_PACKAGED_PART' IN TYPEOF(SELF))" label="WR3"/>
      </entity>
      <entity name="Protocol_physical_layer_definition" supertypes="Requirement_view_definition">
         <explicit name="host_connector" optional="YES">
            <typename name="Minimally_defined_connector"/>
         </explicit>
         <explicit name="host_functional_definition">
            <typename name="Functional_unit_usage_view"/>
         </explicit>
         <explicit name="media_connector" optional="YES">
            <typename name="Minimally_defined_connector"/>
         </explicit>
         <explicit name="media_functional_definition" optional="YES">
            <typename name="Functional_unit_usage_view"/>
         </explicit>
         <derived expression="get_document_definition(SELF, 'reference document', &#10;  &#9;&#9;&#9;'DOCUMENT_DEFINITION_ARM.DOCUMENT_DEFINITION')" name="reference_document">
            <aggregate lower="0" type="SET" upper="?"/>
            <typename name="Document_definition"/>
         </derived>
         <where expression="EXISTS (reference_document) AND (SIZEOF(reference_document) = 1)" label="WR1"/>
         <where expression="EXISTS(host_functional_definition) OR EXISTS(media_functional_definition)" label="WR2"/>
      </entity>
      <entity name="Protocol_physical_layer_definition_with_characterization" supertypes="Protocol_physical_layer_definition">
         <explicit name="host_rcv_functional_characterization" optional="YES">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Functional_specification_definition"/>
         </explicit>
         <explicit name="host_xmt_functional_characterization" optional="YES">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Functional_specification_definition"/>
         </explicit>
         <explicit name="media_functional_characterization" optional="YES">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Functional_specification_definition"/>
         </explicit>
         <where expression="EXISTS (host_rcv_functional_characterization) OR EXISTS (host_xmt_functional_characterization) OR EXISTS(media_functional_characterization)" label="WR1"/>
      </entity>
   </schema>
</express>
