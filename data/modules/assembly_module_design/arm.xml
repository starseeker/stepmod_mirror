<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1636">
   <application name="JSDAI" owner="LKSoft" source="assembly_module_design_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Assembly_module_design_arm">
      <interface kind="use" schema="Altered_package_arm"/>
      <interface kind="use" schema="Assembly_module_usage_view_arm"/>
      <interface kind="use" schema="Bare_die_arm"/>
      <interface kind="use" schema="Physical_component_feature_arm"/>
      <interface kind="use" schema="Layered_interconnect_simple_template_arm"/>
      <type name="assembled_feature_select">
         <select extensible="YES" genericentity="YES" selectitems="Bare_die_component_terminal Component_mounting_feature Packaged_component_join_terminal"/>
      </type>
      <type name="assembly_module_or_assembly_group_component">
         <select extensible="YES" genericentity="YES" selectitems="Assembly_module_design_view"/>
      </type>
      <entity name="Assembly_connection_zone_position_relationship">
         <explicit name="associating_design_view_shape">
            <typename name="Geometric_model"/>
         </explicit>
         <explicit name="associated_usage">
            <typename name="Connection_zone_in_design_view"/>
         </explicit>
         <explicit name="associated_connection_zone_shape_definition">
            <typename name="Geometric_model"/>
         </explicit>
         <explicit name="associated_usage_placement">
            <typename name="Axis_placement"/>
         </explicit>
         <explicit name="associating_design_view">
            <typename name="Assembly_module_design_view"/>
         </explicit>
      </entity>
      <entity name="Assembly_module_design_view" super.expression="ONEOF (Layered_assembly_panel_design_view, Layered_assembly_module_design_view)" supertypes="Assembly_definition Part_design_view">
         <where expression="NOT EXISTS(SELF\Product_view_definition.additional_characterization)" label="WR1"/>
      </entity>
      <entity name="Bare_die_component" supertypes="Physical_component Single_instance">
         <explicit name="derived_from">
            <typename name="Bare_die"/>
            <redeclaration entity-ref="Definition_based_product_occurrence"/>
         </explicit>
         <where expression="NOT EXISTS(SELF\Product_view_definition.name)" label="WR1"/>
      </entity>
      <entity name="Bare_die_component_terminal" supertypes="Physical_component_terminal">
         <explicit name="definition">
            <typename name="Bare_die_terminal"/>
            <redeclaration entity-ref="Component_feature"/>
         </explicit>
         <explicit name="associated_definition">
            <typename name="Bare_die_component"/>
            <redeclaration entity-ref="Component_feature"/>
         </explicit>
         <where expression="NOT EXISTS(SELF\Shape_element.description)" label="WR1"/>
      </entity>
      <entity name="Component_external_reference">
         <explicit name="design_owner">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="part_number">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="revision_code">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="product_definition_id">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="reference_designation">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="Component_feature_external_reference">
         <explicit name="name">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="associated_component">
            <typename name="Component_external_reference"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="associated_component"/>
            <unique.attribute attribute="name"/>
         </unique>
      </entity>
      <entity name="Component_mounting_clearance_relationship">
         <explicit name="feature_relationship">
            <typename name="Component_feature_relationship"/>
         </explicit>
         <explicit name="mounting_clearance">
            <typename name="Length_tolerance_characteristic"/>
         </explicit>
      </entity>
      <entity name="Component_mounting_feature" supertypes="Physical_component_feature">
         <explicit name="definition">
            <typename name="Part_mounting_feature"/>
            <redeclaration entity-ref="Component_feature"/>
         </explicit>
      </entity>
      <entity name="Component_overlap_relationship">
         <explicit name="previously_placed_component">
            <typename name="Next_assembly_usage"/>
         </explicit>
         <explicit name="current_component">
            <typename name="Next_assembly_usage"/>
         </explicit>
         <explicit name="added_clearance">
            <typename name="Length_tolerance_characteristic"/>
         </explicit>
      </entity>
      <entity name="Layered_assembly_module_design_view" supertypes="Assembly_module_design_view">
         <explicit name="usage_view">
            <typename name="Layered_assembly_module_usage_view"/>
            <redeclaration entity-ref="Part_design_view"/>
         </explicit>
      </entity>
      <entity name="Layered_assembly_panel_design_view" supertypes="Assembly_module_design_view"/>
      <entity name="Minimally_defined_component_terminal" supertypes="Physical_component_terminal">
         <explicit name="definition">
            <typename name="Part_terminal_external_reference"/>
            <redeclaration entity-ref="Component_feature"/>
         </explicit>
         <where expression="NOT EXISTS(SELF\Shape_element.description)" label="WR1"/>
      </entity>
      <entity name="Movable_packaged_component_join_terminal" supertypes="Packaged_component_join_terminal">
         <explicit name="wire_terminal_length" optional="YES">
            <typename name="Length_tolerance_characteristic"/>
         </explicit>
         <where expression="SIZEOF(QUERY(pt &lt;* SELF\Component_feature.definition\Packaged_part_terminal.terminal_of_package |&#10;            NOT('PACKAGE_ARM.WIRE_TERMINAL' IN TYPEOF (pt)))) = 0" label="WR1"/>
      </entity>
      <entity name="Packaged_component" supertypes="Physical_component Single_instance">
         <explicit name="derived_from">
            <typename name="Packaged_part"/>
            <redeclaration entity-ref="Definition_based_product_occurrence"/>
         </explicit>
         <explicit name="selected_package_alternate" optional="YES">
            <typename name="Altered_package"/>
         </explicit>
         <inverse attribute="associated_definition" entity="Packaged_component_join_terminal" name="access_mechanism">
            <inverse.aggregate lower="0" type="SET" upper="?"/>
         </inverse>
      </entity>
      <entity name="Packaged_component_join_terminal" supertypes="Physical_component_terminal">
         <explicit name="definition">
            <typename name="Packaged_part_join_terminal"/>
            <redeclaration entity-ref="Component_feature"/>
         </explicit>
         <explicit name="associated_definition">
            <typename name="Packaged_component"/>
            <redeclaration entity-ref="Component_feature"/>
         </explicit>
         <where expression="NOT EXISTS(SELF\Shape_element.description)" label="WR1"/>
      </entity>
      <entity name="Routed_interconnect_component" supertypes="Physical_component">
         <explicit name="routed_centreline_shape">
            <typename name="Path_area_with_parameters"/>
         </explicit>
         <where expression="NOT EXISTS(SELF\Product_view_definition.name)" label="WR1"/>
      </entity>
      <entity name="Routed_physical_component" supertypes="Physical_component">
         <explicit name="routed_centreline_shape">
            <typename name="Path_area_with_parameters"/>
         </explicit>
      </entity>
      <subtype.constraint entity="Physical_component" name="physical_component_subtypes" super.expression="ONEOF (Bare_die_component, Packaged_component, Routed_interconnect_component, Routed_physical_component)"/>
      <subtype.constraint entity="Physical_component_terminal" name="amd_physical_component_terminal_subtypes" super.expression="ONEOF (Bare_die_component_terminal, Minimally_defined_component_terminal, Packaged_component_join_terminal)"/>
   </schema>
</express>
