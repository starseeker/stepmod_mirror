<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2004-11-23T15:02:37" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="assembly_module_macro_definition_arm schema_instance"/>
   <schema name="Assembly_module_macro_definition_arm">
      <interface kind="use" schema="Interconnect_module_with_macros_arm"/>
      <interface kind="use" schema="Assembly_module_design_arm"/>
      <entity name="Component_terminal_to_assembly_module_join_terminal_assignment" supertypes="Component_feature_to_physical_usage_view_assignment">
         <derived name="assigned_terminal" expression=" SELF \ component_feature_to_physical_usage_view_assignment . assigned_design_object ">
            <typename name="Interconnect_module_macro_component_join_terminal"/>
         </derived>
         <derived name="usage_view_terminal" expression=" SELF \ component_feature_to_physical_usage_view_assignment . defined_part_feature ">
            <typename name="Assembly_module_join_terminal"/>
         </derived>
         <unique label="ur1">
            <unique.attribute entity-ref="Component_terminal_to_assembly_module_join_terminal_assignment" attribute="usage_view_terminal"/>
         </unique>
      </entity>
      <entity name="Assembly_module_join_terminal" supertypes="Assembly_module_terminal">
         <inverse name="defining_terminal" entity="Component_feature_to_physical_usage_view_assignment" attribute="defined_part_feature"/>
         <where label="WR3" expression=" NOT ( SIZEOF ( USEDIN ( SELF , '' ) ) = 0 ) OR ( SIZEOF ( QUERY ( ct &lt;* USEDIN ( SELF , '' ) | NOT ( 'AP210_ARM.ASSEMBLY_MODULE_MACRO_COMPONENT_JOIN_TERMINAL' IN TYPEOF ( ct ) ) ) ) = 0 ) "/>
         <where label="WR2" expression=" 'AP210_ARM.' + 'COMPONENT_TERMINAL_TO_ASSEMBLY_MODULE_JOIN_TERMINAL_ASSIGNMENT' IN TYPEOF ( defining_terminal ) "/>
         <where label="WR1" expression=" 'AP210_ARM.INTERCONNECT_MODULE_JOIN_TERMINAL' IN TYPEOF ( defining_terminal . assigned_terminal ) "/>
      </entity>
   </schema>
</express>
