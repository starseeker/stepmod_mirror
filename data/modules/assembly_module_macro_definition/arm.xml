<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date: 2007-03-21 02:26:07 -0700 (Tr, 21 Kov 2007) $" rcs.revision="$Revision: 21 $" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="assembly_module_macro_definition_arm schema_instance"/>
   <schema name="Assembly_module_macro_definition_arm">
      <interface kind="use" schema="Interconnect_module_with_macros_arm"/>
      <entity name="Assembly_module_macro_terminal" supertypes="Assembly_module_terminal">
         <inverse name="defining_terminal" entity="Component_feature_to_physical_usage_view_assignment" attribute="defined_part_feature"/>
         <where label="WR1" expression="'LAYERED_INTERCONNECT_MODULE_DESIGN_ARM.INTERCONNECT_MODULE_MACRO_TERMINAL' IN TYPEOF(defining_terminal.assigned_terminal)"/>
         <where label="WR2" expression="'ASSEMBLY_MODULE_MACRO_DEFINITION_ARM.' + 'COMPONENT_TERMINAL_TO_ASSEMBLY_MODULE_MACRO_TERMINAL_ASSIGNMENT' IN TYPEOF(defining_terminal)"/>
         <where label="WR3" expression="NOT (SIZEOF(USEDIN(SELF,&#10; '')) &gt; 0) OR (SIZEOF(QUERY(ct &lt;* USEDIN(SELF,&#10; '') | NOT ('ASSEMBLY_MODULE_WITH_MACRO_COMPONENT_ARM.ASSEMBLY_MODULE_MACRO_COMPONENT_JOIN_TERMINAL' IN TYPEOF(ct)))) = 0)"/>
      </entity>
      <entity name="Component_terminal_to_assembly_module_macro_terminal_assignment" supertypes="Component_feature_to_physical_usage_view_assignment">
         <explicit name="usage_view_terminal">
            <typename name="Assembly_module_macro_terminal"/>
            <redeclaration entity-ref="Component_feature_to_physical_usage_view_assignment" old_name="defined_part_feature"/>
         </explicit>
         <explicit name="assigned_terminal">
            <typename name="Interconnect_module_macro_component_join_terminal"/>
            <redeclaration entity-ref="Component_feature_to_physical_usage_view_assignment" old_name="assigned_design_object"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="usage_view_terminal"/>
         </unique>
      </entity>
      <subtype.constraint name="assembly_module_terminal_subtypes" entity="Assembly_module_terminal" super.expression="(ONEOF (Assembly_module_interface_terminal, Assembly_module_macro_terminal) ANDOR Pca_terminal)"/>
   </schema>
</express>
