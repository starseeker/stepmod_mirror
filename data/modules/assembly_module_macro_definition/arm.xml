<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-01-05T11:42:13" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="assembly_module_macro_definition_arm schema_instance"/>
   <schema name="Assembly_module_macro_definition_arm">
      <interface kind="use" schema="Interconnect_module_with_macros_arm"/>
      <interface kind="use" schema="Assembly_module_design_arm"/>
      <entity name="Assembly_module_join_terminal" supertypes="Assembly_module_terminal">
         <inverse name="defining_terminal" entity="Component_feature_to_physical_usage_view_assignment" attribute="defined_part_feature"/>
         <where label="WR1" expression=" 'ASSEMBLY_MODULE_MACRO_DEFINITION_ARM.INTERCONNECT_MODULE_JOIN_TERMINAL' IN TYPEOF ( defining_terminal . assigned_terminal ) "/>
         <where label="WR2" expression=" 'ASSEMBLY_MODULE_MACRO_DEFINITION_ARM.' + 'COMPONENT_TERMINAL_TO_ASSEMBLY_MODULE_JOIN_TERMINAL_ASSIGNMENT' IN TYPEOF ( defining_terminal ) "/>
         <where label="WR3" expression=" NOT ( SIZEOF ( USEDIN ( SELF , '' ) ) &gt; 0 ) OR ( SIZEOF ( QUERY ( ct &lt;* USEDIN ( SELF , '' ) | NOT ( 'ASSEMBLY_MODULE_MACRO_DEFINITION_ARM.ASSEMBLY_MODULE_MACRO_COMPONENT_JOIN_TERMINAL' IN TYPEOF ( ct ) ) ) ) = 0 ) "/>
      </entity>
      <entity name="Component_terminal_to_assembly_module_join_terminal_assignment" supertypes="Component_feature_to_physical_usage_view_assignment">
         <explicit name="usage_view_terminal">
            <typename name="Assembly_module_join_terminal"/>
            <redeclaration entity-ref="Component_feature_to_physical_usage_view_assignment" old_name="defined_part_feature"/>
         </explicit>
         <explicit name="assigned_terminal">
            <typename name="Interconnect_module_macro_component_join_terminal"/>
            <redeclaration entity-ref="Component_feature_to_physical_usage_view_assignment" old_name="assigned_design_object"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute entity-ref="Component_terminal_to_assembly_module_join_terminal_assignment" attribute="usage_view_terminal"/>
         </unique>
      </entity>
      <subtype.constraint name="assembly_module_terminal_subtypes" entity="Assembly_module_terminal" super.expression="(ONEOF (Assembly_module_interface_terminal, Assembly_module_join_terminal) ANDOR Pca_terminal)"/>
   </schema>
</express>
