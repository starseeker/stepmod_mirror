<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-04-04T09:45:43" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="assembly_module_usage_view_arm schema_instance"/>
   <schema name="Assembly_module_usage_view_arm">
      <interface kind="use" schema="Network_functional_usage_view_arm"/>
      <interface kind="use" schema="Packaged_connector_model_arm"/>
      <entity name="Assembly_module_interface_terminal" supertypes="Assembly_module_terminal"/>
      <entity name="Assembly_module_terminal" supertypes="Part_terminal" super.expression="Assembly_module_interface_terminal ANDOR Pca_terminal">
         <explicit name="associated_definition">
            <typename name="Assembly_module_usage_view"/>
            <redeclaration entity-ref="Part_feature"/>
         </explicit>
         <explicit name="reference_terminal" optional="YES">
            <typename name="Packaged_part_interface_terminal"/>
         </explicit>
         <explicit name="related_connector" optional="YES">
            <typename name="Next_assembly_usage"/>
         </explicit>
         <explicit name="terminal_connection_zone" optional="YES">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="Connection_zone_in_usage_view"/>
         </explicit>
         <where label="WR1" expression=" NOT EXISTS ( related_connector ) OR ( SELF \ Shape_element . containing_shape = related_connector \ Assembly_component_relationship . related_view ) "/>
         <where label="WR2" expression=" EXISTS ( related_connector . location_indicator ) "/>
      </entity>
      <entity name="Assembly_module_usage_view" supertypes="Part_usage_view Assembly_definition" super.expression="Pca_usage_view">
         <explicit name="implemented_function">
            <typename name="Functional_unit_usage_view"/>
         </explicit>
         <inverse name="access_mechanisms" entity="Assembly_module_terminal" attribute="associated_definition">
            <inverse.aggregate type="SET" lower="2" upper="?"/>
         </inverse>
      </entity>
      <entity name="Pca_terminal" supertypes="Assembly_module_terminal">
         <explicit name="associated_definition">
            <typename name="Pca_usage_view"/>
            <redeclaration entity-ref="Part_feature"/>
         </explicit>
      </entity>
      <entity name="Pca_usage_view" supertypes="Assembly_module_usage_view">
         <explicit name="maximum_negative_component_height" optional="YES">
            <typename name="Datum_based_length_measure"/>
         </explicit>
         <explicit name="maximum_positive_component_height" optional="YES">
            <typename name="Datum_based_length_measure"/>
         </explicit>
      </entity>
   </schema>
</express>
