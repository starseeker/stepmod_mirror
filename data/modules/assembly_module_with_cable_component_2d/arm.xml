<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date: 2007-03-21 02:26:07 -0700 (Tr, 21 Kov 2007) $" rcs.revision="$Revision: 21 $" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="assembly_module_with_cable_component_2d_arm schema_instance"/>
   <schema name="Assembly_module_with_cable_component_2d_arm">
      <interface kind="use" schema="Assembly_module_with_cable_component_arm"/>
      <interface kind="use" schema="Physical_unit_2d_design_view_arm"/>
      <entity name="Routed_cable_component_2d_shape_model" supertypes="Assembly_component_2d_shape_model">
         <explicit name="cable_path_shape">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="Curve"/>
         </explicit>
         <explicit name="connector_placement">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="Sub_model_placement_2d"/>
         </explicit>
         <explicit name="junction_placement" optional="YES">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="Geometric_model_element_relationship"/>
         </explicit>
         <explicit name="shape_characterized_component">
            <aggregate type="SET" lower="1" upper="1"/>
            <typename name="Cable_component"/>
            <redeclaration entity-ref="Assembly_component_2d_shape_model"/>
         </explicit>
         <where label="WR1" expression="NOT EXISTS(SELF\Representation.description)"/>
      </entity>
      <entity name="Sub_model_placement_2d">
         <explicit name="placement">
            <typename name="Axis_placement_2d"/>
         </explicit>
         <explicit name="sub_model">
            <typename name="Component_2d_location"/>
         </explicit>
      </entity>
   </schema>
</express>
