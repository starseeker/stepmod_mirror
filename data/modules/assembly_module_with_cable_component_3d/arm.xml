<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:07" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="assembly_module_with_cable_component_3d_arm schema_instance"/>
   <schema name="ASSEMBLY_MODULE_WITH_CABLE_COMPONENT_3D_ARM">
      <interface kind="use" schema="ASSEMBLY_MODULE_WITH_CABLE_COMPONENT_ARM"/>
      <interface kind="use" schema="CURVE_SWEPT_SOLID_ARM"/>
      <interface kind="use" schema="PHYSICAL_UNIT_3D_DESIGN_VIEW_ARM"/>
      <entity name="routed_cable_component_3d_shape" supertypes="assembly_component_3d_shape">
         <explicit name="cable_path_shape">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="curve_swept_solid"/>
         </explicit>
         <explicit name="connector_placement">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="sub_model_placement_3d"/>
         </explicit>
         <explicit name="junction_placement" optional="YES">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="shape_item_relationship"/>
         </explicit>
         <explicit name="shape_characterized_component">
            <aggregate type="SET" lower="1" upper="1"/>
            <typename name="cable_component"/>
            <redeclaration entity-ref="assembly_component_3d_shape"/>
         </explicit>
      </entity>
      <entity name="sub_model_placement_3d">
         <explicit name="placement">
            <typename name="axis_placement_3d"/>
         </explicit>
         <explicit name="sub_model">
            <typename name="component_3d_location"/>
         </explicit>
      </entity>
   </schema>
