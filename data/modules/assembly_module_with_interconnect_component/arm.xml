<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:07" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="assembly_module_with_interconnect_component_arm schema_instance"/>
   <schema name="ASSEMBLY_MODULE_WITH_INTERCONNECT_COMPONENT_ARM">
      <interface kind="use" schema="ASSEMBLY_MODULE_DESIGN_ARM"/>
      <interface kind="use" schema="INTERCONNECT_MODULE_USAGE_VIEW_ARM"/>
      <interface kind="use" schema="INTERFACE_COMPONENT_ARM"/>
      <interface kind="use" schema="PART_OCCURRENCE_ARM"/>
      <interface kind="use" schema="PHYSICAL_UNIT_DESIGN_VIEW_ARM"/>
      <type name="amwic_assembled_feature_select">
         <select extensible="YES" genericentity="YES" basedon="assembled_feature_select" selectitems="interconnect_component_join_terminal interconnect_component_interface_terminal interconnect_module_component_surface_feature interconnect_module_component_terminal"/>
      </type>
      <entity name="interconnect_component_interface_terminal" supertypes="physical_component_interface_terminal">
         <explicit name="connection" optional="YES">
            <typename name="connection_zone_in_design_view"/>
         </explicit>
         <unique label="ur1">
            <unique.attribute entity-ref="interconnect_component_interface_terminal" attribute="element_name"/>
            <unique.attribute entity-ref="interconnect_component_interface_terminal" attribute="associated_component"/>
         </unique>
      </entity>
      <entity name="interconnect_component_join_terminal" supertypes="physical_component_terminal">
         <explicit name="connection" optional="YES">
            <typename name="connection_zone_in_design_view"/>
         </explicit>
         <explicit name="associated_component">
            <typename name="physical_component"/>
            <redeclaration entity-ref="component_feature"/>
         </explicit>
         <unique label="ur1">
            <unique.attribute entity-ref="interconnect_component_join_terminal" attribute="element_name"/>
            <unique.attribute entity-ref="interconnect_component_join_terminal" attribute="associated_component"/>
         </unique>
      </entity>
      <entity name="interconnect_module_component" supertypes="physical_component">
         <explicit name="derived_from">
            <typename name="interconnect_module_usage_view"/>
            <redeclaration entity-ref="definition_based_product_occurrence"/>
         </explicit>
      </entity>
      <entity name="interconnect_module_component_surface_feature" supertypes="physical_component_feature">
         <explicit name="definition">
            <typename name="interconnect_module_surface_feature"/>
            <redeclaration entity-ref="component_feature"/>
         </explicit>
      </entity>
      <entity name="interconnect_module_component_terminal" supertypes="physical_component_terminal">
         <explicit name="definition">
            <typename name="interconnect_module_terminal"/>
            <redeclaration entity-ref="component_feature"/>
         </explicit>
         <explicit name="associated_component">
            <typename name="interconnect_module_component"/>
            <redeclaration entity-ref="component_feature"/>
         </explicit>
      </entity>
   </schema>
</express>
