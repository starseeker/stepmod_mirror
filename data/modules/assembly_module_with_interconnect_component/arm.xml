<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1643">
   <application name="JSDAI" owner="LKSoft" source="assembly_module_with_interconnect_component_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Assembly_module_with_interconnect_component_arm">
      <interface kind="use" schema="Assembly_module_design_arm"/>
      <interface kind="use" schema="Interconnect_module_usage_view_arm"/>
      <interface kind="use" schema="Interface_component_arm"/>
      <type name="amwic_assembled_feature_select">
         <select basedon="assembled_feature_select" extensible="YES" genericentity="YES" selectitems="Interconnect_component_join_terminal Interconnect_component_interface_terminal Interconnect_module_component_surface_feature Interconnect_module_component_terminal"/>
      </type>
      <type name="conductive_interconnect_terminal_or_interconnect_component_join_terminal">
         <select selectitems="Interconnect_component_interface_terminal Interconnect_component_join_terminal Movable_packaged_component_join_terminal"/>
      </type>
      <entity name="Design_view_terminal_component_shape_relationship">
         <explicit name="associating_component_shape">
            <typename name="Geometric_model"/>
         </explicit>
         <explicit name="source_placement">
            <typename name="Axis_placement"/>
         </explicit>
         <explicit name="associated_usage">
            <typename name="conductive_interconnect_terminal_or_interconnect_component_join_terminal"/>
         </explicit>
         <explicit name="associated_terminal_shape_definition">
            <typename name="Geometric_model"/>
         </explicit>
         <explicit name="associated_usage_placement">
            <typename name="Axis_placement"/>
         </explicit>
      </entity>
      <entity name="Interconnect_component_interface_terminal" supertypes="Physical_component_interface_terminal">
         <explicit name="connection" optional="YES">
            <typename name="Connection_zone_in_design_view"/>
         </explicit>
         <where expression="NOT EXISTS(SELF\Shape_element.description)" label="WR1"/>
      </entity>
      <entity name="Interconnect_component_join_terminal" supertypes="Physical_component_terminal">
         <explicit name="connection_area" optional="YES">
            <aggregate lower="1" type="SET" upper="1"/>
            <typename name="Connection_zone_in_design_view"/>
         </explicit>
         <explicit name="associated_component">
            <typename name="Physical_component"/>
            <redeclaration entity-ref="Component_feature"/>
         </explicit>
         <where expression="NOT EXISTS(SELF\Shape_element.description)" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Component_feature.definition) OR&#10;             NOT( 'FUNCTIONAL_ASSIGNMENT_TO_PART_ARM.PART_TERMINAL' IN TYPEOF(SELF\Component_feature.definition))" label="WR2"/>
      </entity>
      <entity name="Interconnect_module_component" supertypes="Physical_component Single_instance">
         <explicit name="derived_from">
            <typename name="Layered_interconnect_module_usage_view"/>
            <redeclaration entity-ref="Definition_based_product_occurrence"/>
         </explicit>
         <where expression="NOT EXISTS(SELF\Product_view_definition.name)" label="WR1"/>
      </entity>
      <entity name="Interconnect_module_component_surface_feature" supertypes="Physical_component_feature">
         <explicit name="definition">
            <typename name="Interconnect_module_surface_feature"/>
            <redeclaration entity-ref="Component_feature"/>
         </explicit>
         <where expression="NOT EXISTS(SELF\Shape_element.description)" label="WR1"/>
      </entity>
      <entity name="Interconnect_module_component_terminal" supertypes="Physical_component_terminal">
         <explicit name="definition">
            <typename name="Interconnect_module_terminal"/>
            <redeclaration entity-ref="Component_feature"/>
         </explicit>
         <explicit name="associated_component">
            <typename name="Interconnect_module_component"/>
            <redeclaration entity-ref="Component_feature"/>
         </explicit>
         <where expression="NOT EXISTS(SELF\Shape_element.description)" label="WR1"/>
      </entity>
   </schema>
</express>
