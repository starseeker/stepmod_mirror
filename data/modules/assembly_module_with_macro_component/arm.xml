<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date: 2007-03-21 02:26:07 -0700 (Tr, 21 Kov 2007) $" rcs.revision="$Revision: 21 $" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="assembly_module_with_macro_component_arm schema_instance"/>
   <schema name="Assembly_module_with_macro_component_arm">
      <interface kind="use" schema="Assembly_module_macro_definition_arm"/>
      <interface kind="use" schema="Assembly_module_with_subassembly_arm"/>
      <entity name="Assembly_module_macro_component" supertypes="Physical_component">
         <explicit name="derived_from">
            <typename name="Assembly_module"/>
            <redeclaration entity-ref="Definition_based_product_occurrence"/>
         </explicit>
         <inverse name="join_access_mechanisms" entity="Assembly_module_macro_component_join_terminal" attribute="associated_component">
            <inverse.aggregate type="SET" lower="1" upper="?"/>
         </inverse>
         <where label="WR1" expression="SIZEOF(QUERY(ammcjt &lt;* join_access_mechanisms | ammcjt\Component_feature.definition\Shape_element.containing_shape :&lt;&gt;: SELF\Definition_based_product_occurrence.derived_from\Part_design_view.usage_view)) = 0"/>
         <where label="WR2" expression="NOT EXISTS(SELF\Product_view_definition.additional_characterization)"/>
      </entity>
      <entity name="Assembly_module_macro_component_join_terminal" supertypes="Physical_component_terminal">
         <explicit name="definition">
            <typename name="Assembly_module_macro_terminal"/>
            <redeclaration entity-ref="Component_feature"/>
         </explicit>
         <explicit name="associated_component">
            <typename name="Assembly_module_macro_component"/>
            <redeclaration entity-ref="Component_feature"/>
         </explicit>
      </entity>
   </schema>
</express>
