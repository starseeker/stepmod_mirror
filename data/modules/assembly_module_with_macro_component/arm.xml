<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:07" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="assembly_module_with_macro_component_arm schema_instance"/>
   <schema name="ASSEMBLY_MODULE_WITH_MACRO_COMPONENT_ARM">
      <interface kind="use" schema="ASSEMBLY_MODULE_MACRO_DEFINITION_ARM"/>
      <interface kind="use" schema="ASSEMBLY_MODULE_WITH_SUBASSEMBLY_ARM"/>
      <entity name="assembly_module_macro_component" supertypes="assembly_module_component">
         <explicit name="design_definition">
            <typename name="assembly_module"/>
         </explicit>
         <inverse name="join_access_mechanisms" entity="assembly_module_macro_component_join_terminal" attribute="associated_component">
            <inverse.aggregate type="SET" lower="1" upper="?"/>
         </inverse>
         <where label="wr1" expression=" sizeof ( query ( ammcjt &lt;* join_access_mechanisms | ammcjt \ component_feature . definition \ shape_element . containing_shape :&lt;&gt;: design_definition \ part_design_view . usage_view ) ) = 0 "/>
      </entity>
      <entity name="assembly_module_macro_component_join_terminal" supertypes="assembly_module_component_terminal">
         <explicit name="definition">
            <typename name="assembly_module_join_terminal"/>
            <redeclaration entity-ref="component_feature"/>
         </explicit>
         <explicit name="associated_component">
            <typename name="assembly_module_macro_component"/>
            <redeclaration entity-ref="component_feature"/>
         </explicit>
      </entity>
   </schema>
</express>
