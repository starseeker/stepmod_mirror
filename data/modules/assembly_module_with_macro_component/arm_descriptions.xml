</ext_description>

<!-- +++++++++++ Assembly_module_macro_component +++++++++++ -->
<ext_description linkend="Assembly_module_with_macro_component_arm.Assembly_module_macro_component">
	An <b>Assembly_module_macro_component</b> is a type of 
	<express_ref linkend="Assembly_component:arm:Assembly_component_arm.Physical_component"/> 
	and a type of
	<express_ref linkend="product_occurrence:arm:Product_occurrence_arm.Single_instance"/>
	that is intended to support design reuse.  
	An <b>Assembly_module_macro_component</b> is an instantiation of a design fragment that has been stored in a library.  Design fragments may be stored in a library for later use in other designs.
</ext_description>

<!-- join_access_mechanisms -->
<ext_description linkend="Assembly_module_with_macro_component_arm.Assembly_module_macro_component.join_access_mechanisms">
	specifies an inverse relationship that specifies that the existence of the <b>Assembly_module_macro_component</b> 
	is dependent on the existence of the  
	<express_ref linkend="Assembly_module_with_macro_component:arm:Assembly_module_with_macro_component_arm.Assembly_module_macro_component_join_terminal"/> 
	that specifies the <b>Assembly_module_macro_component</b> as its 
	<express_ref linkend="Assembly_module_with_macro_component:arm:Assembly_module_with_macro_component_arm.Assembly_module_macro_component_join_terminal.associated_definition"/>. 
	There shall be one or more  
	<express_ref linkend="Assembly_module_with_macro_component:arm:Assembly_module_with_macro_component_arm.Assembly_module_macro_component_join_terminal"/> 
	for an <b>Assembly_module_macro_component</b>.
</ext_description>

<!-- derived_from -->
<ext_description linkend="Assembly_module_with_macro_component_arm.Assembly_module_macro_component.derived_from">
	specifies the role of the 
	<express_ref linkend="Assembly_module_design:arm:Assembly_module_design_arm.Assembly_module_design_view"/> 
	for the <b>Assembly_module_macro_component</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Assembly_module_with_macro_component_arm.Assembly_module_macro_component.wr:WR1">
	The 
	<express_ref linkend="Assembly_module_usage_view:arm:Assembly_module_usage_view_arm.Assembly_module_usage_view"/> 
	referenced by the 
	<express_ref linkend="physical_unit_design_view:arm:Physical_unit_design_view_arm.Part_design_view.usage_view"/>
	of <b>derived_from</b> shall be the same 
	<express_ref linkend="Assembly_module_usage_view:arm:Assembly_module_usage_view_arm.Assembly_module_usage_view"/>
	as referenced by the 
	<express_ref linkend="assembly_module_usage_view:arm:Assembly_module_usage_view_arm.Assembly_module_terminal.associated_definition"/>
	which is referenced by the 
	<express_ref linkend="assembly_module_with_macro_component:arm:Assembly_module_with_macro_component_arm.Assembly_module_macro_component_join_terminal.definition"/>
	of the <b>join_access_mechanisms</b>.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Assembly_module_with_macro_component_arm.Assembly_module_macro_component.wr:WR2">
	The
	<express_ref linkend="Product_view_definition:arm:Product_view_definition_arm.Product_view_definition.additional_characterization"/>
	shall not be populated.
</ext_description>

<!-- +++++++++++ Assembly_module_macro_component_join_terminal +++++++++++ -->
<ext_description linkend="Assembly_module_with_macro_component_arm.Assembly_module_macro_component_join_terminal">
	An <b>Assembly_module_macro_component_join_terminal</b> is a type of 
	<express_ref linkend="Physical_component_feature:arm:Physical_component_feature_arm.Physical_component_terminal"/>.  
	An <b>Assembly_module_macro_component_join_terminal</b> is a terminal of a design fragment that has been instantiated in a design.  
	It is associated with an <express_ref linkend="Assembly_module_with_macro_component:arm:Assembly_module_with_macro_component_arm.Assembly_module_macro_component"/>.
</ext_description>

<ext_description linkend="Assembly_module_with_macro_component_arm.Assembly_module_macro_component_join_terminal.associated_definition">
	specifies the role of the  
	<express_ref linkend="Assembly_module_with_macro_component:arm:Assembly_module_with_macro_component_arm.Assembly_module_macro_component"/> 
	for the <b>Assembly_module_macro_component_join_terminal</b>.
</ext_description>

<ext_description linkend="Assembly_module_with_macro_component_arm.Assembly_module_macro_component_join_terminal.definition">
	specifies the role of the  
	<express_ref linkend="Assembly_module_macro_definition:arm:Assembly_module_macro_definition_arm.Assembly_module_macro_terminal"/> 
	for the <b>Assembly_module_macro_component_join_terminal</b>.
</ext_description>

</ext_descriptions>

