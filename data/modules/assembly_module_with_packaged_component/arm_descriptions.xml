</ext_description>



<!-- +++++++++++ Packaged_component +++++++++++ -->
<ext_description linkend="Assembly_module_with_packaged_component_arm.Packaged_component">
	A <b>Packaged_component</b> is a type of 
	<express_ref linkend="Assembly_component:arm:Assembly_component_arm.Physical_component"/>
	and a type of
	<express_ref linkend="product_occurrence:arm:Product_occurrence_arm.Single_instance"/>. 
	A <b>Packaged_component</b> is an instantiation of a 
	<express_ref linkend="Packaged_part_black_box_model:arm:Packaged_part_black_box_model_arm.Packaged_part"/>
	at a given location within the context of an assembly.
</ext_description>

<!-- access_mechanism -->
<ext_description linkend="Assembly_module_with_packaged_component_arm.Packaged_component.access_mechanism">
	specifies an inverse relationship that specifies that the existence of the <b>Packaged_component</b> is dependent on the existence of the  
	<express_ref linkend="assembly_module_with_packaged_component:arm:Assembly_module_with_packaged_component_arm.Packaged_component_join_terminal"/> 
	that specifies the <b>Packaged_component</b> as its 
	<express_ref linkend="assembly_module_with_packaged_component:arm:Assembly_module_with_packaged_component_arm.Packaged_component_join_terminal.associated_definition"/>. 
	There shall be zero or more
	<express_ref linkend="assembly_module_with_packaged_component:arm:Assembly_module_with_packaged_component_arm.Packaged_component_join_terminal"/> 
	for a <b>Packaged_component</b>.
</ext_description>

<!-- selected_package_alternate -->
<ext_description linkend="Assembly_module_with_packaged_component_arm.Packaged_component.selected_package_alternate">
	specifies the role of the 
	<express_ref linkend="Altered_package:arm:Altered_package_arm.Altered_package"/> 
	for the <b>Packaged_component</b>. The value of this attribute overrides the
	<express_ref linkend="Packaged_part_black_box_model:arm:Packaged_part_black_box_model_arm.Packaged_part.used_package"/>
	when the size of that attribute is one or selects a specific item when there are more than one in the aggregate.
	The value of this attribute need not be specified.
</ext_description>

<!-- derived_from -->
<ext_description linkend="Assembly_module_with_packaged_component_arm.Packaged_component.derived_from">
	specifies the role of the 
	<express_ref linkend="Packaged_part_black_box_model:arm:Packaged_part_black_box_model_arm.Packaged_part"/> 
	for the <b>Packaged_component</b>.
</ext_description>

<!-- +++++++++++ Packaged_component_join_terminal +++++++++++ -->
<ext_description linkend="Assembly_module_with_packaged_component_arm.Packaged_component_join_terminal">
<p>
	A <b>Packaged_component_join_terminal</b> is a type of 
	<express_ref linkend="physical_component_feature:arm:Physical_component_feature_arm.Physical_component_terminal"/>.
	A <b>Packaged_component_join_terminal</b> is one termination for a 
	<express_ref linkend="assembly_module_with_packaged_component:arm:Assembly_module_with_packaged_component_arm.Packaged_component"/>.
	The terminal provides access to the electrical functionality of the 
	<express_ref linkend="Packaged_part_black_box_model:arm:Packaged_part_black_box_model_arm.Packaged_part"/> 
	being terminated.
</p>
	<example>
	<p>
		<module_ref linkend="assembly_module_design:4_entities:figure:pacjt">Figure 1</module_ref> 
		illustrates a detail of an assembly drawing that shows a microcircuit plugged into a socket, 
		and the role of the <b>Packaged_component_join_terminal</b>.
	</p>
	</example>
	<figure id="pacjt" number="1">
		<title><b>Packaged_component_join_terminal</b></title>
		<img src="pacjt.gif"/>
	</figure>
</ext_description>

<!-- associated_component -->
<ext_description linkend="Assembly_module_with_packaged_component_arm.Packaged_component_join_terminal.associated_definition">
	specifies the 
	<express_ref linkend="assembly_module_with_packaged_component:arm:Assembly_module_with_packaged_component_arm.Packaged_component"/> 
	being accessed by the <b>Packaged_component_join_terminal</b>.
</ext_description>

<!-- definition -->
<ext_description linkend="Assembly_module_with_packaged_component_arm.Packaged_component_join_terminal.definition">
	specifies the role of the 
	<express_ref linkend="Packaged_part_black_box_model:arm:Packaged_part_black_box_model_arm.Packaged_part_join_terminal"/> 
	for the <b>Packaged_component_join_terminal</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Assembly_module_with_packaged_component_arm.Packaged_component_join_terminal.wr:WR1">
	The
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element.description"/>
	shall not be populated.
</ext_description>



</ext_descriptions>
