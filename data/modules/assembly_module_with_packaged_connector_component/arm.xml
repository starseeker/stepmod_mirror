<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date: 2007-03-21 02:26:07 -0700 (Tr, 21 Kov 2007) $" rcs.revision="$Revision: 21 $" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="assembly_module_with_packaged_connector_component_arm schema_instance"/>
   <schema name="Assembly_module_with_packaged_connector_component_arm">
      <interface kind="use" schema="Assembly_module_design_arm"/>
      <interface kind="use" schema="Interface_component_arm"/>
      <type name="amwpcc_assembled_feature_select">
         <select extensible="YES" genericentity="YES" basedon="assembled_feature_select" selectitems="Packaged_connector_component_interface_terminal"/>
      </type>
      <entity name="Interface_mounted_join">
         <explicit name="connection_to_interconnect_module_component">
            <typename name="Packaged_connector_component_interface_terminal"/>
         </explicit>
         <explicit name="access_to_interface_mounted_component">
            <typename name="Physical_component_terminal"/>
         </explicit>
      </entity>
      <entity name="Packaged_connector_component" supertypes="Interface_component Packaged_component">
         <explicit name="derived_from">
            <typename name="Packaged_connector"/>
            <redeclaration entity-ref="Definition_based_product_occurrence"/>
         </explicit>
         <inverse name="interface_access_mechanism" entity="Packaged_connector_component_interface_terminal" attribute="associated_component">
            <inverse.aggregate type="SET" lower="1" upper="?"/>
         </inverse>
         <where label="WR1" expression="NOT EXISTS(SELF\Product_view_definition.name)"/>
      </entity>
      <entity name="Packaged_connector_component_interface_terminal" supertypes="Physical_component_interface_terminal">
         <explicit name="definition">
            <typename name="Packaged_part_interface_terminal"/>
            <redeclaration entity-ref="Component_feature"/>
         </explicit>
         <explicit name="associated_component">
            <typename name="Packaged_connector_component"/>
            <redeclaration entity-ref="Component_feature"/>
         </explicit>
         <where label="WR1" expression="NOT EXISTS(SELF\Shape_element.description)"/>
      </entity>
   </schema>
</express>
