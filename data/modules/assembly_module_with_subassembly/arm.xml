<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:07" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="assembly_module_with_subassembly_arm schema_instance"/>
   <schema name="ASSEMBLY_MODULE_WITH_SUBASSEMBLY_ARM">
      <interface kind="use" schema="ASSEMBLY_MODULE_DESIGN_ARM"/>
      <interface kind="use" schema="PHYSICAL_COMPONENT_FEATURE_ARM"/>
      <type name="amws_assembled_feature_select">
         <select extensible="YES" genericentity="YES" basedon="assembled_feature_select" selectitems="assembly_module_component_terminal"/>
      </type>
      <entity name="assembly_module_component" supertypes="physical_component">
         <explicit name="derived_from">
            <typename name="assembly_module_usage_view"/>
            <redeclaration entity-ref="definition_based_product_occurrence"/>
         </explicit>
         <inverse name="access_mechanisms" entity="assembly_module_component_terminal" attribute="associated_component">
            <inverse.aggregate type="SET" lower="2" upper="?"/>
         </inverse>
      </entity>
      <entity name="assembly_module_component_terminal" supertypes="physical_component_terminal">
         <explicit name="definition">
            <typename name="assembly_module_terminal"/>
            <redeclaration entity-ref="component_feature"/>
         </explicit>
         <explicit name="associated_component">
            <typename name="assembly_module_component"/>
            <redeclaration entity-ref="component_feature"/>
         </explicit>
      </entity>
   </schema>
</express>
