<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1641">
   <application name="JSDAI" owner="LKSoft" source="assembly_module_with_subassembly_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Assembly_module_with_subassembly_arm">
      <interface kind="use" schema="Assembly_module_design_arm"/>
      <entity name="Assembly_module_component" supertypes="Physical_component Single_instance">
         <explicit name="derived_from">
            <typename name="Assembly_module_design_view"/>
            <redeclaration entity-ref="Definition_based_product_occurrence"/>
         </explicit>
         <derived expression="derived_from\Part_design_view.usage_view" name="usage_view">
            <typename name="Assembly_module_usage_view"/>
         </derived>
         <inverse attribute="associated_definition" entity="Assembly_module_component_terminal" name="access_mechanisms">
            <inverse.aggregate lower="2" type="SET" upper="?"/>
         </inverse>
         <where expression="NOT EXISTS(SELF\Product_view_definition.name)" label="WR1"/>
         <where expression="usage_view = access_mechanisms[1]\Assembly_module_component_terminal.definition\Assembly_module_terminal.associated_definition" label="WR2"/>
      </entity>
      <entity name="Assembly_module_component_terminal" supertypes="Physical_component_terminal">
         <explicit name="definition">
            <typename name="Assembly_module_terminal"/>
            <redeclaration entity-ref="Component_feature"/>
         </explicit>
         <explicit name="associated_definition">
            <typename name="Assembly_module_component"/>
            <redeclaration entity-ref="Component_feature"/>
         </explicit>
         <where expression="NOT EXISTS(SELF\Shape_element.description)" label="WR1"/>
      </entity>
   </schema>
</express>
