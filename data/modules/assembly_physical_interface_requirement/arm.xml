<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date: 2007-05-08 08:34:58 -0700 (An, 08 Geg 2007) $" rcs.revision="$Revision: 730 $" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="assembly_physical_interface_requirement_arm schema_instance"/>
   <schema name="Assembly_physical_interface_requirement_arm">
      <interface kind="use" schema="Assembly_functional_interface_requirement_arm"/>
      <interface kind="use" schema="Layered_3d_shape_arm"/>
      <interface kind="use" schema="Printed_physical_layout_template_arm"/>
      <interface kind="reference" schema="Requirement_decomposition_arm">
         <interfaced.item name="get_rvd"/>
      </interface>
      <type name="apir_requirement_assignment_item">
         <select extensible="YES" genericentity="YES" basedon="requirement_assignment_item" selectitems="Mating_connector_termination Termination_constraint"/>
      </type>
      <type name="mating_connector_definition_select">
         <select selectitems="Packaged_part_terminal Printed_part_template_terminal"/>
      </type>
      <type name="three_dimensional_shape">
         <select selectitems="Advanced_brep_shape_representation Curve_swept_solid Geometrically_bounded_surface_shape_representation"/>
      </type>
      <entity name="Interface_requirement_occurrence" supertypes="Predefined_requirement_view_definition">
         <explicit name="interface_to_next_higher_assembly" optional="YES">
            <typename name="Next_higher_assembly_interface_requirement"/>
         </explicit>
         <explicit name="reference_protocol" optional="YES">
            <typename name="Protocol_physical_layer_definition"/>
         </explicit>
         <where label="WR1" expression="SELF\Product_view_definition.initial_context.life_cycle_stage = 'design'"/>
      </entity>
      <entity name="Mating_connector_placement_relationship">
         <explicit name="placed_connector">
            <typename name="Mating_connector_usage"/>
         </explicit>
         <explicit name="interface_context">
            <typename name="Next_higher_assembly_interface_requirement"/>
         </explicit>
         <explicit name="connector_placement">
            <typename name="Axis_placement"/>
         </explicit>
      </entity>
      <entity name="Mating_connector_termination">
         <explicit name="name">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="definition">
            <typename name="mating_connector_definition_select"/>
         </explicit>
         <explicit name="accessed_connector">
            <typename name="Mating_connector_usage"/>
         </explicit>
         <derived name="signal_definition" expression="get_rvd(SELF, 'signal definition')">
            <aggregate type="SET" lower="0" upper="?"/>
            <typename name="Requirement_view_definition"/>
         </derived>
         <unique label="UR1">
            <unique.attribute attribute="definition"/>
            <unique.attribute attribute="accessed_connector"/>
         </unique>
         <where label="WR1" expression="(SIZEOF(signal_definition) &lt;= 1)"/>
      </entity>
      <entity name="Mating_connector_usage" supertypes="Product_view_definition">
         <explicit name="definition">
            <typename name="Product_view_definition"/>
         </explicit>
         <explicit name="reference_designation" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <inverse name="placement_context" entity="Mating_connector_placement_relationship" attribute="placed_connector"/>
      </entity>
      <entity name="Next_higher_assembly_interface_requirement">
         <explicit name="shape">
            <typename name="three_dimensional_shape"/>
         </explicit>
         <explicit name="version_of_next_higher_assembly">
            <typename name="Product_version"/>
         </explicit>
      </entity>
      <entity name="Protocol_requirement_allocation_to_part_terminal" supertypes="Requirement_assignment">
         <explicit name="assigned_to">
            <typename name="Part_terminal"/>
            <redeclaration entity-ref="Requirement_assignment"/>
         </explicit>
         <explicit name="protocol_signal">
            <typename name="Functional_unit_usage_view_terminal_definition"/>
         </explicit>
         <explicit name="assigned_requirement_property">
            <typename name="Interface_requirement_occurrence"/>
            <redeclaration entity-ref="Requirement_assignment" old_name="assigned_requirement"/>
         </explicit>
         <where label="WR1" expression="EXISTS(assigned_requirement_property\Interface_requirement_occurrence.reference_protocol)"/>
         <where label="WR2" expression="protocol_signal IN assigned_requirement_property\Interface_requirement_occurrence.reference_protocol.host_functional_definition\Functional_unit_usage_view.access_mechanism"/>
      </entity>
      <entity name="Termination_constraint" supertypes="Constraint_occurrence">
         <explicit name="constrained_termination">
            <aggregate type="SET" lower="2" upper="?"/>
            <typename name="Mating_connector_termination"/>
         </explicit>
         <derived name="termination_usage_constraint" expression="get_rvd(SELF, 'termination usage constraint')">
            <aggregate type="SET" lower="0" upper="?"/>
            <typename name="Requirement_view_definition"/>
         </derived>
         <where label="WR1" expression="(SIZEOF(termination_usage_constraint) &lt;= 1)"/>
      </entity>
   </schema>
</express>
