<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="assembly_physical_requirement_allocation_arm schema_instance"/>
   <schema name="Assembly_physical_requirement_allocation_arm">
      <interface kind="use" schema="Assembly_physical_interface_requirement_arm"/>
      <interface kind="use" schema="Assembly_technology_arm"/>
      <interface kind="use" schema="Discrete_shield_arm"/>
      <interface kind="reference" schema="Requirement_decomposition_arm">
         <interfaced.item name="get_requirement_definition_property"/>
      </interface>
      <type name="apra_requirement_assignment_item">
         <select extensible="YES" basedon="requirement_assignment_item" selectitems="component_or_feature Physical_shield" genericentity="NO"/>
      </type>
      <type name="component_or_feature">
         <select extensible="YES" genericentity="YES" selectitems="Assembly_component Assembly_joint Component_feature"/>
      </type>
      <entity name="Assembly_requirement_allocation" supertypes="Requirement_assignment" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="assigned_to" optional="NO">
            <typename name="component_or_feature"/>
            <redeclaration entity-ref="Requirement_assignment"/>
         </explicit>
      </entity>
      <entity name="Assembly_separation_requirement_allocation" supertypes="Assembly_requirement_allocation" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="disjoint_assignment" optional="NO">
            <aggregate type="SET" lower="1" upper="?" unique="NO" optional="NO"/>
            <typename name="component_or_feature"/>
         </explicit>
      <where label="IP1"/>
      <where label="IP1"/>
      </entity>
      <entity name="Assembly_shield_allocation" supertypes="Requirement_assignment" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="assigned_to" optional="NO">
            <typename name="Physical_shield"/>
            <redeclaration entity-ref="Requirement_assignment"/>
         </explicit>
         <derived name="assigned_requirement_property" expression="get_requirement_definition_property(SELF,&#10;'ASSEMBLY_PHYSICAL_REQUIREMENT_ALLOCATION_ARM.'+'ELECTROMAGNETIC_REQUIREMENT_OCCURRENCE')">
