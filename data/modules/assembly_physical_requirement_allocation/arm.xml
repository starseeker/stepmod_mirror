<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$">
   <application name="JSDAI" owner="LKSoft" source="assembly_physical_requirement_allocation_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Assembly_physical_requirement_allocation_arm">
      <interface kind="use" schema="Assembly_physical_interface_requirement_arm"/>
      <interface kind="use" schema="Assembly_technology_arm"/>
      <interface kind="use" schema="Discrete_shield_arm"/>
      <type name="apra_requirement_assignment_item">
         <select basedon="requirement_assignment_item" extensible="YES" selectitems="component_or_feature Physical_shield"/>
      </type>
      <type name="component_or_feature">
         <select extensible="YES" genericentity="YES" selectitems="Assembly_component Assembly_joint Component_feature"/>
      </type>
      <entity name="Assembly_requirement_allocation" supertypes="Requirement_assignment">
         <explicit name="assigned_to">
            <typename name="component_or_feature"/>
            <redeclaration entity-ref="Requirement_assignment"/>
         </explicit>
      </entity>
      <entity name="Assembly_separation_requirement_allocation" supertypes="Assembly_requirement_allocation">
         <explicit name="disjoint_assignment">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="component_or_feature"/>
         </explicit>
      </entity>
      <entity name="Assembly_shield_allocation" supertypes="Requirement_assignment">
         <explicit name="assigned_to">
            <typename name="Physical_shield"/>
            <redeclaration entity-ref="Requirement_assignment"/>
         </explicit>
         <explicit name="assigned_requirement_property">
            <typename name="Predefined_requirement_view_definition"/>
            <redeclaration entity-ref="Requirement_assignment" old_name="assigned_requirement"/>
         </explicit>
         <where expression="assigned_requirement_property\Product_view_definition.initial_context\View_definition_context.application_domain = 'electrical'" label="WR1"/>
      </entity>
      <entity name="Electromagnetic_compatibility_requirement_allocation" supertypes="Assembly_requirement_allocation"/>
      <entity name="Interface_component_allocation">
         <explicit name="mating_solution">
            <typename name="Interface_component"/>
         </explicit>
         <explicit name="mating_requirement">
            <typename name="Mating_connector_usage"/>
         </explicit>
      </entity>
      <entity name="Thermal_requirement_allocation" supertypes="Assembly_requirement_allocation">
         <explicit name="assigned_to">
            <typename name="Thermal_component"/>
            <redeclaration entity-ref="Requirement_assignment"/>
         </explicit>
         <explicit name="assigned_requirement_property">
            <typename name="Predefined_requirement_view_definition"/>
            <redeclaration entity-ref="Requirement_assignment" old_name="assigned_requirement"/>
         </explicit>
         <where expression="assigned_requirement_property\Product_view_definition.initial_context\View_definition_context.application_domain = 'thermal'" label="WR1"/>
      </entity>
   </schema>
</express>
