         </derived>
      </entity>
      <entity name="Electromagnetic_compatibility_requirement_allocation" supertypes="Assembly_requirement_allocation" abstract.supertype="NO" abstract.entity="NO"/>
      <entity name="Interface_component_allocation" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="mating_solution" optional="NO">
            <typename name="Interface_component"/>
         </explicit>
         <explicit name="mating_requirement" optional="NO">
            <typename name="Mating_connector_component"/>
         </explicit>
      </entity>
      <entity name="Thermal_requirement_allocation" supertypes="Assembly_requirement_allocation" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="assigned_to" optional="NO">
            <typename name="Thermal_component"/>
            <redeclaration entity-ref="Requirement_assignment"/>
         </explicit>
         <derived name="assigned_requirement_property" expression="get_requirement_definition_property(SELF, 'ASSEMBLY_PHYSICAL_REQUIREMENT_ALLOCATION_ARM.'+'THERMAL_REQUIREMENT_OCCURRENCE')">
            <typename name="Requirement_definition_property"/>
         </derived>
      </entity>
   </schema>
