<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:08" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="assembly_physical_requirement_allocation_arm schema_instance"/>
   <schema name="ASSEMBLY_PHYSICAL_REQUIREMENT_ALLOCATION_ARM">
      <interface kind="use" schema="ASSEMBLY_PHYSICAL_INTERFACE_REQUIREMENT_ARM"/>
      <interface kind="use" schema="ASSEMBLY_TECHNOLOGY_ARM"/>
      <interface kind="use" schema="INTERFACE_COMPONENT_ARM"/>
      <interface kind="use" schema="PHYSICAL_UNIT_DESIGN_VIEW_ARM"/>
      <interface kind="use" schema="REQUIREMENT_ASSIGNMENT_ARM"/>
      <interface kind="use" schema="REQUIREMENT_DECOMPOSITION_ARM"/>
      <interface kind="use" schema="SHIELD_ARM"/>
      <interface kind="reference" schema="REQUIREMENT_DECOMPOSITION_ARM">
         <interfaced.item name="get_requirement_definition_property"/>
      </interface>
      <type name="component_or_feature">
         <select extensible="YES" genericentity="YES" selectitems="assembly_component assembly_joint component_feature"/>
      </type>
      <entity name="assembly_requirement_allocation" supertypes="requirement_assignment">
         <explicit name="assigned_to">
            <typename name="component_or_feature"/>
            <redeclaration entity-ref="requirement_assignment"/>
         </explicit>
      </entity>
      <entity name="assembly_separation_requirement_allocation" supertypes="assembly_requirement_allocation">
         <explicit name="disjoint_assignment">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="component_or_feature"/>
         </explicit>
      </entity>
      <entity name="electromagnetic_compatibility_requirement_allocation" supertypes="assembly_requirement_allocation" super.expression="shield_allocation"/>
      <entity name="interface_component_allocation">
         <explicit name="mating_solution">
            <typename name="interface_component"/>
         </explicit>
         <explicit name="mating_requirement">
            <typename name="mating_connector_component"/>
         </explicit>
      </entity>
      <entity name="shield_allocation" supertypes="electromagnetic_compatibility_requirement_allocation">
         <explicit name="assigned_to">
            <typename name="shield"/>
            <redeclaration entity-ref="requirement_assignment"/>
         </explicit>
         <derived name="assigned_requirement_property" expression=" get_requirement_definition_property ( self , 'assembly_physical_requirement_allocation_arm.' + 'electromagnetic_requirement_occurrence' ) ">
            <typename name="requirement_definition_property"/>
         </derived>
      </entity>
      <entity name="thermal_requirement_allocation" supertypes="assembly_requirement_allocation">
         <explicit name="assigned_to">
            <typename name="thermal_component"/>
            <redeclaration entity-ref="requirement_assignment"/>
         </explicit>
         <derived name="assigned_requirement_property" expression=" get_requirement_definition_property ( self , 'assembly_physical_requirement_allocation_arm.' + 'thermal_requirement_occurrence' ) ">
            <typename name="requirement_definition_property"/>
         </derived>
      </entity>
   </schema>
</express>
