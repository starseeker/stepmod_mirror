<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:55:28" rcs.revision="1.0" description.file="mim_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="assembly_physical_requirement_allocation_mim schema_instance"/>
   <schema name="ASSEMBLY_PHYSICAL_REQUIREMENT_ALLOCATION_MIM">
      <interface kind="use" schema="ASSEMBLY_PHYSICAL_INTERFACE_REQUIREMENT_MIM"/>
      <interface kind="use" schema="ASSEMBLY_TECHNOLOGY_MIM"/>
      <interface kind="use" schema="INTERFACE_COMPONENT_MIM"/>
      <interface kind="use" schema="PHYSICAL_UNIT_DESIGN_VIEW_MIM"/>
      <interface kind="use" schema="REQUIREMENT_ASSIGNMENT_MIM"/>
      <interface kind="use" schema="REQUIREMENT_DECOMPOSITION_MIM"/>
      <interface kind="use" schema="SHIELD_MIM"/>
      <type name="apra_groupable_item">
         <select extensible="YES" genericentity="YES" basedon="groupable_item" selectitems="shape_aspect product_definition"/>
      </type>
      <type name="apra_requirement_assigned_item">
         <select extensible="YES" genericentity="YES" basedon="requirement_assigned_item" selectitems="product_definition shape_aspect"/>
      </type>
      <entity name="electromagnetic_compatibility_requirement_allocation" supertypes="requirement_assignment" super.expression="shield_allocation"/>
      <entity name="shield_allocation" supertypes="electromagnetic_compatibility_requirement_allocation"/>
      <entity name="thermal_requirement_allocation" supertypes="requirement_assignment"/>
      <rule name="interface_component_allocation_constraint" appliesto="product_definition_relationship">
         <where label="wr1" expression=" sizeof ( query ( pdr &lt;* product_definition_relationship | ( pdr \ product_definition_relationship . name = 'interface component allocation' ) and not ( ( 'assembly_physical_requirement_allocation_mim.' + 'component_definition' in typeof ( pdr . relating_product_definition ) ) and ( pdr . relating_product_definition \ product_definition . description = 'mating connector' ) ) ) ) = 0 "/>
      </rule>
   </schema>
</express>
