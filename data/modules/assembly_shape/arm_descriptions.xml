<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: Assembly_shape_arm
   <express_ref linkend="assembly_shape:arm:Assembly_shape_arm"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Assembly_shape_arm">
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Assembly_shape_arm  Type:   reflected_shape_element_id
   <express_ref linkend="assembly_shape:arm:Assembly_shape_arm.reflected_shape_element_id"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Assembly_shape_arm.reflected_shape_element_id">
	The <b>reflected_shape_element_id</b> type allows for the designation of the data type
	<express_ref linkend="support_resource:arm:Support_resource_arm.identifier" />.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Assembly_shape_arm  Entity: Assembly_shape  
   <express_ref linkend="assembly_shape:arm:Assembly_shape_arm.Assembly_shape"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Assembly_shape_arm.Assembly_shape">
	An <b>Assembly_shape</b> is a type of
	<express_ref linkend="part_shape:arm:Part_shape_arm.Part_shape" /> 
	that represents the shape of an assembly and the assembly components.
</ext_description>

<ext_description linkend="Assembly_shape_arm.Assembly_shape.described_element">
	an inherited attribute whose value shall be of type
	<express_ref linkend="Item_definition_structure:arm:Item_definition_structure_arm.Assembly_definition" />
	for the <b>Assembly_shape</b>.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Assembly_shape_arm  Entity:   Component_path_shape_element
   <express_ref linkend="assembly_shape:arm:Assembly_shape_arm.Component_path_shape_element"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Assembly_shape_arm.Component_path_shape_element">
	A <b>Component_path_shape_element</b> is a type of 
	<express_ref linkend="part_shape:arm:Part_shape_arm.Part_shape_element"/>
	of an assembly that identifies a 
	<express_ref linkend="part_shape:arm:Part_shape_arm.Part_shape_element"/>
	of an occurrence of a part within this assembly. 
	The referenced part and its 
	<express_ref linkend="part_shape:arm:Part_shape_arm.Part_shape_element"/>
	might be defined locally or externally. 
</ext_description>

<!-- location -->
<ext_description linkend="Assembly_shape_arm.Component_path_shape_element.location">
	the occurrence of a part by its 
	<express_ref linkend="assembly_structure:arm:Assembly_structure_arm.Next_assembly_usage"/> 
	or 
	<express_ref linkend="assembly_structure:arm:Assembly_structure_arm.Multi_level_reference_designator"/>. 
</ext_description>

<!-- component_shape_element -->
<ext_description linkend="Assembly_shape_arm.Component_path_shape_element.component_shape_element">
	the locally or externally defined 
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element"/>.
	In the case that it is externally defined, only the id of the externally defined 
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element"/>
	is provided.
</ext_description>

<!-- UR1 -->
<ext_description linkend="Assembly_shape_arm.Component_path_shape_element.ur:UR1">
	The combination of <b>location</b> and <b>component_shape_element</b> shall be unique within a population of <b>Component_path_shape_element</b>. 
</ext_description>

<!-- WR1 -->
<ext_description linkend="Assembly_shape_arm.Component_path_shape_element.wr:WR1">
	If the 	
	<express_ref linkend="assembly_shape:arm:Assembly_shape_arm.Component_path_shape_element.component_shape_element"/>
	is a
	<express_ref linkend="assembly_structure:arm:Assembly_structure_arm.Next_assembly_usage"/> 
	the	
	<express_ref linkend="assembly_structure:arm:Assembly_structure_arm.Assembly_component_relationship.location_indicator"/>
	shall be provided. 
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Assembly_shape_arm  Entity:   Multi_level_reference_designator
   <express_ref linkend="assembly_shape:arm:Assembly_shape_arm.Multi_level_reference_designator"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Assembly_shape_arm.Multi_level_reference_designator">
	A <b>Multi_level_reference_designator</b> is a ..
</ext_description>

<!-- name -->
<ext_description linkend="Assembly_shape_arm.Multi_level_reference_designator.name">
	the 
	<express_ref linkend="support_resource:arm:Support_resource_arm.label"/> 
	by which the <b>Multi_level_reference_designator</b> is known.
	The value of this attribute need not be specified.
</ext_description>

<!-- description -->
<ext_description linkend="Assembly_shape_arm.Multi_level_reference_designator.description">
	the 
	<express_ref linkend="support_resource:arm:Support_resource_arm.text"/> 
	that provides further information about the <b>Multi_level_reference_designator</b>.
	The value of this attribute need not be specified.
</ext_description>

<!-- location -->
<ext_description linkend="Assembly_shape_arm.Multi_level_reference_designator.location">
	specifies the role of the
	<express_ref linkend="assembly_structure:arm:Assembly_structure_arm.Next_assembly_usage" />
	for the <b>Multi_level_reference_designator</b>.
	There shall exist at least one
	<express_ref linkend="assembly_structure:arm:Assembly_structure_arm.Next_assembly_usage" />
	for the <b>Multi_level_reference_designator</b>.
</ext_description>

<!-- root_node -->
<ext_description linkend="Assembly_shape_arm.Multi_level_reference_designator.root_node">
	specifies the role of the 
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition" />
	for the <b>Multi_level_reference_designator</b>.
</ext_description>

<!-- UR1 -->
<ext_description linkend="Assembly_shape_arm.Multi_level_reference_designator.ur:UR1">
	The <b>location</b> shall be unique within a population of <b>Multi_level_reference_designator</b>.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Assembly_shape_arm  Entity: Part_occurrence_shape  
   <express_ref linkend="assembly_shape:arm:Assembly_shape_arm.Part_occurrence_shape"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Assembly_shape_arm.Part_occurrence_shape">
	A <b>Part_occurrence_shape</b> is a type of
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Item_shape" />
	that represents the shape of an occurrence of a part in a product structure. 
</ext_description>

<ext_description linkend="Assembly_shape_arm.Part_occurrence_shape.described_element">
	an inherited attribute whose value shall be of type
	<express_ref linkend="product_occurrence:arm:Product_occurrence_arm.Definition_based_product_occurrence" />.
</ext_description>

</ext_descriptions>
