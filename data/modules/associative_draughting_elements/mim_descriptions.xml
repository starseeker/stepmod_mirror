++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Associative_draughting_elements_mim">
</ext_description>

<!-- +++++++++++ draughting_model_item_association +++++++++++ -->
<ext_description linkend="Associative_draughting_elements_mim.draughting_model_item_association">
	A <b>draughting_model_item_association</b> is a type of
	<express_ref linkend="product_property_representation_schema:ir_express:product_property_representation_schema.item_identified_representation_usage"/>.
</ext_description>

<!-- definition -->
<ext_description linkend="Associative_draughting_elements_mim.draughting_model_item_association.definition">
	an attribute inherited from the
	<express_ref linkend="product_property_representation_schema:ir_express:product_property_representation_schema.item_identified_representation_usage"/>
	shall be redeclared as the
	<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.shape_aspect"/>
	for the <b>draughting_model_item_association</b>.
</ext_description>

<!-- used_representation -->
<ext_description linkend="Associative_draughting_elements_mim.draughting_model_item_association.used_representation">
	an attribute inherited from the
	<express_ref linkend="product_property_representation_schema:ir_express:product_property_representation_schema.item_identified_representation_usage"/>
	shall be redeclared as the
	<express_ref linkend="aic_associative_draughting_elements:ir_express:aic_associative_draughting_elements.draughting_model"/>
	for the <b>draughting_model_item_association</b>.
</ext_description>

<!-- identified_item -->
<ext_description linkend="Associative_draughting_elements_mim.draughting_model_item_association.identified_item">
	an attribute inherited from the
	<express_ref linkend="product_property_representation_schema:ir_express:product_property_representation_schema.item_identified_representation_usage"/>
	shall be redeclared as the
	<express_ref linkend="Associative_draughting_elements:mim:Associative_draughting_elements_mim.draughting_model_item_association_select"/>
	for the <b>draughting_model_item_association</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Associative_draughting_elements_mim.draughting_model_item_association.wr:WR1">
	<p>
		If <b>identified_item</b> is of type 
		<express_ref linkend="aic_draughting_elements:ir_express:aic_draughting_elements.datum_feature_callout"/>, 
		associated <b>definition</b> must be of type 
		<express_ref linkend="shape_aspect_definition_schema:ir_express:shape_aspect_definition_schema.datum_feature"/>.
	</p>
</ext_description>

<!-- WR2 -->
<ext_description linkend="Associative_draughting_elements_mim.draughting_model_item_association.wr:WR2">
	<p>
		If <b>identified_item</b> is of type 
		<express_ref linkend="aic_draughting_elements:ir_express:aic_draughting_elements.datum_target_callout"/>, 
		associated <b>definition</b> must be of type 
		<express_ref linkend="shape_aspect_definition_schema:ir_express:shape_aspect_definition_schema.datum_target"/>.
	</p>
</ext_description>

<!-- WR3 -->
<ext_description linkend="Associative_draughting_elements_mim.draughting_model_item_association.wr:WR3">
	<p>
		If <b>identified_item</b> is of type 
		<express_ref linkend="aic_draughting_elements:ir_express:aic_draughting_elements.angular_dimension"/>, 
		associated <b>definition</b> must be of type 
		<express_ref linkend="shape_dimension_schema:ir_express:shape_dimension_schema.angular_size"/>.
	</p>
</ext_description>

<!-- WR4 -->
<ext_description linkend="Associative_draughting_elements_mim.draughting_model_item_association.wr:WR4">
	<p>
		If <b>identified_item</b> is of type 
		<express_ref linkend="aic_draughting_elements:ir_express:aic_draughting_elements.curve_dimension"/>, 
		associated <b>definition</b> must be of type 
		<express_ref linkend="shape_dimension_schema:ir_express:shape_dimension_schema.dimensional_size"/>
		and its 
		<express_ref linkend="shape_dimension_schema:ir_express:shape_dimension_schema.dimensional_size.name"/> 
		must be equal to 'curve dimension'.
	</p>
</ext_description>

<!-- WR5 -->
<ext_description linkend="Associative_draughting_elements_mim.draughting_model_item_association.wr:WR5">
	<p>
		If <b>identified_item</b> is of type 
		<express_ref linkend="aic_draughting_elements:ir_express:aic_draughting_elements.diameter_dimension"/>, 
		associated <b>definition</b> must be of type 
		<express_ref linkend="shape_dimension_schema:ir_express:shape_dimension_schema.dimensional_size"/> 
		and its 
		<express_ref linkend="shape_dimension_schema:ir_express:shape_dimension_schema.dimensional_size.name"/> 
		must be equal to 'diameter'.
	</p>
</ext_description>

<!-- WR6 -->
<ext_description linkend="Associative_draughting_elements_mim.draughting_model_item_association.wr:WR6">
	<p>
		If <b>identified_item</b> is of type 
		<express_ref linkend="aic_draughting_elements:ir_express:aic_draughting_elements.linear_dimension"/>, 
		associated <b>definition</b> must be of type 
		<express_ref linkend="shape_dimension_schema:ir_express:shape_dimension_schema.dimensional_size"/> 
		and its 
		<express_ref linkend="shape_dimension_schema:ir_express:shape_dimension_schema.dimensional_size.name"/> 
		must be equal to either 'heigth' or 'length' or 'width'.
	</p>
</ext_description>

<!-- WR7 -->
<ext_description linkend="Associative_draughting_elements_mim.draughting_model_item_association.wr:WR7">
	<p>
		If <b>identified_item</b> is of type 
		<express_ref linkend="aic_draughting_elements:ir_express:aic_draughting_elements.radius_dimension"/>, 
		associated <b>definition</b> must be of type 
		<express_ref linkend="shape_dimension_schema:ir_express:shape_dimension_schema.dimensional_size"/> 
		and its 
		<express_ref linkend="shape_dimension_schema:ir_express:shape_dimension_schema.dimensional_size.name"/> 
		must be equal to either 'radius'.
	</p>
</ext_description>

<!-- WR8 -->
<ext_description linkend="Associative_draughting_elements_mim.draughting_model_item_association.wr:WR8">
	<p>
		If <b>identified_item</b> is of type 
		<express_ref linkend="aic_draughting_elements:ir_express:aic_draughting_elements.geometrical_tolerance_callout"/>, 
		associated <b>definition</b> must be of type 
		<express_ref linkend="shape_tolerance_schema:ir_express:shape_tolerance_schema.geometric_tolerance"/>.
	</p>
</ext_description>

<!-- WR9 -->
<ext_description linkend="Associative_draughting_elements_mim.draughting_model_item_association.wr:WR9">
	<p>
		If associated <b>definition</b> is of type 
		<express_ref linkend="shape_dimension_schema:ir_express:shape_dimension_schema.dimensional_size"/>, 
		<b>identified_item</b> must be of type 
		<express_ref linkend="draughting_dimension_schema:ir_express:draughting_dimension_schema.dimension_callout"/> 
		or 
		<express_ref linkend="draughting_element_schema:ir_express:draughting_element_schema.dimension_curve_directed_callout"/> 
		or 
		<express_ref linkend="aic_draughting_elements:ir_express:aic_draughting_elements.leader_directed_dimension"/> 
		or 
		<express_ref linkend="aic_draughting_elements:ir_express:aic_draughting_elements.ordinate_dimension"/> 
		or 
		<express_ref linkend="aic_draughting_elements:ir_express:aic_draughting_elements.structured_dimension_callout"/>.
	</p>
</ext_description>

</ext_descriptions>
