</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Attachment_slot_arm  Entity: Attachment_slot_planned_to_realized  Attribute: name 
   <express_ref linkend="attachment_slot:arm:Attachment_slot_arm.Attachment_slot_planned_to_realized.name"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
