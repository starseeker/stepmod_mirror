{ iso standard 10303 part (11) version (4) }

SCHEMA Attribute_classification_arm;

USE FROM Class_arm;    -- ISO/CD-TS 10303-1070


TYPE classified_attribute_select = EXTENSIBLE SELECT ;
END_TYPE;

ENTITY Attribute_classification;
  allowed_values : Class;
  attribute_name : STRING;
  classified_entities : classified_attribute_select;
END_ENTITY;

END_SCHEMA;
