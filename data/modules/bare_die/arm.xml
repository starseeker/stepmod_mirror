<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2004-12-02T11:57:02" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="bare_die_arm schema_instance"/>
   <schema name="Bare_die_arm">
      <interface kind="use" schema="Value_with_unit_extension_arm"/>
      <interface kind="use" schema="Elemental_geometric_shape_arm"/>
      <interface kind="use" schema="Part_feature_function_arm">
         <interfaced.item name="Part_mounting_feature"/>
      </interface>
      <interface kind="use" schema="Extended_geometric_tolerance_arm"/>
      <interface kind="use" schema="Pdm_material_aspects_arm"/>
      <interface kind="use" schema="Physical_unit_usage_view_arm"/>
      <interface kind="use" schema="Network_functional_usage_view_arm"/>
      <interface kind="use" schema="Non_feature_shape_element_arm"/>
      <interface kind="use" schema="Shape_composition_arm"/>
      <interface kind="use" schema="Part_terminal_arm"/>
      <interface kind="use" schema="Pdm_material_aspects_arm"/>
      <interface kind="use" schema="Part_occurrence_arm">
         <interfaced.item name="Definition_based_product_occurrence"/>
      </interface>
      <entity name="Bare_die" supertypes="Part_usage_view">
         <explicit name="implemented_function">
            <typename name="Functional_unit_usage_view"/>
         </explicit>
         <explicit name="maximum_material_condition_centroid_location" optional="YES">
            <typename name="Cartesian_point"/>
         </explicit>
         <explicit name="maximum_height_above_seating_plane" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="least_material_condition_centroid_location" optional="YES">
            <typename name="Cartesian_point"/>
         </explicit>
         <inverse name="die_seating_plane" entity="Non_feature_shape_element" attribute="scope"/>
         <inverse name="access_mechanisms" entity="Minimally_defined_bare_die_terminal" attribute="associated_definition">
            <inverse.aggregate type="SET" lower="2"/>
         </inverse>
         <unique label="UR1">
            <unique.attribute entity-ref="Bare_die" attribute="implemented_function"/>
         </unique>
         <where label="WR1" expression=" SIZEOF ( QUERY ( pf &lt;* USEDIN ( SELF , 'AP210_ARM.shape_element.containing_shape' ) | ( 'AP210_ARM.' + 'BARE_DIE_TOP_SURFACE' IN TYPEOF ( pf ) ) ) ) &lt;= 1 "/>
         <where label="WR2" expression=" SIZEOF ( QUERY ( pf &lt;* USEDIN ( SELF , 'AP210_ARM.shape_element.containing_shape' ) | ( 'AP210_ARM.' + 'BARE_DIE_BOTTOM_SURFACE' IN TYPEOF ( pf ) ) ) ) &lt;= 1 "/>
         <where label="WR3" expression=" SIZEOF ( QUERY ( pf &lt;* USEDIN ( SELF , 'AP210_ARM.shape_element.containing_shape' ) | ( 'AP210_ARM.' + 'BARE_DIE_EDGE_SURFACE' IN TYPEOF ( pf ) ) ) ) &lt;= 1 "/>
         <where label="WR4" expression=" 'AP210_ARM.SEATING_PLANE' IN TYPEOF ( die_seating_plane ) "/>
      </entity>
      <entity name="Bare_die_bottom_surface" supertypes="Bare_die_surface"/>
      <entity name="Bare_die_edge_segment_surface" supertypes="Bare_die_surface">
         <explicit name="end_vertex">
            <typename name="Edge_segment_vertex"/>
         </explicit>
         <explicit name="start_vertex">
            <typename name="Edge_segment_vertex"/>
         </explicit>
         <explicit name="composed_surface">
            <typename name="Bare_die_edge_surface"/>
         </explicit>
         <where label="WR1" expression=" start_vertex :&lt;&gt;: end_vertex "/>
         <where label="WR2" expression=" SELF \ shape_element . containing_shape :=: composed_surface \ shape_element . containing_shape "/>
      </entity>
      <entity name="Bare_die_edge_surface" supertypes="Bare_die_surface"/>
      <entity name="Bare_die_surface" abstract.entity="YES" supertypes="Part_mounting_feature" super.expression="ONEOF (Bare_die_top_surface, Bare_die_bottom_surface, Bare_die_edge_segment_surface, Bare_die_edge_surface)">
         <explicit name="associated_definition">
            <typename name="Bare_die"/>
            <redeclaration entity-ref="Part_feature"/>
         </explicit>
      </entity>
      <entity name="Bare_die_terminal" supertypes="Minimally_defined_bare_die_terminal">
         <explicit name="seating_plane_zone" optional="YES">
            <typename name="Connection_zone_bare_die_interface_plane_relationship"/>
         </explicit>
         <explicit name="external_connection_area" optional="YES">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="Connection_zone_in_usage_view"/>
         </explicit>
         <explicit name="material">
            <typename name="Material_identification"/>
         </explicit>
      </entity>
      <entity name="Bare_die_terminal_surface_constituent_relationship" supertypes="Shape_element_constituent_relationship">
         <explicit name="related">
            <typename name="Bare_die_terminal"/>
            <redeclaration entity-ref="Shape_element_relationship"/>
         </explicit>
         <explicit name="relating">
            <typename name="Bare_die_surface"/>
            <redeclaration entity-ref="Shape_element_relationship"/>
         </explicit>
      </entity>
      <entity name="Bare_die_top_surface" supertypes="Bare_die_surface"/>
      <entity name="Connection_zone_bare_die_interface_plane_relationship">
         <explicit name="derived_zone_shape_class">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="derived_zone">
            <typename name="Connection_zone_in_usage_view"/>
         </explicit>
         <explicit name="mating_zone">
            <typename name="Connection_zone_in_usage_view"/>
         </explicit>
         <explicit name="interface_plane">
            <typename name="Seating_plane"/>
         </explicit>
         <inverse name="associated_bare_die_terminal" entity="Bare_die_terminal" attribute="seating_plane_zone">
            <inverse.aggregate type="SET" lower="0" upper="1"/>
         </inverse>
         <unique label="UR1">
            <unique.attribute entity-ref="Connection_zone_bare_die_interface_plane_relationship" attribute="interface_plane"/>
            <unique.attribute entity-ref="Connection_zone_bare_die_interface_plane_relationship" attribute="mating_zone"/>
         </unique>
         <where label="WR1" expression=" derived_zone_shape_class IN [ 'area' , 'edge' ] "/>
      </entity>
      <entity name="Guided_wave_bare_die_terminal" supertypes="Minimally_defined_bare_die_terminal"/>
      <entity name="Minimally_defined_bare_die_terminal" supertypes="Part_terminal" super.expression="Guided_wave_bare_die_terminal ANDOR Bare_die_terminal">
         <explicit name="associated_definition">
            <typename name="Bare_die"/>
            <redeclaration entity-ref="Part_feature"/>
         </explicit>
      </entity>
   </schema>
</express>
