<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml2.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.38"
    source="../data/modules/basic_data_representation/arm.exp"/>

  <schema name="Basic_data_representation_arm">

    <interface
      kind="use"
      schema="Foundation_representation_arm">
    </interface>
    <entity
      name="Boolean_representation_item"
      supertypes="Representation_item">
      <explicit
        name="the_value">
        <builtintype
          type="BOOLEAN"/>
      </explicit>
    </entity>

    <entity
      name="Integer_representation_item"
      supertypes="Representation_item">
      <explicit
        name="the_value">
        <builtintype
          type="INTEGER"/>
      </explicit>
    </entity>

    <entity
      name="Rational_representation_item"
      supertypes="Representation_item">
      <explicit
        name="numerator">
        <builtintype
          type="INTEGER"/>
      </explicit>
      <explicit
        name="denominator">
        <builtintype
          type="INTEGER"/>
      </explicit>
    </entity>

    <entity
      name="Real_representation_item"
      supertypes="Representation_item">
      <explicit
        name="the_value">
        <builtintype
          type="REAL"/>
      </explicit>
    </entity>

    <entity
      name="Sequence_representation_item"
      supertypes="Representation_item">
      <explicit
        name="the_value">
        <aggregate
          type="LIST"/>
        <typename
          name="representation_item"/>
      </explicit>
    </entity>

    <entity
      name="Set_representation_item"
      supertypes="Representation_item">
      <explicit
        name="the_value">
        <aggregate
          type="SET"/>
        <typename
          name="representation_item"/>
      </explicit>
    </entity>

  </schema>
</express>
