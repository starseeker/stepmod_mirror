	<ext_description linkend="Basic_data_representation_arm.Sequence_representation_item.the_value">
	the value of the <b>Sequence_representation_item</b>.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Basic_data_representation_arm  Entity: Set_representation_item  
   <express_ref linkend="basic_data_representation:arm:Basic_data_representation_arm.Set_representation_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Basic_data_representation_arm.Set_representation_item">
        A <b>Set_representation_item</b> is a type of 
        <express_ref linkend="Basic_data_representation:arm:Basic_data_representation_arm.Compound_representation_item"/> 
