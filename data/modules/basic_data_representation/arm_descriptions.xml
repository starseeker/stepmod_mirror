<ext_descriptions module_directory="basic_data_representation" schema_file="arm.xml" describe.selects="YES" describe.subtype_constraints="YES" rcs.date="$Date: 2011/07/31 10:01:45 $" rcs.revision="$Revision: 1.3 $">
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: Basic_data_representation_arm
   <express_ref linkend="basic_data_representation:arm:Basic_data_representation_arm"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<ext_description linkend="Basic_data_representation_arm">
</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Basic_data_representation_arm  Entity: Boolean_representation_item  
   <express_ref linkend="basic_data_representation:arm:Basic_data_representation_arm.Boolean_representation_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Basic_data_representation_arm.Boolean_representation_item">
        A <b>Boolean_representation_item</b> is a type of <express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation_item"/> whose value is either true or false.
</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Basic_data_representation_arm  Entity: Boolean_representation_item  Attribute: the_value 
   <express_ref linkend="basic_data_representation:arm:Basic_data_representation_arm.Boolean_representation_item.the_value"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Basic_data_representation_arm.Boolean_representation_item.the_value">
		the value of the <b>Boolean_representation_item</b>.
	</ext_description>
	
<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Basic_data_representation_arm  Entity:   Compound_representation_item
   <express_ref linkend="basic_data_representation:arm:Basic_data_representation_arm.Compound_representation_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Basic_data_representation_arm.Compound_representation_item">
		A <b>Compound_representation_item</b> is a type of
		<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation_item"/>.
		A <b>Compound_representation_item</b> is a
		<express_ref linkend="Basic_data_representation:arm:Basic_data_representation_arm.Sequence_representation_item"/>
		or a
		<express_ref linkend="Basic_data_representation:arm:Basic_data_representation_arm.Set_representation_item"/>.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Basic_data_representation_arm  Entity: Composite_representation_item  
   <express_ref linkend="basic_data_representation:arm:Basic_data_representation_arm.Composite_representation_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Basic_data_representation_arm.Composite_representation_item">
        A <b>Composite_representation_item</b> is a type of <express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation_item"/> whose value is a set of <express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation"/> objects.
</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Basic_data_representation_arm  Entity: Composite_representation_item  Attribute: content 
   <express_ref linkend="basic_data_representation:arm:Basic_data_representation_arm.Composite_representation_item.content"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Basic_data_representation_arm.Composite_representation_item.content">
		the property values that make up the <b>Composite_representation_item</b>.
</ext_description>

<!-- +++++++++++ Date_time_representation_item +++++++++++ -->
<ext_description linkend="Basic_data_representation_arm.Date_time_representation_item">
	A <b>Date_time_representation_item</b> is a type of
	<express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation_item"/>
	whose value is
	<express_ref linkend="date_time:arm:Date_time_arm.date_or_date_time_select"/>.
</ext_description>

<!-- the_value -->
<ext_description linkend="Basic_data_representation_arm.Date_time_representation_item.the_value">
	specifies the 
	<express_ref linkend="date_time:arm:Date_time_arm.date_or_date_time_select"/>.
	for the <b>Date_time_representation_item</b>.
</ext_description>

	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Basic_data_representation_arm  Entity: External_representation_item  
   <express_ref linkend="basic_data_representation:arm:Basic_data_representation_arm.External_representation_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Basic_data_representation_arm.External_representation_item">
        An <b>External_representation_item</b> is a type of <express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation_item"/> whose value is defined in an external library.
</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Basic_data_representation_arm  Entity: External_representation_item  Attribute: id 
   <express_ref linkend="basic_data_representation:arm:Basic_data_representation_arm.External_representation_item.id"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Basic_data_representation_arm.External_representation_item.id">
       the identifier that distinguishes the <express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation_item"/> in the external library.
      </ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Basic_data_representation_arm  Entity: External_representation_item  Attribute: external_source 
   <express_ref linkend="basic_data_representation:arm:Basic_data_representation_arm.External_representation_item.external_source"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Basic_data_representation_arm.External_representation_item.external_source">
	the identification of the external library that provides the value of the <express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation_item"/>.
</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Basic_data_representation_arm  Entity: External_string_representation_item  
   <express_ref linkend="basic_data_representation:arm:Basic_data_representation_arm.External_string_representation_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Basic_data_representation_arm.External_string_representation_item">
        An <b>External_string_representation_item</b> is a type of <express_ref linkend="basic_data_representation:arm:Basic_data_representation_arm.External_representation_item"/> whose value is a string.
</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Basic_data_representation_arm  Entity: Integer_representation_item  
   <express_ref linkend="basic_data_representation:arm:Basic_data_representation_arm.Integer_representation_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Basic_data_representation_arm.Integer_representation_item">
        A <b>Integer_representation_item</b> is a type of <express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation_item"/> whose value is an integer.
</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Basic_data_representation_arm  Entity: Integer_representation_item  Attribute: the_value 
   <express_ref linkend="basic_data_representation:arm:Basic_data_representation_arm.Integer_representation_item.the_value"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Basic_data_representation_arm.Integer_representation_item.the_value">
	the value of the <b>Integer_representation_item</b>.
	</ext_description>
	
<!-- +++++++++++ Logical_representation_item +++++++++++ -->
<ext_description linkend="Basic_data_representation_arm.Logical_representation_item">
	A <b>Logical_representation_item</b> is a type of
	<express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation_item"/>
	for the representation of logical and boolean values.
	<note>
		A <b>Logical_representation_item</b> is included in the characteristic select via its supertype 
		<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.String_representation_item"/>.
		
	</note>
</ext_description>

<!-- the_value -->
<ext_description linkend="Basic_data_representation_arm.Logical_representation_item.the_value">
	the value of the <b>Logical_representation_item</b>.
</ext_description>


	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Basic_data_representation_arm  Entity: Rational_representation_item  
   <express_ref linkend="basic_data_representation:arm:Basic_data_representation_arm.Rational_representation_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Basic_data_representation_arm.Rational_representation_item">
        A <b>Rational_representation_item</b> is a type of <express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation_item"/> whose value is a rational number.
</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Basic_data_representation_arm  Entity: Rational_representation_item  Attribute: numerator 
   <express_ref linkend="basic_data_representation:arm:Basic_data_representation_arm.Rational_representation_item.numerator"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Basic_data_representation_arm.Rational_representation_item.numerator">
	the numerator of the rational number.
</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Basic_data_representation_arm  Entity: Rational_representation_item  Attribute: denominator 
   <express_ref linkend="basic_data_representation:arm:Basic_data_representation_arm.Rational_representation_item.denominator"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Basic_data_representation_arm.Rational_representation_item.denominator">
	the denominator of the rational number.
</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Basic_data_representation_arm  Entity: Real_representation_item  
   <express_ref linkend="basic_data_representation:arm:Basic_data_representation_arm.Real_representation_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Basic_data_representation_arm.Real_representation_item">
        A <b>Real_representation_item</b> is a type of 
        <express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation_item"/> 
        whose value is a real number.
        <note>
        	Use 
        	<express_ref linkend="Value_with_unit:arm:Value_with_unit_arm.Value_with_unit"/> 
        	instead of 
        	<express_ref linkend="Basic_data_representation:arm:Basic_data_representation_arm.Real_representation_item"/> 
        	for all cases of measurement and when a physical unit is needed.
        </note>
</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Basic_data_representation_arm  Entity: Real_representation_item  Attribute: the_value 
   <express_ref linkend="basic_data_representation:arm:Basic_data_representation_arm.Real_representation_item.the_value"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Basic_data_representation_arm.Real_representation_item.the_value">
	the value of the <b>Real_representation_item</b>.
	</ext_description>
