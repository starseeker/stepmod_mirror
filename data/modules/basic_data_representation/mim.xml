<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="mim_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml2.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.38"
    source="../data/modules/basic_data_representation/mim.exp"/>

  <schema name="Basic_data_representation_mim">

    <interface
      kind="use"
      schema="Foundation_representation_mim">
    </interface>
    <interface
      kind="use"
      schema="representation_schema">
    </interface>
    <interface
      kind="use"
      schema="ISO13584_expressions_schema">
    </interface>
    <entity
      name="boolean_representation_item"
      supertypes="representation_item boolean_literal">
    </entity>

    <entity
      name="integer_representation_item"
      supertypes="representation_item int_literal">
    </entity>

    <entity
      name="rational_representation_item"
      supertypes="representation_item binary_numeric_expression">
    </entity>

    <entity
      name="real_representation_item"
      supertypes="representation_item real_literal">
    </entity>

    <entity
      name="sequence_representation_item"
      supertypes="compound_representation_item">
      <where
        label="WR1"
        expression="'REPRESENTATION_SCHEMA.LIST_REPRESENTATION_ITEM' IN TYPEOF(SELF\compound_representation_item.item_element)">
      </where>
    </entity>

    <entity
      name="ensemble_representation_item"
      supertypes="compound_representation_item">
      <where
        label="WR1"
        expression="'REPRESENTATION_SCHEMA.SET_REPRESENTATION_ITEM' IN TYPEOF(SELF\compound_representation_item.item_element)">
      </where>
    </entity>

  </schema>
</express>
