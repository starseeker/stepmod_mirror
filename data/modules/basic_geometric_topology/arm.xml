<?xml version="1.0"?>
<!-- $Id: arm.xml,v 1.7 2002/07/08 07:04:14 goset1 Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: 2002/07/08 07:04:14 $"
  rcs.revision="$Revision: 1.7 $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.16"
    source="../data/modules/basic_geometric_topology/arm.exp"/>

  <schema name="Basic_geometric_topology_arm">
    <interface
      kind="use"
      schema="Elemental_topology_arm">
    </interface>
 <interface
       kind="use"
       schema="Elemental_geometric_shape_arm">
     </interface>

 
     
    <entity
      name="Vertex"
      supertypes="Detailed_topological_model_element">
     
    </entity>

  
  <entity
        name="Vertex_point"
        supertypes="Detailed_geometric_model_element, Vertex">
        <explicit name="vertex_position">
                  <typename name="Cartesian_point"/>
    </explicit>     
      </entity>

   <entity
          name="Curve"
          supertypes="Detailed_geometric_model_element">
            
      </entity>
 
 <entity
         name="Edge"
         supertypes="Detailed_topological_model_element">
      <explicit name="start_vertex">
                <typename name="Vertex"/>
     </explicit> 
     <explicit name="end_vertex">
              <typename name="Vertex"/>
     </explicit> 
   </entity>
 
  <entity
         name="Edge_curve"
         supertypes="Detailed_geometric_model_element, Edge">
         <explicit name="edge_geometry">
                   <typename name="Curve"/>
         </explicit>
         <explicit name="same_sense">
	                    <typename name="BOOLEAN"/>
         </explicit>
    </entity>

  </schema>

</express>
