<?xml version="1.0" encoding="utf-8"?><?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express
  SYSTEM "../../../dtd/express.dtd">
<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="">
   <application name="JSDAI" owner="LKSoft" source="basic_geometric_topology_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Basic_geometric_topology_arm">
      <interface kind="use" schema="Basic_geometry_arm"/>
      <interface kind="use" schema="Elemental_topology_arm"/>
      <entity name="Edge_curve" supertypes="Detailed_geometric_model_element Edge">
         <explicit name="edge_geometry">
            <typename name="Curve"/>
         </explicit>
         <explicit name="same_sense">
            <builtintype type="BOOLEAN"/>
         </explicit>
      </entity>
      <entity name="Face_surface" supertypes="Detailed_geometric_model_element Face">
         <explicit name="face_geometry">
            <typename name="Surface"/>
         </explicit>
         <explicit name="same_sense">
            <builtintype type="BOOLEAN"/>
         </explicit>
      </entity>
      <entity name="Poly_loop" supertypes="Detailed_geometric_model_element Loop">
         <explicit name="polygon">
            <aggregate lower="3" type="LIST" unique="YES" upper="?"/>
            <typename name="Cartesian_point"/>
         </explicit>
      </entity>
      <entity name="Vertex_point" supertypes="Detailed_geometric_model_element Vertex">
         <explicit name="vertex_position">
            <typename name="Cartesian_point"/>
         </explicit>
      </entity>
   </schema>
</express>