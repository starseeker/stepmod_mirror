<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="basic_geometric_topology_arm schema_instance"/>
   <schema name="Basic_geometric_topology_arm">
      <interface kind="use" schema="Elemental_topology_arm"/>
      <interface kind="use" schema="Basic_geometry_arm"/>
      <entity name="Edge" supertypes="Detailed_topological_model_element">
         <explicit name="start_vertex">
            <typename name="Vertex"/>
         </explicit>
         <explicit name="end_vertex">
            <typename name="Vertex"/>
         </explicit>
      </entity>
      <entity name="Edge_curve" supertypes="Edge Detailed_geometric_model_element">
         <explicit name="edge_geometry">
            <typename name="Curve"/>
         </explicit>
         <explicit name="same_sense">
            <builtintype type="BOOLEAN"/>
         </explicit>
      </entity>
      <entity name="Vertex" supertypes="Detailed_topological_model_element"/>
      <entity name="Vertex_point" supertypes="Vertex Detailed_geometric_model_element">
         <explicit name="vertex_position">
            <typename name="Cartesian_point"/>
         </explicit>
      </entity>
   </schema>
</express>
