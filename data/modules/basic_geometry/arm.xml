<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1652">
   <application name="JSDAI" owner="LKSoft" source="basic_geometry_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Basic_geometry_arm">
      <interface kind="use" schema="Elemental_geometric_shape_arm"/>
      <entity abstract.entity="NO" abstract.supertype="YES" name="Bounded_curve" supertypes="Curve"/>
      <entity abstract.entity="NO" abstract.supertype="YES" name="Bounded_surface" supertypes="Surface"/>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Circle" supertypes="Conic">
         <explicit name="radius" optional="NO">
            <typename name="positive_length_measure"/>
         </explicit>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Conic" super.expression="ONEOF (Circle, Ellipse, Hyperbola, Parabola)" supertypes="Curve">
         <explicit name="position" optional="NO">
            <typename name="Axis_placement"/>
         </explicit>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Conical_surface" supertypes="Surface">
         <explicit name="position" optional="NO">
            <typename name="Axis_placement_3d"/>
         </explicit>
         <explicit name="radius" optional="NO">
            <typename name="positive_length_measure"/>
         </explicit>
         <explicit name="semi_angle" optional="NO">
            <typename name="plane_angle_measure"/>
         </explicit>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Curve" super.expression="ONEOF (Conic, Line)" supertypes="Detailed_geometric_model_element"/>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Cylindrical_point" supertypes="Cartesian_point">
         <explicit name="r" optional="NO">
            <typename name="positive_length_measure"/>
         </explicit>
         <explicit name="theta" optional="NO">
            <typename name="plane_angle_measure"/>
         </explicit>
         <explicit name="z" optional="NO">
            <typename name="length_measure"/>
         </explicit>
         <derived expression="[r*cos(theta), r*sin(theta), z]" name="coordinates">
            <aggregate lower="3" optional="NO" type="LIST" unique="NO" upper="3"/>
            <typename name="length_measure"/>
            <redeclaration entity-ref="Cartesian_point"/>
         </derived>
      <where label="IP1"/>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Cylindrical_surface" supertypes="Surface">
         <explicit name="position" optional="NO">
            <typename name="Axis_placement_3d"/>
         </explicit>
         <explicit name="radius" optional="NO">
            <typename name="positive_length_measure"/>
         </explicit>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Ellipse" supertypes="Conic">
         <explicit name="first_semi_axis" optional="NO">
            <typename name="positive_length_measure"/>
         </explicit>
         <explicit name="second_semi_axis" optional="NO">
            <typename name="positive_length_measure"/>
         </explicit>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Extruded_surface" supertypes="Swept_surface">
         <explicit name="extrusion_axis" optional="NO">
            <typename name="Direction"/>
         </explicit>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Hyperbola" supertypes="Conic">
         <explicit name="semi_axis" optional="NO">
            <typename name="positive_length_measure"/>
         </explicit>
         <explicit name="imaginary_semi_axis" optional="NO">
            <typename name="positive_length_measure"/>
         </explicit>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Line" supertypes="Curve">
         <explicit name="point" optional="NO">
            <typename name="Cartesian_point"/>
         </explicit>
         <explicit name="line_direction" optional="NO">
            <typename name="Direction"/>
         </explicit>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Parabola" supertypes="Conic">
         <explicit name="focal_distance" optional="NO">
            <typename name="length_measure"/>
         </explicit>
         <where expression="focal_distance &lt;&gt; 0.0" label="WR1"/>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Plane" supertypes="Surface">
         <explicit name="position" optional="NO">
            <typename name="Axis_placement_3d"/>
         </explicit>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Point_on_curve" supertypes="Point">
         <explicit name="supporting_curve" optional="NO">
            <typename name="Curve"/>
         </explicit>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Point_on_surface" supertypes="Point">
         <explicit name="supporting_surface" optional="NO">
            <typename name="Surface"/>
         </explicit>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Polar_point" supertypes="Cartesian_point">
         <explicit name="r" optional="NO">
            <typename name="positive_length_measure"/>
         </explicit>
         <explicit name="theta" optional="NO">
            <typename name="plane_angle_measure"/>
         </explicit>
         <derived expression="[r*cos(theta), r*sin(theta)]" name="coordinates">
            <aggregate lower="2" optional="NO" type="LIST" unique="NO" upper="2"/>
            <typename name="length_measure"/>
            <redeclaration entity-ref="Cartesian_point"/>
         </derived>
      <where label="IP1"/>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Spherical_point" supertypes="Cartesian_point">
         <explicit name="r" optional="NO">
            <typename name="positive_length_measure"/>
         </explicit>
         <explicit name="theta" optional="NO">
            <typename name="plane_angle_measure"/>
         </explicit>
         <explicit name="phi" optional="NO">
            <typename name="plane_angle_measure"/>
         </explicit>
         <derived expression="[r*sin(theta)*cos(phi), r*sin(theta)*sin(phi), r*cos(theta)]" name="coordinates">
            <aggregate lower="3" optional="NO" type="LIST" unique="NO" upper="3"/>
            <typename name="length_measure"/>
            <redeclaration entity-ref="Cartesian_point"/>
         </derived>
      <where label="IP1"/>
         <where label="IP2"/>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Spherical_surface" supertypes="Surface">
         <explicit name="position" optional="NO">
            <typename name="Axis_placement_3d"/>
         </explicit>
         <explicit name="radius" optional="NO">
            <typename name="positive_length_measure"/>
         </explicit>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Surface" super.expression="ONEOF (Conical_surface, Cylindrical_surface, Plane, Spherical_surface, Swept_surface, Toroidal_surface)" supertypes="Detailed_geometric_model_element"/>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Surface_of_revolution" supertypes="Swept_surface">
         <explicit name="axis_direction" optional="NO">
            <typename name="Direction"/>
         </explicit>
         <explicit name="axis_point" optional="NO">
            <typename name="Cartesian_point"/>
         </explicit>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Swept_surface" super.expression="ONEOF (Extruded_surface, Surface_of_revolution)" supertypes="Surface">
         <explicit name="swept_curve" optional="NO">
            <typename name="Curve"/>
         </explicit>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Toroidal_surface" supertypes="Surface">
         <explicit name="position" optional="NO">
            <typename name="Axis_placement_3d"/>
         </explicit>
         <explicit name="radius" optional="NO">
            <typename name="positive_length_measure"/>
         </explicit>
         <explicit name="minor_radius" optional="NO">
            <typename name="positive_length_measure"/>
         </explicit>
      </entity>
      <subtype.constraint abstract.supertype="NO" entity="Detailed_geometric_model_element" name="alternate_geometry_items" super.expression="ONEOF (Direction, Axis_placement, Cartesian_transformation_2d, Cartesian_transformation_3d, Curve, Surface)"/>
      <subtype.constraint abstract.supertype="NO" entity="Point" name="point_subtypes" super.expression="ONEOF (Cartesian_point, Point_on_curve, Point_on_surface)"/>
      <subtype.constraint abstract.supertype="NO" entity="Cartesian_point" name="cartesian_point_subtypes" super.expression="ONEOF (Cylindrical_point, Polar_point, Spherical_point)"/>
   </schema>
</express>
