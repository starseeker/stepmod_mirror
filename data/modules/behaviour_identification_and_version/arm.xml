    source="D:\Users\eurostep limited\CLIENTS\Vivace\ST3.6.3.1\AP233_arm.exp"/>

  <schema name="Behaviour_identification_and_version_arm">
    <interface
      kind="use"
      schema="Alias_identification_arm">
    </interface>
    <interface
      kind="use"
      schema="Product_version_relationship_arm">
    </interface>
    <type name="behaviour_and_version_identification_item">
      <select
        basedon="alias_identification_item"
        selectitems="Behaviour Behaviour_version"
      />
    </type>

    <entity name="Behaviour"
      supertypes="Product"
    >
    </entity>

    <entity name="Behaviour_version"
      supertypes="Product_version"
    >
    </entity>

    <entity name="Behaviour_version_sequence"
      supertypes="Product_version_relationship"
    >
      <explicit
        name="predecessor">
        <typename name="Behaviour_version"/>
        <redeclaration
          entity-ref="Product_version_relationship"
          old_name="relating_version"/>
      </explicit>
      <explicit
        name="succecessor">
        <typename name="Behaviour_version"/>
        <redeclaration
          entity-ref="Product_version_relationship"
          old_name="related_version"/>
      </explicit>
    </entity>
  </schema>

</express>