<ext_description linkend="Behaviour_identification_and_version_mim.behaviour_and_version_identification_item">
</ext_description><!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Behaviour_identification_and_version_mim  Entity: behaviour  
   <express_ref linkend="behaviour_identification_and_version:mim:Behaviour_identification_and_version_mim.behaviour"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Behaviour_identification_and_version_mim.behaviour">
        A <b>behaviour</b> is a type of
    <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product" /> that that collects the definitional information of the versions of a behaviour object.
</ext_description><!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Behaviour_identification_and_version_mim  Entity: behaviour_definition_formation  
   <express_ref linkend="behaviour_identification_and_version:mim:Behaviour_identification_and_version_mim.behaviour_definition_formation"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Behaviour_identification_and_version_mim.behaviour_definition_formation">
        A <b>behaviour_definition_formation</b> is a type of
    <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_formation" /> that that identifies a version of a behaviour object.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Behaviour_identification_and_version_mim  Entity: behaviour_definition_formation  Attribute: of_product 
   <express_ref linkend="behaviour_identification_and_version:mim:Behaviour_identification_and_version_mim.behaviour_definition_formation.of_product"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Behaviour_identification_and_version_mim.behaviour_definition_formation.of_product">
    the <express_ref linkend="behaviour_identification_and_version:mim:Behaviour_identification_and_version_mim.behaviour"/> 
    for which a version is identified by the 
    <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_formation" />.
</ext_description><!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Behaviour_identification_and_version_mim  Entity: behaviour_definition_formation_sequence  
   <express_ref linkend="behaviour_identification_and_version:mim:Behaviour_identification_and_version_mim.behaviour_definition_formation_sequence"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Behaviour_identification_and_version_mim.behaviour_definition_formation_sequence">
    A <b>behaviour_definition_formation_sequence</b> is a type of 
    <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_formation_relationship" /> that is used to assert that one 
    <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_formation" /> 
    (the relating formation) supersedes another 
    <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_formation" /> (the related formation).
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Behaviour_identification_and_version_mim  Entity: behaviour_definition_formation_sequence  Attribute: related_product_definition_formation 
   <express_ref linkend="behaviour_identification_and_version:mim:Behaviour_identification_and_version_mim.behaviour_definition_formation_sequence.related_product_definition_formation"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Behaviour_identification_and_version_mim.behaviour_definition_formation_sequence.related_product_definition_formation">
    the <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_formation" /> that is superseded by the relating formation.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Behaviour_identification_and_version_mim  Entity: behaviour_definition_formation_sequence  Attribute: relating_product_definition_formation 
   <express_ref linkend="behaviour_identification_and_version:mim:Behaviour_identification_and_version_mim.behaviour_definition_formation_sequence.relating_product_definition_formation"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Behaviour_identification_and_version_mim.behaviour_definition_formation_sequence.relating_product_definition_formation">
    the <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_formation" /> that supersedes the related formation.
</ext_description>
