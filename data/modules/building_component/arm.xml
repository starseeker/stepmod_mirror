<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.23 2002/09/24 16:42:56 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.23"
    source="../data/modules/building_components/arm.exp"/>

  <schema name="Building_component_arm">
    <type name="main">
      <builtintype
        type="LOGICAL">
      </builtintype>


    </type>
    <type name="opening_name">
      <builtintype
        type="STRING">
      </builtintype>


    </type>
    <type name="opening_functional_type">
      <enumeration
        items="doorway service_opening window_opening">
      </enumeration>


    </type>
    <type name="opening_type">
      <select
        selectitems="opening_name opening_functional_type">
      </select>

    </type>
    <entity
      name="Building_element_component"
      super.expression="(ONEOF(Positive_component,Negative_component))">
      <explicit
        name="component_functional_type">
        <typename
          name="main"/>
      </explicit>
    </entity>

    <entity
      name="Negative_component"
      super.expression="(ONEOF(Recess,Opening))"
      supertypes="Building_element_component">
    </entity>

    <entity
      name="Opening"
      supertypes="Negative_component">
      <explicit
        name="opening_type">
        <typename
          name="opening_type"/>
      </explicit>
    </entity>

    <entity
      name="Positive_component"
      supertypes="Building_element_component">
    </entity>

    <entity
      name="Recess"
      supertypes="Negative_component">
    </entity>

  </schema>

</express>
