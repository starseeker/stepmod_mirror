<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: Building_item_arm
   <express_ref linkend="building_item:arm:Building_item_arm"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Building_item_arm">
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Enumeration: fixture_equipment_functional_type 
   <express_ref linkend="building_item:arm:Building_item_arm.fixture_equipment_functional_type"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.fixture_equipment_functional_type">A <b>fixture_equipment_functional_type</b> specifies a descriptive label that indicates the function of the <express_ref
      linkend="building_item:arm:Building_item_arm.Fixture_equipment_element"/>, using predefined values. 
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Enumeration: fixture_equipment_functional_type  Item: ceiling 
   <express_ref linkend="building_item:arm:Building_item_arm.fixture_equipment_functional_type.ceiling"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.fixture_equipment_functional_type.ceiling">the upper interior surface of a room.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Enumeration: fixture_equipment_functional_type  Item: covering_element 
   <express_ref linkend="building_item:arm:Building_item_arm.fixture_equipment_functional_type.covering_element"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.fixture_equipment_functional_type.covering_element">material that is applied to a surface of a <express_ref
      linkend="building_item:arm:Building_item_arm.Building_item"/> to
protect, insulate, or decorate.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Enumeration: fixture_equipment_functional_type  Item: door 
   <express_ref linkend="building_item:arm:Building_item_arm.fixture_equipment_functional_type.door"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.fixture_equipment_functional_type.door">a movable element that is used to seal an accessway.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Enumeration: fixture_equipment_functional_type  Item: floor_covering 
   <express_ref linkend="building_item:arm:Building_item_arm.fixture_equipment_functional_type.floor_covering"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.fixture_equipment_functional_type.floor_covering">a covering element that is applied to an lower interior surface of a room.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Enumeration: fixture_equipment_functional_type  Item: furniture 
   <express_ref linkend="building_item:arm:Building_item_arm.fixture_equipment_functional_type.furniture"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.fixture_equipment_functional_type.furniture">movable articles used in living or working activities that are not implements of
production.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Enumeration: fixture_equipment_functional_type  Item: wall_covering 
   <express_ref linkend="building_item:arm:Building_item_arm.fixture_equipment_functional_type.wall_covering"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.fixture_equipment_functional_type.wall_covering">a covering element that is applied to a surface of a wall.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Enumeration: fixture_equipment_functional_type  Item: window 
   <express_ref linkend="building_item:arm:Building_item_arm.fixture_equipment_functional_type.window"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.fixture_equipment_functional_type.window">a framed structure which is spanned with glass and mounted in a window opening;
it may be mounted to permit opening and closing.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Type: fixture_equipment_type 
   <express_ref linkend="building_item:arm:Building_item_arm.fixture_equipment_type"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.fixture_equipment_type">A <express_ref
      linkend="building_item:arm:Building_item_arm.fixture_equipment_functional_type"/> specifies a descriptive label that indicates the function of the <express_ref
      linkend="building_item:arm:Building_item_arm.Fixture_equipment_element"/>. The value is either user defined or predefined.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Type: fixture_equipment_type_name 
   <express_ref linkend="building_item:arm:Building_item_arm.fixture_equipment_type_name"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.fixture_equipment_type_name">A <b>fixture_equipment_type_name</b> specifies a descriptive label that indicates the function of the <express_ref
      linkend="building_item:arm:Building_item_arm.Fixture_equipment_element"/>. The value is user defined.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Enumeration: service_functional_type 
   <express_ref linkend="building_item:arm:Building_item_arm.service_functional_type"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.service_functional_type">The <b>service_functional_type</b> specifies a descriptive label that indicates the function of the element using a predefined value.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Enumeration: service_functional_type  Item: electrical_system 
   <express_ref linkend="building_item:arm:Building_item_arm.service_functional_type.electrical_system"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.service_functional_type.electrical_system">a network of electrical equipment and conducting wire that provides power
or communication throughout a building complex.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Enumeration: service_functional_type  Item: HVAC_system 
   <express_ref linkend="building_item:arm:Building_item_arm.service_functional_type.HVAC_system"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.service_functional_type.HVAC_system">a network of equipment and ducting that provides ventilation and heated or
cooled air throughout a building complex.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Enumeration: service_functional_type  Item: plumbing_system 
   <express_ref linkend="building_item:arm:Building_item_arm.service_functional_type.plumbing_system"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.service_functional_type.plumbing_system">a network of pipes, fixtures, and other apparatus that provide water, gas,
or sewage removal services throughout a building complex.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Enumeration: service_functional_type  Item: transport_system 
   <express_ref linkend="building_item:arm:Building_item_arm.service_functional_type.transport_system"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.service_functional_type.transport_system">mechanical apparatuses that moves articles or people from one point in
a building complex to another.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Type: service_type 
   <express_ref linkend="building_item:arm:Building_item_arm.service_type"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.service_type">The <b>service_type</b> specifies a descriptive label that indicates the function of the element. The value
is either user defined or predefined.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Type: service_type_name 
   <express_ref linkend="building_item:arm:Building_item_arm.service_type_name"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.service_type_name">A <b>service_type_name</b> specifies a descriptive label that indicates the function of the <express_ref
      linkend="building_item:arm:Building_item_arm.Service_element"/>. The value is user defined.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Enumeration: structure_enclosure_functional_type 
   <express_ref linkend="building_item:arm:Building_item_arm.structure_enclosure_functional_type"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.structure_enclosure_functional_type">The <b>structure_enclosure_functional_type</b> specifies a descriptive label that indicates the function of a <express_ref
      linkend="building_item:arm:Building_item_arm.Structure_enclosure_element"/> using a predefined value.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Enumeration: structure_enclosure_functional_type  Item: beam 
   <express_ref linkend="building_item:arm:Building_item_arm.structure_enclosure_functional_type.beam"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.structure_enclosure_functional_type.beam">a long structural element that bears loads perpendicular to its primary dimension,
and is supported at different points along that dimension.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Enumeration: structure_enclosure_functional_type  Item: brace 
   <express_ref linkend="building_item:arm:Building_item_arm.structure_enclosure_functional_type.brace"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.structure_enclosure_functional_type.brace">a long structural element that bears compressive or tensile loads parallel to its
primary dimension.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Enumeration: structure_enclosure_functional_type  Item: column 
   <express_ref linkend="building_item:arm:Building_item_arm.structure_enclosure_functional_type.column"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.structure_enclosure_functional_type.column">a tall, upright structural element that bears loads primarily parallel to its primary
dimension.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Enumeration: structure_enclosure_functional_type  Item: floor 
   <express_ref linkend="building_item:arm:Building_item_arm.structure_enclosure_functional_type.floor"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.structure_enclosure_functional_type.floor">a flat, horizontal plate that bears loads primarily perpendicular to its surface. Floors
directly bear the life loads of a building. A single floor may consist of several distinct sections.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Enumeration: structure_enclosure_functional_type  Item: foundation 
   <express_ref linkend="building_item:arm:Building_item_arm.structure_enclosure_functional_type.foundation"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.structure_enclosure_functional_type.foundation">the base structural element of a building upon which the building is
constructed. It is usually built below ground and includes footings, cellar walls, and isolated column
footings.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Enumeration: structure_enclosure_functional_type  Item: structural_wire 
   <express_ref linkend="building_item:arm:Building_item_arm.structure_enclosure_functional_type.structural_wire"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.structure_enclosure_functional_type.structural_wire">a long, commonly cylindrical, structural element that usually bears tensile
structural loads along its axis.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Enumeration: structure_enclosure_functional_type  Item: wall 
   <express_ref linkend="building_item:arm:Building_item_arm.structure_enclosure_functional_type.wall"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.structure_enclosure_functional_type.wall">a continuous, flat, usually vertical structural element that bears loads primarily
parallel to its surface and encloses, divides, or bounds a space.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Type: structure_enclosure_type 
   <express_ref linkend="building_item:arm:Building_item_arm.structure_enclosure_type"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.structure_enclosure_type">The <b>structure_enclosure_type</b> specifies a descriptive label that indicates the function of a <express_ref
      linkend="building_item:arm:Building_item_arm.Structure_enclosure_element"/>. The value
is either user defined or predefined.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Type: structure_enclosure_type_name 
   <express_ref linkend="building_item:arm:Building_item_arm.structure_enclosure_type_name"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.structure_enclosure_type_name">A <b>structure_enclosure_type_name</b> specifies a descriptive label that indicates the function of a <express_ref
      linkend="building_item:arm:Building_item_arm.Structure_enclosure_element"/>. The value is user defined.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Entity: Building_element  
   <express_ref linkend="building_item:arm:Building_item_arm.Building_element"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.Building_element">A <b>Building_element</b> is a type of <express_ref
      linkend="building_item:arm:Building_item_arm.Building_item"/> that is a physical part of a building or
building system that has a characteristic function. A <b>Building_element</b> may be one of the following:
a <express_ref
      linkend="building_item:arm:Building_item_arm.Structure_enclosure_element"/>, a <express_ref
      linkend="building_item:arm:Building_item_arm.Service_element"/>, or a <express_ref
      linkend="building_item:arm:Building_item_arm.Fixture_equipment_element"/>.
Each <b>Building_element</b> shall be positioned within the coordinated space of one <express_ref
      linkend="building_structure:arm:Building_structure_arm.Building_section"/> or a <express_ref
      linkend="building_item:arm:Building_item_arm.Space"/>.
Each <b>Building_element</b> shall have exactly one <express_ref
      linkend="building_component:arm:Building_component_arm.Positive_component"/> as its main component.			
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Entity: Building_item  
   <express_ref linkend="building_item:arm:Building_item_arm.Building_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.Building_item">A <b>Building_item</b> is a type of <express_ref
      linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/>. It defines a part of a building that has a characteristic function. A <b>Building_item</b> is either
a <express_ref linkend="building_item:arm:Building_item_arm.Building_element"/> or a <express_ref linkend="building_item:arm:Building_item_arm.Space"/>.
<note>Life cycle status information information can be provided by 
<express_ref
      linkend="product_view_definition:arm:Product_view_definition_arm.View_definition_context"/>, that is associated with the <b>Building_item</b>.</note>
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Entity: Fixture_equipment_element  
   <express_ref linkend="building_item:arm:Building_item_arm.Fixture_equipment_element"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.Fixture_equipment_element">A <b>Fixture_equipment_element</b> is a type of <express_ref
      linkend="building_item:arm:Building_item_arm.Building_element"/> that is a solid, tangible part
of a building placed or installed in the building. It is a standalone element that provides a service
without complex interdependence on other building elements.
<example>Doors, furniture, window shades, hand railing, fireplace mantlepiece, installed shelving.</example>
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Entity: Fixture_equipment_element  Attribute: functional_type 
   <express_ref linkend="building_item:arm:Building_item_arm.Fixture_equipment_element.functional_type"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.Fixture_equipment_element.functional_type">a descriptive label that indicates the function of the element. The value
is either user defined or predefined.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Entity: Service_element  
   <express_ref linkend="building_item:arm:Building_item_arm.Service_element"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.Service_element">A <b>Service_element</b> is a type of <express_ref
      linkend="building_item:arm:Building_item_arm.Building_element"/>. A <b>Service_element</b> is a solid, tangible
part of a building that is a component in one or more systems installed in a building which provide
a service to the building. <example>Building services include HVAC, plumbing, and electrical systems. A <b>Service_element</b>
may therefore be a duct or pipe, a collection of ducts or pipes, or a piece of equipment like a compressor
or pump.</example>
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Entity: Service_element  Attribute: functional_type 
   <express_ref linkend="building_item:arm:Building_item_arm.Service_element.functional_type"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.Service_element.functional_type">a descriptive label that indicates the function of the element. The value
is either user defined or predefined.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Entity: Space  
   <express_ref linkend="building_item:arm:Building_item_arm.Space"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.Space">A <b>Space</b> is a type of <express_ref
      linkend="building_item:arm:Building_item_arm.Building_item"/>. A <b>Space</b> is an intangible, volumetric region of a
building that represents the portions of a building usable by the building occupants or building service
equipment. Spaces are used for planning, describing, and specifying the use of a building. Spaces are
created in buildings by the erection of the surrounding walls, floors, and ceilings.
Each <b>Space</b> shall be positioned within the coordinated space of one <express_ref
      linkend="building_structure:arm:Building_structure_arm.Building_section"/>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Entity: Structure_enclosure_element  
   <express_ref linkend="building_item:arm:Building_item_arm.Structure_enclosure_element"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.Structure_enclosure_element">A <b>Structure_enclosure_element</b> is a type of <express_ref
      linkend="building_item:arm:Building_item_arm.Building_element"/> that is an identifiable part
of a building that contributes to the basic form or function of a building.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Entity: Structure_enclosure_element  Attribute: functional_type 
   <express_ref linkend="building_item:arm:Building_item_arm.Structure_enclosure_element.functional_type"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.Structure_enclosure_element.functional_type">a descriptive label that indicates the form or function of the element. The value is either user defined or predefined.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Building_item_arm  Entity: Structure_enclosure_element  Attribute: load_bearing 
   <express_ref linkend="building_item:arm:Building_item_arm.Structure_enclosure_element.load_bearing"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Building_item_arm.Structure_enclosure_element.load_bearing">a logical data element that specifies whether the <b>Structure_enclosure_element</b> bears loads of the building.
</ext_description>
</ext_descriptions>
