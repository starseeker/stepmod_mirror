<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.24 2002/10/21 13:28:40 goset1 Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.24"
    source="../data/modules/building_structure/arm.exp"/>

  <schema name="Building_structure_arm">
    <interface
      kind="use"
      schema="Approval_arm">
    </interface>

    <interface
      kind="use"
      schema="Building_item_arm">
    </interface>

    <interface
      kind="use"
      schema="Person_organization_assignment_arm">
    </interface>

    <interface
      kind="use"
      schema="Product_view_definition_arm">
    </interface>

    <interface
      kind="use"
      schema="Product_view_definition_relationship_arm">
    </interface>

    <type name="assembly_functional_type">
      <enumeration
        items="roof stairway vertical_passage_enclosure">
      </enumeration>


    </type>
    <type name="assembly_item">
      <select
        selectitems="Item_assembly Building_item">
      </select>

    </type>
    <type name="assembly_type">
      <select
        selectitems="assembly_functional_type assembly_type_name">
      </select>

    </type>
    <type name="assembly_type_name">
      <builtintype
        type="STRING">
      </builtintype>


    </type>
    <type name="building_organization_or_person_in_organization_item">
      <select
        basedon="organization_or_person_in_organization_item"
        selectitems="Building">
      </select>

    </type>
    <type name="relationship_functional_type">
      <enumeration
        items="space_to_element penetration touch">
      </enumeration>


    </type>
    <type name="relationship_type">
      <select
        selectitems="relationship_functional_type relationship_type_name">
      </select>

    </type>
    <type name="relationship_type_name">
      <builtintype
        type="STRING">
      </builtintype>


    </type>
    <entity
      name="Building"
      supertypes="Product_view_definition">
      <explicit
        name="status">
        <typename
          name="Approval_status"/>
      </explicit>
    </entity>

    <entity
      name="Building_level"
      super.expression="(Sublevel)"
      supertypes="Product_view_definition">
    </entity>

    <entity
      name="Building_level_assignment"
      supertypes="View_definition_relationship">
      <explicit
        name="related_view RENAMED assigned_item">
        <redeclaration
          entity-ref="View_definition_relationship"/>
          <typename
            name="Building_item"/>
        </explicit>
        <explicit
          name="relating_view RENAMED assigned_level">
          <redeclaration
            entity-ref="View_definition_relationship"/>
            <typename
              name="Building_level"/>
          </explicit>
        </entity>

        <entity
          name="Building_organization_or_person_in_organization_assignment"
          supertypes="organization_or_person_in_organization_assignment">
          <explicit
            name="items">
            <redeclaration
              entity-ref="organization_or_person_in_organization_assignment"/>
              <typename
                name="building_organization_or_person_in_organization_item"/>
            </explicit>
          </entity>

          <entity
            name="Building_section"
            supertypes="Product_view_definition">
            <explicit
              name="status">
              <typename
                name="Approval_status"/>
            </explicit>
          </entity>

          <entity
            name="Item_assembly"
            supertypes="Product_view_definition">
            <explicit
              name="assembly_type">
              <typename
                name="assembly_type"/>
            </explicit>
            <explicit
              name="components">
              <aggregate
                type="SET"
                lower="1"
                upper="?"/>
              <typename
                name="assembly_item"/>
            </explicit>
          </entity>

          <entity
            name="Item_proximity_relationship"
            supertypes="View_definition_relationship">
            <explicit
              name="related_view RENAMED item">
              <redeclaration
                entity-ref="View_definition_relationship"/>
                <typename
                  name="Building_item"/>
              </explicit>
              <explicit
                name="relating_view RENAMED item_in_proximity">
                <redeclaration
                  entity-ref="View_definition_relationship"/>
                  <typename
                    name="Building_item"/>
                </explicit>
                <explicit
                  name="relation_type RENAMED relationship_type">
                  <redeclaration
                    entity-ref="View_definition_relationship"/>
                    <typename
                      name="relationship_type"/>
                  </explicit>
                </entity>

                <entity
                  name="Sublevel"
                  supertypes="Building_level">
                  <explicit
                    name="belongs_to">
                    <typename
                      name="Building_level"/>
                  </explicit>
                </entity>

              </schema>

            </express>
