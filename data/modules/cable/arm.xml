<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1653">
   <application name="JSDAI" owner="LKSoft" source="cable_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Cable_arm">
      <interface kind="use" schema="Assembly_component_arm"/>
      <interface kind="use" schema="Component_feature_arm"/>
      <interface kind="use" schema="Design_product_data_management_arm"/>
      <interface kind="use" schema="Functional_assignment_to_part_arm"/>
      <type name="cc_part_category">
         <enumeration basedon="part_category" items="wire cable"/>
      </type>
      <entity name="Cable_component" supertypes="Assembly_component Product_occurrence_with_quantity">
         <explicit name="derived_from">
            <typename name="Cable_usage_view"/>
            <redeclaration entity-ref="Definition_based_product_occurrence"/>
         </explicit>
         <derived expression="QUERY(lc &lt;* terminal_location_groups | lc.name = 'end a')" name="ends_a">
            <aggregate lower="1" type="SET" upper="1"/>
            <typename name="Cable_component_terminal_location_group_identification"/>
         </derived>
         <derived expression="QUERY(lc &lt;* terminal_location_groups | lc.name = 'end b')" name="ends_b">
            <aggregate lower="1" type="SET" upper="1"/>
            <typename name="Cable_component_terminal_location_group_identification"/>
         </derived>
         <derived expression="QUERY(lc &lt;* terminal_location_groups | lc.name[1:13] = 'intermediate')" name="intermediates">
            <aggregate lower="0" type="SET" upper="?"/>
            <typename name="Cable_component_terminal_location_group_identification"/>
         </derived>
         <inverse attribute="associated_definition" entity="Cable_component_terminal_location_group_identification" name="terminal_location_groups">
            <inverse.aggregate lower="0" type="SET" upper="?"/>
         </inverse>
         <where expression="EXISTS(ends_a)" label="WR1"/>
         <where expression="EXISTS(ends_b)" label="WR2"/>
         <where expression="SIZEOF(ends_a[1]\Cable_component_terminal_location_group_identification.terminals) = SIZEOF(ends_b[1]\Cable_component_terminal_location_group_identification.terminals)" label="WR3"/>
         <where expression="raw_material_by_length IN SELF\Definition_based_product_occurrence.derived_from.defined_version.of_product\Part_with_additional_categories.categories" label="WR4"/>
         <where expression="cable IN SELF\Definition_based_product_occurrence.derived_from.defined_version.of_product\Part_with_additional_categories.categories" label="WR5"/>
      </entity>
      <entity name="Cable_component_terminal" supertypes="Derived_component_terminal">
         <explicit name="location_group">
            <typename name="Cable_component_terminal_location_group_identification"/>
            <redeclaration entity-ref="Component_feature"/>
         </explicit>
         <explicit name="derived_from">
            <typename name="Wire_identification"/>
            <redeclaration entity-ref="Derived_component_terminal"/>
         </explicit>
         <derived expression="SELF\Component_feature.location_group.associated_definition" name="associated_definition">
            <typename name="Cable_component"/>
            <redeclaration entity-ref="Component_feature"/>
         </derived>
      </entity>
      <entity name="Cable_component_terminal_location_group_identification" supertypes="Component_feature_location_group_identification">
         <explicit name="associated_definition">
            <typename name="Cable_component"/>
            <redeclaration entity-ref="Component_feature"/>
         </explicit>
         <inverse attribute="location_group" entity="Cable_component_terminal" name="terminals">
            <inverse.aggregate lower="1" type="SET" upper="?"/>
         </inverse>
      </entity>
      <entity name="Cable_usage_view" supertypes="Part_usage_view">
         <inverse attribute="associated_definition" entity="Wire_identification" name="conductors">
            <inverse.aggregate lower="1" type="SET" upper="?"/>
         </inverse>
      </entity>
      <entity name="Wire_color_based_identification_code" supertypes="Independent_property"/>
      <entity name="Wire_component" supertypes="Assembly_component Product_occurrence_with_quantity">
         <derived expression="QUERY(t &lt;* terminals | t.name = 'end a')" name="ends_a">
            <aggregate lower="1" type="SET" upper="1"/>
            <typename name="Wire_component_terminal"/>
         </derived>
         <derived expression="QUERY(t &lt;* terminals | t.name = 'end b')" name="ends_b">
            <aggregate lower="1" type="SET" upper="1"/>
            <typename name="Wire_component_terminal"/>
         </derived>
         <derived expression="QUERY(t &lt;* terminals | t.name[1:13] = 'intermediate ')" name="intermediates">
            <aggregate lower="0" type="SET" upper="?"/>
            <typename name="Wire_component_terminal"/>
         </derived>
         <inverse attribute="associated_definition" entity="Wire_component_terminal" name="terminals">
            <inverse.aggregate lower="2" type="SET" upper="?"/>
         </inverse>
         <where expression="SIZEOF(ends_a) = 1" label="WR1"/>
         <where expression="SIZEOF(ends_b) = 1" label="WR2"/>
         <where expression="SIZEOF(QUERY(ct &lt;* USEDIN(SELF, 'COMPONENT_FEATURE_ARM.COMPONET_TERMINAL.ASSOCIATED_DEFINITION') | &#10;             NOT ('COMPONENT_FEATURE_ARM.WIRE_COMPONENT_TERMINAL' IN TYPEOF(ct)) )) = 0" label="WR3"/>
         <where expression="raw_material_by_length IN SELF\Definition_based_product_occurrence.derived_from.defined_version.of_product\Part_with_additional_categories.categories" label="WR4"/>
         <where expression="wire IN SELF\Definition_based_product_occurrence.derived_from.defined_version.of_product\Part_with_additional_categories.categories" label="WR5"/>
      </entity>
      <entity name="Wire_component_terminal" supertypes="Derived_component_terminal">
         <explicit name="associated_definition">
            <typename name="Wire_component"/>
            <redeclaration entity-ref="Component_feature"/>
         </explicit>
         <explicit name="derived_from">
            <typename name="Wire_identification"/>
            <redeclaration entity-ref="Derived_component_terminal"/>
         </explicit>
         <where expression="NOT EXISTS(SELF\Component_feature.location_group)" label="WR1"/>
      </entity>
      <entity name="Wire_identification" supertypes="General_part_feature">
         <explicit name="code">
            <typename name="Wire_color_based_identification_code"/>
         </explicit>
         <explicit name="conductivity_type" optional="YES">
            <typename name="fatp_part_connected_terminals_definition_domain_enumeration"/>
         </explicit>
      </entity>
      <subtype.constraint entity="Assembly_component" name="wacc_assembly_component_subtypes" super.expression="ONEOF (Wire_component, Cable_component)"/>
      <subtype.constraint entity="Derived_component_terminal" name="wacc_derived_component_terminal_subtypes" super.expression="ONEOF (Cable_component_terminal, Wire_component_terminal)"/>
   </schema>
</express>
