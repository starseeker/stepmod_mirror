<!DOCTYPE ext_descriptions SYSTEM "../../../dtd/description.dtd">
<ext_descriptions module_directory="cable" schema_file="arm.xml" describe.selects="YES" describe.subtype_constraints="YES" rcs.date="$Date: 2016/07/14 09:20:03 $" rcs.revision="$Revision: 1.22 $">
<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: Cable_arm
   <express_ref linkend="cable:arm:Cable_arm"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Cable_arm">
</ext_description>

<ext_description linkend="Cable_arm.cc_part_category">
	The <b>cc_part_category</b> provides pre-defined categories for 
	<express_ref linkend="design_product_data_management:arm:Design_product_data_management_arm.Part_with_additional_categories"/>.
</ext_description>
<ext_description linkend="Cable_arm.cc_part_category.wire">
	the part category for a <i>wire</i>;
</ext_description>
<ext_description linkend="Cable_arm.cc_part_category.cable">
	the part category for a <i>cable</i>;
</ext_description>
<ext_description linkend="Cable_arm.cc_part_category.flat_cable">
	the part category for a <i>flat (multicore) cable</i>.
</ext_description>


<!-- +++++++++++ Cable_component +++++++++++ -->
<ext_description linkend="Cable_arm.Cable_component">
	A <b>Cable_component</b> is a type of 
	<express_ref linkend="assembly_component:arm:Assembly_component_arm.Assembly_component"/>
	and of
	<express_ref linkend="product_occurrence:arm:Product_occurrence_arm.Product_occurrence_with_quantity"/>.
	A <b>Cable_component</b> is an occurrence of a 
	<express_ref linkend="cable:arm:Cable_arm.Cable_usage_view"/>
	of a particular length.
</ext_description>
<ext_description linkend="Cable_arm.Cable_component.derived_from">
	specifies the  
	<express_ref linkend="Cable:arm:Cable_arm.Cable_usage_view"/> 
	from which the <b>Cable_component</b> is derived from.
</ext_description>
<ext_description linkend="Cable_arm.Cable_component.ends_a">
	the
	<express_ref linkend="cable:arm:Cable_arm.Cable_component_terminal_location_group_identification"/>
	with the name 'end a'.
</ext_description>
<ext_description linkend="Cable_arm.Cable_component.ends_b">
	the 
	<express_ref linkend="cable:arm:Cable_arm.Cable_component_terminal_location_group_identification"/>
	with the name 'end b'.
</ext_description>
<ext_description linkend="Cable_arm.Cable_component.intermediates">
	the set of
	<express_ref linkend="cable:arm:Cable_arm.Cable_component_terminal_location_group_identification"/>s
	with the name 'intermediate'.
</ext_description>
<ext_description linkend="Cable_arm.Cable_component.terminal_location_groups">
	the set of all
	<express_ref linkend="cable:arm:Cable_arm.Cable_component_terminal_location_group_identification"/>s
	with the name 'intermediate'.
</ext_description>
<ext_description linkend="Cable_arm.Cable_component.wr:WR1">
	a <b>Cable_component</b> shall have exactly one
	<express_ref linkend="cable:arm:Cable_arm.Cable_component_terminal_location_group_identification"/>
	with the name 'end a'.
</ext_description>
<ext_description linkend="Cable_arm.Cable_component.wr:WR2">
	a <b>Cable_component</b> shall have exactly one
	<express_ref linkend="cable:arm:Cable_arm.Cable_component_terminal_location_group_identification"/>
	with the name 'end b'.
</ext_description>
<ext_description linkend="Cable_arm.Cable_component.wr:WR3">
	the
	<express_ref linkend="cable:arm:Cable_arm.Cable_component_terminal_location_group_identification"/>
	with the name 'end a' shall have the same number of 
	<express_ref linkend="cable:arm:Cable_arm.Cable_component_terminal"/>s
	as the
	<express_ref linkend="cable:arm:Cable_arm.Cable_component_terminal_location_group_identification"/>
	with the name 'end b'. 
</ext_description>
<ext_description linkend="Cable_arm.Cable_component.wr:WR4">
	a <b>Cable_component</b> shall belong to a 
	<express_ref linkend="part_and_version_identification:arm:Part_and_version_identification_arm.Part"/>
	that is a
	<express_ref linkend="design_product_data_management:arm:Design_product_data_management_arm.Part_with_additional_categories"/>
	and that contains the category
	<express_ref linkend="design_product_data_management:arm:Design_product_data_management_arm.part_category.raw_material_by_length"/>.
</ext_description>
<ext_description linkend="Cable_arm.Cable_component.wr:WR5">
	a <b>Cable_component</b> shall belong to a 
	<express_ref linkend="part_and_version_identification:arm:Part_and_version_identification_arm.Part"/>
	that is a
	<express_ref linkend="design_product_data_management:arm:Design_product_data_management_arm.Part_with_additional_categories"/>
	and that contains the category
	<express_ref linkend="cable:arm:Cable_arm.cc_part_category.cable"/>.
</ext_description>


<ext_description linkend="Cable_arm.Cable_component_terminal">
	A <b>Cable_component_terminal</b> is a type of 
	<express_ref linkend="component_feature:arm:Component_feature_arm.Derived_component_terminal"/>
	for the identification of a terminal on a 
	<express_ref linkend="cable:arm:Cable_arm.Cable_component"/>.
	The terminal may be located at one of the ends or somewhere in the middle of a wire in the cable. 
</ext_description>
<ext_description linkend="Cable_arm.Cable_component_terminal.associated_definition">
	an inherited attribute that shall have a value of type
	<express_ref linkend="cable:arm:Cable_arm.Cable_component"/>.
</ext_description>
<ext_description linkend="Cable_arm.Cable_component_terminal.derived_from">
	an inherited attribute that shall have a value of type
	<express_ref linkend="cable:arm:Cable_arm.Wire_identification"/>.
</ext_description>
<ext_description linkend="Cable_arm.Cable_component_terminal.location_group">
	an inverse attribute that specifies the
	<express_ref linkend="cable:arm:Cable_arm.Cable_component_terminal_location_group_identification"/>
	to which the <b>Cable_component_terminal</b> belongs.
</ext_description>
<ext_description linkend="Cable_arm.Cable_component_terminal.wr:WR1">
	The <b>Cable_component_terminal</b> shall belong to a
	<express_ref linkend="cable:arm:Cable_arm.Cable_component_terminal_location_group_identification"/>.
</ext_description>

<ext_description linkend="Cable_arm.Cable_component_terminal_location_group_identification">
	A <b>Cable_component_terminal_location_group_identification</b> is a type of 
	<express_ref linkend="component_feature:arm:Component_feature_arm.Component_feature_group_identification"/>
	that represents the grouping of 
	<express_ref linkend="cable:arm:Cable_arm.Cable_component_terminal"/>s
	on one of the ends or at intermediate positions between the ends.
</ext_description>
<ext_description linkend="Cable_arm.Cable_component_terminal_location_group_identification.associated_definition">
	an inherited attribute that shall have a value of type
	<express_ref linkend="cable:arm:Cable_arm.Cable_component"/>.
</ext_description>
<ext_description linkend="Cable_arm.Cable_component_terminal_location_group_identification.elements">
	the set of 
	<express_ref linkend="cable:arm:Cable_arm.Cable_component_terminal"/>s
	at this <b>Cable_component_terminal_location_group_identification</b>.
</ext_description>


<ext_description linkend="Cable_arm.Cable_usage_view">
	A <b>Cable_usage_view</b> is a type of 
	<express_ref linkend="physical_unit_usage_view:arm:Physical_unit_usage_view_arm.Part_usage_view"/>
	for a single <i>wire</i> or a <i>multiconductor cable</i> that provides the needed
	<express_ref linkend="cable:arm:Cable_arm.Wire_identification"/>s
	to be able to distinguish the wires.
</ext_description>
<ext_description linkend="Cable_arm.Cable_usage_view.conductors">
	the set of available
	<express_ref linkend="cable:arm:Cable_arm.Wire_identification"/>s.
	There shall be at least one
	<express_ref linkend="cable:arm:Cable_arm.Wire_identification"/>.
</ext_description>


<ext_description linkend="Cable_arm.Wire_color_based_identification_code">
	A <b>Wire_color_based_identification_code</b> is a type of
	<express_ref linkend="independent_property:arm:Independent_property_arm.Independent_property"/>
	that identifies a particular color code for a wire.
</ext_description>


<ext_description linkend="Cable_arm.Wire_component">
	A <b>Wire_component</b> is a type of 
	<express_ref linkend="assembly_component:arm:Assembly_component_arm.Assembly_component"/>
	and of
	<express_ref linkend="product_occurrence:arm:Product_occurrence_arm.Product_occurrence_with_quantity"/>
	for the identification of the occurrence of a <i>wire</i> of a particular length in an assembly. 
</ext_description>
<ext_description linkend="Cable_arm.Wire_component.derived_from">
	specifies the  
	<express_ref linkend="Cable:arm:Cable_arm.Cable_usage_view"/> 
	from which the <b>Wire_component</b> is derived from.
</ext_description>
<ext_description linkend="Cable_arm.Wire_component.ends_a">
	the single
	<express_ref linkend="cable:arm:Cable_arm.Wire_component_terminal"/>
	on one end of the <b>Wire_component</b>.
</ext_description>
<ext_description linkend="Cable_arm.Wire_component.ends_b">
	the single
	<express_ref linkend="cable:arm:Cable_arm.Wire_component_terminal"/>
	on the other end of the <b>Wire_component</b>.
</ext_description>
<ext_description linkend="Cable_arm.Wire_component.intermediates">
	the zero, one or more
	<express_ref linkend="cable:arm:Cable_arm.Wire_component_terminal"/>s
	at intermediate positions between the ends of the <b>Wire_component</b>.
</ext_description>
<ext_description linkend="Cable_arm.Wire_component.terminals">
	the set of all
	<express_ref linkend="cable:arm:Cable_arm.Wire_component_terminal"/>s
	of the <b>Wire_component</b>.
</ext_description>
<ext_description linkend="Cable_arm.Wire_component.wr:WR1">
	there shall be one
	<express_ref linkend="cable:arm:Cable_arm.Wire_component_terminal"/>
	on one end of the <b>Wire_component</b>.
</ext_description>
<ext_description linkend="Cable_arm.Wire_component.wr:WR2">
	there shall be one
	<express_ref linkend="cable:arm:Cable_arm.Wire_component_terminal"/>
	on the other end of the <b>Wire_component</b>.
</ext_description>
<ext_description linkend="Cable_arm.Wire_component.wr:WR3">
	all 
	<express_ref linkend="component_feature:arm:Component_feature_arm.Component_terminal"/>
	for a <b>Wire_component</b> shall be of type
	<express_ref linkend="cable:arm:Cable_arm.Wire_component_terminal"/>.
</ext_description>
<ext_description linkend="Cable_arm.Wire_component.wr:WR4">
	a <b>Wire_component</b> shall belong to a 
	<express_ref linkend="part_and_version_identification:arm:Part_and_version_identification_arm.Part"/>
	that is a
	<express_ref linkend="design_product_data_management:arm:Design_product_data_management_arm.Part_with_additional_categories"/>
	and that contains the category
	<express_ref linkend="design_product_data_management:arm:Design_product_data_management_arm.part_category.raw_material_by_length"/>.
</ext_description>
<ext_description linkend="Cable_arm.Wire_component.wr:WR5">
	a <b>Wire_component</b> shall belong to a 
	<express_ref linkend="part_and_version_identification:arm:Part_and_version_identification_arm.Part"/>
	that is a
	<express_ref linkend="design_product_data_management:arm:Design_product_data_management_arm.Part_with_additional_categories"/>
	and that contains the category
	<express_ref linkend="cable:arm:Cable_arm.cc_part_category.wire"/>.
</ext_description>


<ext_description linkend="Cable_arm.Wire_component_terminal">
	A <b>Wire_component_terminal</b> is a type of 
	<express_ref linkend="component_feature:arm:Component_feature_arm.Derived_component_terminal"/>
	for the identification of a terminal on a 
	<express_ref linkend="cable:arm:Cable_arm.Wire_component"/>.
	The terminal may be located at one of the ends or somewhere in the middle of the wire. 
</ext_description>
<ext_description linkend="Cable_arm.Wire_component_terminal.associated_definition">
	an inherited attribute that shall have a value of type
	<express_ref linkend="cable:arm:Cable_arm.Wire_component"/>.
</ext_description>
<ext_description linkend="Cable_arm.Wire_component_terminal.derived_from">
	an inherited attribute that shall have a value of type
	<express_ref linkend="cable:arm:Cable_arm.Wire_identification"/>.
</ext_description>
<ext_description linkend="Cable_arm.Wire_component_terminal.wr:WR1">
	the inherited attribute
	<express_ref linkend="component_feature:arm:Component_feature_arm.Component_feature.location_group"/>
	shall not be populated.
</ext_description>


<ext_description linkend="Cable_arm.Wire_identification">
	A <b>Wire_identification</b> is a type of 
	<express_ref linkend="part_shape:arm:Part_shape_arm.General_part_feature"/>
	that represents the identification information of a wire, e.g. by a specific colour or colour pattern. 
</ext_description>
<ext_description linkend="Cable_arm.Wire_identification.code">
	the
	<express_ref linkend="cable:arm:Cable_arm.Wire_color_based_identification_code"/>
	that provides the colour or colour pattern identification for the <b>Wire_identification</b>.
</ext_description>
<ext_description linkend="Cable_arm.Wire_identification.conductivity_type">
	specifies the conductivity type of the wire.
</ext_description>


</ext_descriptions>

