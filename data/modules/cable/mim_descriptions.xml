<ext_description linkend="Cable_mim.cable_component">
	A <b>cable_component</b> is a type of 
	<express_ref linkend="assembly_component:mim:Assembly_component_mim.physical_component"/> 
	that implements the ARM concept of  
	<express_ref linkend="Cable:arm:Cable_arm.Cable_component"/>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: cable_terminal
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Cable_mim.cable_terminal">
A <b>cable_terminal</b> is a type of 
<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.shape_aspect"/>.
</ext_description>

<!-- of_shape -->
<ext_description linkend="Cable_mim.cable_terminal.of_shape">
	an attribute inherited from the
	<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.shape_aspect"/> 
	shall be redeclared as the
	<express_ref linkend="Cable:mim:Cable_mim.cable_usage_view"/>
	for the <b>cable_terminal</b>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
