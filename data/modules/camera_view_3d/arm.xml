<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="camera_view_3d_arm schema_instance"/>
   <schema name="Camera_view_3d_arm">
      <interface kind="use" schema="Presentation_hierarchy_arm"/>
      <type name="presented_model_select">
         <select extensible="YES" genericentity="YES"/>
      </type>
      <type name="view_projection_type">
         <enumeration items="central parallel"/>
      </type>
      <entity name="Clipping_plane" supertypes="Detailed_geometric_model_element"/>
      <entity name="Model_image_3d">
         <explicit name="name" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="is_displaying">
            <typename name="presented_model_select"/>
         </explicit>
         <explicit name="is_presented_in">
            <typename name="View"/>
         </explicit>
         <explicit name="scale" optional="YES">
            <typename name="Value_with_unit"/>
         </explicit>
         <explicit name="view_projection">
            <typename name="view_projection_type"/>
         </explicit>
         <explicit name="clipping" optional="YES">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="Clipping_plane"/>
         </explicit>
         <explicit name="hidden_line_removal">
            <builtintype type="BOOLEAN"/>
         </explicit>
      </entity>
   </schema>
</express>
