<ext_description linkend="Camera_view_3d_mim">
</ext_description>

<!-- +++++++++++ hidden_element_over_riding_styled_item +++++++++++ -->
<ext_description linkend="Camera_view_3d_mim.hidden_element_over_riding_styled_item">
	A <b>hidden_element_over_riding_styled_item</b> is a type of
	<express_ref linkend="presentation_appearance_schema:ir_express:presentation_appearance_schema.context_dependent_over_riding_styled_item"/>.
</ext_description>

<!-- style_context -->
<ext_description linkend="Camera_view_3d_mim.hidden_element_over_riding_styled_item.style_context">
	an attribute inherited from the
	<express_ref linkend="presentation_appearance_schema:ir_express:presentation_appearance_schema.context_dependent_over_riding_styled_item"/>
	shall be redeclared as the
	<express_ref linkend="presentation_organization_schema:ir_express:presentation_organization_schema.presentation_view"/>
	for the <b>hidden_element_over_riding_styled_item</b>.
	There shall exist exactly one
	<express_ref linkend="presentation_organization_schema:ir_express:presentation_organization_schema.presentation_view"/>
	for the <b>hidden_element_over_riding_styled_item</b>.
</ext_description>

<!-- item -->
<ext_description linkend="Camera_view_3d_mim.hidden_element_over_riding_styled_item.item">
	an attribute inherited from the
	<express_ref linkend="presentation_appearance_schema:ir_express:presentation_appearance_schema.styled_item"/>
	shall be redeclared as the
	<express_ref linkend="presentation_organization_schema:ir_express:presentation_organization_schema.camera_image"/>
	for the <b>hidden_element_over_riding_styled_item</b>.
</ext_description>

<!-- container -->
<ext_description linkend="Camera_view_3d_mim.hidden_element_over_riding_styled_item.container">
	specifies an inverse relationship that specifies that the existence of the <b>hidden_element_over_riding_styled_item</b> is dependent on the existence of the 
	<express_ref linkend="presentation_organization_schema:ir_express:presentation_organization_schema.presentation_view"/> 
	that specifies the <b>hidden_element_over_riding_styled_item</b> as its 
	<express_ref linkend="representation_schema:ir_express:representation_schema.representation.items"/>. 
</ext_description>

<!-- WR1 -->
<ext_description linkend="Camera_view_3d_mim.hidden_element_over_riding_styled_item.wr:WR1">
	<p>
		The 
		<express_ref linkend="representation_schema:ir_express:representation_schema.representation_map.mapping_origin"/>
		of the item shall be a
		<express_ref linkend="presentation_organization_schema:ir_express:presentation_organization_schema.camera_model_d3_with_hlhsr"/>.
	</p>
</ext_description>

</ext_descriptions>
