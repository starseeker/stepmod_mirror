RULE addresses_are_assigned FOR\par
    (Address);\par
WHERE\par
WR1: SIZEOF ( QUERY ( a <* address | \par
\tab SIZEOF( USEDIN( a, schema_name + '.ADDRESS_ASSIGNMENT.ASSIGNED_ADDRESS')) =0\par
     ))=0;\par
END_RULE;\par
\par
RULE Person_requires_person_and_organization FOR (Person);\par
WHERE\par
WR1: SIZEOF ( QUERY ( p <* Person | \par
\tab SIZEOF ( USEDIN ( p ,schema_name + '.PERSON_IN_ORGANIZATION.CONCERNED_PERSON' )) =  0 )) =  0;\par
END_RULE; \par
\par
RULE Shape_requires_product_view FOR (Item_shape);\par
WHERE\par
WR1: SIZEOF(QUERY( is <* Item_shape |\par
\tab NOT (schema_name + '.PRODUCT_VIEW_DEFINITION' IN TYPEOF(is.described_element))\par
\tab ))= 0;\par
WR2: SIZEOF(QUERY( is <* Item_shape |\par
\tab SIZEOF(TYPEOF(is.described_element)*\par
\tab [schema_name + '.FUNCTIONAL_ELEMENT_DEFINITION',\par
\tab schema_name + '.DESIGN_CONSTRAINT_DEFINITION'])>0\par
\tab ))= 0;\par
END_RULE;\par
\par
RULE Design_or_specification_for_individual FOR (Product_as_individual);\par
WHERE\par
WR1: SIZEOF(QUERY( pai <* Product_as_individual |\par
  SIZEOF(USEDIN(pai, schema_name + '.PRODUCT_DESIGN_TO_INDIVIDUAL.INDIVIDUAL_PRODUCT')) +\par
  SIZEOF(USEDIN(pai, schema_name + '.PRODUCT_SPECIFICATION_TO_INDIVIDUAL.INDIVIDUAL_PRODUCT'))\par
\tab >1 ))=0;\par
END_RULE;\par
