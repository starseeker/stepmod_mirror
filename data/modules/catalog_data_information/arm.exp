SUBTYPE_CONSTRAINT exclusive_part_definitions_236cc1 FOR Part_view_definition;\par
ONEOF (Assembly_definition, Collection_definition);\par
END_SUBTYPE_CONSTRAINT;\par
\par
SUBTYPE_CONSTRAINT exclusive_product_definitions_236cc1 FOR Product_view_definition;\par
ONEOF (Part_view_definition,\par
\tab Functional_element_definition,\par
\tab Physical_element_definition,\par
\tab Individual_definition,\par
\tab Design_constraint_definition);\par
END_SUBTYPE_CONSTRAINT;\par
