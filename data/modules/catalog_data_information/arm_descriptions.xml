   <express_ref linkend="catalog_data_information:arm:Catalog_data_information_arm.Document_language_identification.document"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Catalog_data_information_arm.Document_language_identification.document">
the <express_ref linkend="file_identification:arm:File_identification_arm.File "/> or <express_ref linkend="document_definition:arm:Document_definition_arm.Document_definition"/> that is considered.
</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Catalog_data_information_arm  Entity: Document_language_identification  Attribute: used_language 
   <express_ref linkend="catalog_data_information:arm:Catalog_data_information_arm.Document_language_identification.used_language"/>
