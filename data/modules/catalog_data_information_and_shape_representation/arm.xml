<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: 2003/04/03 08:42:41 $"
  rcs.revision="$Revision: 1.4 $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.26"
    source="../data/modules/catalog_data_information_and_shape_representation/arm.exp"/>

  <schema name="Catalog_data_information_and_shape_representation_arm">
    <interface
      kind="use"
      schema="Advanced_boundary_representation_arm">
    </interface>

    <interface
      kind="use"
      schema="Constructive_solid_geometry_3d_arm">
    </interface>

    <interface
      kind="use"
      schema="Dimension_tolerance_arm">
    </interface>

    <interface
      kind="use"
      schema="Edge_based_wireframe_arm">
    </interface>

    <interface
      kind="use"
      schema="Faceted_boundary_representation_arm">
    </interface>

    <interface
      kind="use"
      schema="Geometric_tolerance_arm">
    </interface>

    <interface
      kind="use"
      schema="Geometric_validation_property_representation_arm">
    </interface>

    <interface
      kind="use"
      schema="Geometrically_bounded_surface_arm">
    </interface>

    <interface
      kind="use"
      schema="Geometrically_bounded_wireframe_arm">
    </interface>

    <interface
      kind="use"
      schema="Manifold_surface_arm">
    </interface>

    <interface
      kind="use"
      schema="Shell_based_wireframe_arm">
    </interface>

    <interface
      kind="use"
      schema="Surface_conditions_arm">
    </interface>

    <interface
      kind="use"
      schema="Wireframe_2d_arm">
    </interface>

    <interface
      kind="use"
      schema="Furniture_catalog_and_interior_design_cc1_arm">
    </interface>

    <constant name="schema_name"
      expression="'CATALOG_DATA_INFORMATION_AND_SHAPE_REPRESENTATION_ARM'">
	  <builtintype type="STRING"/>
    </constant>
    <type name="string_select_236cc2">
      <select
        extensible="YES"
        basedon="string_select_236cc1"
        selectitems="Geometric_tolerance">
      </select>

    </type>
    <subtype.constraint name="subtype_exclusiveness_geometric_model"
      entity="Geometric_model"
      super.expression="ONEOF (	Advanced_brep_shape_representation ,	Edge_based_wireframe_shape_representation ,	Faceted_brep_shape_representation ,	Geometrically_bounded_2d_wireframe_shape_representation ,	Geometrically_bounded_surface_shape_representation ,	Geometrically_bounded_wireframe_shape_representation ,	Manifold_surface_shape_representation ,	Shell_based_wireframe_shape_representation)">
    </subtype.constraint>

  </schema>

</express>
