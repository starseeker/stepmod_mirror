../data/modules/Basic_Curve/arm.exp
   ------------------------------------------------------------
*)


	
	

	SUBTYPE_CONSTRAINT curve_subtypes FOR Curve;         
	    ONEOF (Composite_curve,
		Trimmed_curve);    
	END_SUBTYPE_CONSTRAINT;

  ENTITY Closed_curve
    SUBTYPE OF (Curve);
  END_ENTITY;

