<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.6 2002/01/16 12:11:43 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.6"
    source="../data/modules/certification/arm.exp"/>

  <schema name="Certification_arm">
    <interface
      kind="use"
      schema="Approval_arm">
    </interface>

    <type name="certification_item">
      <select
        extensible="YES"
        selectitems="">
      </select>

    </type>
    <type name="certification_approval_item">
      <select
        basedon="approval_item"
        selectitems="Certification">
      </select>

    </type>
    <entity
      name="Certification">
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <inverse
        name="approvals"
        entity="Approval_assignment"
        attribute="items">
        <inverse.aggregate
          type="SET"
          lower="0"
          upper="?"/>
      </inverse>
    </entity>

    <entity
      name="Certification_assignment">
      <explicit
        name="assigned_certification">
        <typename
          name="Certification"/>
      </explicit>
      <explicit
        name="role">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="certification_item"/>
      </explicit>
    </entity>

  </schema>

</express>
