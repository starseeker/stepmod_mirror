<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express rcs.date="$Date: 2002/04/03 21:35:50 $" rcs.revision="$Revision: 1.6 $">
   <application name="pdm_module2mim.xml" owner="PDTec GmbH" url="http://www.pdtec.de" version="0.8" source="../data/modules/approval/mim.exp"/>
   <schema name="certification_mim">
      <interface kind="use" schema="approval_mim"/>
      <interface kind="use" schema="certification_schema">
         <interfaced.item name="certification_type"/>
         <interfaced.item name="certification"/>
      </interface>
      <interface kind="use" schema="management_resources_schema">
         <interfaced.item name="certification_assignment"/>
      </interface>
      <type name="certification_item">
         <select extensible="YES"/>
      </type>
      <type name="certification_approval_item">
         <select basedon="approval_item" selectitems="certification"/>
      </type>
      <entity name="applied_certification_assignment" supertypes="certification_assignment">
         <explicit name="items">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="certification_item"/>
         </explicit>
      </entity>
   </schema>
</express>