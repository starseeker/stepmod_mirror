<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/02/13 09:35:34 $"
  rcs.revision="$Revision: 1.4 $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.6"
    source="../data/modules/certification/mim.exp"/>

  <schema name="Certification_mim">
    <interface
      kind="use"
      schema="Approval_mim">
    </interface>

    <interface
      kind="use"
      schema="certification_schema">
      <interfaced.item
        name="certification"/>
      <interfaced.item
        name="certification_type"/>
    </interface>

    <interface
      kind="use"
      schema="management_resources_schema">
      <interfaced.item
        name="certification_assignment"/>
    </interface>

    <type name="certification_item">
      <description>A <b>certification_item</b> specifies those objects to which a <b>certification</b> may be assigned.</description>
      <select
        extensible="YES">
      </select>

    </type>
    <type name="certification_approval_item">
      <description>A <b>certification_approval_item</b> extends approval assignment to <b>certification</b> instances.</description>
      <select
        basedon="approval_item"
        selectitems="certification">
      </select>

    </type>
    <entity
      name="applied_certification_assignment"
      supertypes="certification_assignment">
      <description></description>
      <explicit
        name="items">
      <description></description>
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="certification_item"/>
      </explicit>
    </entity>

  </schema>

</express>
