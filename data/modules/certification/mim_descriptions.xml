   <ext_description linkend="Certification_mim.certification_item">A <b>certification_item</b> specifies those objects to which a <b>certification</b> may be assigned.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Certification_mim  Type: certification_approval_item 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Certification_mim.certification_approval_item">A <b>certification_approval_item</b> extends approval assignment to <b>certification</b> instances.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Certification_mim  Entity: applied_certification_assignment 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Certification_mim.applied_certification_assignment"/> An <b>applied_certification_assignment</b> assigns a <b>certification</b> to a certification_item.


  <!-- 
