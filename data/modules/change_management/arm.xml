<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="">
   <application name="JSDAI" owner="LKSoft" source="change_management_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Change_management_arm">
      <interface kind="use" schema="Approval_arm"/>
      <interface kind="use" schema="Basic_data_representation_arm"/>
      <interface kind="use" schema="Product_version_relationship_arm"/>
      <interface kind="use" schema="Support_resource_arm"/>
      <type name="change_management_object">
         <select extensible="YES" genericentity="YES"/>
      </type>
      <type name="change_relationship_select">
         <select extensible="YES" genericentity="YES" selectitems="Product_version_relationship"/>
      </type>
      <type name="dm_approval_item">
         <select basedon="approval_item" extensible="YES" genericentity="YES" selectitems="Frozen_assignment"/>
      </type>
      <entity name="Add_element" supertypes="Change_element">
         <explicit name="current_design_object">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="change_management_object"/>
         </explicit>
      </entity>
      <entity name="Change">
         <explicit name="id" optional="YES">
            <typename name="identifier"/>
         </explicit>
         <explicit name="name">
            <typename name="label"/>
         </explicit>
         <explicit name="description" optional="YES">
            <typename name="text"/>
         </explicit>
         <explicit name="described_change" optional="YES">
            <typename name="change_relationship_select"/>
         </explicit>
      </entity>
      <entity abstract.supertype="YES" name="Change_element" super.expression="ONEOF (Modify_element, Modify_single_element, Delete_element, Add_element)">
         <explicit name="attribute_name" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="change_location_in_aggregate_attribute" optional="YES">
            <aggregate lower="1" type="LIST" upper="?"/>
            <builtintype type="INTEGER"/>
         </explicit>
         <explicit name="change_set">
            <typename name="Change"/>
         </explicit>
      </entity>
      <entity name="Change_element_sequence">
         <explicit name="description" optional="YES">
            <typename name="text"/>
         </explicit>
         <explicit name="previous">
            <typename name="Change_element"/>
         </explicit>
         <explicit name="next">
            <typename name="Change_element"/>
         </explicit>
      </entity>
      <entity name="Delete_element" supertypes="Change_element">
         <explicit name="previous_design_object">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="change_management_object"/>
         </explicit>
      </entity>
      <entity name="Frozen_assignment">
         <explicit name="frozen_item">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="change_management_object"/>
         </explicit>
      </entity>
      <entity name="Modify_element" supertypes="Change_element">
         <explicit name="current_design_object">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="change_management_object"/>
         </explicit>
      </entity>
      <entity name="Modify_single_element" supertypes="Change_element">
         <explicit name="current_design_object">
            <typename name="change_management_object"/>
         </explicit>
         <explicit name="previous_design_object">
            <typename name="change_management_object"/>
         </explicit>
         <where expression="current_design_object &lt;&gt; previous_design_object" label="WR1"/>
      </entity>
   </schema>
</express>
