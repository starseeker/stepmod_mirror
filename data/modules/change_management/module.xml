 <!-- Clause 5.1 Mapping specification -->
 <mapping_table>


<!--  ++  ADD_ELEMENT  ++ -->
<ae entity="Add_element">
<aimelt>add_element</aimelt>
<source>ISO 10303-1661</source>
<refpath>
  add_element &lt;=
  change_element &lt;=
  group
</refpath>

<aa attribute="current_design_object" assertion_to="change_management_object">
<aimelt>PATH</aimelt>
<refpath>
  add_element &lt;=
  change_element &lt;=
  group &lt;-
  group_assignment.assigned_group
  group_assignment =&gt;
  current_change_element_assignment
  current_change_element_assignment.items[i] -&gt;
  change_relationship_select
</refpath>
</aa>
</ae> 

