<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: Characteristic_arm
   <express_ref linkend="characteristic:arm:Characteristic_arm"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Characteristic_arm">
</ext_description>

<!-- +++++++++++ TYPE range_class +++++++++++ -->
<ext_description linkend="Characteristic_arm.range_class">
A <b>range_class</b> defines the possible boundary types of a <express_ref linkend="Characteristic:arm:Characteristic_arm.Range_characteristic"/>.
</ext_description>
<ext_description linkend="Characteristic_arm.range_class.closed">
specifies that value is closed;
</ext_description>
<ext_description linkend="Characteristic_arm.range_class.lower_open">
specifies that value is lower opened;
</ext_description>
<ext_description linkend="Characteristic_arm.range_class.upper_open">
specifies that value is upper opened;
</ext_description>
<ext_description linkend="Characteristic_arm.range_class.open">
specifies that value is open;
</ext_description>

<!-- +++++++++++ Characteristic_data_table +++++++++++ -->
<ext_description linkend="Characteristic_arm.Characteristic_data_table">
	A <b>Characteristic_data_table</b> is a type of 
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation_item"/> 
	that is described by one or more 
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Characteristic_data_table_row">Characteristic_data_table_rows</express_ref>.
</ext_description>

<!-- property_value -->
<ext_description linkend="Characteristic_arm.Characteristic_data_table.property_value">
	specifies the list of one or more 
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Characteristic_data_table_row"/>
	defining the <b>Characteristic_data_table</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Characteristic_arm.Characteristic_data_table.wr:WR1">
	The size of each row in the table shall be the same as the size of the first row in the table.
</ext_description>

<!-- +++++++++++ Characteristic_data_table_header +++++++++++ -->
<ext_description linkend="Characteristic_arm.Characteristic_data_table_header">
	A <b>Characteristic_data_table_header</b> is a type of
	<express_ref linkend="Independent_property:arm:Independent_property_arm.Independent_property"/>
	for specifying the header of a table for measurements.
</ext_description>

<!-- column -->
<ext_description linkend="Characteristic_arm.Characteristic_data_table_header.column">
	specifies a list of
	<express_ref linkend="Independent_property:arm:Independent_property_arm.Independent_property"/>
	for the <b>Characteristic_data_table_header</b>.
	There shall exist one or more 
	<express_ref linkend="Independent_property:arm:Independent_property_arm.Independent_property">Independent_properties</express_ref>
	for the <b>Characteristic_data_table_header</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Characteristic_arm.Characteristic_data_table_header.wr:WR1">
	There shall exist more than one <b>column</b> for the <b>Characteristic_data_table_header</b>.
</ext_description>

<!-- +++++++++++ Characteristic_data_table_header_link +++++++++++ -->
<ext_description linkend="Characteristic_arm.Characteristic_data_table_header_link">
	A <b>Characteristic_data_table_header_link</b> is a type of 
	<express_ref linkend="Independent_property:arm:Independent_property_arm.Independent_property"/>
	and 
	<express_ref linkend="Independent_property:arm:Independent_property_arm.Independent_property_relationship"/>
	that define the structure of the columns of a data table.
</ext_description>

<!-- precedent -->
<ext_description linkend="Characteristic_arm.Characteristic_data_table_header_link.precedent">
	specifies the 
	<express_ref linkend="Independent_property:arm:Independent_property_arm.Independent_property"/> 
	that is the previous column header.
</ext_description>

<!-- subsequent -->
<ext_description linkend="Characteristic_arm.Characteristic_data_table_header_link.subsequent">
	specifies the 
	<express_ref linkend="Independent_property:arm:Independent_property_arm.Independent_property"/>
	that is the next column header.
</ext_description>

<!-- relation_type -->
<ext_description linkend="Characteristic_arm.Characteristic_data_table_header_link.relation_type">
	specifies the relation type 'sequence'.
</ext_description>

<!-- table -->
<ext_description linkend="Characteristic_arm.Characteristic_data_table_header_link.table">
	specifies the 
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Characteristic_data_table_header"/> 
	that is the complete header row of the table.	
</ext_description>

<!-- UR1 -->
<ext_description linkend="Characteristic_arm.Characteristic_data_table_header_link.ur:UR1">
	The combination of <b>precedent</b> and <b>table</b> shall be unique within a population of the <b>Characteristic_data_table_header_link</b>.
</ext_description>

<!-- UR2 -->
<ext_description linkend="Characteristic_arm.Characteristic_data_table_header_link.ur:UR2">
	The combination of <b>subsequent</b> and <b>table</b> shall be unique within a population of the <b>Characteristic_data_table_header_link</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Characteristic_arm.Characteristic_data_table_header_link.wr:WR1">
	The <b>table</b> shall exist for the <b>Characteristic_data_table_header_link</b>
</ext_description>

<!-- +++++++++++ Characteristic_data_table_representation +++++++++++ -->
<ext_description linkend="Characteristic_arm.Characteristic_data_table_representation">
	A <b>Characteristic_data_table_representation</b> is a type of
	<express_ref linkend="Measure_representation:arm:Measure_representation_arm.Property_value_representation"/>.
</ext_description>

<!-- items -->
<ext_description linkend="Characteristic_arm.Characteristic_data_table_representation.items">
	an attribute inherited from the
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation"/>
	shall be redeclared as the
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Characteristic_data_table"/>
	for the <b>Characteristic_data_table_representation</b>.
</ext_description>

<!-- +++++++++++ Characteristic_data_table_row +++++++++++ -->
<ext_description linkend="Characteristic_arm.Characteristic_data_table_row">
	A <b>Characteristic_data_table_row</b> is a type of
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation_item"/>
	that is described by one or more 
	<express_ref linkend="Characteristic:arm:Characteristic_arm.characteristic">characteristics</express_ref>.
</ext_description>

<!-- cell -->
<ext_description linkend="Characteristic_arm.Characteristic_data_table_row.cell">
	specifies the list of one or more 
	<express_ref linkend="Characteristic:arm:Characteristic_arm.characteristic"/>
	defining the <b>Characteristic_data_table_row</b>.
</ext_description>

<!-- +++++++++++ Externally_defined_type_qualifier +++++++++++ -->
<ext_description linkend="Characteristic_arm.Externally_defined_type_qualifier">
	A <b>Externally_defined_type_qualifier</b> is a type of 
    <express_ref linkend="Qualified_measure:arm:Qualified_measure_arm.Type_qualifier"/> 
    whose meaning is defined in an external library.
</ext_description>

<!-- external_source -->
<ext_description linkend="Characteristic_arm.Externally_defined_type_qualifier.external_source">
	the identification of the 
	<express_ref linkend="External_source:arm:External_source_arm.External_source"/> 
	that provides the meaning of the <b>Externally_defined_type_qualifier</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Characteristic_arm.Externally_defined_type_qualifier.wr:WR1">
	The 
	<express_ref linkend="Qualified_measure:arm:Qualified_measure_arm.Type_qualifier.name"/>
	shall not be in type of
	<express_ref linkend="Qualified_measure:arm:Qualified_measure_arm.pre_defined_type_qualifiers"/>.
</ext_description>

<!-- +++++++++++ Length_tolerance_characteristic +++++++++++ -->
<ext_description linkend="Characteristic_arm.Length_tolerance_characteristic">
	A <b>Length_tolerance_characteristic</b> is a type of
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Tolerance_characteristic"/>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Characteristic_arm.Length_tolerance_characteristic.wr:WR1">
	If type of SELF is not 
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Statistical_tolerance_characteristic"/>,
	not 
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Symmetrical_tolerance_characteristic"/>,
	and not
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Plus_minus_tolerance_characteristic"/>,
	then all members of 
	<express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation.items"/>
	shall be of type 
	<express_ref linkend="value_with_unit_extension:arm:Value_with_unit_extension_arm.Length_data_element"/>.	
</ext_description>

<!-- WR2 -->
<ext_description linkend="Characteristic_arm.Length_tolerance_characteristic.wr:WR2">
If type of SELF is <express_ref linkend="characteristic:arm:Characteristic_arm.Symmetrical_tolerance_characteristic"/>,
then there shall be only one member of <express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation.items"/>
that is of type <express_ref linkend="value_with_unit_extension:arm:Value_with_unit_extension_arm.Length_data_element"/>.
</ext_description>

<!-- WR3 -->
<ext_description linkend="Characteristic_arm.Length_tolerance_characteristic.wr:WR3">
If type of SELF is <express_ref linkend="characteristic:arm:Characteristic_arm.Statistical_tolerance_characteristic"/>,
then there shall be only one member of <express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation.items"/>
that is of type <express_ref linkend="value_with_unit_extension:arm:Value_with_unit_extension_arm.Length_data_element"/>.
</ext_description>

<!-- WR4 -->
<ext_description linkend="Characteristic_arm.Length_tolerance_characteristic.wr:WR4">
The <express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation.description"/>
shall not be provided.	
</ext_description>

<!-- WR5 -->
<ext_description linkend="Characteristic_arm.Length_tolerance_characteristic.wr:WR5">
	If an instance of <b>Length_tolerance_characteristic</b> is a complex of <b>Length_tolerance_characteristic</b> and of 
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Plus_minus_tolerance_characteristic"/>, 
	then the 
	<express_ref linkend="Measure_representation:arm:Measure_representation_arm.Numerical_item_with_unit"/> 
	in the role of 
	<express_ref linkend="Extended_measure_representation:arm:Extended_measure_representation_arm.Value_with_tolerances.item_value"/> 
	for the 
	<express_ref linkend="Extended_measure_representation:arm:Extended_measure_representation_arm.Value_with_tolerances"/> 
	in the role of 
	<express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation.items"/> 
	inherited from the 
	<express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation"/>
	shall be a complex with 
	<express_ref linkend="value_with_unit_extension:arm:Value_with_unit_extension_arm.Length_data_element"/>.
</ext_description>

<!-- +++++++++++ Maximum_tolerance_characteristic +++++++++++ -->
<ext_description linkend="Characteristic_arm.Maximum_tolerance_characteristic">
	A <b>Maximum_tolerance_characteristic</b> is a type of 
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Tolerance_characteristic"/>. 
	The <b>Maximum_tolerance_characteristic</b> indicates the maximum allowed value for the parameter specified.
</ext_description>

<!-- items -->
<ext_description linkend="Characteristic_arm.Maximum_tolerance_characteristic.items">
	specifies a role of the 
	<express_ref linkend="Extended_measure_representation:arm:Extended_measure_representation_arm.Value_limit"/> 
	for the <b>Maximum_tolerance_characteristic</b>.  There is exactly one  
	<express_ref linkend="Extended_measure_representation:arm:Extended_measure_representation_arm.Value_limit"/> 
	for each <b>items</b>. The <b>items</b> specifies the maximum value of the 
	<express_ref linkend="Extended_measure_representation:arm:Extended_measure_representation_arm.Value_limit"/> 
	for the <b>Maximum_tolerance_characteristic</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Characteristic_arm.Maximum_tolerance_characteristic.wr:WR1">
The <express_ref linkend="Qualified_measure:arm:Qualified_measure_arm.Type_qualifier.name"/>
of the only member of <express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation.items"/>
shall be set to 'maximum'.
</ext_description>

<!-- +++++++++++ Minimum_tolerance_characteristic +++++++++++ -->
<ext_description linkend="Characteristic_arm.Minimum_tolerance_characteristic">
	A <b>Minimum_tolerance_characteristic</b> is a type of 
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Tolerance_characteristic"/>. 
	The <b>Minimum_tolerance_characteristic</b> indicates the minimum allowed value for the parameter specified.
</ext_description>

<!-- items -->
<ext_description linkend="Characteristic_arm.Minimum_tolerance_characteristic.items">
	specifies a role of the 
	<express_ref linkend="Extended_measure_representation:arm:Extended_measure_representation_arm.Value_limit"/> 
	for the <b>Minimum_tolerance_characteristic</b>. There shall be exactly one  
	<express_ref linkend="Extended_measure_representation:arm:Extended_measure_representation_arm.Value_limit"/> 
	for an <b>items</b>. 
	The <b>items</b> specifies the minimum value of the 
	<express_ref linkend="Extended_measure_representation:arm:Extended_measure_representation_arm.Value_limit"/> 
	for the <b>Minimum_tolerance_characteristic</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Characteristic_arm.Minimum_tolerance_characteristic.wr:WR1">
The <express_ref linkend="Qualified_measure:arm:Qualified_measure_arm.Type_qualifier.name"/>
of the only member of <express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation.items"/>
shall be set to 'minimum'.
</ext_description>


<!-- +++++++++++ Plus_minus_tolerance_characteristic +++++++++++ -->
<ext_description linkend="Characteristic_arm.Plus_minus_tolerance_characteristic">
	A <b>Plus_minus_tolerance_characteristic</b> is a type of 
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Tolerance_characteristic"/>. 
	The <b>Plus_minus_tolerance_characteristic</b> includes a minimum, maximum, and nominal characteristic numeric parameter.  
	All three have the same base unit, but may have different prefixes.
</ext_description>

<!-- items -->
<ext_description linkend="Characteristic_arm.Plus_minus_tolerance_characteristic.items">
	specifies the nominal, minimum, and maximum values of the 
	<express_ref linkend="Extended_measure_representation:arm:Extended_measure_representation_arm.Value_with_tolerances"/> 
	for the <b>Plus_minus_tolerance_characteristic</b>. There shall be one nominal, one minimum and one maximum value 
	for the <b>Plus_minus_tolerance_characteristic</b>.
</ext_description>


<!-- +++++++++++ Statistical_tolerance_characteristic +++++++++++ -->
<ext_description linkend="Characteristic_arm.Statistical_tolerance_characteristic">
	A <b>Statistical_tolerance_characteristic</b> is a type of 
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Tolerance_characteristic"/>. 
	The <b>Statistical_tolerance_characteristic</b> supports the exchange of properties defined by statistical distributions.
</ext_description>

<!-- items -->
<ext_description linkend="Characteristic_arm.Statistical_tolerance_characteristic.items">
	specifies a value of the 
	<express_ref linkend="Measure_representation:arm:Measure_representation_arm.Numerical_item_with_unit"/> 
	for the <b>Statistical_tolerance_characteristic</b>. There shall two or more  
	<express_ref linkend="Measure_representation:arm:Measure_representation_arm.Numerical_item_with_unit">Numerical_item_with_units</express_ref> 
	for the <b>Statistical_tolerance_characteristic</b>.
</ext_description>

<!-- IP1 -->
<ext_description linkend="Characteristic_arm.Statistical_tolerance_characteristic.wr:IP1">
	The 
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.items"/> 
	inherited from 
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation"/> 
	shall all participate in the characterization of one statistical tolerance.
</ext_description>

<!-- IP2 -->
<ext_description linkend="Characteristic_arm.Statistical_tolerance_characteristic.wr:IP2">
	The 
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation_item.name"/> 
	inherited from 
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation"/> 
	shall be a recognized statistical tolerance type.
</ext_description>

<!-- +++++++++++ Symmetrical_tolerance_characteristic +++++++++++ -->
<ext_description linkend="Characteristic_arm.Symmetrical_tolerance_characteristic">
<p>
A <b>Symmetrical_tolerance_characteristic</b> 
is a type of <express_ref linkend="Characteristic:arm:Characteristic_arm.Tolerance_characteristic"/> that has symmetrical tolerances around the nominal value. 
The structure is a nominal value with a ratio.  The ratio is applied to the nominal value and applies to both plus and minus.
</p>
<example>
A resistance of 22 Ohm plus or minus 10% would be represented with two values, 22 Ohm and .10 since the same quantity 10% is 
referenced on both sides of the nominal value.
</example>
<note>
An unsymmetrical distribution would be represented with a member of <express_ref linkend="Characteristic:arm:Characteristic_arm.Plus_minus_tolerance_characteristic"/>.
</note>
</ext_description>

<!-- items -->
<ext_description linkend="Characteristic_arm.Symmetrical_tolerance_characteristic.items">
specifies a value of the <express_ref linkend="Measure_representation:arm:Measure_representation_arm.Numerical_item_with_unit"/> for the 
<b>Symmetrical_tolerance_characteristic</b>. There shall be exactly two  <express_ref linkend="Measure_representation:arm:Measure_representation_arm.Numerical_item_with_unit"/> for the 
<b>Symmetrical_tolerance_characteristic</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Characteristic_arm.Symmetrical_tolerance_characteristic.wr:WR1">
There shall be exactly one <express_ref linkend="value_with_unit_extension:arm:Value_with_unit_extension_arm.Ratio_data_element"/>
in <b>items</b> of <b>Symmetrical_tolerance_characteristic</b>.
</ext_description>
<!-- +++++++++++ Tolerance_characteristic +++++++++++ -->
<ext_description linkend="Characteristic_arm.Tolerance_characteristic">
	A <b>Tolerance_characteristic</b> is a type of 
	<express_ref linkend="Measure_representation:arm:Measure_representation_arm.Property_value_representation"/>. 
	A <b>Tolerance_characteristic</b> is one of a
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Maximum_tolerance_characteristic"/>,
	a 
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Minimum_tolerance_characteristic"/>, 
	a
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Plus_minus_tolerance_characteristic"/>, 
	a
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Statistical_tolerance_characteristic"/>,
	a
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Nominal_tolerance_characteristic"/>,
	or a
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Symmetrical_tolerance_characteristic"/>
	or additionally may be a
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Length_tolerance_characteristic"/>.
</ext_description>

<!-- items -->
<ext_description linkend="Characteristic_arm.Tolerance_characteristic.items">
<p>
	specifies the role of the 
	<express_ref linkend="Measure_representation:arm:Measure_representation_arm.Numerical_item_with_unit"/> 
	for 
	the <b>Tolerance_characteristic</b>. There shall be one or more  
	<express_ref linkend="Measure_representation:arm:Measure_representation_arm.Numerical_item_with_unit"/> 
	for the <b>Tolerance_characteristic</b>.
</p>
	<note>
		Members of this application object are not exchanged to support geometric tolerancing.
	</note>
</ext_description>

<!-- WR1 -->
<ext_description linkend="Characteristic_arm.Tolerance_characteristic.wr:WR1">
	The 
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation"/>.<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.context_of_items"/> \ <express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation_context"/>.<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation_context.id"/> 
	shall be 'tolerance values'.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Characteristic_arm.Tolerance_characteristic.wr:WR2">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.name"/>
	shall not be populated.
</ext_description>	

<!-- +++++++++++ Range_characteristic +++++++++++ -->
<ext_description linkend="Characteristic_arm.Range_characteristic">
	A <b>Range_characteristic</b> is a type of 
	<express_ref linkend="Measure_representation:arm:Measure_representation_arm.Property_value_representation"/> 
	that specifies a range of values. The range may be open or closed. The range may not be single ended.
</ext_description>

<!-- range_type -->
<ext_description linkend="Characteristic_arm.Range_characteristic.range_type">
	specifies the classification of the <b>Range_characteristic</b>.  
	The <b>Range_characteristic</b> shall be 
	<express_ref linkend="Characteristic:arm:Characteristic_arm.range_class">open</express_ref>, 
	<express_ref linkend="Characteristic:arm:Characteristic_arm.range_class">closed</express_ref>, 
	<express_ref linkend="Characteristic:arm:Characteristic_arm.range_class">lower_open</express_ref> 
	or 
	<express_ref linkend="Characteristic:arm:Characteristic_arm.range_class">upper_open</express_ref>.  
	Open shall be interpreted as not including the end point or points in the range.  
	Closed shall be interpreted as including the end point or points in the range. 
</ext_description>

<!-- items -->
<ext_description linkend="Characteristic_arm.Range_characteristic.items">
	specifies a value of the 
	<express_ref linkend="Measure_representation:arm:Measure_representation_arm.Numerical_item_with_unit"/> 
	for the <b>Range_characteristic</b>. There shall be exactly two  
	<express_ref linkend="Measure_representation:arm:Measure_representation_arm.Numerical_item_with_unit"/> 
	for the <b>Range_characteristic</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Characteristic_arm.Range_characteristic.wr:WR1">
	type of the first element in the 
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.items"/> 
	shall be the same as the type of the second element in the 
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.items"/>.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Characteristic_arm.Range_characteristic.wr:WR2">
	type of the 
	<express_ref linkend="Value_with_unit:arm:Value_with_unit_arm.Value_with_unit.unit"/> 
	of the first element in the 
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.items"/> 
	shall be the same as the type of the 
	<express_ref linkend="Value_with_unit:arm:Value_with_unit_arm.Value_with_unit.unit"/> 
	of the second element in the 
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.items"/>.
</ext_description>

<!-- WR3 -->
<ext_description linkend="Characteristic_arm.Range_characteristic.wr:WR3">
	the first element in the 
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.items"/> 
	and the second element in the 
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.items"/> 
	shall be in valid range.
</ext_description>

<!-- +++++++++++ Nominal_tolerance_characteristic +++++++++++ -->
<ext_description linkend="Characteristic_arm.Nominal_tolerance_characteristic">
	A <b>Nominal_tolerance_characteristic</b> is a type of 
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Tolerance_characteristic"/>.
</ext_description>

<!-- items -->
<ext_description linkend="Characteristic_arm.Nominal_tolerance_characteristic.items">
	specifies a value of the 
	<express_ref linkend="Qualified_measure:arm:Qualified_measure_arm.Qualified_numerical_item_with_unit"/> 
	for the <b>Nominal_tolerance_characteristic</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Characteristic_arm.Nominal_tolerance_characteristic.wr:WR1">
	The 
	<express_ref linkend="Qualified_measure:arm:Qualified_measure_arm.Type_qualifier.name"/>
	of the only member of 
	<express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation.items"/>
	shall be set to 'nominal'.
</ext_description>

<!-- +++++++++++ Text_representation_item +++++++++++ -->
<ext_description linkend="Characteristic_arm.Text_representation_item">
	A <b>Text_representation_item</b> is a type of 
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.String_representation_item"/>.
</ext_description>

<!-- +++++++++++ FUNCTION get_characterized_column_headers +++++++++++ -->
<ext_description linkend="Characteristic_arm.get_characterized_column_headers">
	The <b>get_characterized_column_headers</b> function forms list of 
	<express_ref linkend="Independent_property:arm:Independent_property_arm.Independent_property"/> 
	from 
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Characteristic_data_table_header_link">Characteristic_data_table_header_links</express_ref> 
	which are pointing to 
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Characteristic_data_table_header"/>.
</ext_description>

<!-- input -->
<ext_description linkend="Characteristic_arm.get_characterized_column_headers.input">
	the specified
	<express_ref linkend="Characteristic:arm:Characteristic_arm.Characteristic_data_table_header"/>.
</ext_description>

