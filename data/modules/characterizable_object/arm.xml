<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1765">
   <application name="JSDAI" owner="LKSoft" source="characterizable_object_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Characterizable_object_arm">
      <interface kind="use" schema="Shape_property_assignment_arm"/>
      <interface kind="use" schema="Support_resource_arm"/>
      <type name="characterizable_object_shape_type_enumeration">
         <enumeration extensible="YES" items="unspecified"/>
      </type>
      <entity name="Characterizable_object">
         <explicit name="name">
            <typename name="label"/>
         </explicit>
         <explicit name="description" optional="YES">
            <typename name="text"/>
         </explicit>
         <explicit name="shape_type" optional="YES">
            <typename name="characterizable_object_shape_type_enumeration"/>
         </explicit>
         <explicit name="primary_shape_representation" optional="YES">
            <typename name="shape_model"/>
         </explicit>
         <explicit name="auxiliary_shape_representations" optional="YES">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="shape_model"/>
         </explicit>
         <where expression="NOT (EXISTS(auxiliary_shape_representations)) OR EXISTS(primary_shape_representation)" label="WR1"/>
      </entity>
   </schema>
</express>
