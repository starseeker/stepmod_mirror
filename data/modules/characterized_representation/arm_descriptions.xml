<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Characterized_representation_arm  Entity: Characterized_chain_based_item_within_representation  
   <express_ref linkend="characterized_representation:arm:Characterized_representation_arm.Characterized_chain_based_item_within_representation"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Characterized_representation_arm.Characterized_chain_based_item_within_representation">
	A <b>Characterized_chain_based_item_within_representation</b> is a type of
	<express_ref linkend="characterized_representation:arm:Characterized_representation_arm.Characterized_item_within_representation"/>
	for the characterization of a
	<express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation_item"/>
	through a directed graph of 
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation"/>,
	where the undirected links in the graph can be instances of entity data types 
	<express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation_context"/>,
	<express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation_relationship"/>, or
	<express_ref linkend="elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_placement_operation"/>.
</ext_description>

<ext_description linkend="Characterized_representation_arm.Characterized_chain_based_item_within_representation.nodes">
  the ordered list of  
  <express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation"/>,
  in the chain. 
</ext_description>

<ext_description linkend="Characterized_representation_arm.Characterized_chain_based_item_within_representation.undirected_link">
  the list of items that relate nodes in the chain.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Characterized_representation_arm  Entity: Characterized_item_within_representation  
   <express_ref linkend="characterized_representation:arm:Characterized_representation_arm.Characterized_item_within_representation"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Characterized_representation_arm.Characterized_item_within_representation">
	A <b>Characterized_item_within_representation</b> is a type of
	<express_ref linkend="Characterizable_object:arm:Characterizable_object_arm.Characterizable_object"/>
	for the characterization of a 
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation_item"/>
	that is used within a 
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation"/>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Characterized_representation_arm  Entity: Characterized_item_within_representation  Attribute: item 
   <express_ref linkend="characterized_representation:arm:Characterized_representation_arm.Characterized_item_within_representation.item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Characterized_representation_arm.Characterized_item_within_representation.item">
	specifies the role of the
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation_item"/>
	for the <b>Characterized_item_within_representation</b>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Characterized_representation_arm  Entity: Characterized_item_within_representation  Attribute: rep 
   <express_ref linkend="characterized_representation:arm:Characterized_representation_arm.Characterized_item_within_representation.rep"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Characterized_representation_arm.Characterized_item_within_representation.rep">
	specifies the role of the
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation"/>
	for the <b>Characterized_item_within_representation</b>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Characterized_representation_arm  Entity: Characterized_item_within_representation  Unique: UR1 
   <express_ref linkend="characterized_representation:arm:Characterized_representation_arm.Characterized_item_within_representation.ur:UR1"/>

  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Characterized_representation_arm.Characterized_item_within_representation.ur:UR1">
	The combination of <b>item</b> and <b>rep</b> 
	shall be unique within a population of the <b>Characterized_item_within_representation</b>.
	
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Characterized_representation_arm  Entity: Characterized_item_within_representation  Unique: WR1 
   <express_ref linkend="characterized_representation:arm:Characterized_representation_arm.Characterized_item_within_representation.wr:WR1"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Characterized_representation_arm.Characterized_item_within_representation.wr:WR1">
	the
   <express_ref linkend="characterized_representation:arm:Characterized_representation_arm.Characterized_item_within_representation.item"/>
   shall be used within the 
   <express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation"/>
   indicated by
   <express_ref linkend="characterized_representation:arm:Characterized_representation_arm.Characterized_item_within_representation.rep"/>.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
