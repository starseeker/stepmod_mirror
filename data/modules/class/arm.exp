{ iso standard 10303 part (11) version (4) }


SCHEMA Class_arm;

   ENTITY Class
   SUPERTYPE OF (ONEOF(Class_by_extension, Class_by_intension));
     id : STRING;
     name : STRING;
     description : OPTIONAL STRING;
   END_ENTITY;

   ENTITY Class_by_extension
   SUBTYPE OF (Class);
   END_ENTITY;

   ENTITY Class_by_intension
   SUBTYPE OF (Class);
   END_ENTITY;

END_SCHEMA;
