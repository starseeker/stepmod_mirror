SCHEMA class_mim;

USE FROM classification_schema
(class,
 class_by_extension,
 class_by_intension);

USE FROM group_schema
(group);

USE FROM identification_assignment_mim;

TYPE class_identification_item = SELECT BASED_ON identification_item WITH
  (class);
END_TYPE;

TYPE class_versionable_item = SELECT BASED_ON versionable_item WITH
  (class);
END_TYPE;

END_SCHEMA;
