{ iso standard 10303 part (11) version (4) }

SCHEMA Classification_arm;

   USE FROM Class_arm;
  
   TYPE Classified_select = EXTENSIBLE SELECT;
   END_TYPE;
   
   ENTITY Classification;      
      classified : Classified_select;      
      classifier : Class;
   END_ENTITY;

END_SCHEMA;