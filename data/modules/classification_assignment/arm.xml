<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.22 2002/08/14 06:43:41 goset1 Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.22"
    source="../data/modules/classification_assignment/arm.exp"/>

  <schema name="Classification_assignment_arm">
    <interface
      kind="use"
      schema="Class_arm">
    </interface>

    <interface
      kind="use"
      schema="Group_arm">
    </interface>

    <type name="classification_item">
      <select
        extensible="YES"
        genericentity="YES">
      </select>

    </type>
    <type name="grouping_select">
      <select
        selectitems="Class Group">
      </select>

    </type>
    <entity
      name="Classification_assignment">
      <explicit
        name="assigned_grouping">
        <typename
          name="grouping_select"/>
      </explicit>
      <explicit
        name="items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="classification_item"/>
      </explicit>
      <explicit
        name="role">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

  </schema>

</express>
