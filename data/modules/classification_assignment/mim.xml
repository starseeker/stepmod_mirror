<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.22 2002/08/14 06:43:41 goset1 Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="mim_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.22"
    source="../data/modules/classification_assignment/mim.exp"/>

  <schema name="Classification_assignment_mim">
    <interface
      kind="use"
      schema="Classification_with_attributes_mim">
    </interface>

    <interface
      kind="use"
      schema="Group_mim">
    </interface>

    <interface
      kind="use"
      schema="management_resources_schema">
      <interfaced.item
        name="classification_assignment"/>
    </interface>

    <type name="classification_item">
      <select
        extensible="YES"
        genericentity="YES">
      </select>

    </type>
    <entity
      name="applied_classification_assignment"
      supertypes="classification_assignment">
      <explicit
        name="items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="classification_item"/>
      </explicit>
      <where
        label="WR1"
        expression="SIZEOF(['CLASSIFICATION_ASSIGNMENT_MIM.' + 'GROUP', 'CLASSIFICATION_ASSIGNMENT_MIM.' + 'CHARACTERIZED_CLASS']	* TYPEOF(SELF\classification_assignment.assigned_class))=1">
      </where>
    </entity>

  </schema>

</express>
