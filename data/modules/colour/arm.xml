<?xml version="1.0"?>
<?xml-stylesheet 
  type="text/xsl" 
  href="../../../xsl/express.xsl"
?> <!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express language_version="2" rcs.date="$Date: $" rcs.revision="$Revision: $"
  description.file="arm_descriptions.xml"
>
<application name="GraphicalEXPRESS" owner="Eurostep" url="www.eurostep.com" version="1.3" source="Colour_arm_v001.vsd" />
<schema name="Colour_arm">
<entity name="Colour">
<explicit name="name">
<builtintype type="STRING" />
</explicit>
<graphic.element page="2" xcoord="25" ycoord="194" />
</entity>
<entity name="Externally_defined_colour" supertypes="Colour">
<explicit name="source">
<builtintype type="STRING" />
</explicit>
<graphic.element page="2" xcoord="102" ycoord="158" />
</entity>
<entity name="Pre_defined_colour" supertypes="Colour">
<where label="WR1" expression="SELF.name IN ['red', 'green', 'blue', 'yellow', 'magenta', 'cyan', 'black', 'white']">
</where>
<graphic.element page="2" xcoord="93" ycoord="123" />
</entity>
<entity name="User_defined_colour" supertypes="Colour">
<explicit name="red">
<builtintype type="REAL" />
</explicit>
<explicit name="green">
<builtintype type="REAL" />
</explicit>
<explicit name="blue">
<builtintype type="REAL" />
</explicit>
<where label="WR1" expression="{0.0 &lt;= red &lt;= 1.0}">
</where>
<where label="WR2" expression="{0.0 &lt;= green &lt;= 1.0}">
</where>
<where label="WR3" expression="{0.0 &lt;= blue &lt;= 1.0}">
</where>
<graphic.element page="2" xcoord="92" ycoord="89" />
</entity>
<graphic.element page="1" xcoord="70" ycoord="70" />
</schema>
</express>
