   <ext_description linkend="Colour_arm"/>
   
   
    
<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: Colour_arm  Entity: Colour 
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Colour_arm.Colour">
	A <b>Colour</b> is a name for a property of reflecting light at a particular wavelength.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: Colour Attribute: name
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Colour_arm.Colour.name">
	the name specifies the word or group of words by which the 
	<express_ref linkend="colour:arm:Colour_arm.Colour"/> 
	is known.
</ext_description>
			

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
 Schema: Colour_arm  Entity: Externally_defined_colour
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Colour_arm.Externally_defined_colour">
	An <b>Externally_defined_colour</b> is a type of 
	<express_ref linkend="colour:arm:Colour_arm.Colour"/> 
	that has its definition established and maintained by a source outside of ISO 10303.
</ext_description>
			
<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: Externally_defined_colour Attribute: source
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Colour_arm.Externally_defined_colour.source">
	the source specifies the identification of the organization responsible for the 
	<express_ref linkend="colour:arm:Colour_arm.Colour"/> 
	definition.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: Colour_arm  Entity: Pre_defined_colour
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Colour_arm.Pre_defined_colour">
	A <b>Pre_defined_colour</b> is a type of 
	<express_ref linkend="colour:arm:Colour_arm.Colour"/> 
	that has its definition established and maintained in the 
	<express_ref linkend="colour:arm:Colour_arm.Colour"/> module.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: Pre_defined_colour  Where: WR1
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Colour_arm.Pre_defined_colour.wr:WR1">
	The <b>Pre_defined_colour</b> shall be red, green, blue, yellow, magenta, cyan, black, white.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: Colour_arm  Entity: User_defined_colour
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Colour_arm.User_defined_colour">
	A <b>User_defined_colour</b> is a type of 
	<express_ref linkend="colour:arm:Colour_arm.Colour"/> 
	that has its definition established without referencing any standard. 
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: User_defined_colour Attribute: red
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Colour_arm.User_defined_colour.red">
	the intensity of the red 
	<express_ref linkend="colour:arm:Colour_arm.Colour"/> 
	component.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: User_defined_colour Attribute: green 
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Colour_arm.User_defined_colour.green">
	the intensity of the green 
	<express_ref linkend="colour:arm:Colour_arm.Colour"/> 
	component.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: User_defined_colour Attribute: blue
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Colour_arm.User_defined_colour.blue">
	the intensity of the blue 
	<express_ref linkend="colour:arm:Colour_arm.Colour"/> 
	component.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: User_defined_colour Where: WR1
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Colour_arm.User_defined_colour.wr:WR1">
	The intensity of the red component shall be a real number between 0.0 and 1.0., inclusive.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: User_defined_colour Where: WR2
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Colour_arm.User_defined_colour.wr:WR2">
	The intensity of the blue component shall be a real number between 0.0 and 1.0., inclusive.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: Colour_arm  Entity: User_defined_colour Where: WR3
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Colour_arm.User_defined_colour.wr:WR3">
	The intensity of the green component shall be a real number between 0.0 and 1.0, inclusive.
</ext_description>

 
		 	
</ext_descriptions>	 