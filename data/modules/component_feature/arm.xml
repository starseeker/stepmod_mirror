<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$">
   <application name="JSDAI" owner="LKSoft" source="component_feature_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Component_feature_arm">
      <interface kind="use" schema="Physical_unit_design_view_arm"/>
      <type name="cf_groupable_item">
         <select basedon="groupable_item" selectitems="Component_feature"/>
      </type>
      <type name="feature_select">
         <select extensible="YES" genericentity="YES" selectitems="Part_feature"/>
      </type>
      <entity abstract.supertype="YES" name="Component_feature" supertypes="Shape_element">
         <explicit name="definition" optional="YES">
            <typename name="feature_select"/>
         </explicit>
         <explicit name="associated_component">
            <typename name="Assembly_component"/>
            <redeclaration entity-ref="Shape_element" old_name="containing_shape"/>
         </explicit>
         <explicit name="element_name">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="Shape_element"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="definition"/>
            <unique.attribute attribute="associated_component"/>
         </unique>
         <unique label="UR2">
            <unique.attribute attribute="element_name"/>
            <unique.attribute attribute="associated_component"/>
         </unique>
      </entity>
      <entity abstract.supertype="YES" name="Component_feature_joint" supertypes="Component_feature_relationship Shape_element">
         <explicit name="element_name">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="Shape_element"/>
         </explicit>
         <explicit name="feature_1">
            <typename name="Component_feature"/>
            <redeclaration entity-ref="Shape_element_relationship" old_name="relating"/>
         </explicit>
         <explicit name="feature_2">
            <typename name="Component_feature"/>
            <redeclaration entity-ref="Shape_element_relationship" old_name="related"/>
         </explicit>
         <explicit name="associated_definition">
            <typename name="Item_shape"/>
            <redeclaration entity-ref="Shape_element" old_name="containing_shape"/>
         </explicit>
         <where expression="feature_1 &lt;&gt; feature_2" label="WR1"/>
         <where expression="'PRODUCT_VIEW_DEFINITION_ARM.PRODUCT_VIEW_DEFINITION' in TYPEOF(associated_definition)" label="WR2"/>
         <where expression="SELF\Shape_element.containing_shape\Item_shape.described_element = feature_1\Shape_element.containing_shape\Item_shape.described_element" label="WR3"/>
      </entity>
      <entity name="Component_feature_make_from_relationship" supertypes="Component_feature_relationship">
         <explicit name="reusable_feature">
            <typename name="Component_feature"/>
            <redeclaration entity-ref="Shape_element_relationship" old_name="relating"/>
         </explicit>
         <explicit name="resultant_feature">
            <typename name="Component_feature"/>
            <redeclaration entity-ref="Shape_element_relationship" old_name="related"/>
         </explicit>
      </entity>
      <entity name="Component_feature_relationship" super.expression="ONEOF (Component_feature_joint, Component_feature_make_from_relationship)" supertypes="Shape_element_relationship">
         <explicit name="relating">
            <typename name="Component_feature"/>
            <redeclaration entity-ref="Shape_element_relationship"/>
         </explicit>
         <explicit name="related">
            <typename name="Component_feature"/>
            <redeclaration entity-ref="Shape_element_relationship"/>
         </explicit>
      </entity>
      <entity name="Component_feature_to_physical_usage_view_assignment">
         <explicit name="defined_part_feature">
            <typename name="Part_feature"/>
         </explicit>
         <explicit name="assigned_design_object">
            <typename name="Component_feature"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="assigned_design_object"/>
         </unique>
         <unique label="UR2">
            <unique.attribute attribute="defined_part_feature"/>
         </unique>
      </entity>
      <entity abstract.supertype="YES" name="Component_terminal" supertypes="Component_feature">
         <explicit name="local_swappable" optional="YES">
            <builtintype type="BOOLEAN"/>
         </explicit>
         <explicit name="global_swappable" optional="YES">
            <builtintype type="BOOLEAN"/>
         </explicit>
         <explicit name="swap_code" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
   </schema>
</express>
