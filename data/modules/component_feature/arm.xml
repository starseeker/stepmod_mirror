<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:08" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="component_feature_arm schema_instance"/>
   <schema name="COMPONENT_FEATURE_ARM">
      <interface kind="use" schema="PHYSICAL_UNIT_DESIGN_VIEW_ARM"/>
      <interface kind="use" schema="SHAPE_PROPERTY_ASSIGNMENT_ARM"/>
      <type name="part_feature_or_template_feature">
         <select extensible="YES" genericentity="YES" selectitems="part_feature"/>
      </type>
      <entity name="component_feature" abstract.supertype="YES" supertypes="shape_element">
         <explicit name="definition" optional="YES">
            <typename name="part_feature_or_template_feature"/>
         </explicit>
         <explicit name="associated_component">
            <typename name="assembly_component"/>
            <redeclaration entity-ref="shape_element" old_name="containing_shape"/>
         </explicit>
         <unique label="ur1">
            <unique.attribute entity-ref="component_feature" attribute="definition"/>
            <unique.attribute entity-ref="component_feature" attribute="associated_component"/>
         </unique>
      </entity>
      <entity name="component_feature_make_from_relationship" supertypes="component_feature_relationship">
         <explicit name="reusable_feature">
            <typename name="component_feature"/>
            <redeclaration entity-ref="shape_element_relationship" old_name="relating"/>
         </explicit>
         <explicit name="resultant_feature">
            <typename name="component_feature"/>
            <redeclaration entity-ref="shape_element_relationship" old_name="related"/>
         </explicit>
      </entity>
      <entity name="component_feature_relationship" supertypes="shape_element_relationship">
         <explicit name="relating">
            <typename name="component_feature"/>
            <redeclaration entity-ref="shape_element_relationship"/>
         </explicit>
         <explicit name="related">
            <typename name="component_feature"/>
            <redeclaration entity-ref="shape_element_relationship"/>
         </explicit>
      </entity>
      <entity name="component_feature_to_physical_usage_view_assignment">
         <explicit name="defined_part_feature">
            <typename name="part_feature"/>
         </explicit>
         <explicit name="assigned_design_object">
            <typename name="component_feature"/>
         </explicit>
         <unique label="ur1">
            <unique.attribute entity-ref="component_feature_to_physical_usage_view_assignment" attribute="assigned_design_object"/>
         </unique>
         <unique label="ur2">
            <unique.attribute entity-ref="component_feature_to_physical_usage_view_assignment" attribute="defined_part_feature"/>
         </unique>
      </entity>
      <entity name="component_terminal" supertypes="component_feature">
         <explicit name="local_swappable" optional="YES">
            <builtintype type="BOOLEAN"/>
         </explicit>
         <explicit name="global_swappable" optional="YES">
            <builtintype type="BOOLEAN"/>
         </explicit>
         <explicit name="swap_code" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
   </schema>
</express>
