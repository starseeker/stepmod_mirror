<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1657">
   <application name="JSDAI" owner="LKSoft" source="component_feature_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Component_feature_arm">
      <interface kind="use" schema="Assembly_component_arm"/>
      <interface kind="use" schema="Assembly_shape_arm"/>
      <type name="cf_groupable_item">
         <select basedon="groupable_item" extensible="NO" genericentity="NO" selectitems="Component_feature"/>
      </type>
      <entity abstract.entity="NO" abstract.supertype="YES" name="Component_feature" super.expression="ONEOF (Component_feature_location_group_identification, Component_terminal)" supertypes="Shape_element">
         <explicit name="definition" optional="YES">
            <typename name="General_part_feature"/>
         </explicit>
         <explicit name="associated_definition" optional="NO">
            <typename name="Assembly_component"/>
            <redeclaration entity-ref="Shape_element"/>
         </explicit>
         <explicit name="location_group" optional="YES">
            <typename name="Component_feature_location_group_identification"/>
         </explicit>
         <derived expression="TRUE" name="product_definitional">
            <builtintype fixed="NO" type="BOOLEAN"/>
            <redeclaration entity-ref="Shape_element"/>
         </derived>
         <unique label="UR1">
            <unique.attribute attribute="definition"/>
            <unique.attribute attribute="associated_definition"/>
         </unique>
         <unique label="UR2">
            <unique.attribute attribute="SELF\Shape_element.element_name"/>
            <unique.attribute attribute="associated_definition"/>
         </unique>
         <where expression="SELF\Shape_element.associated_definition = location_group\Shape_element.associated_definition" label="WR1"/>
      <where label="IP1"/>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="YES" name="Component_feature_joint" supertypes="Component_feature_relationship Shape_element">
         <explicit name="feature_1" optional="NO">
            <typename name="Component_feature"/>
            <redeclaration entity-ref="Component_feature_relationship" old_name="relating"/>
         </explicit>
         <explicit name="feature_2" optional="NO">
            <typename name="Component_feature"/>
            <redeclaration entity-ref="Component_feature_relationship" old_name="related"/>
         </explicit>
         <explicit name="associated_definition" optional="NO">
            <typename name="Assembly_definition"/>
            <redeclaration entity-ref="Shape_element"/>
         </explicit>
         <where expression="feature_1 &lt;&gt; feature_2" label="WR1"/>
         <where expression="'PRODUCT_VIEW_DEFINITION_ARM.PRODUCT_VIEW_DEFINITION' in TYPEOF(associated_definition)" label="WR2"/>
         <where expression="associated_definition :=: feature_1\Shape_element.associated_definition" label="WR3"/>
         <where expression="associated_definition :=: feature_2\Shape_element.associated_definition" label="WR4"/>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Component_feature_location_group_identification" supertypes="Component_feature">
         <inverse attribute="location_group" entity="Component_feature" name="elements">
            <inverse.aggregate lower="0" type="SET" upper="?"/>
         </inverse>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Component_feature_make_from_relationship" supertypes="Component_feature_relationship">
         <explicit name="reusable_feature" optional="NO">
            <typename name="Component_feature"/>
            <redeclaration entity-ref="Shape_element_relationship" old_name="relating"/>
         </explicit>
         <explicit name="resultant_feature" optional="NO">
            <typename name="Component_feature"/>
            <redeclaration entity-ref="Shape_element_relationship" old_name="related"/>
         </explicit>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Component_feature_relationship" super.expression="ONEOF (Component_feature_joint, Component_feature_make_from_relationship)" supertypes="Shape_element_relationship">
         <explicit name="relating" optional="NO">
            <typename name="Component_feature"/>
            <redeclaration entity-ref="Shape_element_relationship"/>
         </explicit>
         <explicit name="related" optional="NO">
            <typename name="Component_feature"/>
            <redeclaration entity-ref="Shape_element_relationship"/>
         </explicit>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="YES" name="Component_terminal" supertypes="Component_feature">
         <explicit name="local_swappable" optional="YES">
            <builtintype fixed="NO" type="BOOLEAN"/>
         </explicit>
         <explicit name="global_swappable" optional="YES">
            <builtintype fixed="NO" type="BOOLEAN"/>
         </explicit>
         <explicit name="swap_code" optional="YES">
            <builtintype fixed="NO" type="STRING"/>
         </explicit>
      <where label="IP1"/>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Derived_component_terminal" supertypes="Component_terminal">
         <explicit name="derived_from" optional="NO">
            <typename name="General_part_feature"/>
         </explicit>
         <where expression="NOT EXISTS(SELF\Component_feature.definition)" label="WR1"/>
         <where expression="SELF\Shape_element.associated_definition.derived_from = derived_from\Shape_element.associated_definition" label="WR2"/>
      </entity>
   </schema>
</express>
