<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="component_feature_arm schema_instance"/>
   <schema name="Component_feature_arm">
      <interface kind="use" schema="Physical_unit_design_view_arm"/>
      <type name="cf_groupable_item">
         <select basedon="groupable_item" selectitems="Component_feature" genericentity="NO" extensible="NO"/>
      </type>
      <type name="feature_select">
         <select extensible="YES" genericentity="YES" selectitems="Part_feature"/>
      </type>
      <entity name="Component_feature" abstract.supertype="YES" supertypes="Shape_element" abstract.entity="NO">
         <explicit name="definition" optional="YES">
            <typename name="feature_select"/>
         </explicit>
         <explicit name="associated_component" optional="NO">
            <typename name="Assembly_component"/>
            <redeclaration entity-ref="Shape_element" old_name="containing_shape"/>
         </explicit>
         <explicit name="element_name" optional="NO">
            <builtintype type="STRING" fixed="NO"/>
            <redeclaration entity-ref="Shape_element"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="definition"/>
            <unique.attribute attribute="associated_component"/>
         </unique>
         <unique label="UR2">
            <unique.attribute attribute="element_name"/>
            <unique.attribute attribute="associated_component"/>
         </unique>
      <where label="IP1"/>
         <where label="IP2"/>
      </entity>
      <entity name="Component_feature_joint" abstract.supertype="YES" supertypes="Component_feature_relationship Shape_element" abstract.entity="NO">
         <explicit name="element_name" optional="NO">
            <builtintype type="STRING" fixed="NO"/>
            <redeclaration entity-ref="Shape_element"/>
         </explicit>
         <explicit name="feature_1" optional="NO">
            <typename name="Component_feature"/>
            <redeclaration entity-ref="Shape_element_relationship" old_name="relating"/>
         </explicit>
         <explicit name="feature_2" optional="NO">
            <typename name="Component_feature"/>
            <redeclaration entity-ref="Shape_element_relationship" old_name="related"/>
         </explicit>
         <explicit name="associated_definition" optional="NO">
            <typename name="Item_shape"/>
            <redeclaration entity-ref="Shape_element" old_name="containing_shape"/>
         </explicit>
         <where label="WR1" expression="feature_1 &lt;&gt; feature_2"/>
         <where label="WR2" expression="'PRODUCT_VIEW_DEFINITION_ARM.PRODUCT_VIEW_DEFINITION' in TYPEOF(associated_definition)"/>
         <where label="WR3" expression="SELF\Shape_element.containing_shape\Item_shape.described_element = feature_1\Shape_element.containing_shape\Item_shape.described_element"/>
      </entity>
      <entity name="Component_feature_make_from_relationship" supertypes="Component_feature_relationship" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="reusable_feature" optional="NO">
            <typename name="Component_feature"/>
            <redeclaration entity-ref="Shape_element_relationship" old_name="relating"/>
         </explicit>
         <explicit name="resultant_feature" optional="NO">
            <typename name="Component_feature"/>
            <redeclaration entity-ref="Shape_element_relationship" old_name="related"/>
         </explicit>
      </entity>
      <entity name="Component_feature_relationship" supertypes="Shape_element_relationship" super.expression="ONEOF (Component_feature_joint, Component_feature_make_from_relationship)" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="relating" optional="NO">
            <typename name="Component_feature"/>
            <redeclaration entity-ref="Shape_element_relationship"/>
         </explicit>
         <explicit name="related" optional="NO">
            <typename name="Component_feature"/>
            <redeclaration entity-ref="Shape_element_relationship"/>
         </explicit>
      </entity>
      <entity name="Component_feature_to_physical_usage_view_assignment" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="defined_part_feature" optional="NO">
            <typename name="Part_feature"/>
         </explicit>
         <explicit name="assigned_design_object" optional="NO">
            <typename name="Component_feature"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="assigned_design_object"/>
         </unique>
         <unique label="UR2">
            <unique.attribute attribute="defined_part_feature"/>
         </unique>
      </entity>
      <entity name="Component_terminal" supertypes="Component_feature" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="local_swappable" optional="YES">
            <builtintype type="BOOLEAN" fixed="NO"/>
         </explicit>
         <explicit name="global_swappable" optional="YES">
            <builtintype type="BOOLEAN" fixed="NO"/>
         </explicit>
         <explicit name="swap_code" optional="YES">
            <builtintype type="STRING" fixed="NO"/>
         </explicit>
      <where label="IP1"/>
      </entity>
   </schema>
</express>
