<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: component_feature
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Component_feature_mim.component_feature">
	A <b>component_feature</b> is a type of 
	<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.shape_aspect"/>  
	that implements the ARM concept of 
	<express_ref linkend="Component_feature:arm:Component_feature_arm.Component_feature"/>.
</ext_description>

<!-- of_shape -->
<ext_description linkend="Component_feature_mim.component_feature.of_shape">
	an inherited attribute  shall be of type
  <express_ref linkend="assembly_component:mim:Assembly_component_mim.assembly_component"/> 
	for the <b>component_feature</b>.
</ext_description>

