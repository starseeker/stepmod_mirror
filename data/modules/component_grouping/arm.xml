<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:08" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="component_grouping_arm schema_instance"/>
   <schema name="COMPONENT_GROUPING_ARM">
      <interface kind="use" schema="ASSEMBLY_STRUCTURE_ARM"/>
      <interface kind="use" schema="INTERFACE_COMPONENT_ARM"/>
      <interface kind="use" schema="PHYSICAL_UNIT_DESIGN_VIEW_ARM"/>
      <interface kind="use" schema="VALUE_WITH_UNIT_EXTENSION_ARM"/>
      <entity name="array_placement_group" abstract.supertype="YES" supertypes="assembly_group_component" super.expression="ONEOF (linear_array_placement_group_component, rectangular_array_placement_group_component)"/>
      <entity name="assembly_component_placement_link">
         <explicit name="precedent_element">
            <typename name="assembly_component"/>
         </explicit>
         <explicit name="subsequent_element">
            <typename name="assembly_component"/>
         </explicit>
         <inverse name="referenced_by" entity="linear_array_placement_group_component" attribute="element"/>
      </entity>
      <entity name="assembly_group_component" supertypes="assembly_component">
         <inverse name="components" entity="next_assembly_usage" attribute="relating_view">
            <inverse.aggregate type="SET" lower="0" upper="?"/>
         </inverse>
      </entity>
      <entity name="interfaced_group_component" supertypes="assembly_group_component">
         <explicit name="group_interface_component">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="interface_component"/>
         </explicit>
      </entity>
      <entity name="linear_array_placement_group_component" supertypes="array_placement_group">
         <explicit name="element">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="assembly_component_placement_link"/>
         </explicit>
         <explicit name="initial_node_positive_x_displacement">
            <typename name="length_data_element"/>
         </explicit>
         <explicit name="terminus_node_positive_x_displacement">
            <typename name="length_data_element"/>
         </explicit>
         <explicit name="location_uncertainty">
            <typename name="length_data_element"/>
         </explicit>
      </entity>
      <entity name="linear_array_placement_group_component_link">
         <explicit name="precedent_element">
            <typename name="linear_array_placement_group_component"/>
         </explicit>
         <explicit name="subsequent_element">
            <typename name="linear_array_placement_group_component"/>
         </explicit>
         <inverse name="referenced_by" entity="rectangular_array_placement_group_component" attribute="element"/>
      </entity>
      <entity name="rectangular_array_placement_group_component" supertypes="array_placement_group">
         <explicit name="element">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="linear_array_placement_group_component_link"/>
         </explicit>
         <explicit name="initial_row_positive_y_displacement">
            <typename name="length_data_element"/>
         </explicit>
         <explicit name="terminus_row_positive_y_displacement">
            <typename name="length_data_element"/>
         </explicit>
         <explicit name="location_uncertainty">
            <typename name="length_data_element"/>
         </explicit>
      </entity>
   </schema>
</express>
