<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: assembly_group_component
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Component_grouping_mim.assembly_group_component">
	An <b>assembly_group_component</b> is a type of 
	<express_ref linkend="Physical_unit_design_view:mim:Physical_unit_design_view_mim.assembly_component"/> 
	that implements the ARM concept of
	<express_ref linkend="Component_grouping:arm:Component_grouping_arm.Assembly_group_component"/>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: assembly_group_component_definition_placement_link
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Component_grouping_mim.assembly_group_component_definition_placement_link">
<p>
	A <b>assembly_group_component_definition_placement_link</b> is a type of 
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition"/> 
	and a  
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_relationship"/> 
	that implements the ARM concept of 
	<express_ref linkend="Component_grouping:arm:Component_grouping_arm.Assembly_component_placement_link"/>.
</p>
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Attribute: assembly_group_component_definition_placement_link.relating_product_definition
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Component_grouping_mim.assembly_group_component_definition_placement_link.relating_product_definition">
	specifies a role of the 
	<express_ref linkend="Physical_unit_design_view:mim:Physical_unit_design_view_mim.assembly_component"/>
	for the <b>assembly_group_component_definition_placement_link</b>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Attribute: assembly_group_component_definition_placement_link.related_product_definition
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Component_grouping_mim.assembly_group_component_definition_placement_link.related_product_definition">
	specifies a role of the 
	<express_ref linkend="Physical_unit_design_view:mim:Physical_unit_design_view_mim.assembly_component"/>
	for the <b>assembly_group_component_definition_placement_link</b>.
</ext_description>


<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
WR: assembly_group_component_definition_placement_link.WR1
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Component_grouping_mim.assembly_group_component_definition_placement_link.wr:WR1">
	The <b>related_product_definition</b> shall not be the <b>relating_product_definition</b>. 
</ext_description>

<!-- WR2 -->
<ext_description linkend="Component_grouping_mim.assembly_group_component_definition_placement_link.wr:WR2">
	<p>
	The graph of instances of the entity <b>assembly_group_component_definition_placement_link</b> shall not contain a cycle.
	
	</p>
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: array_placement_group
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Component_grouping_mim.array_placement_group">
<p>
	An <b>array_placement_group</b> is a type of 
	<express_ref linkend="Component_grouping:mim:Component_grouping_mim.assembly_group_component"/> 
	that implements the ARM concept of
	<express_ref linkend="Component_grouping:arm:Component_grouping_arm.Array_placement_group"/>.
	An <b>array_placement_group</b> may be one of a
	<express_ref linkend="Component_grouping:mim:Component_grouping_mim.linear_array_placement_group_component"/> 
	or a
	<express_ref linkend="Component_grouping:mim:Component_grouping_mim.rectangular_array_placement_group_component"/>.
</p>
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
