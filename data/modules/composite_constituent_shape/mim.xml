   <schema name="Composite_constituent_shape_mim">
      <interface kind="use" schema="Advanced_boundary_representation_mim"/>
      <interface kind="use" schema="Construction_geometry_mim"/>
      <interface kind="use" schema="Edge_based_wireframe_mim"/>
      <interface kind="use" schema="Faceted_boundary_representation_mim"/>
      <interface kind="use" schema="Geometrically_bounded_surface_mim"/>
      <interface kind="use" schema="Geometrically_bounded_wireframe_mim"/>
      <interface kind="use" schema="Manifold_surface_mim"/>
      <interface kind="use" schema="Shell_based_wireframe_mim"/>
