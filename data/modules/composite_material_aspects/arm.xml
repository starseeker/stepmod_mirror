<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$">
   <application name="JSDAI" owner="LKSoft" source="composite_material_aspects_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Composite_material_aspects_arm">
      <interface kind="use" schema="Part_and_zone_laminate_tables_arm"/>
      <type name="compma_material_item_select">
         <select basedon="material_item_select" extensible="YES" genericentity="YES" selectitems="Anisotropic_material Braided_assembly Isotropic_material Laminate_table Woven_assembly"/>
      </type>
      <entity name="Composite_material_identification" supertypes="Material_identification">
         <explicit name="composite_items">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="material_item_select"/>
            <redeclaration entity-ref="Material_identification" old_name="items"/>
         </explicit>
         <explicit name="composite_material_name">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="Material_identification" old_name="material_name"/>
         </explicit>
      </entity>
   </schema>
</express>
