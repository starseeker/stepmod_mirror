   <schema name="Composite_material_aspects_arm">
      <interface kind="use" schema="Part_and_zone_laminate_tables_arm"/>
      <type name="cma_material_item_select">
         <select extensible="YES" genericentity="YES" basedon="material_item_select" selectitems="Anisotropic_material Braided_assembly Isotropic_material Laminate_table Woven_assembly"/>
      </type>
      <entity name="Composite_material_identification" supertypes="Material_identification">
         <explicit name="composite_items">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="cma_material_item_select"/>
