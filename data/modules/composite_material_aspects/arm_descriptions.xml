<!-- +++++++++++ Composite_material_identification +++++++++++ -->
<ext_description linkend="Composite_material_aspects_arm.Composite_material_identification">
	A <b>Composite_material_identification</b> is a type of
	<express_ref linkend="Generic_material_aspects:arm:Generic_material_aspects_arm.Material_identification"/>.
</ext_description>

<!-- composite_items -->
<ext_description linkend="Composite_material_aspects_arm.Composite_material_identification.composite_items">
	an attribute inherited from  
	<express_ref linkend="Generic_material_aspects:arm:Generic_material_aspects_arm.Material_identification"/> 
	that specifies a <express_ref linkend="Composite_material_aspects:arm:Composite_material_aspects_arm.cma_material_item_select"/> 
	for the <b>Composite_material_identification</b>.
	There shall exist one or more
	<express_ref linkend="Composite_material_aspects:arm:Composite_material_aspects_arm.cma_material_item_select">cma_material_item_selects</express_ref> 
	for the <b>Composite_material_identification</b>.
</ext_description>

<!-- composite_material_name -->
<ext_description linkend="Composite_material_aspects_arm.Composite_material_identification.composite_material_name">
	an attribute inherited from  
	<express_ref linkend="Generic_material_aspects:arm:Generic_material_aspects_arm.Material_identification"/> 
	that specifies a STRING value. 
</ext_description>

