<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date: 2007-05-30 06:38:11 -0700 (Tr, 30 Geg 2007) $" rcs.revision="$Revision: 1067 $" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="composite_property_arm schema_instance"/>
   <schema name="Composite_property_arm">
      <interface kind="use" schema="Property_assignment_arm"/>
      <interface kind="use" schema="Group_arm"/>
      <entity name="Composite_property" supertypes="Assigned_property">
         <where label="WR1" expression="SIZEOF ( USEDIN (SELF, 'PROPERTY_ASSIGNMENT_ARM.'+&#10;	  'PROPERTY_DEFINITION_REPRESENTATION.DEFINITION') ) =0"/>
      </entity>
      <entity name="Property_composition_relationship">
         <explicit name="parent">
            <typename name="Composite_property"/>
         </explicit>
         <explicit name="child">
            <typename name="Assigned_property"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="child"/>
         </unique>
         <where label="WR1" expression="parent.described_element = child.described_element"/>
      </entity>
   </schema>
</express>
