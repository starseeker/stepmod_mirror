<!DOCTYPE ext_descriptions SYSTEM "../../../dtd/description.dtd">
<ext_descriptions module_directory="composite_property" schema_file="arm.xml" describe.selects="YES" describe.subtype_constraints="YES" rcs.date="$Date: 2007-04-03 00:47:40 -0700 (An, 03 Bal 2007) $" rcs.revision="$Revision: 218 $">
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: Composite_property_arm
   <express_ref linkend="composite_property:arm:Composite_property_arm"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<ext_description linkend="Composite_property_arm">
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: Composite_property_arm  Entity: Composite_property  
<express_ref linkend="composite_property:arm:Composite_property_arm.composite_property"/>
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Composite_property_arm.Composite_property">
	A <b>Composite_property</b> is a type of 
	<express_ref linkend="Property_assignment:arm:Property_assignment_arm.Assigned_property"/> 
	that consists of a set of 
	<express_ref linkend="property_assignment:arm:Property_assignment_arm.Property_representation"/> 
	objects.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Composite_property_arm  Entity: Composite_property  Attribute: components 
   <express_ref linkend="composite_property:arm:Composite_property_arm.Composite_property.components"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++  
<ext_description linkend="Composite_property_arm.Composite_property.components">
	the 
	<express_ref linkend="property_assignment:arm:Property_assignment_arm.Property_representation"/> 
	that is an element of the <b>composite_property</b>
</ext_description>-->

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: Composite_property_arm  Entity:   Composite_property WR:WR1
<express_ref linkend="composite_property:arm:Composite_property_arm.Composite_property.wr:WR1"/>
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Composite_property_arm.Composite_property.wr:WR1">
	<p>
	A <b>Composite_property</b> should not be used by any 
	<express_ref linkend="property_assignment:arm:Property_assignment_arm.Property_definition_representation"/>.
	</p>
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: Composite_property_arm  Entity:   Property_composition_relationship
<express_ref linkend="composite_property:arm:Composite_property_arm.Property_composition_relationship"/>
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Composite_property_arm.Property_composition_relationship">
	A <b>Property_composition_relationship</b> is a relationship between a
	<express_ref linkend="Composite_property:arm:Composite_property_arm.Composite_property"/>
	and an
	<express_ref linkend="Property_assignment:arm:Property_assignment_arm.Assigned_property"/>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: Composite_property_arm  Entity:   Property_composition_relationship		Attribute: parent
<express_ref linkend="composite_property:arm:Composite_property_arm.Property_composition_relationship.parent"/>
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Composite_property_arm.Property_composition_relationship.parent">
	specifies a role of the
	<express_ref linkend="Composite_property:arm:Composite_property_arm.Composite_property"/>
	for the <b>Property_composition_relationship</b>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: Composite_property_arm  Entity:   Property_composition_relationship		Attribute: child
<express_ref linkend="composite_property:arm:Composite_property_arm.Property_composition_relationship.child"/>
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Composite_property_arm.Property_composition_relationship.child">
	specifies a role of the
	<express_ref linkend="Property_assignment:arm:Property_assignment_arm.Assigned_property"/>
	for the <b>Property_composition_relationship</b>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: Composite_property_arm  Entity:   Property_composition_relationship		UR: UR1
<express_ref linkend="composite_property:arm:Composite_property_arm.Property_composition_relationship.ur:UR1"/>
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Composite_property_arm.Property_composition_relationship.ur:UR1">
	The <b>child</b> should be unique within a population of the <b>Property_composition_relationships</b>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: Composite_property_arm  Entity:   Property_composition_relationship		WR: WR1
<express_ref linkend="composite_property:arm:Composite_property_arm.Property_composition_relationship.wr:WR1"/>
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Composite_property_arm.Property_composition_relationship.wr:WR1">
	<p>
	A <b>parent</b> and a  <b>child</b> must share the same
	<express_ref linkend="Property_assignment:arm:Property_assignment_arm.Assigned_property.described_element"/>.
	</p>
</ext_description>

</ext_descriptions>