<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date: 2007-05-30 06:38:11 -0700 (Tr, 30 Geg 2007) $" rcs.revision="$Revision: 1067 $" description.file="mim_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="composite_property_mim schema_instance"/>
   <schema name="Composite_property_mim">
      <interface kind="use" schema="Property_assignment_mim"/>
      <interface kind="use" schema="Group_mim"/>
      <interface kind="use" schema="material_property_definition_schema">
         <interfaced.item name="property_definition_relationship"/>
      </interface>
      <type name="composite_representation_item_groupable_item">
         <select extensible="YES" genericentity="YES" basedon="groupable_item" selectitems="property_definition"/>
      </type>
      <entity name="composite_property" supertypes="property_definition">
         <where label="WR1" expression="SIZEOF ( USEDIN (SELF, 'PRODUCT_PROPERTY_REPRESENTATION_SCHEMA.'+&#10;	  'PROPERTY_DEFINITION_REPRESENTATION.DEFINITION') ) =0"/>
      </entity>
      <entity name="property_composition_relationship" supertypes="property_definition_relationship">
         <explicit name="parent">
            <typename name="composite_property"/>
            <redeclaration entity-ref="property_definition_relationship" old_name="relating_property_definition"/>
         </explicit>
         <explicit name="child">
            <typename name="property_definition"/>
            <redeclaration entity-ref="property_definition_relationship" old_name="related_property_definition"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute entity-ref="property_definition_relationship" attribute="related_property_definition"/>
         </unique>
         <where label="WR1" expression="relating_property_definition.definition = related_property_definition.definition"/>
      </entity>
   </schema>
</express>
