<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: Composite_property_mim  Entity: composite_property  
<express_ref linkend="composite_property:mim:Composite_representation_item_mim.composite_property"/>
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Composite_property_mim.composite_property">
	A <b>composite_property</b> is a type of
    <express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.property_definition"/>
    that implemenst the ARM concept of
    <express_ref linkend="Composite_property:arm:Composite_property_arm.Composite_property"/>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: Composite_property_mim  Entity: composite_property  Where: WR1 
<express_ref linkend="composite_property:mim:Composite_property_mim.composite_property.wr:WR1"/>
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Composite_property_mim.composite_property.wr:WR1">
	A <b>composite_property</b> should not be used by any 
	<express_ref linkend="product_property_representation_schema:ir_express:product_property_representation_schema.property_definition_representation"/>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: Composite_property_mim  Entity:   property_composition_relationship
<express_ref linkend="composite_property:mim:Composite_representation_item_mim.property_composition_relationship"/>
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Composite_property_mim.property_composition_relationship">
	A <b>property_composition_relationship</b> is a type of
    <express_ref linkend="material_property_definition_schema:ir_express:material_property_definition_schema.property_definition_relationship"/>
    that implemenst the ARM concept of
    <express_ref linkend="Composite_property:arm:Composite_property_arm.Property_composition_relationship"/>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: Composite_property_mim  Entity:   property_composition_relationship	Attribute: parent
<express_ref linkend="composite_property:mim:Composite_representation_item_mim.property_composition_relationship.parent"/>
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Composite_property_mim.property_composition_relationship.parent">
	specifies a role of the
	<express_ref linkend="Composite_property:mim:Composite_property_mim.composite_property"/>
	for the <b>property_composition_relationship</b>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: Composite_property_mim  Entity:   property_composition_relationship	Attribute: child
<express_ref linkend="composite_property:mim:Composite_representation_item_mim.property_composition_relationship.child"/>
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Composite_property_mim.property_composition_relationship.child">
	specifies a role of the
	<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.property_definition"/>
	for the <b>property_composition_relationship</b>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: Composite_property_mim  Entity:   property_composition_relationship	UR:UR1 
<express_ref linkend="composite_property:mim:Composite_representation_item_mim.property_composition_relationship.ur:UR1"/>
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Composite_property_mim.property_composition_relationship.ur:UR1">
	The 
	<express_ref linkend="material_property_definition_schema:ir_express:material_property_definition_schema.property_definition_relationship.related_property_definition"/>
	should be unique within a population of the <b>property_composition_relationships</b>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: Composite_property_mim  Entity:   property_composition_relationship	WR: WR1
<express_ref linkend="composite_property:mim:Composite_representation_item_mim.property_composition_relationship.wr:WR1"/>
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Composite_property_mim.property_composition_relationship.wr:WR1">
	<p>
	A 
	<express_ref linkend="material_property_definition_schema:ir_express:material_property_definition_schema.property_definition_relationship.relating_property_definition"/>
	and a  
	<express_ref linkend="material_property_definition_schema:ir_express:material_property_definition_schema.property_definition_relationship.related_property_definition"/>
	must share the same
	<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.property_definition.definition"/>.
	</p>
</ext_description>

