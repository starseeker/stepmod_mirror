<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="mim_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml2.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.38"
    source="../data/modules/composite_representation_item/mim.exp"/>

  <schema name="Composite_representation_item_mim">

    <interface
      kind="use"
      schema="Foundation_representation_mim">
    </interface>
    <interface
      kind="use"
      schema="Group_mim">
    </interface>
    <interface
      kind="use"
      schema="Property_assignment_mim">
    </interface>
    <type name="composite_representation_item_groupable_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="groupable_item"
        selectitems="property_definition">
      </select>
    </type>

    <entity
      name="composite_representation_item"
      supertypes="representation_item group">
      <where
        label="WR1"
        expression="SIZEOF(QUERY(x &lt;* USEDIN(SELF,'GROUP_MIM.' + 'APPLIED_GROUP_ASSIGNMENT.ITEMS') | NOT('PRODUCT_PROPERTY_DEFINITION_SCHEMA.PROPERTY_DEFINITION' IN TYPEOF(x)))) = 0">
      </where>
    </entity>

  </schema>
</express>
