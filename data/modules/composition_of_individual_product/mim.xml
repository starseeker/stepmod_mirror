<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<?xml-stylesheet type="text/xsl"
href="../../../xsl/express.xsl" ?>
<express language_version="2">
  <schema name="Composition_of_individual_product_mim">
  <interface kind="use" schema="Product_as_individual_mim"/>
  <interface kind="use" schema="Product_version_relationship_mim"/>

  <entity name="composition_of_individual_product"
          supertypes="product_definition_formation_relationship">
  <description>
  A <b>composition_of_individual_product</b> is a type of <b>product_definition_formation_relationship</b> that is a
<express_ref linkend="composition_of_individual_product:arm:Composition_of_individual_product_arm.Composition_of_individual_product">
Composition of individual product</express_ref>.
</description>
  <explicit name="related_product_definition_formation">
  <redeclaration entity-ref="product_definition_formation_relationship"/>
  <typename name="product_as_individual" />
  <description>
  a <b>product_as_individual</b> that is the part.
  </description>
</explicit>
  <explicit name="relating_product_definition_formation">
     <redeclaration entity-ref="product_definition_formation_relationship"/>
     <typename name="product_as_individual" />
   <description>
  a <b>product_as_individual</b> that is the whole.
  </description>
  </explicit>
</entity>
</schema>
</express>

