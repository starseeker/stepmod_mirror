  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description
linkend="Condition_evaluation_arm.Condition_evaluation.description">
  A text based description of the <b>Condition_evaluation</b>.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Condition_evaluation_arm  Entity: Condition_evaluation  Attribute: result 
   <express_ref linkend="condition_evaluation:arm:Condition_evaluation_arm.Condition_evaluation.result"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description
  linkend="Condition_evaluation_arm.Condition_evaluation.result">
  The result of the evaluation of the 
  <express_ref linkend="condition:arm:Condition_arm.Condition"/>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Condition_evaluation_arm  Entity: Condition_evaluation  Attribute: condition 
