(*
$Id: mim.exp,v 1.8 2003/02/20 18:04:52 shiangyulee Exp $
ISO TC184/SC4/WG12 N1431 - ISO/CD-TS 10303-1254 Condition evaluation - EXPRESS MIM
*) 
SCHEMA Condition_evaluation_mim; 

USE FROM Condition_criteria_mim;    -- ISO/CD-TS 10303-1296

USE FROM Condition_mim;    -- ISO/CD-TS 10303-1253

USE FROM Expression_applications_mim;    -- ISO/CD-TS 10303-1273

REFERENCE FROM expression_extensions_schema;    -- ISO 10303-57

REFERENCE FROM logical_expressions_schema;    -- ISO/CD- 10303-57


ENTITY condition_assertion
  SUBTYPE OF (evaluation_record);
END_ENTITY;

ENTITY criterion_assertion
  SUBTYPE OF (evaluation_record);
END_ENTITY;

ENTITY evaluation_record
  SUPERTYPE OF (ONEOF (condition_assertion,
                       criterion_assertion,
                       parameter_record))
  SUBTYPE OF (literal_assignment);
END_ENTITY;

ENTITY parameter_record
  SUBTYPE OF (evaluation_record);
END_ENTITY;

END_SCHEMA;  -- Condition_evaluation_mim
