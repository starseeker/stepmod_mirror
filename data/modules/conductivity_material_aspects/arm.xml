<?xml version="1.0" encoding="utf-8"?><?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express
  SYSTEM "../../../dtd/express.dtd">
<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="">
   <application name="JSDAI" owner="LKSoft" source="conductivity_material_aspects_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Conductivity_material_aspects_arm">
      <interface kind="use" schema="Classification_assignment_arm"/>
      <interface kind="use" schema="Generic_material_aspects_arm"/>
      <interface kind="use" schema="Part_view_definition_arm"/>
      <interface kind="reference" schema="Support_resource_arm">
         <interfaced.item name="bag_to_set"/>
      </interface>
      <type name="cma_classification_item">
         <select basedon="classification_item" extensible="YES" genericentity="YES" selectitems="Material_identification_with_conductivity_classification"/>
      </type>
      <type name="cma_material_item_select">
         <select basedon="material_item_select" extensible="YES" genericentity="YES" selectitems="Part_view_definition"/>
      </type>
      <entity abstract.supertype="YES" name="Conductivity_material_property_class" super.expression="ONEOF (Material_electrical_conductivity_class, Material_thermal_conductivity_class)" supertypes="Class">
         <where expression="SELF\Class.name IN &#xA;       ['conductive', 'non conductive', 'resistive', 'semi conductive', 'super conductive']" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Class.description)" label="WR2"/>
      </entity>
      <entity name="Material_electrical_conductivity_class" supertypes="Conductivity_material_property_class"/>
      <entity name="Material_identification_with_conductivity_classification" supertypes="Material_identification">
         <derived expression="get_cond_class(SELF,'MATERIAL_ELECTRICAL_CONDUCTIVITY_CLASS')" name="electrical_conductivity_classification">
            <builtintype type="STRING"/>
         </derived>
         <derived expression="get_cond_class(SELF,'RELATIVE_PERMEABILITY_MATERIAL_PROPERTY_CLASS')" name="magnetic_permeability_classification">
            <builtintype type="STRING"/>
         </derived>
         <derived expression="get_cond_class(SELF,'RELATIVE_OPTICAL_INSERTION_LOSS_MATERIAL_PROPERTY_CLASS')" name="optical_insertion_loss_classification">
            <builtintype type="STRING"/>
         </derived>
         <derived expression="get_cond_class(SELF,'MATERIAL_THERMAL_CONDUCTIVITY_CLASS')" name="thermal_conductivity_classification">
            <builtintype type="STRING"/>
         </derived>
         <derived expression="get_cond_class(SELF,'RELATIVE_PERMITTIVITY_MATERIAL_PROPERTY_CLASS')" name="dielectric_permittivity_classification">
            <builtintype type="STRING"/>
         </derived>
         <where expression="(SIZEOF(QUERY(ca &lt;* USEDIN(SELF,&#xA;&#x9; &#x9;'CLASSIFICATION_ASSIGNMENT_ARM.' + &#xA;&#x9; &#x9;'CLASSIFICATION_ASSIGNMENT.' + 'ITEMS') |&#xA;&#x9; &#x9;('CONDUCTIVITY_MATERIAL_ASPECTS_ARM.'+&#xA;&#x9; &#x9;'MATERIAL_ELECTRICAL_CONDUCTIVITY_CLASS' IN TYPEOF (ca.assigned_class)))) = 1)" label="WR1"/>
         <where expression="(SIZEOF(QUERY(ca &lt;* USEDIN(SELF,&#xA;&#x9; &#x9;'CLASSIFICATION_ASSIGNMENT_ARM.' + &#xA;&#x9; &#x9;'CLASSIFICATION_ASSIGNMENT.' + 'ITEMS') |&#xA;&#x9; &#x9;('CONDUCTIVITY_MATERIAL_ASPECTS_ARM.'+&#xA;&#x9; &#x9;'MATERIAL_THERMAL_CONDUCTIVITY_CLASS' IN TYPEOF (ca.assigned_class)))) &lt;= 1)" label="WR2"/>
         <where expression="(SIZEOF(QUERY(ca &lt;* USEDIN(SELF,&#xA;&#x9; &#x9;'CLASSIFICATION_ASSIGNMENT_ARM.' + &#xA;&#x9; &#x9;'CLASSIFICATION_ASSIGNMENT.' + 'ITEMS') |&#xA;&#x9; &#x9;('CONDUCTIVITY_MATERIAL_ASPECTS_ARM.'+&#xA;&#x9; &#x9;'RELATIVE_PERMEABILITY_MATERIAL_PROPERTY_CLASS' IN TYPEOF (ca.assigned_class)))) &lt;= 1)" label="WR3"/>
         <where expression="(SIZEOF(QUERY(ca &lt;* USEDIN(SELF,&#xA;&#x9; &#x9;'CLASSIFICATION_ASSIGNMENT_ARM.' + &#xA;&#x9; &#x9;'CLASSIFICATION_ASSIGNMENT.' + 'ITEMS') |&#xA;&#x9; &#x9;('CONDUCTIVITY_MATERIAL_ASPECTS_ARM.'+&#xA;&#x9; &#x9;'RELATIVE_PERMITTIVITY_MATERIAL_PROPERTY_CLASS' IN TYPEOF (ca.assigned_class)))) &lt;= 1)" label="WR4"/>
         <where expression="(SIZEOF(QUERY(ca &lt;* USEDIN(SELF,&#xA;&#x9; &#x9;'CLASSIFICATION_ASSIGNMENT_ARM.' + &#xA;&#x9; &#x9;'CLASSIFICATION_ASSIGNMENT.' + 'ITEMS') |&#xA;&#x9; &#x9;('CONDUCTIVITY_MATERIAL_ASPECTS_ARM.'+&#xA;&#x9; &#x9;'RELATIVE_OPTICAL_INSERTION_LOSS_MATERIAL_PROPERTY_CLASS' IN TYPEOF (ca.assigned_class)))) &lt;= 1)" label="WR5"/>
      </entity>
      <entity name="Material_thermal_conductivity_class" supertypes="Conductivity_material_property_class"/>
      <entity name="Relative_optical_insertion_loss_material_property_class" supertypes="Class">
         <where expression="SELF\Class.name IN &#xA;       ['vacuum', 'very low loss', 'low loss', 'medium loss', 'high loss']" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Class.description)" label="WR2"/>
      </entity>
      <entity name="Relative_permeability_material_property_class" supertypes="Class">
         <where expression="SELF\Class.name IN &#xA;       ['free space permeabilty', 'low permeability', 'medium permeability', 'highly permeable']" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Class.description)" label="WR2"/>
      </entity>
      <entity name="Relative_permittivity_material_property_class" supertypes="Class">
         <where expression="SELF\Class.name IN &#xA;       ['vacuum permittivity', 'low permittivity', 'medium permittivity', 'high permittivity']" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Class.description)" label="WR2"/>
      </entity>
      <subtype.constraint entity="Class" name="cma_class_subtypes" super.expression="ONEOF (Conductivity_material_property_class, Relative_permeability_material_property_class, Relative_optical_insertion_loss_material_property_class, Relative_permittivity_material_property_class)"/>
      <function name="get_cond_class">
         <parameter name="input1">
            <typename name="Material_identification_with_conductivity_classification"/>
         </parameter>
         <parameter name="input2">
            <builtintype type="STRING"/>
         </parameter>
         <builtintype type="STRING"/>
         <algorithm>LOCAL
  car : SET OF Classification_assignment := [];
 END_LOCAL;
  car := QUERY(ca &lt;* bag_to_set(USEDIN(input1,
	 	'CLASSIFICATION_ASSIGNMENT_ARM.' + 
	 	'CLASSIFICATION_ASSIGNMENT.' + 'ITEMS')) |
	 	('CONDUCTIVITY_MATERIAL_ASPECTS_ARM.'+
	 	input2 IN TYPEOF (ca.assigned_class)));
 IF SIZEOF(car) = 1
  THEN
    RETURN(car[1].assigned_class\class.name);
  ELSE
    RETURN('?');
 END_IF;</algorithm>
      </function>
   </schema>
</express>