<!-- +++++++++++++++ Relative_optical_insertion_loss_material_property_class +++++++++++++++++++++++++ -->
<ext_description linkend="Conductivity_material_aspects_arm.Relative_optical_insertion_loss_material_property_class">
	A <b>Relative_optical_insertion_loss_material_property_class</b> is a type of a 
	<express_ref linkend="Class:arm:Class_arm.Class"/>. 
</ext_description>

<!-- WR1 -->
<ext_description linkend="Conductivity_material_aspects_arm.Relative_optical_insertion_loss_material_property_class.wr:WR1">
<p>
	The
	<express_ref linkend="Class:arm:Class_arm.Class.name"/> 
	attribute inherited from
	<express_ref linkend="Class:arm:Class_arm.Class"/>
	shall be set to one of: 'vacuum', 'very low loss', 'low loss', 'medium loss', or 'high loss'.
</p>
</ext_description>

<!-- WR2 -->
<ext_description linkend="Conductivity_material_aspects_arm.Relative_optical_insertion_loss_material_property_class.wr:WR2">
	The
	<express_ref linkend="Class:arm:Class_arm.Class.description"/>
	shall not be populated.
</ext_description>

