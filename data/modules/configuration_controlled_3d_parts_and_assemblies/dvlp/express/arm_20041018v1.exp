G:\abf\Data\stepmod\ballots\ballots\configuration_controlled_3d_parts_and_assemblies\express\arm\Constructive_solid_geometry_3d_arm.exp
   ------------------------------------------------------------
*)

(* $Id: arm.exp,v 1.11 2002/08/05 07:15:15 goset1 Exp $
 ISO/TC184/SC4 WG12N1261  ISO/TS 10303-1068 Constructive_solid_geometry_3d  - EXPRESS ARM *)

SCHEMA Constructive_solid_geometry_3d_arm;

USE FROM Elemental_geometric_shape_arm;

TYPE boolean_operand_3d = SELECT 
  (Boolean_result_3d,
   Csg_item_3d,
   Half_space_3d,
   Solid_model);
END_TYPE;

TYPE boolean_operator_3d = ENUMERATION OF 
  (union,
   difference,
   intersection);
END_TYPE;

TYPE csg_primitive_3d = EXTENSIBLE ENUMERATION OF 
  (block,
   convex_hexahedron,
   cyclide_segment_solid,
   eccentric_cone,
   ellipsoid,
   faceted_primitive,
   rectangular_pyramid,
   right_angular_wedge,
   right_circular_cone,
   right_circular_cylinder,
   sphere,
   tetrahedron,
   torus);
END_TYPE;

TYPE csg_select_3d = SELECT 
  (Boolean_result_3d,
   Csg_item_3d);
END_TYPE;

TYPE swept_area_solid_type = EXTENSIBLE ENUMERATION OF 
  (revolved_area_solid,
   surface_curve_swept_area_solid,
   extruded_area_solid);
END_TYPE;

TYPE swept_face_solid_type = EXTENSIBLE ENUMERATION OF 
  (revolved_face_solid,
   surface_curve_swept_face_solid,
   extruded_face_solid);
END_TYPE;

ENTITY Boolean_result_3d
SUBTYPE OF (Detailed_geometric_model_element);
  operator : boolean_operator_3d;
  first_operand : boolean_operand_3d;
  second_operand : boolean_operand_3d;
END_ENTITY;

ENTITY Csg_item_3d
SUBTYPE OF (Detailed_geometric_model_element);
  kind : csg_primitive_3d;
END_ENTITY;

ENTITY Csg_model
SUBTYPE OF (Solid_model);
  tree_root_expression : csg_select_3d;
END_ENTITY;

ENTITY Half_space_3d
SUBTYPE OF (Detailed_geometric_model_element);
END_ENTITY;

ENTITY Manifold_solid_brep
SUBTYPE OF (Solid_model);
  faceted : BOOLEAN;
END_ENTITY;

ENTITY Solid_model
ABSTRACT SUPERTYPE OF (ONEOF(Csg_model, Manifold_solid_brep, Solid_replica, Swept_area_solid, Swept_face_solid))
SUBTYPE OF (Detailed_geometric_model_element);
END_ENTITY;

ENTITY Solid_replica
SUBTYPE OF (Solid_model);
  parent_solid : Solid_model;
  transformation : Cartesian_transformation_3d;
END_ENTITY;

ENTITY Swept_area_solid
SUBTYPE OF (Solid_model);
  kind : swept_area_solid_type;
END_ENTITY;

ENTITY Swept_face_solid
SUBTYPE OF (Solid_model);
  kind : swept_face_solid_type;
END_ENTITY;

END_SCHEMA;



(*
   ------------------------------------------------------------
G:\abf\Data\stepmod\ballots\ballots\configuration_controlled_3d_parts_and_assemblies\express\arm\Curve_appearance_arm.exp
   ------------------------------------------------------------
*)

(*
ISO TC184/SC4/WG12 N672 - ISO/TS 10303-1003 Curve appearance - EXPRESS ARM
*) 


SCHEMA Curve_appearance_arm; 

USE FROM Colour_arm;    -- ISO/TS 10303-1002

USE FROM Elemental_geometric_shape_arm;    -- ISO/TS 10303-1004


TYPE curve_appearance_select = SELECT
   (Curve_appearance, 
    Point_appearance, 
    Vector_appearance);
END_TYPE; 

ENTITY Curve_appearance;
  colour : Colour;
  font : Curve_font;
  width : REAL;
END_ENTITY;

ENTITY Curve_font
  SUPERTYPE OF (ONEOF (Externally_defined_curve_font,
                       Predefined_curve_font,
                       User_defined_curve_font));
  curve_font_name : STRING;
END_ENTITY;

ENTITY Curve_font_pattern;
  on_segment : REAL;
  off_segment : REAL;
END_ENTITY;

ENTITY Externally_defined_curve_font
  SUBTYPE OF (Curve_font);
  curve_font_reference : STRING;
END_ENTITY;

ENTITY Externally_defined_marker
  SUBTYPE OF (Marker);
  marker_reference : STRING;
END_ENTITY;

ENTITY Externally_defined_terminator
  SUBTYPE OF (Terminator);
  terminator_reference : STRING;
END_ENTITY;

ENTITY Marker;
  marker_name : STRING;
END_ENTITY;

ENTITY Point_appearance;
  marker : Marker;
  colour : Colour;
  size : REAL;
END_ENTITY;

ENTITY Predefined_curve_font
  SUBTYPE OF (Curve_font);
WHERE
  WR1: SELF.curve_font_name IN ['continuous', 'chain', 'chain double dash', 'dashed', 'dotted'];
END_ENTITY;

ENTITY Predefined_marker
  SUBTYPE OF (Marker);
WHERE
  WR1: SELF.marker_name IN ['asterisk', 'circle', 'dot', 'plus', 'square', 'triangle', 'x'];
END_ENTITY;

ENTITY Predefined_terminator
  SUBTYPE OF (Terminator);
WHERE
  WR1: SELF.terminator_name IN ['blanked arrow', 'blanked box', 'dimension origin', 'filled arrow', 'filled box', 'filled dot', 'integral symbol', 'open arrow', 'slash', 'unfilled arrow'] ;
END_ENTITY;

ENTITY Terminator;
  terminator_name : STRING;
END_ENTITY;

ENTITY User_defined_curve_font
  SUBTYPE OF (Curve_font);
  pattern : SET[1:?] OF Curve_font_pattern;
  segment_symbol : Geometric_model;
END_ENTITY;

ENTITY User_defined_marker
  SUBTYPE OF (Marker);
  symbol : Geometric_model;
END_ENTITY;

ENTITY User_defined_terminator
  SUBTYPE OF (Terminator);
  symbol : Geometric_model;
END_ENTITY;

ENTITY Vector_appearance
  SUBTYPE OF (Curve_appearance);
  terminator_type : Terminator;
END_ENTITY;

END_SCHEMA;  -- Curve_appearance_arm


