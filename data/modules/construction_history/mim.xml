<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.22 2002/08/14 06:43:41 goset1 Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="mim_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.22"
    source="../data/modules/construction_history/mim.exp"/>

  <schema name="construction_history_mim">
    <interface
      kind="use"
      schema="constructive_solid_geometry_3d_mim">
    </interface>

    <interface
      kind="use"
      schema="measure_representation_mim">
    </interface>

    <interface
      kind="use"
      schema="feature_based_process_planning">
      <interfaced.item
        name="shape_defining_relationship"/>
      <interfaced.item
        name="shape_representation_with_parameters"/>
    </interface>

    <interface
      kind="use"
      schema="procedural_model_schema">
    </interface>

    <interface
      kind="use"
      schema="procedural_shape_model_schema">
    </interface>

    <entity
      name="transition_feature_solid"
      abstract.supertype="YES"
      super.expression="(edge_blend)"
      supertypes="shape_aspect solid_model selected_geometric_element">
    </entity>

    <entity
      name="edge_blend"
      abstract.supertype="YES"
      super.expression="(edge_blend_fillet, edge_blend_round, edge_blend_chamfer)"
      supertypes="transition_feature_solid">
    </entity>

    <entity
      name="edge_blend_fillet"
      super.expression="(edge_blend_fillet_constant_radius)">
      <explicit
        name="SUBTYPE">
        <typename
          name="edge_blend"/>
      </explicit>
    </entity>

    <entity
      name="edge_blend_fillet_constant_radius"
      supertypes="edge_blend_fillet">
    </entity>

    <entity
      name="edge_blend_round"
      super.expression="(edge_blend_round_constant_radius)">
      <explicit
        name="SUBTYPE">
        <typename
          name="edge_blend"/>
      </explicit>
    </entity>

    <entity
      name="edge_blend_round_constant_radius"
      supertypes="edge_blend_round">
    </entity>

    <entity
      name="edge_blend_chamfer"
      supertypes="edge_blend">
    </entity>

  </schema>

</express>
