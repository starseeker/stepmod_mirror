<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.17 2002/07/09 14:29:45 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.17"
    source="../data/modules/constructive_solid_geometry_3d/arm.exp"/>

  <schema name="Constructive_solid_geometry_3d_arm">
    <interface
      kind="use"
      schema="Advanced_boundary_representation_arm">
    </interface>

    <interface
      kind="use"
      schema="Elemental_geometric_shape_arm">
    </interface>

    <interface
      kind="use"
      schema="Solid_model_arm">
    </interface>

    <type name="boolean_operand_3d">
      <select
        selectitems="Boolean_result_3d csg_primitive_3d Half_space_solid Solid_model">
      </select>

    </type>
    <type name="boolean_operator_3d">
      <enumeration
        items="union difference intersection">
      </enumeration>


    </type>
    <type name="csg_primitive_3d">
      <select
        selectitems="Block Cyclide_segment_solid Eccentric_cone Ellipsoid Faceted_primitive Rectangular_pyramid Right_angular_wedge Right_circular_cone Right_circular_cylinder Sphere Torus">
      </select>

    </type>
    <type name="csg_select_3d">
      <select
        selectitems="Boolean_result_3d csg_primitive_3d">
      </select>

    </type>
    <entity
      name="Block"
      supertypes="Detailed_geometric_model_element">
    </entity>

    <entity
      name="Convex_hexahedron"
      supertypes="Faceted_primitive">
    </entity>

    <entity
      name="Csg_shape_representation_3d"
      supertypes="Csg_shape_representation">
    </entity>

    <entity
      name="Cyclide_segment_solid"
      supertypes="Detailed_geometric_model_element">
    </entity>

    <entity
      name="Eccentric_cone"
      supertypes="Detailed_geometric_model_element">
    </entity>

    <entity
      name="Ellipsoid"
      supertypes="Detailed_geometric_model_element">
    </entity>

    <entity
      name="Faceted_primitive"
      supertypes="Detailed_geometric_model_element">
    </entity>

    <entity
      name="Half_space_solid"
      supertypes="Detailed_geometric_model_element">
    </entity>

    <entity
      name="Rectangular_pyramid"
      supertypes="Detailed_geometric_model_element">
    </entity>

    <entity
      name="Right_angular_wedge"
      supertypes="Detailed_geometric_model_element">
    </entity>

    <entity
      name="Right_circular_cone"
      supertypes="Detailed_geometric_model_element">
    </entity>

    <entity
      name="Right_circular_cylinder"
      supertypes="Detailed_geometric_model_element">
    </entity>

    <entity
      name="Sphere"
      supertypes="Detailed_geometric_model_element">
    </entity>

    <entity
      name="Tetrahedron"
      supertypes="Faceted_primitive">
    </entity>

    <entity
      name="Torus"
      supertypes="Detailed_geometric_model_element">
    </entity>

    <entity
      name="Boolean_result_3d"
      supertypes="Detailed_geometric_model_element">
      <explicit
        name="operator">
        <typename
          name="boolean_operator_3d"/>
      </explicit>
      <explicit
        name="first_operand">
        <typename
          name="boolean_operand_3d"/>
      </explicit>
      <explicit
        name="second_operand">
        <typename
          name="boolean_operand_3d"/>
      </explicit>
    </entity>

    <entity
      name="Csg_model"
      supertypes="Solid_model">
      <explicit
        name="tree_root_expression">
        <typename
          name="csg_select_3d"/>
      </explicit>
    </entity>

    <entity
      name="Csg_shape_representation"
      supertypes="Geometric_model">
      <where
        label="WR1"
        expression="(SIZEOF(QUERY ( it &lt;* SELF.items | 	((SIZEOF([ 'CONSTRUCTIVE_SOLID_GEOMETRY_3D_ARM.CSG_MODEL', 	'CONSTRUCTIVE_SOLID_GEOMETRY_3D_ARM.TEMPLATE_INSTANCE'] * TYPEOF(it)) &lt;&gt; 1)) )) = 0)">
      </where>
      <where
        label="WR2"
        expression="SELF\Representation.context_of_items\Geometric_coordinate_space.dimension_count = 3">
      </where>
    </entity>

  </schema>

</express>
