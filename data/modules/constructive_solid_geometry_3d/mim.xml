<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.6"
    source="../data/modules/constructive_solid_geometry_3d/mim.exp"/>

  <schema name="Constructive_solid_geometry_3d_mim">
    <interface
      kind="use"
      schema="Constructive_solid_geometry_mim">
    </interface>

    <interface
      kind="use"
      schema="Boundary_representation_model_mim">
    </interface>

    <interface
      kind="use"
      schema="Faceted_boundary_representation_model_mim">
    </interface>

    <interface
      kind="use"
      schema="aic_csg">
    </interface>

    <interface
      kind="use"
      schema="geometric_model_schema">
      <interfaced.item
        name="block"/>
      <interfaced.item
        name="ellipsoid"/>
      <interfaced.item
        name="sphere"/>
      <interfaced.item
        name="torus"/>
      <interfaced.item
        name="eccentric_cone"/>
      <interfaced.item
        name="right_circular_cylinder"/>
      <interfaced.item
        name="right_angular_wedge"/>
      <interfaced.item
        name="rectangular_pyramid"/>
      <interfaced.item
        name="right_circular_cone"/>
      <interfaced.item
        name="cyclide_segment_solid"/>
      <interfaced.item
        name="faceted_primitive"/>
      <interfaced.item
        name="tetrahedron"/>
      <interfaced.item
        name="convex_hexahedron"/>
      <interfaced.item
        name="half_space_solid"/>
    </interface>

  </schema>

</express>
