<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.0 2001-11-15 11:24:44+00 rob Exp rob $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.0"
    source="../data/modules/contract/arm.exp"/>

  <schema name="Contract_arm">
    <interface
      kind="use"
      schema="Approval_arm">
    </interface>

    <interface
      kind="use"
      schema="Date_time_assignment_arm">
    </interface>

    <interface
      kind="use"
      schema="Person_organisation_assignment_arm">
    </interface>

    <type name="contract_item">
      <select
        extensible="YES"
        selectitems="">
      </select>

    </type>
    <type name="contract_organisation_or_person_in_organisation_item">
      <select
        basedon="organisation_or_person_in_organisation_item"
        selectitems="Contract">
      </select>

    </type>
    <type name="contract_date_or_date_time_item">
      <select
        basedon="date_or_date_time_item"
        selectitems="Contract">
      </select>

    </type>
    <type name="contract_approval_item">
      <select
        basedon="approval_item"
        selectitems="Contract">
      </select>

    </type>
    <entity
      name="Contract">
      <explicit
        name="id">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="purpose">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="type">
        <builtintype
          type="STRING"/>
      </explicit>
      <inverse
        name="identifier"
        entity="Organisation_or_person_in_organisation_assignment"
        attribute="items">
        <inverse.aggregate
          type="SET"
          lower="0"
          upper="1"/>
      </inverse>
      <inverse
        name="contractor"
        entity="Organisation_or_person_in_organisation_assignment"
        attribute="items">
        <inverse.aggregate
          type="SET"
          lower="1"
          upper="?"/>
      </inverse>
      <inverse
        name="contract_date"
        entity="Date_or_date_time_assignment"
        attribute="items">
        <inverse.aggregate
          type="SET"
          lower="0"
          upper="1"/>
      </inverse>
      <inverse
        name="approval"
        entity="Approval_assignment"
        attribute="items">
        <inverse.aggregate
          type="SET"
          lower="1"
          upper="?"/>
      </inverse>
    </entity>

    <entity
      name="Contract_assignment">
      <explicit
        name="assigned_contract">
        <typename
          name="Contract"/>
      </explicit>
      <explicit
        name="items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="contract_item"/>
      </explicit>
    </entity>

  </schema>

</express>
