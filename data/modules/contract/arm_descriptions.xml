<ext_descriptions module_directory="contract" schema_file="arm.xml" rcs.date="$Date: 2002/05/20 17:40:48 $" rcs.revision="$Revision: 1.5 $">


  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Contract_arm 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
   <ext_description linkend="Contract_arm"/>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Contract_arm  Type: contract_item 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Contract_arm.contract_item">This select is a place holder for assignments of contract to elements of product data.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Contract_arm  Type: contract_organization_or_person_in_organization_item 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Contract_arm.contract_organization_or_person_in_organization_item">This select extends the assignments of an organization or a person in an organization to <b>Contract</b> instances.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Contract_arm  Type: contract_date_or_date_time_item 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Contract_arm.contract_date_or_date_time_item">This select extends the assignments of a date or a date and time to <b>Contract</b> instances.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Contract_arm  Type: contract_approval_item 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Contract_arm.contract_approval_item">This select extends an approval_assignment to apply to <b>Contract</b> instances.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Contract_arm  Entity: Contract 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Contract_arm.Contract">A <b>Contract</b> is an agreement to perform some action.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Contract_arm  Entity: Contract  Attribute: id 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Contract_arm.Contract.id">the identification of the contract assigned by the organization</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Contract_arm  Entity: Contract  Attribute: purpose 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Contract_arm.Contract.purpose"> an informal description of the reason for the contract.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Contract_arm  Entity: Contract  Attribute: classification 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Contract_arm.Contract.classification"> specifies the kind of information a contract conveys.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Contract_arm  Entity: Contract  Attribute: identifier 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Contract_arm.Contract.identifier">the optional organization (or person in an organization) that assigned and owns the contract id.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Contract_arm  Entity: Contract  Attribute: contractor 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Contract_arm.Contract.contractor">the optional organizations (or persons in organizations) that performs work required by the contract.</ext_description>
 
 <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Contract_arm  Entity: Contract  Attribute: contract_date 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
  <ext_description linkend="Contract_arm.Contract.contract_date">the date which is assigned to the contract.</ext_description>

  <!-- 
   ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Contract_arm  Entity: Contract  Attribute: approval 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Contract_arm.Contract.approval">the optional approvals for the contract.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Contract_arm  Entity: Contract_assignment 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Contract_arm.Contract_assignment">A <b>Contract_assignment</b> assigns a Contract to product data.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Contract_arm  Entity: Contract_assignment  Attribute: assigned_contract 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Contract_arm.Contract_assignment.assigned_contract">the <b>Contract</b> to be assigned to product data.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Contract_arm  Entity: Contract_assignment  Attribute: items 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Contract_arm.Contract_assignment.items">the product data to which the <b>Contract</b> is assigned.</ext_description>
</ext_descriptions>