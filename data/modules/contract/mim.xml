<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/01/16 14:45:44 $"
  rcs.revision="$Revision: 1.3 $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.6"
    source="../data/modules/contract/mim.exp"/>

  <schema name="Contract_mim">
    <interface
      kind="use"
      schema="Approval_mim">
    </interface>

    <interface
      kind="use"
      schema="Date_time_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="Person_organisation_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="contract_schema">
      <interfaced.item
        name="contract"/>
      <interfaced.item
        name="contract_type"/>
    </interface>

    <interface
      kind="use"
      schema="management_resources_schema">
      <interfaced.item
        name="contract_assignment"/>
    </interface>

    <type name="contract_item">
      <description>A <b>contract_item</b> specifies those objects to which a <b>contract</b> may be assigned.</description>
      <select
        extensible="YES"
        selectitems="">
      </select>

    </type>
    <type name="contract_approval_item">
      <description>A <b>contract_approval_item</b> extends approval assignment to <b>contract</b> instances.</description>
      <select
        basedon="approval_item"
        selectitems="contract">
      </select>

    </type>
    <type name="contract_date_item">
      <description>A <b>contract_date_item</b> extends date assignment to <b>contract</b> instances.</description>
      <select
        basedon="date_item"
        selectitems="contract">
      </select>

    </type>
    <type name="contract_date_and_time_item">
      <description>A <b>contract_date_and_time_item</b> extends date and time assignment to <b>contract</b> instances.</description>
      <select
        basedon="date_and_time_item"
        selectitems="contract">
      </select>

    </type>
    <type name="contract_organization_item">
      <description>An <b>contract_organization_item</b> extends organization assignment to <b>contract</b> instances.</description>
      <select
        basedon="organization_item"
        selectitems="contract">
      </select>

    </type>
    <type name="contract_person_and_organization_item">
      <description>A <b>contract_person_and_organization_item</b> extends person and organization assignment to <b>contract</b> instances.</description>
      <select
        basedon="person_and_organization_item"
        selectitems="contract">
      </select>

    </type>
    <entity
      name="applied_contract_assignment"
      supertypes="contract_assignment">
      <description></description>
      <explicit
        name="items">
      <description></description>
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="contract_item"/>
      </explicit>
    </entity>

  </schema>

</express>
