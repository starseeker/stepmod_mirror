<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.2 2001/11/21 15:38:19 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.2"
    source="../data/modules/contract/mim.exp"/>

  <schema name="Contract_mim">
    <interface
      kind="use"
      schema="Approval_mim">
    </interface>

    <interface
      kind="use"
      schema="Date_time_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="Person_organisation_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="contract_schema">
      <interfaced.item
        name="contract"/>
      <interfaced.item
        name="contract_type"/>
    </interface>

    <interface
      kind="use"
      schema="management_resources_schema">
      <interfaced.item
        name="contract_assignment"/>
    </interface>

    <type name="contract_item">
      <select
        extensible="YES"
        selectitems="">
      </select>

    </type>
    <type name="contract_approval_item">
      <select
        basedon="approval_item"
        selectitems="contract">
      </select>

    </type>
    <type name="contract_date_item">
      <select
        basedon="date_item"
        selectitems="contract">
      </select>

    </type>
    <type name="contract_date_and_time_item">
      <select
        basedon="date_and_time_item"
        selectitems="contract">
      </select>

    </type>
    <type name="contract_organization_item">
      <select
        basedon="organization_item"
        selectitems="contract">
      </select>

    </type>
    <type name="contract_person_and_organization_item">
      <select
        basedon="person_and_organization_item"
        selectitems="contract">
      </select>

    </type>
    <entity
      name="applied_contract_assignment"
      supertypes="contract_assignment">
      <explicit
        name="items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="contract_item"/>
      </explicit>
    </entity>

  </schema>

</express>
