<?xml version="1.0" encoding="utf-8"?><?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express
  SYSTEM "../../../dtd/express.dtd">
<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="">
   <application name="JSDAI" owner="LKSoft" source="curve_appearance_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Curve_appearance_arm">
      <interface kind="use" schema="Styled_curve_arm"/>
      <entity name="Curve_appearance">
         <explicit name="colour">
            <typename name="Colour"/>
         </explicit>
         <explicit name="font">
            <typename name="Curve_font"/>
         </explicit>
         <explicit name="width">
            <builtintype type="REAL"/>
         </explicit>
      </entity>
      <entity name="Externally_defined_marker" supertypes="Marker">
         <explicit name="marker_reference">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="Externally_defined_terminator" supertypes="Terminator">
         <explicit name="terminator_reference">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="Marker"/>
      <entity name="Point_appearance">
         <explicit name="marker">
            <typename name="Marker"/>
         </explicit>
         <explicit name="colour">
            <typename name="Colour"/>
         </explicit>
         <explicit name="size">
            <builtintype type="REAL"/>
         </explicit>
      </entity>
      <entity name="Predefined_marker" supertypes="Marker"/>
      <entity name="Predefined_terminator" supertypes="Terminator"/>
      <entity name="Terminator"/>
      <entity name="User_defined_marker" supertypes="Marker">
         <explicit name="symbol">
            <typename name="Geometric_model"/>
         </explicit>
      </entity>
      <entity name="User_defined_terminator" supertypes="Terminator">
         <explicit name="symbol">
            <typename name="Geometric_model"/>
         </explicit>
      </entity>
      <entity name="Vector_appearance" supertypes="Curve_appearance">
         <explicit name="terminator_type">
            <typename name="Terminator"/>
         </explicit>
      </entity>
   </schema>
</express>