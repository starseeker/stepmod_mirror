<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.22"
    source="../data/modules/date_time_assignment/arm.exp"/>

  <schema name="Date_time_assignment_arm">
    <interface
      kind="use"
      schema="Date_time_arm">
    </interface>

    <type name="date_or_date_time_item">
      <select
        extensible="YES">
      </select>

    </type>
    <entity
      name="Date_or_date_time_assignment">
      <explicit
        name="assigned_date">
        <typename
          name="date_or_date_time_select"/>
      </explicit>
      <explicit
        name="role">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="date_or_date_time_item"/>
      </explicit>
    </entity>

  </schema>

</express>
