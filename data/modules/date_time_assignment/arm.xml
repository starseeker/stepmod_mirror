<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/05/28 12:44:59 $"
  rcs.revision="$Revision: 1.6 $">
  description.file="arm_descriptions.xml">

  <schema name="Date_time_assignment_arm">
    <interface
      kind="use"
      schema="Date_time_arm">
    </interface>

    <type name="date_or_date_time_item">
      <select
        extensible="YES"
        selectitems="">
      </select>

    </type>
    <entity
      name="Date_or_date_time_assignment">
      <explicit
        name="assigned_date">
        <typename
          name="date_or_date_time_select"/>
      </explicit>
      <explicit
        name="role">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="date_or_date_time_item"/>
      </explicit>
    </entity>

  </schema>

</express>
