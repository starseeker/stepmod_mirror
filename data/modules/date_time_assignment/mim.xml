<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="mim_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1014">
   <application name="JSDAI" owner="LKSoft" source="date_time_assignment_mim schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Date_time_assignment_mim">
      <interface kind="use" schema="Extended_date_mim"/>
      <interface kind="use" schema="date_time_schema">
         <interfaced.item name="date"/>
         <interfaced.item name="date_role"/>
         <interfaced.item name="date_time_role"/>
      </interface>
      <interface kind="use" schema="management_resources_schema">
         <interfaced.item name="date_and_time_assignment"/>
         <interfaced.item name="date_assignment"/>
      </interface>
      <type name="date_and_time_item">
         <select extensible="YES"/>
      </type>
      <type name="date_item">
         <select extensible="YES"/>
      </type>
      <entity name="applied_date_and_time_assignment" supertypes="date_and_time_assignment">
         <explicit name="items">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="date_and_time_item"/>
         </explicit>
      </entity>
      <entity name="applied_date_assignment" supertypes="date_assignment">
         <explicit name="items">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="date_item"/>
         </explicit>
      </entity>
   </schema>
</express>
