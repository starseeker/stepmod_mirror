  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Date_time_assignment_mim  Type: date_item 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Date_time_assignment_mim.date_item">
	 The <b>date_item</b> type is an extensible list of alternate entity data types.
	 It provides a mechanism to refer to instances of the data types included in the types 
	 that extend the <b>date_item</b> type. 
	 <note>This empty extensible select type requires extension in a further module,
	 to ensure that any entity data type that refers to it can have a valid instantiation. 
	 </note>
	 </ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Date_time_assignment_mim  Type: date_and_time_item 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Date_time_assignment_mim.date_and_time_item">
	 The <b>date_and_time_item</b> type is an extensible list of alternate entity data types.
   It provides a mechanism to refer to instances of the data types included in the types 
   that extend the <b>date_and_time_item</b> type. 

   <note>This empty extensible select type requires extension in a further module,
   to ensure that any entity data type that refers to it can have a valid instantiation. 
   </note>
  </ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Date_time_assignment_mim  Entity: applied_date_assignment 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Date_time_assignment_mim.applied_date_assignment">
	 
	 An <b>applied_date_assignment</b> is a type of <b>date_assignment</b> 
	 that assigns a <b>date</b> to a <b>date_item</b>.
	 </ext_description>
 	 
  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Date_time_assignment_mim  Entity: applied_date_assignment  Attribute: items 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Date_time_assignment_mim.applied_date_assignment.items">
	 
	 The set of items to which the <b>date</b> is assigned. 
	 </ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Date_time_assignment_mim  Entity: applied_date_and_time_assignment 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Date_time_assignment_mim.applied_date_and_time_assignment">
	 
	 An <b>applied_date_and_time_assignment</b> is a type of <b>date_and_time_assignment</b> 
	 that assigns a <b>date_and_time</b> to a <b>date_and_time_item</b>.
	 </ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Date_time_assignment_mim  Entity: applied_date_assignment  Attribute: items 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Date_time_assignment_mim.applied_date_and_time_assignment.items">
	 
	 The set of items to which the <b>date_and_time</b> is assigned. 
	 </ext_description>

