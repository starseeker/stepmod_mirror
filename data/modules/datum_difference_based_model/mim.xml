      <interface kind="use" schema="Network_functional_usage_view_mim"/>
      <entity name="datum_difference_based_model_parameter" supertypes="model_parameter">
         <where label="WR1" expression="SIZEOF(QUERY ( r &lt;* USEDIN(SELF,&#10;               'DATUM_DIFFERENCE_BASED_MODEL_MIM.' + &#10;               'REPRESENTATION.ITEMS') | (SIZEOF(QUERY (prd2 &lt;*&#10;                QUERY ( prd &lt;* USEDIN(r,&#10;               'DATUM_DIFFERENCE_BASED_MODEL_MIM.' + &#10;               'PROPERTY_DEFINITION_REPRESENTATION.USED_REPRESENTATION') | &#10;               ((('DATUM_DIFFERENCE_BASED_MODEL_MIM.' &#10;               + 'PROPERTY_DEFINITION_REPRESENTATION') IN TYPEOF(prd)) AND &#10;               (('DATUM_DIFFERENCE_BASED_MODEL_MIM.' +&#10;                'PROPERTY_DEFINITION') IN TYPEOF(prd\&#10;               property_definition_representation.definition))) ) |&#10;               ('DATUM_DIFFERENCE_BASED_MODEL_MIM.' +&#10;                'DATUM_DIFFERENCE' IN&#10;               TYPEOF(prd2\property_definition_representation.definition\&#10;                                        property_definition.definition))))&#10;                = 1) )) &#10;               = 1"/>
      </entity>
      <entity name="datum_difference_functional_unit_usage_view_terminal_assignment" supertypes="shape_aspect_relationship">
