         <where label="WR2" expression="'DATUM_DIFFERENCE_BASED_MODEL_MIM.' +&#10;       'FUNCTIONAL_UNIT_TERMINAL_DEFINITION' IN TYPEOF &#10;        (SELF\shape_aspect_relationship.related_shape_aspect)"/>
         <where label="WR3" expression="'DATUM_DIFFERENCE_BASED_MODEL_MIM.' +&#10;       'DATUM_DIFFERENCE' IN TYPEOF &#10;        (SELF\shape_aspect_relationship.relating_shape_aspect)"/>
      </entity>
      <subtype.constraint name="ddbm_model_parameter_subtypes" entity="model_parameter" super.expression="ONEOF (datum_difference_based_model_parameter, schema_based_model_parameter)"/>
   </schema>
