    x := QUERY(pd <* bag_to_set(USEDIN(relation.relating_product_definition, 'DECISION_SUPPORT_MIM_LF.' + 'PRODUCT_DEFINITION_RELATIONSHIP.' + 'RELATED_PRODUCT_DEFINITION')) | specific_relation IN TYPEOF(pd));
