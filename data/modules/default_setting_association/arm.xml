<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="">
   <application name="JSDAI" owner="LKSoft" source="default_setting_association_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Default_setting_association_arm">
      <interface kind="use" schema="Shape_property_assignment_arm"/>
      <interface kind="use" schema="Default_tolerance_arm"/>
      <constant expression="[&#10;      'general tolerance']" name="default_shape_property_name">
         <aggregate lower="0" type="LIST" upper="?"/>
         <builtintype type="STRING"/>
      </constant>
      <type name="default_setting_select">
         <select selectitems="General_tolerances"/>
      </type>
      <entity name="Default_shape_property" supertypes="Assigned_property">
         <explicit name="described_element">
            <typename name="Item_shape"/>
            <redeclaration entity-ref="Assigned_property"/>
         </explicit>
         <inverse attribute="definition" entity="Default_shape_setting_association" name="used_representation"/>
         <where expression="SELF\Assigned_property.name IN default_shape_property_name" label="WR1"/>
      </entity>
      <entity name="Default_shape_setting_association" supertypes="Shape_dependent_property_representation">
         <explicit name="definition">
            <typename name="Default_shape_property"/>
            <redeclaration entity-ref="Property_definition_representation"/>
         </explicit>
         <explicit name="used_representation">
            <typename name="default_setting_select"/>
            <redeclaration entity-ref="Property_definition_representation"/>
         </explicit>
      </entity>
   </schema>
</express>
