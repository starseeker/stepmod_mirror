<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="">
   <application name="JSDAI" owner="LKSoft" source="default_setting_association_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Default_setting_association_arm">
      <interface kind="use" schema="Shape_property_assignment_arm"/>
      <interface kind="use" schema="Value_with_unit_extension_arm"/>
      <entity name="Default_chamfer" supertypes="Default_shape_property">
         <explicit name="offset_value">
            <typename name="Length_data_element"/>
         </explicit>
         <derived expression="'default chamfer'" name="name">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="Assigned_property"/>
         </derived>
      </entity>
      <entity name="Default_draft_angle" supertypes="Default_shape_property">
         <explicit name="draft_angle_value">
            <typename name="Angle_data_element"/>
         </explicit>
         <derived expression="'default draft angle'" name="name">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="Assigned_property"/>
         </derived>
      </entity>
      <entity name="Default_edge_round" supertypes="Default_shape_property">
         <explicit name="radius_value">
            <typename name="Length_data_element"/>
         </explicit>
         <derived expression="'default edge round'" name="name">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="Assigned_property"/>
         </derived>
      </entity>
      <entity name="Default_fillet" supertypes="Default_shape_property">
         <explicit name="radius_value">
            <typename name="Length_data_element"/>
         </explicit>
         <derived expression="'default fillet'" name="name">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="Assigned_property"/>
         </derived>
      </entity>
      <entity abstract.supertype="YES" name="Default_shape_property" super.expression="ONEOF (Default_chamfer, Default_draft_angle, Default_edge_round, Default_fillet, Default_thickness)" supertypes="Assigned_shape_property">
         <explicit name="described_element">
            <typename name="shapeable_item"/>
            <redeclaration entity-ref="Assigned_shape_property"/>
         </explicit>
      </entity>
      <entity name="Default_thickness" supertypes="Default_shape_property">
         <explicit name="thickness_value">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="parameter_name" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <derived expression="'default thickness'" name="name">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="Assigned_property"/>
         </derived>
      </entity>
   </schema>
</express>
