<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1052">
   <application name="JSDAI" owner="LKSoft" source="default_tolerance_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Default_tolerance_arm">
      <interface kind="use" schema="Document_assignment_arm"/>
      <interface kind="use" schema="Foundation_representation_arm"/>
      <interface kind="use" schema="Measure_representation_arm"/>
      <interface kind="use" schema="Default_setting_association_arm"/>
      <type name="cell_entry_select">
         <select selectitems="Plus_minus_toleranced_datum Upper_lower_toleranced_datum"/>
      </type>
      <type name="general_tolerance_table_select">
         <select selectitems="General_tolerance_table Document_version Document"/>
      </type>
      <type name="limit_select">
         <select selectitems="Number_of_decimal_places Significant_number_of_digits Upper_lower_limit"/>
      </type>
      <entity name="General_tolerance_table">
         <explicit name="tolerance_type">
            <builtintype type="STRING"/>
         </explicit>
         <inverse attribute="tolerance_table" entity="Tolerance_table_cell" name="cell">
            <inverse.aggregate lower="1" type="SET" upper="?"/>
         </inverse>
      </entity>
      <entity name="General_tolerances" supertypes="Default_shape_property">
         <explicit name="table_definition">
            <typename name="general_tolerance_table_select"/>
         </explicit>
         <explicit name="tolerance_class">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="Number_of_decimal_places">
         <explicit name="number_of_places_after_decimal_separator">
            <builtintype type="INTEGER"/>
         </explicit>
      </entity>
      <entity name="Plus_minus_toleranced_datum">
         <explicit name="tolerance_value">
            <typename name="Numerical_item_with_unit"/>
         </explicit>
      </entity>
      <entity name="Significant_number_of_digits">
         <explicit name="number_of_digits">
            <builtintype type="INTEGER"/>
         </explicit>
      </entity>
      <entity name="Tolerance_table_cell">
         <explicit name="cell_value" optional="YES">
            <typename name="cell_entry_select"/>
         </explicit>
         <explicit name="description">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="limit">
            <typename name="limit_select"/>
         </explicit>
         <explicit name="tolerance_class" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="tolerance_table">
            <typename name="General_tolerance_table"/>
         </explicit>
      </entity>
      <entity name="Upper_lower_limit">
         <explicit name="upper_limit">
            <typename name="Numerical_item_with_unit"/>
         </explicit>
         <explicit name="lower_limit">
            <typename name="Numerical_item_with_unit"/>
         </explicit>
      </entity>
      <entity name="Upper_lower_toleranced_datum">
         <explicit name="lower_tolerance_value">
            <typename name="Numerical_item_with_unit"/>
         </explicit>
         <explicit name="upper_tolerance_value">
            <typename name="Numerical_item_with_unit"/>
         </explicit>
      </entity>
   </schema>
</express>
