<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date: 2006/02/23 11:56:05 $" rcs.revision="$Revision: 1.2 $" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="default_tolerance_arm schema_instance"/>
   <schema name="Default_tolerance_arm">
      <interface kind="use" schema="Foundation_representation_arm"/>
      <interface kind="use" schema="Measure_representation_arm"/>
      <interface kind="use" schema="Shape_property_assignment_arm"/>
      <type name="cell_entry_select">
         <select selectitems="Plus_minus_toleranced_datum Upper_lower_toleranced_datum"/>
      </type>
      <type name="default_context_select">
         <select selectitems="Item_shape"/>
      </type>
      <type name="default_setting_select">
         <select extensible="YES" selectitems="General_tolerances"/>
      </type>
      <type name="general_tolerance_table_select">
         <select extensible="YES" selectitems="General_tolerance_table"/>
      </type>
      <type name="limit_select">
         <select selectitems="Significant_number_of_digits Upper_lower_limit"/>
      </type>
      <entity name="Default_setting_association">
         <explicit name="default_value">
            <aggregate type="SET" lower="0" upper="?"/>
            <typename name="default_setting_select"/>
         </explicit>
         <explicit name="default_context">
            <typename name="default_context_select"/>
         </explicit>
      </entity>
      <entity name="General_tolerance_table">
         <explicit name="tolerance_type">
            <builtintype type="STRING"/>
         </explicit>
         <inverse name="cell" entity="Tolerance_table_cell" attribute="tolerance_table">
            <inverse.aggregate type="SET" lower="1" upper="?"/>
         </inverse>
      </entity>
      <entity name="General_tolerances">
         <explicit name="table_definition">
            <typename name="general_tolerance_table_select"/>
         </explicit>
         <explicit name="tolerance_class">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="Plus_minus_toleranced_datum">
         <explicit name="tolerance_value">
            <typename name="Numerical_item_with_unit"/>
         </explicit>
      </entity>
      <entity name="Significant_number_of_digits">
         <explicit name="number_of_digits">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="Tolerance_table_cell">
         <explicit name="cell_value">
            <typename name="cell_entry_select"/>
         </explicit>
         <explicit name="description">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="limit">
            <typename name="limit_select"/>
         </explicit>
         <explicit name="tolerance_class">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="tolerance_table">
            <typename name="General_tolerance_table"/>
         </explicit>
      </entity>
      <entity name="Upper_lower_limit">
         <explicit name="upper_limit">
            <typename name="Numerical_item_with_unit"/>
         </explicit>
         <explicit name="lower_limit">
            <typename name="Numerical_item_with_unit"/>
         </explicit>
      </entity>
      <entity name="Upper_lower_toleranced_datum">
         <explicit name="lower_tolerance_value">
            <typename name="Numerical_item_with_unit"/>
         </explicit>
         <explicit name="upper_tolerance_value">
            <typename name="Numerical_item_with_unit"/>
         </explicit>
      </entity>
   </schema>
</express>
