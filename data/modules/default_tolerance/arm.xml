<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.30 2003/02/25 06:04:24 thendrix Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.30"
    source="../data/modules/default_tolerance/arm.exp"/>

  <schema name="Default_tolerance_arm">
    <interface
      kind="use"
      schema="Foundation_representation_arm">
    </interface>

    <interface
      kind="use"
      schema="Measure_representation_arm">
    </interface>

    <interface
      kind="use"
      schema="Shape_property_assignment_arm">
    </interface>

    <type name="cell_entry_select">
      <select
        selectitems="Plus_minus_toleranced_datum Upper_lower_toleranced_datum">
      </select>

    </type>
    <type name="default_context_select">
      <select
        selectitems="Item_shape">
      </select>

    </type>
    <type name="default_setting_select">
      <select
        extensible="YES"
        selectitems="General_tolerances">
      </select>

    </type>
    <type name="general_tolerance_table_select">
      <select
        extensible="YES"
        selectitems="General_tolerance_table">
      </select>

    </type>
    <type name="limit_select">
      <select
        selectitems="Significant_number_of_digits Upper_lower_limit">
      </select>

    </type>
    <entity
      name="Default_setting_association">
      <explicit
        name="default_value">
        <aggregate
          type="SET"
          lower="0"
          upper="?"/>
        <typename
          name="default_setting_select"/>
      </explicit>
      <explicit
        name="default_context">
        <typename
          name="default_context_select"/>
      </explicit>
    </entity>

    <entity
      name="General_tolerance_table">
      <explicit
        name="tolerance_type">
        <builtintype
          type="STRING"/>
      </explicit>
      <inverse
        name="cell"
        entity="Tolerance_table_cell"
        attribute="tolerance_table">
        <inverse.aggregate
          type="SET"
          lower="1"
          upper="?"/>
      </inverse>
    </entity>

    <entity
      name="General_tolerances">
      <explicit
        name="table_definition">
        <typename
          name="general_tolerance_table_select"/>
      </explicit>
      <explicit
        name="tolerance_class">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="Plus_minus_toleranced_datum">
      <explicit
        name="tolerance_value">
        <typename
          name="Numerical_item_with_unit"/>
      </explicit>
    </entity>

    <entity
      name="Significant_number_of_digits">
      <explicit
        name="number_of_digits">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="Tolerance_table_cell">
      <explicit
        name="cell_value">
        <typename
          name="cell_entry_select"/>
      </explicit>
      <explicit
        name="description">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="limit">
        <typename
          name="limit_select"/>
      </explicit>
      <explicit
        name="tolerance_class">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="tolerance_table">
        <typename
          name="General_tolerance_table"/>
      </explicit>
    </entity>

    <entity
      name="Upper_lower_limit">
      <explicit
        name="upper_limit">
        <typename
          name="Numerical_item_with_unit"/>
      </explicit>
      <explicit
        name="lower_limit">
        <typename
          name="Numerical_item_with_unit"/>
      </explicit>
    </entity>

    <entity
      name="Upper_lower_toleranced_datum">
      <explicit
        name="lower_tolerance_value">
        <typename
          name="Numerical_item_with_unit"/>
      </explicit>
      <explicit
        name="upper_tolerance_value">
        <typename
          name="Numerical_item_with_unit"/>
      </explicit>
    </entity>

  </schema>

</express>
