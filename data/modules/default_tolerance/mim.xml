<entity name="default_tolerance_table" supertypes="representation">
<where label="WR1" expression="SIZEOF( QUERY( i &lt;* SELF.items | &#xA;NOT('DEFAULT_TOLERANCES_FOR_GEOMETRIC_DIMENSIONS.DEFAULT_TOLERANCE_TABLE_CELL' IN &#xA;TYPEOF(i)) &#xA;)) = 0">
</where>
<where label="WR2" expression="(SIZEOF( QUERY( rr &lt;* USEDIN(SELF,'DEFAULT_TOLERANCES_FOR_GEOMETRIC_DIMENSIONS.' + &#xA;'REPRESENTATION_RELATIONSHIP.REP_1') | &#xA;rr.name &lt; 'general tolerance definition' &#xA;)) = 0) AND &#xA;(SIZEOF( QUERY( rr &lt;* USEDIN(SELF,'DEFAULT_TOLERANCES_FOR_GEOMETRIC_DIMENSIONS.' + &#xA;'REPRESENTATION_RELATIONSHIP.REP_1') | &#xA;(rr.name = 'general tolerance definition') AND &#xA;(rr.rep2.name &lt; 'default tolerance') &#xA;)) = 0) AND &#xA;(SIZEOF( USEDIN(SELF,'DEFAULT_TOLERANCES_FOR_GEOMETRIC_DIMENSIONS.' + &#xA;'REPRESENTATION_RELATIONSHIP.REP_2') &#xA;) = 0)">
</where>
<graphic.element page="2" xcoord="113" ycoord="651" />
</entity>
<entity name="default_tolerance_table_cell" supertypes="compound_representation_item">
<where label="WR1" expression="SIZEOF(QUERY( x &lt;* USEDIN(SELF,'DEFAULT_TOLERANCES_FOR_GEOMETRIC_DIMENSIONS.' + &#xA;'REPRESENTATION.ITEMS') | 'DEFAULT_TOLERANCES_FOR_GEOMETRIC_DIMENSIONS.' + &#xA;'DEFAULT_TOLERANCE_TABLE' IN TYPEOF(x)))=1&#xA;&#xA;">
