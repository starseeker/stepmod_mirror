<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1130">
   <application name="JSDAI" owner="LKSoft" source="derived_shape_element_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Derived_shape_element_arm">
      <interface kind="use" schema="Construction_geometry_arm"/>
      <interface kind="use" schema="Shape_property_assignment_arm"/>
      <interface kind="use" schema="Value_with_unit_arm"/>
      <type name="dse_shape_model">
         <select basedon="shape_model" extensible="YES" genericentity="YES" selectitems="Constructive_geometry"/>
      </type>
      <entity name="Derived_geometry">
         <explicit name="derived_from">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Shape_element"/>
         </explicit>
         <explicit name="geometry">
            <typename name="constructive_element_select"/>
         </explicit>
         <explicit name="role">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="Derived_shape_element" supertypes="Shape_element">
         <explicit name="derived_from">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Shape_element"/>
         </explicit>
         <explicit name="role">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="Shape_element" old_name="description"/>
         </explicit>
      </entity>
      <entity name="Offset_shape_element" supertypes="Derived_shape_element">
         <explicit name="offset">
            <typename name="Value_with_unit"/>
         </explicit>
      </entity>
   </schema>
</express>
