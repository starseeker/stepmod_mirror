<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.25 2002/11/06 00:59:57 thendrix Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.25"
    source="../data/modules/derived_shape_element/arm.exp"/>

  <schema name="Derived_shape_element_arm">
    <interface
      kind="use"
      schema="Construction_geometry_arm">
    </interface>

    <interface
      kind="use"
      schema="Shape_property_assignment_arm">
    </interface>

    <interface
      kind="use"
      schema="Value_with_unit_arm">
    </interface>

    <entity
      name="Derived_geometry">
      <explicit
        name="derived_from">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="Shape_element"/>
      </explicit>
      <explicit
        name="geometry">
        <typename
          name="constructive_element_select"/>
      </explicit>
      <explicit
        name="role">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="Derived_shape_element"
      supertypes="Shape_element">
      <explicit
        name="derived_from">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="Shape_element"/>
      </explicit>
      <explicit
        name="role">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="Offset_shape_element"
      supertypes="Derived_shape_element">
      <explicit
        name="offset">
        <typename
          name="Value_with_unit"/>
      </explicit>
    </entity>

  </schema>

</express>
