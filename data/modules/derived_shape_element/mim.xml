        name="derived_shape_aspect"/>
      <interfaced.item
        name="extension"/>
      <interfaced.item
        name="geometric_alignment"/>
      <interfaced.item
        name="geometric_intersection"/>
      <interfaced.item
        name="parallel_offset"/>
      <interfaced.item
        name="perpendicular_to"/>
      <interfaced.item
        name="shape_aspect_deriving_relationship"/>
      <interfaced.item
        name="tangent"/>
    </interface>

    <interface
      kind="use"
      schema="Shape_property_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="Value_with_unit_mim">
    </interface>

  </schema>

