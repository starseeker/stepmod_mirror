<!-- +++++++++++ Component_terminal_to_assembly_module_interface_terminal_assignment +++++++++++ -->
<ext_description linkend="Design_specific_assignment_to_assembly_usage_view_arm.Component_terminal_to_assembly_module_interface_terminal_assignment">
	A <b>Component_terminal_to_assembly_module_interface_terminal_assignment</b> is a type of 
	<express_ref linkend="Component_feature:arm:Component_feature_arm.Component_feature_to_physical_usage_view_assignment"/>.
</ext_description>

<!-- usage_view_terminal -->
<ext_description linkend="Design_specific_assignment_to_assembly_usage_view_arm.Component_terminal_to_assembly_module_interface_terminal_assignment.usage_view_terminal">
<p>
	specifies a role of the 
	<express_ref linkend="Assembly_module_usage_view:arm:Assembly_module_usage_view_arm.Assembly_module_interface_terminal"/> 
	for the <b>Component_terminal_to_assembly_module_interface_terminal_assignment</b>. 
</p>
</ext_description>

<!-- assigned_terminal -->
<ext_description linkend="Design_specific_assignment_to_assembly_usage_view_arm.Component_terminal_to_assembly_module_interface_terminal_assignment.assigned_terminal">
<p>
	specifies the 
	<express_ref linkend="Design_specific_assignment_to_assembly_usage_view:arm:Design_specific_assignment_to_assembly_usage_view_arm.pca_interface_select"/> 
	for the <b>Component_terminal_to_assembly_module_interface_terminal_assignment</b>.
</p>
</ext_description>

<!-- UR1 -->
<ext_description linkend="Design_specific_assignment_to_assembly_usage_view_arm.Component_terminal_to_assembly_module_interface_terminal_assignment.ur:UR1">
	The <b>usage_view_terminal</b> shall be unique within a population of 
	<b>Component_terminal_to_assembly_module_interface_terminal_assignment</b>.
</ext_description>

<!-- +++++++++++ Component_terminal_to_pca_terminal_assignment +++++++++++ -->
<ext_description linkend="Design_specific_assignment_to_assembly_usage_view_arm.Component_terminal_to_pca_terminal_assignment">
	A <b>Component_terminal_to_pca_terminal_assignment</b> is a type of 
	<express_ref linkend="Component_feature:arm:Component_feature_arm.Component_feature_to_physical_usage_view_assignment"/>.
	A <b>Component_terminal_to_pca_terminal_assignment</b> is the relationship between a 
	<express_ref linkend="Assembly_module_usage_view:arm:Assembly_module_usage_view_arm.Pca_terminal"/> 
	and one of an 
	<express_ref linkend="Assembly_module_with_subassembly:arm:Assembly_module_with_subassembly_arm.Assembly_module_component_terminal"/>, 
	a 
	<express_ref linkend="Interface_component:arm:Interface_component_arm.Physical_component_interface_terminal"/>, 
	or an 
	<express_ref linkend="Assembly_module_with_interconnect_component:arm:Assembly_module_with_interconnect_component_arm.Interconnect_module_component_terminal"/>, 
	to provide traceability from the data in the design view to the data in the usage view for the design organization for archival purposes.
</ext_description>

<!-- assigned_terminal -->
<ext_description linkend="Design_specific_assignment_to_assembly_usage_view_arm.Component_terminal_to_pca_terminal_assignment.assigned_terminal">
	specifies one of a
	<express_ref linkend="Design_specific_assignment_to_assembly_usage_view:arm:Design_specific_assignment_to_assembly_usage_view_arm.pca_interface_select"/>
	for the <b>Component_terminal_to_pca_terminal_assignment</b>.
</ext_description>

<!-- usage_view_terminal -->
<ext_description linkend="Design_specific_assignment_to_assembly_usage_view_arm.Component_terminal_to_pca_terminal_assignment.usage_view_terminal">
	specifies a role of the 
	<express_ref linkend="Assembly_module_usage_view:arm:Assembly_module_usage_view_arm.Pca_terminal"/> 
	for the <b>Component_terminal_to_pca_terminal_assignment</b>.
</ext_description>

<!-- UR1 -->
<ext_description linkend="Design_specific_assignment_to_assembly_usage_view_arm.Component_terminal_to_pca_terminal_assignment.ur:UR1">
	The <b>usage_view_terminal</b> shall be unique within a population of 
	<b>Component_terminal_to_pca_terminal_assignment</b>s.
</ext_description>
</ext_descriptions>

