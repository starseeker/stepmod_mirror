<!-- +++++++++++ Component_terminal_to_assembly_module_interface_terminal_assignment +++++++++++ -->
<ext_description linkend="Design_specific_assignment_to_assembly_usage_view_arm.Component_terminal_to_assembly_module_interface_terminal_assignment">
	A <b>Component_terminal_to_assembly_module_interface_terminal_assignment</b> is a type of 
	<express_ref linkend="Physical_unit_design_view:arm:Physical_unit_design_view_arm.Component_feature_to_physical_usage_view_assignment"/>.
</ext_description>

<!-- usage_view_terminal -->
<ext_description linkend="Design_specific_assignment_to_assembly_usage_view_arm.Component_terminal_to_assembly_module_interface_terminal_assignment.usage_view_terminal">
<p>
	specifies a role of the 
	<express_ref linkend="Assembly_module_usage_view:arm:Assembly_module_usage_view_arm.Assembly_module_interface_terminal"/> 
	for the <b>Component_terminal_to_assembly_module_interface_terminal_assignment</b>. 
</p>
</ext_description>

<!-- assigned_terminal -->
<ext_description linkend="Design_specific_assignment_to_assembly_usage_view_arm.Component_terminal_to_assembly_module_interface_terminal_assignment.assigned_terminal">
<p>
	specifies the 
	<express_ref linkend="Design_specific_assignment_to_assembly_usage_view:arm:Design_specific_assignment_to_assembly_usage_view_arm.layered_assembly_module_interface_select"/> 
	for the <b>Component_terminal_to_assembly_module_interface_terminal_assignment</b>.
</p>
</ext_description>

<!-- UR1 -->
<ext_description linkend="Design_specific_assignment_to_assembly_usage_view_arm.Component_terminal_to_assembly_module_interface_terminal_assignment.ur:UR1">
	The <b>usage_view_terminal</b> shall be unique within a population of 
	<b>Component_terminal_to_assembly_module_interface_terminal_assignment</b>.
</ext_description>

<!-- +++++++++++ Component_terminal_to_layered_assembly_module_terminal_assignment +++++++++++ -->
<ext_description linkend="Design_specific_assignment_to_assembly_usage_view_arm.Component_terminal_to_layered_assembly_module_terminal_assignment">
	A <b>Component_terminal_to_layered_assembly_module_terminal_assignment</b> is a type of 
	<express_ref linkend="Physical_unit_design_view:arm:Physical_unit_design_view_arm.Component_feature_to_physical_usage_view_assignment"/>.
	A <b>Component_terminal_to_layered_assembly_module_terminal_assignment</b> is the relationship between a 
	<express_ref linkend="assembly_module_with_interconnect_component:arm:Assembly_module_with_interconnect_component_arm.Layered_assembly_module_terminal"/> 
	and a
	<express_ref linkend="Design_specific_assignment_to_assembly_usage_view:arm:Design_specific_assignment_to_assembly_usage_view_arm.layered_assembly_module_interface_select"/> 
	to provide traceability from the data in the design view to the data in the usage view.
</ext_description>

<!-- assigned_terminal -->
<ext_description linkend="Design_specific_assignment_to_assembly_usage_view_arm.Component_terminal_to_layered_assembly_module_terminal_assignment.assigned_terminal">
	specifies one of a
	<express_ref linkend="Design_specific_assignment_to_assembly_usage_view:arm:Design_specific_assignment_to_assembly_usage_view_arm.layered_assembly_module_interface_select"/>
	for the <b>Component_terminal_to_layered_assembly_module_terminal_assignment</b>.
</ext_description>

<!-- usage_view_terminal -->
<ext_description linkend="Design_specific_assignment_to_assembly_usage_view_arm.Component_terminal_to_layered_assembly_module_terminal_assignment.usage_view_terminal">
	specifies a role of the 
	<express_ref linkend="assembly_module_with_interconnect_component:arm:Assembly_module_with_interconnect_component_arm.Layered_assembly_module_terminal"/> 
	for the <b>Component_terminal_to_layered_assembly_module_terminal_assignment</b>.
</ext_description>

<!-- UR1 -->
<ext_description linkend="Design_specific_assignment_to_assembly_usage_view_arm.Component_terminal_to_layered_assembly_module_terminal_assignment.ur:UR1">
	The <b>usage_view_terminal</b> shall be unique within a population of 
	<b>Component_terminal_to_layered_assembly_module_terminal_assignment</b>s.
</ext_description>
</ext_descriptions>

