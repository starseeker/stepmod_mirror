<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-02-11T13:31:51" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="design_specific_assignment_to_interconnect_usage_view_arm schema_instance"/>
   <schema name="Design_specific_assignment_to_interconnect_usage_view_arm">
      <interface kind="use" schema="Fabrication_joint_arm"/>
      <interface kind="use" schema="Interconnect_module_usage_view_arm"/>
      <interface kind="use" schema="Land_arm"/>
      <interface kind="use" schema="Layered_interconnect_module_with_printed_component_design_arm"/>
      <type name="interconnect_or_assembly_module_select">
         <select selectitems="Component_termination_passage_interface_terminal Printed_connector_component_interface_terminal Non_functional_land_interface_terminal Land_interface_terminal"/>
      </type>
      <entity name="Component_terminal_to_interconnect_module_interface_terminal_assignment" supertypes="Component_feature_to_physical_usage_view_assignment">
         <explicit name="usage_view_terminal">
            <typename name="Interconnect_module_interface_terminal"/>
            <redeclaration entity-ref="Component_feature_to_physical_usage_view_assignment" old_name="defined_part_feature"/>
         </explicit>
         <explicit name="assigned_terminal">
            <typename name="interconnect_or_assembly_module_select"/>
            <redeclaration entity-ref="Component_feature_to_physical_usage_view_assignment" old_name="assigned_design_object"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute entity-ref="Component_terminal_to_interconnect_module_interface_terminal_assignment" attribute="usage_view_terminal"/>
         </unique>
      </entity>
      <entity name="Component_terminal_to_interconnect_module_join_terminal_assignment" supertypes="Component_feature_to_physical_usage_view_assignment">
         <explicit name="usage_view_terminal">
            <typename name="Interconnect_module_join_terminal"/>
            <redeclaration entity-ref="Component_feature_to_physical_usage_view_assignment" old_name="defined_part_feature"/>
         </explicit>
         <explicit name="assigned_terminal">
            <typename name="fabricated_feature_select"/>
            <redeclaration entity-ref="Component_feature_to_physical_usage_view_assignment" old_name="assigned_design_object"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute entity-ref="Component_terminal_to_interconnect_module_join_terminal_assignment" attribute="usage_view_terminal"/>
         </unique>
      </entity>
      <entity name="Stratum_concept_to_physical_usage_view_assignment">
         <explicit name="assigned_design_object">
            <typename name="stratum_concept"/>
         </explicit>
         <explicit name="defined_part_feature">
            <typename name="Part_feature"/>
         </explicit>
         <explicit name="purpose" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute entity-ref="Stratum_concept_to_physical_usage_view_assignment" attribute="defined_part_feature"/>
         </unique>
         <where label="WR1" expression="NOT ('DESIGN_SPECIFIC_ASSIGNMENT_TO_INTERCONNECT_USAGE_VIEW_ARM.INTERCONNECT_MODULE_TERMINAL' IN TYPEOF(defined_part_feature))"/>
      </entity>
      <subtype.constraint name="dsatiuv_component_feature_to_physical_usage_view_assignment_subtypes" entity="Component_feature_to_physical_usage_view_assignment" super.expression="ONEOF (Component_terminal_to_interconnect_module_join_terminal_assignment, Component_terminal_to_interconnect_module_interface_terminal_assignment)"/>
   </schema>
</express>
