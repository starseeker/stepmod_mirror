         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="defined_part_feature"/>
         </unique>
         <where expression="NOT ('DESIGN_SPECIFIC_ASSIGNMENT_TO_INTERCONNECT_USAGE_VIEW_ARM.INTERCONNECT_MODULE_TERMINAL' IN TYPEOF(defined_part_feature))" label="WR1"/>
      </entity>
      <subtype.constraint entity="Component_feature_to_physical_usage_view_assignment" name="dsatiuv_component_feature_to_physical_usage_view_assignment_subtypes" super.expression="ONEOF (Component_terminal_to_interconnect_module_macro_terminal_assignment, Component_terminal_to_interconnect_module_interface_terminal_assignment)"/>
   </schema>
