<!-- +++++++++++ Stratum_concept_to_physical_usage_view_assignment +++++++++++ -->
<ext_description linkend="Design_specific_assignment_to_interconnect_usage_view_arm.Stratum_concept_to_physical_usage_view_assignment">
<p>
	A <b>Stratum_concept_to_physical_usage_view_assignment</b> is the relationship between a 
	<express_ref linkend="Physical_unit_usage_view:arm:Physical_unit_usage_view_arm.Part_feature"/> 
	and any one of the 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.stratum_concept"/>
	for the <b>purpose</b> of traceability between the  
	<express_ref linkend="Physical_unit_usage_view:arm:Physical_unit_usage_view_arm.Part_feature"/> 
	and the underlying design feature.  
	The semantics of the <b>assigned_design_object</b> and the <b>defined_part_feature</b> shall be compatible.
</p>
	<example>
		It is inappropriate to assign a  
		<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Stratum_feature"/> 
		as an <b>assigned_design_object</b> and an 
		<express_ref linkend="Interconnect_module_usage_view:arm:Interconnect_module_usage_view_arm.Interconnect_module_cutout_surface"/>
		as a <b>defined_part_feature</b> with a member of <b>Stratum_concept_to_physical_usage_view_assignment</b>.
	</example>
</ext_description>

<!-- purpose -->
<ext_description linkend="Design_specific_assignment_to_interconnect_usage_view_arm.Stratum_concept_to_physical_usage_view_assignment.purpose">
<p>
	the text that specifies a purpose. The value of this attribute need not be specified.
</p>
</ext_description>

<!-- defined_part_feature -->
<ext_description linkend="Design_specific_assignment_to_interconnect_usage_view_arm.Stratum_concept_to_physical_usage_view_assignment.defined_part_feature">
	specifies a role of the 
	<express_ref linkend="Physical_unit_usage_view:arm:Physical_unit_usage_view_arm.Part_feature"/> 
	for the <b>Stratum_concept_to_physical_usage_view_assignment</b>.
</ext_description>

<!-- assigned_design_object -->
<ext_description linkend="Design_specific_assignment_to_interconnect_usage_view_arm.Stratum_concept_to_physical_usage_view_assignment.assigned_design_object">
	specifies any one of the 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.stratum_concept"/>
	as the assigned object for the <b>Stratum_concept_to_physical_usage_view_assignment</b>.
</ext_description>

<!-- UR1 -->
<ext_description linkend="Design_specific_assignment_to_interconnect_usage_view_arm.Stratum_concept_to_physical_usage_view_assignment.ur:UR1">
	The <b>defined_part_feature</b> shall be unique within a population of <b>Stratum_concept_to_physical_usage_view_assignment</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Design_specific_assignment_to_interconnect_usage_view_arm.Stratum_concept_to_physical_usage_view_assignment.wr:WR1">
	The <b>defined_part_feature</b> shall not be 
	<express_ref linkend="interconnect_module_usage_view:arm:Interconnect_module_usage_view_arm.Interconnect_module_terminal"/>.
</ext_description>
