<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T17:39:20" rcs.revision="1.0" description.file="mim_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="device_marking_mim schema_instance"/>
   <schema name="Device_marking_mim">
      <interface kind="use" schema="Approval_mim"/>
      <interface kind="use" schema="Characteristic_mim"/>
      <interface kind="use" schema="Conductivity_material_aspects_mim"/>
      <interface kind="use" schema="Part_template_2d_shape_mim"/>
      <interface kind="use" schema="Part_template_3d_shape_mim"/>
      <interface kind="use" schema="Part_template_non_planar_shape_mim"/>
      <interface kind="use" schema="Person_organization_mim"/>
      <interface kind="use" schema="Physical_unit_2d_shape_mim"/>
      <interface kind="use" schema="Physical_unit_3d_shape_mim"/>
      <interface kind="use" schema="Product_version_mim"/>
      <interface kind="use" schema="Specification_document_mim"/>
      <interface kind="use" schema="Styled_text_mim"/>
      <interface kind="use" schema="presentation_definition_schema">
         <interfaced.item name="annotation_text_occurrence"/>
      </interface>
      <type name="dm_approval_item">
         <select extensible="YES" genericentity="YES" basedon="approval_item" selectitems="representation"/>
      </type>
      <type name="dm_document_reference_item">
         <select extensible="YES" genericentity="YES" basedon="document_reference_item" selectitems="representation"/>
      </type>
      <type name="dm_organization_item">
         <select extensible="YES" genericentity="YES" basedon="organization_item" selectitems="representation"/>
      </type>
      <rule name="marking_constraint" appliesto="representation">
         <where label="WR1" expression=" SIZEOF ( QUERY ( rep &lt;* representation | ( ( rep \ representation . name = 'marking' ) AND ( SIZEOF ( QUERY ( pdr &lt;* USEDIN ( rep , 'DEVICE_MARKING_MIM.' + 'PROPERTY_DEFINITION_REPRESENTATION.USED_REPRESENTATION' ) | ( 'DEVICE_MARKING_MIM.' + 'SHAPE_ASPECT' IN TYPEOF ( pdr . definition ) ) ) ) &gt; 0 ) ) AND NOT ( SIZEOF ( QUERY ( place &lt;* rep . items | ( ( 'DEVICE_MARKING_MIM.' + 'PLACEMENT' IN TYPEOF ( place ) ) AND ( place \ representation_item . name = 'marking location' ) ) ) ) = 1 ) ) ) = 0 "/>
      </rule>
   </schema>
</express>
