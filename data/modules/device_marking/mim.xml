<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-08-31T14:04:06" rcs.revision="1.0" description.file="mim_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="device_marking_mim schema_instance"/>
   <schema name="Device_marking_mim">
      <interface kind="use" schema="Part_template_2d_shape_mim"/>
      <interface kind="use" schema="Part_template_non_planar_shape_mim"/>
      <interface kind="use" schema="Physical_unit_2d_shape_mim"/>
      <interface kind="use" schema="presentation_definition_schema">
         <interfaced.item name="annotation_text_occurrence"/>
      </interface>
      <type name="device_marking_approval_item">
         <select extensible="YES" genericentity="YES" basedon="approval_item" selectitems="representation"/>
      </type>
      <type name="device_marking_document_reference_item">
         <select extensible="YES" genericentity="YES" basedon="document_reference_item" selectitems="representation"/>
      </type>
      <type name="device_marking_organization_item">
         <select extensible="YES" genericentity="YES" basedon="organization_item" selectitems="representation"/>
      </type>
      <rule name="marking_constraint" appliesto="representation">
         <where label="WR1" expression="SIZEOF(QUERY(rep &lt;* representation |&#10;       ((rep\representation.name = 'marking')&#10;       AND (SIZEOF(QUERY(pdr &lt;* USEDIN(rep,&#10;       'DEVICE_MARKING_MIM.'&#10;        + 'PROPERTY_DEFINITION_REPRESENTATION.USED_REPRESENTATION') |&#10;       ('DEVICE_MARKING_MIM.'&#10;        + 'SHAPE_ASPECT' IN TYPEOF(pdr.definition)))) &gt; 0))&#10;       AND NOT (SIZEOF(QUERY(place &lt;* rep.items |&#10;       (('DEVICE_MARKING_MIM.'&#10;        + 'PLACEMENT' IN TYPEOF (place))&#10;       AND (place\representation_item.name = 'marking location'))&#10;       )) = 1)&#10;       )) = 0"/>
      </rule>
   </schema>
</express>
