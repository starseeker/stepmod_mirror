<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="dimension_and_tolerance_callouts_arm schema_instance"/>
   <schema name="Dimension_and_tolerance_callouts_arm">
      <interface kind="use" schema="Draughting_element_specializations_arm"/>
      <interface kind="use" schema="Draughting_annotation_arm"/>
      <interface kind="use" schema="Dimension_tolerance_arm"/>
      <interface kind="use" schema="Associative_draughting_elements_arm"/>
      <interface kind="use" schema="Draughting_dimension_arm"/>
      <interface kind="use" schema="Extended_fill_area_style_arm"/>
      <type name="associated_element_select">
         <select extensible="YES" genericentity="YES" selectitems="Annotation_element Detailed_geometric_model_element"/>
      </type>
      <type name="datc_appearance_select">
         <select extensible="YES" basedon="appearance_select" selectitems="Solid_fill_area"/>
      </type>
      <type name="dimension_callout_select">
         <select selectitems="Dimension_symbol Text"/>
      </type>
      <type name="fill_area_associated_element_select">
         <select extensible="YES" genericentity="YES" selectitems="Annotation_curve Detailed_geometric_model_element"/>
      </type>
      <entity name="Associated_draughting_callout" supertypes="Draughting_callout">
         <explicit name="is_associated_to">
            <typename name="associated_element_select"/>
         </explicit>
      </entity>
      <entity name="Associated_fill_area" supertypes="Fill_area">
         <explicit name="is_derived_from">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="fill_area_associated_element_select"/>
         </explicit>
      </entity>
      <entity name="Associated_leader" supertypes="Leader">
         <explicit name="target">
            <typename name="associated_element_select"/>
         </explicit>
      </entity>
      <entity name="Associated_projection_curve" supertypes="Shape_aspect_associativity">
         <explicit name="is_projected_to">
            <typename name="associated_element_select"/>
         </explicit>
         <explicit name="projection_curve">
            <typename name="Projection_curve"/>
         </explicit>
      </entity>
      <entity name="Dimension" supertypes="Draughting_callout">
         <explicit name="id">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="primary_callout">
            <typename name="Dimension_callout"/>
         </explicit>
         <explicit name="secondary_callout" optional="YES">
            <typename name="Dimension_callout"/>
         </explicit>
      </entity>
      <entity name="Dimension_callout">
         <explicit name="callout_element">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="dimension_callout_select"/>
         </explicit>
         <explicit name="is_displaying" optional="YES">
            <typename name="Geometric_dimension"/>
         </explicit>
      </entity>
      <entity name="Dimension_sequence_pair">
         <explicit name="predecessor">
            <typename name="Dimension"/>
         </explicit>
         <explicit name="successor">
            <typename name="Dimension"/>
         </explicit>
      </entity>
   </schema>
</express>
