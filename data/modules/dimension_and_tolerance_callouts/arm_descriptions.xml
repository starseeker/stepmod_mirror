<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Dimension_and_tolerance_callouts_arm  Entity:   Draughting_model_item_association_with_placeholder
   <express_ref linkend="dimension_and_tolerance_callouts:arm:Dimension_and_tolerance_callouts_arm.Draughting_model_item_association_with_placeholder"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++  -->
<ext_description linkend="Dimension_and_tolerance_callouts_arm.Draughting_model_item_association_with_placeholder">
	An <b>Draughting_model_item_association_with_placeholder</b> is a type of
	<express_ref linkend="Associative_draughting_elements_arm:arm:Associative_draughting_elements_arm.Draughting_model_item_association"/>
	that specifies a placeholder in a presentation model for product and manufacturing information annotation.
</ext_description>
<ext_description linkend="Dimension_and_tolerance_callouts_arm.Draughting_model_item_association_with_placeholder.identified_item">
	the identified_item specifies the draughting_callout that is the context for the placeholder
</ext_description>
<ext_description linkend="Dimension_and_tolerance_callouts_arm.Draughting_model_item_association_with_placeholder.annotation_placeholder">
	the annotation_placeholder specifies the placeholder object.
</ext_description>
<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Dimension_and_tolerance_callouts_arm  Entity:   Geometric_placeholder_set   
   <express_ref linkend="dimension_and_tolerance_callouts:arm:Dimension_and_tolerance_callouts_arm.Geometric_placeholder_set"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++  -->
<ext_description linkend="Dimension_and_tolerance_callouts_arm.Geometric_placeholder_set">
	An <b>Geometric_placeholder_set</b> is a type of
	<express_ref linkend="Elemental_geometric_shape_arm:arm:Elemental_geometric_shape_arm.Detailed_geometric_model_element"/>
	that collects detail model geometric elements including Cartesian_point, Point_on_curve, Point_on_surface, Planar_box and Axis_placement.
</ext_description>
<ext_description linkend="Dimension_and_tolerance_callouts_arm.Geometric_placeholder_set.elements">
	specifies the elements in the collection
</ext_description>







