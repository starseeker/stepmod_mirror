<ext_description linkend="Dimension_and_tolerance_callouts_arm.Associated_dimension_text">
	An <b>Associated_dimension_text</b> is a type of
	<express_ref linkend="Associative_text:arm:Associative_text_arm.Dimension_text"/>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Dimension_and_tolerance_callouts_arm  Entity:   Associated_dimension_text Attribute: is_displaying
   <express_ref linkend="dimension_and_tolerance_callouts:arm:Dimension_and_tolerance_callouts_arm.Associated_dimension_text.is_displaying"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++  -->
<ext_description linkend="Dimension_and_tolerance_callouts_arm.Associated_dimension_text.is_displaying">
	specifies the role of the
	<express_ref linkend="Dimension_tolerance:arm:Dimension_tolerance_arm.Geometric_dimension"/>
	for the <b>Associated_dimension_text</b>.
</ext_description>
  

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
