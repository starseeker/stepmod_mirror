   <express_ref linkend="dimension_and_tolerance_callouts:arm:Dimension_and_tolerance_callouts_arm.Dimension_curve_terminator_to_projection_curve_associativity"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++  -->
<ext_description linkend="Dimension_and_tolerance_callouts_arm.Dimension_curve_terminator_to_projection_curve_associativity">
	An <b>Dimension_curve_terminator_to_projection_curve_associativity</b> is a type of
	<express_ref linkend="Associative_draughting_elements:arm:Associative_draughting_elements_arm.Annotation_occurrence_associativity"/>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Dimension_and_tolerance_callouts_arm  Entity: Dimension_curve_terminator_to_projection_curve_associativity  Attribute: relating_element
   <express_ref linkend="dimension_and_tolerance_callouts:arm:Dimension_and_tolerance_callouts_arm.Dimension_curve_terminator_to_projection_curve_associativity.relating_element"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++  -->
<ext_description linkend="Dimension_and_tolerance_callouts_arm.Dimension_curve_terminator_to_projection_curve_associativity.relating_element">
	an inherited attribute shall be of type
	<express_ref linkend="Draughting_annotation:arm:Draughting_annotation_arm.Dimension_curve_terminator"/>
	for the <b>Dimension_curve_terminator_to_projection_curve_associativity</b>.
</ext_description> 

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Dimension_and_tolerance_callouts_arm  Entity: Dimension_curve_terminator_to_projection_curve_associativity  Attribute: related_element
   <express_ref linkend="dimension_and_tolerance_callouts:arm:Dimension_and_tolerance_callouts_arm.Dimension_curve_terminator_to_projection_curve_associativity.related_element"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++  -->
<ext_description linkend="Dimension_and_tolerance_callouts_arm.Dimension_curve_terminator_to_projection_curve_associativity.related_element">
	an inherited attribute shall be of type
	<express_ref linkend="Draughting_element:arm:Draughting_element_arm.Projection_curve"/>
	for the <b>Dimension_curve_terminator_to_projection_curve_associativity</b>.
</ext_description> 

