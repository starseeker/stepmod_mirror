<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="mim_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1362">
   <application name="JSDAI" owner="LKSoft" source="dimension_and_tolerance_callouts_mim schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Dimension_and_tolerance_callouts_mim">
      <interface kind="use" schema="Associative_draughting_elements_mim"/>
      <interface kind="use" schema="Dimension_tolerance_mim"/>
      <interface kind="use" schema="Draughting_annotation_mim"/>
      <interface kind="use" schema="Draughting_element_specialisations_mim"/>
      <interface kind="use" schema="Geometric_tolerance_mim"/>
      <type name="datc_draughting_model_item_definition">
         <select basedon="draughting_model_item_definition" extensible="YES" genericentity="YES" selectitems="dimensional_size geometric_tolerance"/>
      </type>
      <type name="dimensioning_standard_document_reference_item">
         <select basedon="document_reference_item" extensible="YES" genericentity="YES" selectitems="product_definition"/>
      </type>
      <entity name="dimension_curve_terminator_to_projection_curve_associativity" supertypes="annotation_occurrence_associativity">
         <explicit name="relating_annotation_occurrence">
            <typename name="dimension_curve_terminator"/>
            <redeclaration entity-ref="annotation_occurrence_relationship"/>
         </explicit>
         <explicit name="related_annotation_occurrence">
            <typename name="projection_curve"/>
            <redeclaration entity-ref="annotation_occurrence_relationship"/>
         </explicit>
      </entity>
      <rule appliesto="draughting_model" name="draughting_model_items_constraint">
         <where expression="SIZEOF(QUERY(dm &lt;* draughting_model |&#10;    NOT(SIZEOF(QUERY(it1 &lt;* dm\representation.items |&#10;    (('DRAUGHTING_ELEMENT_SCHEMA.ANNOTATION_OCCURRENCE' IN TYPEOF(it1))&#10;    AND&#10;    NOT('AIC_DRAUGHTING_ANNOTATION.' + 'DRAUGHTING_ANNOTATION_OCCURRENCE' IN TYPEOF(it1)))&#10;      )) = 0)&#10;    )) = 0" label="WR1"/>
         <where expression="SIZEOF(QUERY(dm &lt;* draughting_model |&#10;    NOT(SIZEOF(QUERY(it1 &lt;* dm\representation.items |&#10;    (('DRAUGHTING_ELEMENT_SCHEMA.DRAUGHTING_CALLOUT' IN TYPEOF(it1))&#10;    AND&#10;    NOT('AIC_DRAUGHTING_ELEMENTS.DRAUGHTING_ELEMENTS' IN TYPEOF(it1)))&#10;      )) = 0)&#10;    )) = 0" label="WR2"/>
      </rule>
   </schema>
</express>
