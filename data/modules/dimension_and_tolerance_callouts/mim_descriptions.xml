<!-- +++++++++++ dimension_curve_terminator_to_projection_curve_associativity +++++++++++ -->
<ext_description linkend="Dimension_and_tolerance_callouts_mim.dimension_curve_terminator_to_projection_curve_associativity">
	A <b>dimension_curve_terminator_to_projection_curve_associativity</b> is a type of
	<express_ref linkend="aic_associative_draughting_elements:ir_express:aic_associative_draughting_elements.annotation_occurrence_associativity"/>
	that implements the ARM concept of
	<express_ref linkend="Dimension_and_tolerance_callouts:arm:Dimension_and_tolerance_callouts_arm.Dimension_curve_terminator_to_projection_curve_associativity"/>.
</ext_description>

<!-- relating_annotation_occurrence -->
<ext_description linkend="Dimension_and_tolerance_callouts_mim.dimension_curve_terminator_to_projection_curve_associativity.relating_annotation_occurrence">
	an attribute inherited from the
	<express_ref linkend="aic_associative_draughting_elements:ir_express:aic_associative_draughting_elements.annotation_occurrence_associativity"/>
	shall be redeclared as the
	<express_ref linkend="draughting_element_schema:ir_express:draughting_element_schema.dimension_curve_terminator"/>
	for the <b>dimension_curve_terminator_to_projection_curve_associativity</b>.
</ext_description>

<!-- related_annotation_occurrence -->
<ext_description linkend="Dimension_and_tolerance_callouts_mim.dimension_curve_terminator_to_projection_curve_associativity.related_annotation_occurrence">
	an attribute inherited from the
	<express_ref linkend="aic_associative_draughting_elements:ir_express:aic_associative_draughting_elements.annotation_occurrence_associativity"/>
	shall be redeclared as the
	<express_ref linkend="draughting_element_schema:ir_express:draughting_element_schema.projection_curve"/>
	for the <b>dimension_curve_terminator_to_projection_curve_associativity</b>.
</ext_description>


<!-- +++++++++++ draughting_model_items_constraint +++++++++++ -->
<ext_description linkend="Dimension_and_tolerance_callouts_mim.draughting_model_items_constraint">
	The <b>draughting_model_items_constraint</b> ensures that all 
	<express_ref linkend="presentation_definition_schema:ir_express:presentation_definition_schema.annotation_occurrence"/> 
	and 
	<express_ref linkend="draughting_element_schema:ir_express:draughting_element_schema.draughting_callout">draughting_callouts</express_ref>
	in the set of 
	<express_ref linkend="representation_schema:ir_express:representation_schema.representation.items"/> 
	shall be of the specifc subtypes 
	<express_ref linkend="aic_draughting_annotation:ir_express:aic_draughting_annotation.draughting_annotation_occurrence"/> 
	and 
	<express_ref linkend="aic_draughting_elements:ir_express:aic_draughting_elements.draughting_elements"/> 
	respectively.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Dimension_and_tolerance_callouts_mim.draughting_model_items_constraint.wr:WR1">
	<p>
	Each 
	<express_ref linkend="presentation_definition_schema:ir_express:presentation_definition_schema.annotation_occurrence"/>
	in the set of items shall be of type 
	<express_ref linkend="aic_draughting_annotation:ir_express:aic_draughting_annotation.draughting_annotation_occurrence"/>
	</p>
	<note>
		This is required by ISO 10303-504 for entity 
		<express_ref linkend="aic_draughting_annotation:ir_express:aic_draughting_annotation.draughting_annotation_occurrence"/>.
	</note>
</ext_description>

<!-- WR2 -->
<ext_description linkend="Dimension_and_tolerance_callouts_mim.draughting_model_items_constraint.wr:WR2">
	<p>
	All the 
	<express_ref linkend="draughting_element_schema:ir_express:draughting_element_schema.draughting_callout">draughting_callouts</express_ref> 
	in the set of items shall be of type 
	<express_ref linkend="aic_draughting_elements:ir_express:aic_draughting_elements.draughting_elements"/>.
	</p>
	<note>
		This is required by ISO 10303-506 for entity 
		<express_ref linkend="aic_draughting_elements:ir_express:aic_draughting_elements.draughting_elements"/>.
	</note>
</ext_description>

</ext_descriptions>
