	<express_ref linkend="Dimension_tolerance:arm:Dimension_tolerance_arm.tolerance_principle_type"/>
	for the <b>Geometric_dimension</b>.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Dimension_tolerance_arm  Entity: Geometric_dimension  Attribute: modifiers 
   <express_ref linkend="dimension_tolerance:arm:Dimension_tolerance_arm.Geometric_dimension.modifiers"/>
