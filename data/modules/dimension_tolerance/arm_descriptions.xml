  <ext_description linkend="Dimension_tolerance_arm.Angular_size">
  	An <b>Angular_size</b> is a type of 
  	<express_ref linkend="dimension_tolerance:arm:Dimension_tolerance_arm.Dimensional_size"/> 
  	that represents a dimension representing an angular size of an aspect of a part.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Dimension_tolerance_arm  Entity: Angular_size  Attribute: angle_selection 
   <express_ref linkend="dimension_tolerance:arm:Dimension_tolerance_arm.Angular_size.angle_selection"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
