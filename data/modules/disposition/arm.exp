SCHEMA Disposition_arm;

USE FROM Approval_arm;

USE FROM Identification_assignment_arm;

TYPE disposition_identification_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON identification_item WITH
  (Disposition);
END_TYPE;

ENTITY Disposition
SUBTYPE OF (Approval);
  name : STRING;
END_ENTITY;

END_SCHEMA;

