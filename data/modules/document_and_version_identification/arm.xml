<application name="GraphicalEXPRESS" owner="Eurostep" url="www.eurostep.com" version="1.3" source="Document_and_version_identification_arm_v9.vsd" />
<schema name="Document_and_version_identification_arm">
<interface kind="use" schema="Product_identification_arm">
</interface>
<interface kind="use" schema="Product_version_arm">
</interface>
<entity name="Document" supertypes="Product">
<graphic.element page="2" xcoord="97" ycoord="26" />
</entity>
<entity name="Document_version" 
supertypes="Product_version">
<explicit name="of_product">
<typename name="Document" />
<redeclaration entity-ref="Product_version" />
</explicit>
<graphic.element page="2" xcoord="324" ycoord="26" />
</entity>
<rule 
    name="product_version_constraint" 
    appliesto="Product_version">
<where label="WR1"
expression="SIZEOF ( QUERY ( pv &lt;* Product_version | 
 ( NOT 
    ( 'Document_and_version_identification_arm.' + 
      'Document_version' 
      IN TYPEOF ( pv)
    ) 
 ) 
 AND 
 ( 'Document_and_version_identification_arm.' +
   'Document' 
   IN TYPEOF ( ts.of_product) 
 ) 
 ) ) =0;
"
/>
</rule>
<graphic.element page="1" xcoord="155" ycoord="28" />
</schema>
</express>
