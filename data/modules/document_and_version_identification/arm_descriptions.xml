  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Document_and_version_identification_arm 
   <express_ref linkend="document_and_version_identification:arm:Document_and_version_identification_arm"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
   <ext_description linkend="Document_and_version_identification_arm"/>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Document_and_version_identification_arm  Entity: Document  
   <express_ref linkend="document_and_version_identification:arm:Document_and_version_identification_arm.Document"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
  <ext_description
    linkend="Document_and_version_identification_arm.Document">
    A <b>Document</b> is a type of 
    <express_ref 
      linkend="product_identification:arm:Product_identification_arm.Product">
      Product
    </express_ref> used to identify documentation data that is under
    configuration change management. 
</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Document_and_version_identification_arm  Entity: Document_version   
   <express_ref linkend="document_and_version_identification:arm:Document_and_version_identification_arm.Document_version"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
  <ext_description
    linkend="Document_and_version_identification_arm.Document_version">
    
    A <b>Document_version</b> is a type of 
  <express_ref
    linkend="product_version:arm:Product_version_arm.Product_version">
    Product_version.
  </express_ref>
  A <b>Document_version</b> identifies a particular version of a document. 
  The attribute of_product of a Document_version shall refer to an instance
  of the entity 
  <express_ref
    linkend="document_and_version_identification:arm:Document_and_version_identification_arm.Document">
    Document</express_ref>.  
   </ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Document_and_version_identification_arm  Entity: Document_version  Where: WR1 
   <express_ref linkend="document_and_version_identification:arm:Document_and_version_identification_arm.Document.wr:WR1"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Document_and_version_identification_arm.Document_version.wr:WR1">The <b>Document_version</b> shall be a variant of a 'document'.</ext_description>
</ext_descriptions>