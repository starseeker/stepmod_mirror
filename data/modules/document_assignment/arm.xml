<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1122">
   <application name="JSDAI" owner="LKSoft" source="document_assignment_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Document_assignment_arm">
      <interface kind="use" schema="Document_and_version_identification_arm"/>
      <interface kind="use" schema="Document_definition_arm"/>
      <interface kind="use" schema="File_identification_arm"/>
      <type name="assigned_document_select">
         <select selectitems="Document Document_definition Document_version File"/>
      </type>
      <type name="documented_element_select">
         <select extensible="YES" genericentity="YES" selectitems="Product_view_definition"/>
      </type>
      <entity name="Document_assignment">
         <explicit name="assigned_document">
            <typename name="assigned_document_select"/>
         </explicit>
         <explicit name="is_assigned_to">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="documented_element_select"/>
         </explicit>
         <explicit name="role">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="Partial_document_assignment" supertypes="Document_assignment">
         <explicit name="document_portion">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
   </schema>
</express>
