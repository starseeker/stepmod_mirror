  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Document_assignment_mim  Type: document_reference_item 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Document_assignment_mim.document_reference_item">
		
      <p>
		A <b>document_reference_item</b> type specifies those objects to which a <b>document_reference</b>
		or a <b>document_usage_constraint</b> may be assigned through an <b>applied_document_reference</b>
		or <b>applied_document_usage_constraint_assignment</b> respectively. 
		</p>
	
   </ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Document_assignment_mim  Entity: applied_document_reference 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Document_assignment_mim.applied_document_reference">
		
      <p>An <b>applied_document_reference</b> is a type of <b>document_reference</b>
		that assigns a <b>document</b> to a <b>document_reference_item</b>.
		</p>
	
   </ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Document_assignment_mim  Entity: applied_document_reference  Attribute: items 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Document_assignment_mim.applied_document_reference.items"/>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Document_assignment_mim  Entity: applied_document_usage_constraint_assignment 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Document_assignment_mim.applied_document_usage_constraint_assignment">
		
      <p>An <b>applied_document_usage_constraint_assignment</b> is a type of <b>document_usage_constraint_assignment</b>
		that assigns a <b>document_usage_constraint</b> to a <b>document_reference_item</b>.
		</p>
	
   </ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Document_assignment_mim  Entity: applied_document_usage_constraint_assignment  Attribute: items 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Document_assignment_mim.applied_document_usage_constraint_assignment.items"/>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Document_assignment_mim  Entity: document_product_equivalence 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Document_assignment_mim.document_product_equivalence">
		
      <p>A <b>document_product_equivalence</b> is a type of <b>document_product_association</b>.
		It specifies the equivalence of a <b>product</b>, a <b>product_definition</b>,
		or <b>product_definition_formation</b> instance with a <b>document</b> instance.
		</p>
	
   </ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Document_assignment_mim  Entity: document_product_equivalence  Where: WR1 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Document_assignment_mim.document_product_equivalence.wr:WR1">The name of the <b>document_product_association</b> shall be 'equivalence'.
	</ext_description>
</ext_descriptions>