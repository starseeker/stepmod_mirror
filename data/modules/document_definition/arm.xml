<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.6 2002/01/16 12:11:43 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.6"
    source="../data/modules/document_definition/arm.exp"/>

  <schema name="Document_definition_arm">
    <interface
      kind="use"
      schema="Document_and_version_identification_arm">
    </interface>

    <interface
      kind="use"
      schema="File_identification_arm">
    </interface>

    <entity
      name="Document_definition">
      <explicit
        name="ABSTRACT">
        <typename
          name="ABSTRACT SUPERTYPE OF ( ONEOF ( Physical_document_definition , Digital_document_definition ) ) "/>
      </explicit>
      <explicit
        name="id">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="associated_document_version">
        <typename
          name="Document_version"/>
      </explicit>
    </entity>

    <entity
      name="Digital_document_definition">
      <explicit
        name="SUBTYPE">
        <typename
          name="SUBTYPE OF ( Document_definition ) "/>
      </explicit>
      <explicit
        name="file">
        <aggregate
          type="SET"
          lower="0"
          upper="?"/>
        <typename
          name="digital_file"/>
      </explicit>
    </entity>

    <entity
      name="Physical_document_definition"
      supertypes="Document_definition">
      <explicit
        name="component">
        <aggregate
          type="SET"
          lower="0"
          upper="?"/>
        <typename
          name="hardcopy"/>
      </explicit>
    </entity>

  </schema>

</express>
