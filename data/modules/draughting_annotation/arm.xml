<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1206">
   <application name="JSDAI" owner="LKSoft" source="draughting_annotation_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Draughting_annotation_arm">
      <interface kind="use" schema="Draughting_element_arm"/>
      <type name="da_draughting_callout_content_select">
         <select basedon="draughting_callout_content_select" selectitems="Annotation_subfigure Annotation_symbol"/>
      </type>
      <type name="symbol_definition_select">
         <select selectitems="Dimension_symbol General_symbol Geometric_tolerance_symbol Marker Surface_condition_symbol Terminator"/>
      </type>
      <entity name="Annotation_subfigure" supertypes="Annotation_element"/>
      <entity name="Annotation_symbol" supertypes="Annotation_element">
         <explicit name="symbol_definition">
            <typename name="symbol_definition_select"/>
         </explicit>
      </entity>
      <entity name="Dimension_curve_terminator" supertypes="Terminator_symbol"/>
      <entity name="Dimension_symbol"/>
      <entity name="General_symbol" supertypes="Representation"/>
      <entity name="Geometric_tolerance_symbol"/>
      <entity name="Surface_condition_symbol"/>
      <entity name="Terminator_symbol" supertypes="Annotation_symbol"/>
      <subtype.constraint entity="Annotation_element" name="alternate_annotation_elements" super.expression="ONEOF (Annotation_subfigure, Annotation_symbol, Fill_area)"/>
   </schema>
</express>
