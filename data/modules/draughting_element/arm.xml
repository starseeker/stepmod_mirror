<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1310">
   <application name="JSDAI" owner="LKSoft" source="draughting_element_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Draughting_element_arm">
      <interface kind="use" schema="Associative_text_arm"/>
      <interface kind="use" schema="Curve_appearance_arm"/>
      <type name="annotation_plane_element">
         <select selectitems="Draughting_callout Styled_element"/>
      </type>
      <type name="de_appearance_select">
         <select basedon="appearance_select" selectitems="Curve_appearance Point_appearance Text_style"/>
      </type>
      <type name="draughting_callout_content_select">
         <select extensible="YES" genericentity="YES" selectitems="Annotation_curve Annotation_text_occurrence"/>
      </type>
      <type name="plane_or_planar_box">
         <select selectitems="Planar_box Plane"/>
      </type>
      <entity name="Annotation_fill_area" supertypes="Detailed_geometric_model_element">
         <explicit name="boundaries">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Curve"/>
         </explicit>
      </entity>
      <entity name="Annotation_plane" supertypes="Annotation_element">
         <explicit name="elements" optional="YES">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="annotation_plane_element"/>
         </explicit>
         <explicit name="element">
            <typename name="plane_or_planar_box"/>
            <redeclaration entity-ref="Styled_element"/>
         </explicit>
      </entity>
      <entity name="Dimension_curve" supertypes="Annotation_curve">
         <explicit name="terminator">
            <aggregate lower="0" type="SET" upper="2"/>
            <typename name="Terminator"/>
         </explicit>
      </entity>
      <entity name="Dimension_curve_directed_callout" supertypes="Draughting_callout">
         <explicit name="dimension_curve">
            <typename name="Dimension_curve"/>
         </explicit>
         <where expression="dimension_curve IN SELF\Draughting_callout.contents" label="WR1"/>
      </entity>
      <entity name="Draughting_callout" super.expression="ONEOF (Dimension_curve_directed_callout, Leader_directed_callout, Projection_directed_callout)" supertypes="Representation_item">
         <explicit name="contents">
            <aggregate lower="1" type="LIST" upper="?"/>
            <typename name="draughting_callout_content_select"/>
         </explicit>
      </entity>
      <entity name="Fill_area" supertypes="Annotation_element">
         <explicit name="element">
            <typename name="Annotation_fill_area"/>
            <redeclaration entity-ref="Styled_element"/>
         </explicit>
         <explicit name="reference_point">
            <typename name="Point"/>
         </explicit>
      </entity>
      <entity name="Generic_annotation_curve" supertypes="Annotation_curve"/>
      <entity name="Leader" supertypes="Annotation_curve"/>
      <entity name="Leader_directed_callout" supertypes="Draughting_callout">
         <explicit name="leader">
            <typename name="Leader"/>
         </explicit>
         <where expression="leader IN SELF\Draughting_callout.contents" label="WR1"/>
      </entity>
      <entity name="Projection_curve" supertypes="Annotation_curve"/>
      <entity name="Projection_directed_callout" supertypes="Draughting_callout">
         <explicit name="projection_curve">
            <typename name="Projection_curve"/>
         </explicit>
         <where expression="projection_curve IN SELF\Draughting_callout.contents" label="WR1"/>
      </entity>
      <subtype.constraint entity="Annotation_curve" name="annotation_curve_subtypes" super.expression="(ONEOF (Leader, Projection_curve, Dimension_curve) ANDOR Generic_annotation_curve)"/>
   </schema>
</express>
