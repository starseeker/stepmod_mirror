<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1312">
   <application name="JSDAI" owner="LKSoft" source="draughting_element_specialisations_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Draughting_element_specialisations_arm">
      <interface kind="use" schema="Draughting_element_arm"/>
      <type name="chained_or_parallel">
         <enumeration items="chained parallel"/>
      </type>
      <type name="dimension_callout_select">
         <select extensible="YES" genericentity="YES" selectitems="Annotation_text_occurrence"/>
      </type>
      <type name="dimension_type">
         <enumeration items="angular curve diameter leader_directed linear ordinate radius"/>
      </type>
      <type name="primary_or_secondary">
         <enumeration items="primary secondary"/>
      </type>
      <entity name="Datum_feature_callout" supertypes="Draughting_element"/>
      <entity name="Datum_target_callout" supertypes="Draughting_element"/>
      <entity abstract.supertype="YES" name="Dimension" super.expression="Dimension_callout ANDOR Dimension_value" supertypes="Draughting_element">
         <explicit name="id">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="Dimension_callout" supertypes="Dimension">
         <explicit name="callout_element">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="dimension_callout_select"/>
         </explicit>
         <explicit name="dimension_type">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="dimension_type"/>
         </explicit>
      </entity>
      <entity name="Dimension_callout_relationship">
         <explicit name="relating_draughting_callout">
            <typename name="Dimension_callout"/>
         </explicit>
         <explicit name="related_draughting_callout">
            <typename name="Dimension_value"/>
         </explicit>
         <explicit name="relation_type">
            <typename name="primary_or_secondary"/>
         </explicit>
      </entity>
      <entity name="Dimension_prefix" supertypes="Draughting_element">
         <explicit name="prefix">
            <typename name="Dimension_text"/>
         </explicit>
         <inverse attribute="prefix" entity="Structured_dimension_callout" name="parent"/>
         <where expression="prefix IN SELF\Draughting_callout.contents" label="WR1"/>
         <where expression="prefix\Representation_item.name = 'prefix text'" label="WR2"/>
      </entity>
      <entity name="Dimension_sequence_pair">
         <explicit name="alignment">
            <typename name="chained_or_parallel"/>
         </explicit>
         <explicit name="predecessor">
            <typename name="Dimension_callout"/>
         </explicit>
         <explicit name="successor">
            <typename name="Dimension_callout"/>
         </explicit>
      </entity>
      <entity name="Dimension_suffix" supertypes="Draughting_element">
         <explicit name="suffix">
            <typename name="Dimension_text"/>
         </explicit>
         <inverse attribute="suffix" entity="Structured_dimension_callout" name="parent"/>
         <where expression="suffix IN SELF\Draughting_callout.contents" label="WR1"/>
         <where expression="suffix\Representation_item.name = 'suffix text'" label="WR2"/>
      </entity>
      <entity name="Dimension_value" supertypes="Dimension">
         <explicit name="dimension_value">
            <typename name="Dimension_text"/>
         </explicit>
         <where expression="dimension_value IN SELF\Draughting_callout.contents" label="WR1"/>
         <where expression="dimension_value\Representation_item.name = 'dimension value'" label="WR2"/>
      </entity>
      <entity name="Draughting_element" super.expression="Datum_feature_callout ANDOR Datum_target_callout ANDOR Dimension ANDOR Dimension_prefix ANDOR Dimension_suffix ANDOR Geometric_tolerance_callout ANDOR Surface_condition_callout" supertypes="Draughting_callout"/>
      <entity name="Geometric_tolerance_callout" supertypes="Draughting_element"/>
      <entity name="Structured_dimension_callout" supertypes="Dimension_value">
         <explicit name="prefix" optional="YES">
            <typename name="Dimension_prefix"/>
         </explicit>
         <explicit name="suffix" optional="YES">
            <typename name="Dimension_suffix"/>
         </explicit>
         <explicit name="tolerance_value" optional="YES">
            <typename name="Dimension_text"/>
         </explicit>
         <explicit name="unit_text" optional="YES">
            <typename name="Dimension_text"/>
         </explicit>
         <where expression="tolerance_value IN SELF\Draughting_callout.contents" label="WR1"/>
         <where expression="NOT EXISTS(tolerance_value) OR (tolerance_value\Representation_item.name = 'tolerance value')" label="WR2"/>
         <where expression="unit_text IN SELF\Draughting_callout.contents" label="WR3"/>
         <where expression="NOT EXISTS(unit_text) OR (unit_text\Representation_item.name = 'unit text')" label="WR4"/>
      </entity>
      <entity name="Surface_condition_callout" supertypes="Draughting_element"/>
   </schema>
</express>
