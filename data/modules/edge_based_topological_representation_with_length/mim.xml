<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="mim_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1830">
   <application name="JSDAI" owner="LKSoft" source="edge_based_topological_representation_with_length_mim schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Edge_based_topological_representation_with_length_mim">
      <interface kind="use" schema="Elemental_topology_mim"/>
      <interface kind="use" schema="topology_schema">
         <interfaced.item name="edge_with_length"/>
         <interfaced.item name="subedge"/>
         <interfaced.item name="vertex_on_edge"/>
      </interface>
      <type name="connected_edge_with_length_set_items">
         <select selectitems="connected_edge_set path vertex_on_edge"/>
      </type>
      <entity name="connected_edge_with_length_set_representation" supertypes="representation">
         <explicit name="items">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="connected_edge_with_length_set_items"/>
            <redeclaration entity-ref="representation"/>
         </explicit>
         <where expression="SIZEOF(QUERY ( it &lt;* SELF.items | 'TOPOLOGY_SCHEMA.CONNECTED_EDGE_SET' IN TYPEOF(it)) ) = 1" label="WR1"/>
      </entity>
   </schema>
</express>
