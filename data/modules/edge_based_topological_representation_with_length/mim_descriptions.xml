<ext_description linkend="Edge_based_topological_representation_with_length_mim.connected_edge_with_length_set_representation">
	A <b>connected_edge_with_length_set_representation</b> is a type of 
	<express_ref linkend="representation_schema:ir_express:representation_schema.representation"/>
	that contains a single
	<express_ref linkend="topology_schema:ir_express:topology_schema.connected_edge_set"/>
	and may contain and number of
	<express_ref linkend="topology_schema:ir_express:topology_schema.path"/>
	and
	<express_ref linkend="topology_schema:ir_express:topology_schema.vertex_on_edge"/>s.
</ext_description>
<ext_description linkend="Edge_based_topological_representation_with_length_mim.connected_edge_with_length_set_representation.items">
	an inherited attribute that is constrained to have values of type
	<express_ref linkend="topology_schema:ir_express:topology_schema.connected_edge_set"/>,
	<express_ref linkend="topology_schema:ir_express:topology_schema.path"/>
	or
	<express_ref linkend="topology_schema:ir_express:topology_schema.vertex_on_edge"/>
	only.
</ext_description>
<ext_description linkend="Edge_based_topological_representation_with_length_mim.connected_edge_with_length_set_representation.wr:WR1">
	the <b>items</b> attribute shall contain a single
	<express_ref linkend="topology_schema:ir_express:topology_schema.connected_edge_set"/>.
</ext_description>


