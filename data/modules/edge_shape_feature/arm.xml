<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-10-10T12:46:20" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="edge_shape_feature_arm schema_instance"/>
   <schema name="Edge_shape_feature_arm">
      <interface kind="use" schema="Layered_2d_shape_arm"/>
      <interface kind="use" schema="Value_with_unit_extension_arm"/>
      <entity name="Bevel_edge_feature_shape" supertypes="Edge_shape">
         <explicit name="minimum_bevel_width">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="minimum_bevel_angle">
            <typename name="Angle_data_element"/>
         </explicit>
         <explicit name="maximum_bevel_width">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="maximum_bevel_angle">
            <typename name="Angle_data_element"/>
         </explicit>
      </entity>
      <entity name="Edge_shape" abstract.supertype="YES" supertypes="Planar_shape Feature_shape_definition" super.expression="ONEOF (Bevel_edge_feature_shape, Step_edge_feature_shape, Radius_edge_feature_shape)">
         <derived name="name" expression="''">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="Representation"/>
         </derived>
         <where label="WR1" expression="SIZEOF(QUERY(escss &lt;* USEDIN(SELF,&#10; 'LAYERED_3D_SHAPE_ARM.SHAPE_DEFINITION_3D_INTERSECTION.SURFACE_INTERSECTION_RESULT') | NOT ('EDGE_SHAPE_FEATURE_ARM.EDGE_SEGMENT_CROSS_SECTION' IN TYPEOF(escss)))) = 0"/>
      </entity>
      <entity name="Radius_edge_feature_shape" supertypes="Edge_shape">
         <explicit name="minimum_radius_length">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="maximum_radius_length">
            <typename name="Length_data_element"/>
         </explicit>
      </entity>
      <entity name="Step_edge_feature_shape" supertypes="Edge_shape">
         <explicit name="minimum_step_width">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="maximum_step_width">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="maximum_step_depth">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="minimum_step_depth">
            <typename name="Length_data_element"/>
         </explicit>
      </entity>
   </schema>
</express>
