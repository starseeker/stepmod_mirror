<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1057">
   <application name="JSDAI" owner="LKSoft" source="effectivity_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Effectivity_arm">
      <interface kind="use" schema="Time_interval_arm"/>
      <interface kind="use" schema="Value_with_unit_arm"/>
      <entity name="Dated_effectivity" supertypes="Effectivity">
         <explicit name="start_bound" optional="YES">
            <typename name="date_or_event"/>
         </explicit>
         <explicit name="end_bound" optional="YES">
            <typename name="date_or_event"/>
         </explicit>
         <where expression="EXISTS(start_bound) OR EXISTS(end_bound)" label="WR1"/>
      </entity>
      <entity name="Effectivity" super.expression="ONEOF (Serial_effectivity, Dated_effectivity, Lot_effectivity, Time_interval_effectivity)">
         <explicit name="id">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="name">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="description" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="Effectivity_relationship">
         <explicit name="relation_type">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="description" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="relating_effectivity">
            <typename name="Effectivity"/>
         </explicit>
         <explicit name="related_effectivity">
            <typename name="Effectivity"/>
         </explicit>
      </entity>
      <entity name="Lot_effectivity" supertypes="Effectivity">
         <explicit name="lot_id">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="lot_size">
            <typename name="Value_with_unit"/>
         </explicit>
      </entity>
      <entity name="Serial_effectivity" supertypes="Effectivity">
         <explicit name="start_id">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="end_id" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="Time_interval_effectivity" supertypes="Effectivity">
         <explicit name="effectivity_period">
            <typename name="Time_interval"/>
         </explicit>
      </entity>
   </schema>
</express>
