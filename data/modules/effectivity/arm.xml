<?xml version="1.0"?>
<!-- $Id: arm.xml,v 1.8 2002/04/15 14:44:53 goset1 Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/04/15 14:44:53 $"
  rcs.revision="$Revision: 1.8 $"
  description.file="arm_descriptions.xml">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.9"
    source="../data/modules/effectivity/arm.exp"/>

  <schema name="Effectivity_arm">

    <interface
      kind="use"
      schema="Time_interval_arm">
    </interface>

    <interface
      kind="use"
      schema="Value_with_unit_arm">
    </interface>

    <entity
      name="Dated_effectivity"
      supertypes="Effectivity">
      <explicit
        name="start_bound">
        <typename
          name="date_or_event"/>
      </explicit>
      <explicit
        name="end_bound"
        optional="YES">
        <typename
          name="date_or_event"/>
      </explicit>
      <where
        label="IP1" >
				</where>
	 </entity>

    <entity
      name="Effectivity"
      super.expression="(ONEOF (Serial_effectivity, Dated_effectivity, Lot_effectivity, Time_interval_effectivity))">
      <explicit
        name="id">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="Effectivity_relationship">
      <explicit
        name="relation_type">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="relating_effectivity">
        <typename
          name="Effectivity"/>
      </explicit>
      <explicit
        name="related_effectivity">
        <typename
          name="Effectivity"/>
      </explicit>
    </entity>

    <entity
      name="Lot_effectivity"
      supertypes="Effectivity">
      <explicit
        name="lot_id">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="lot_size">
        <typename
          name="Value_with_unit"/>
      </explicit>
    </entity>

    <entity
      name="Serial_effectivity"
      supertypes="Effectivity">
      <explicit
        name="start_id">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="end_id"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="Time_interval_effectivity"
      supertypes="Effectivity">
      <explicit
        name="effectivity_period">
        <typename
          name="Time_interval"/>
      </explicit>
    </entity>

  </schema>

</express>
