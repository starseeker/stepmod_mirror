<?xml version="1.0" encoding="utf-8"?>
  <?xml-stylesheet 
  type="text/xsl" 
  href="../../../xsl/descriptions.xsl"?>
<?DOCTYPE 
  express SYSTEM "../../../dtd/description.dtd"?>
<ext_descriptions module_directory="effectivity" schema_file="arm.xml" rcs.date="$Date: $" rcs.revision="$Revision: $">


  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Effectivity_arm 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
   <ext_description linkend="Effectivity_arm"/>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Effectivity_arm  Type: effectivity_organization_or_person_in_organization_item 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Effectivity_arm.effectivity_organization_or_person_in_organization_item">
				This select extends the assignments of an organization or a person in an organization to <b>Effectivity</b> instances.
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Effectivity_arm  Entity: Effectivity 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Effectivity_arm.Effectivity">
				An <b>Effectivity</b> is the domain of applicability of product data.
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Effectivity_arm  Entity: Effectivity  Attribute: id 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Effectivity_arm.Effectivity.id">
					the identification of the Effectivity assigned by the organization.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Effectivity_arm  Entity: Effectivity  Attribute: name 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Effectivity_arm.Effectivity.name">
					words by which the Effectivity is known.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Effectivity_arm  Entity: Effectivity  Attribute: description 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Effectivity_arm.Effectivity.description">
					text which describes the Effectivity.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Effectivity_arm  Entity: Effectivity  Attribute: identifier 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Effectivity_arm.Effectivity.identifier">
					the organization (or person in an organization) that assigned and owns the Effectivity id.	
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Effectivity_arm  Entity: Effectivity_relationship 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Effectivity_arm.Effectivity_relationship">
				An <b>Effectivity_relationship</b> specifies that two effectivities are related. This is a foundation concept which may be refined in other modules. This concept can be used to define sequencing.
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Effectivity_arm  Entity: Effectivity_relationship  Attribute: relation_type 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Effectivity_arm.Effectivity_relationship.relation_type">
					an optional type descriptor for the relationship.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Effectivity_arm  Entity: Effectivity_relationship  Attribute: description 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Effectivity_arm.Effectivity_relationship.description">
					an optional description of the relationship.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Effectivity_arm  Entity: Effectivity_relationship  Attribute: effectivity1 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Effectivity_arm.Effectivity_relationship.effectivity1">
					the <b>Effectivity</b> to be related to <b>effectivity2</b>.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Effectivity_arm  Entity: Effectivity_relationship  Attribute: effectivity2 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Effectivity_arm.Effectivity_relationship.effectivity2">
					the <b>Effectivity</b> to be related to <b>effectivity1</b>.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Effectivity_arm  Entity: Serial_effectivity 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Effectivity_arm.Serial_effectivity">
				A <b>Serial_effectivity</b> is applicability of product data based on one or more serialized instances of an end item.
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Effectivity_arm  Entity: Serial_effectivity  Attribute: start 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Effectivity_arm.Serial_effectivity.start">
					the first valid serial number.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Effectivity_arm  Entity: Serial_effectivity  Attribute: end 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Effectivity_arm.Serial_effectivity.end">
					the last valid serial number. If a value for this attribute is not defined, the interval of applicability has no upper bound.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Effectivity_arm  Entity: Dated_effectivity 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Effectivity_arm.Dated_effectivity">
				A <b>Dated_effectivity</b> is applicability of product data based on a date or date range.
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Effectivity_arm  Entity: Dated_effectivity  Attribute: start_date 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Effectivity_arm.Dated_effectivity.start_date">
					the first valid date.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Effectivity_arm  Entity: Dated_effectivity  Attribute: end_date 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Effectivity_arm.Dated_effectivity.end_date">
					the last valid date. If a value for this attribute is not defined, the interval of applicability has no upper bound.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Effectivity_arm  Entity: Time_interval_effectivity 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Effectivity_arm.Time_interval_effectivity">
				A <b>Time_interval_effectivity</b> is applicability of product data based on a span of time.
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Effectivity_arm  Entity: Time_interval_effectivity  Attribute: interval 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Effectivity_arm.Time_interval_effectivity.interval">
					the span of time for which the applicability is valid.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Effectivity_arm  Entity: Lot_effectivity 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Effectivity_arm.Lot_effectivity">
				A <b>Lot_effectivity</b> is applicability of product data based on a lot of a certain size.
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Effectivity_arm  Entity: Lot_effectivity  Attribute: lot_id 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Effectivity_arm.Lot_effectivity.lot_id">
					the identification of the batch of items.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Effectivity_arm  Entity: Lot_effectivity  Attribute: lot_size 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Effectivity_arm.Lot_effectivity.lot_size">
					the size of the batch of items.
				</ext_description>
</ext_descriptions>