<?xml version="1.0"?><!-- $Id: mim.xml,v 1.5 2002/02/21 17:29:49 robbod Exp $ --><?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?><!DOCTYPE express SYSTEM "../../../dtd/express.dtd"><express language_version="2" rcs.date="$Date: 2002/02/21 17:29:49 $" rcs.revision="$Revision: 1.5 $">	<application name="express2xml.js" owner="Eurostep Limited" url="http://www.eurostep.com" version="1.6" source="../data/modules/effectivity/mim.exp"/>	<schema name="Effectivity_mim">		<interface kind="use" schema="Person_organization_assignment_mim"/>		<interface kind="use" schema="Date_time_mim"/>		<interface kind="use" schema="Event_mim"/>		<interface kind="use" schema="effectivity_schema">			<interfaced.item name="effectivity"/>			<interfaced.item name="effectivity_relationship"/>			<interfaced.item name="serial_numbered_effectivity"/>			<interfaced.item name="dated_effectivity"/>			<interfaced.item name="time_interval_based_effectivity"/>			<interfaced.item name="lot_effectivity"/>		</interface>		<type name="effectivity_organization_item">			<description>				An <b>effectivity_organization_item</b> extends organization assignment to <b>effectivity</b> instances.			</description>			<select basedon="organization_item" selectitems="effectivity"/>		</type>		<type name="effectivity_person_and_organization_item">			<description>				An <b>effectivity_person_and_organization_item</b> extends person and organization assignment to <b>effectivity instances</b>.			</description>			<select basedon="person_and_organization_item" selectitems="effectivity"/>		</type>	</schema></express>