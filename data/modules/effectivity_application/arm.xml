<?xml version="1.0"?><!-- edited with XML Spy v3.5 (http://www.xmlspy.com) by Michael Ward (Eurostep Limited) --><!-- $Id: arm.xml,v 1.3 2002/01/16 14:23:55 robbod Exp $ --><?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?><!DOCTYPE express SYSTEM "../../../dtd/express.dtd"><express language_version="2" rcs.date="$Date: 2002/01/16 14:23:55 $" rcs.revision="$Revision: 1.3 $">	<application name="express2xml.js" owner="Eurostep Limited" url="http://www.eurostep.com" version="1.6" source="../data/modules/effectivity_application/arm.exp"/>	<schema name="Effectivity_application_arm">		<interface kind="use" schema="End_item_identification_arm"/>		<interface kind="use" schema="Product_version_arm"/>		<interface kind="use" schema="Product_view_definition_arm"/>		<interface kind="use" schema="Product_view_definition_structure_arm"/>		<interface kind="use" schema="Effectivity_arm"/>		<type name="effectivity_context_select">			<description>				This selects between a contexts for an effectvity.			</description>			<select extensible="YES" selectitems="Configuration_item"/>		</type>		<type name="effectivity_item">			<description>				This select is a place holder for assignments of effectivity to elements of product data.			</description>			<select extensible="YES" selectitems="Product_view_definition Product_version View_definition_relationship"/>		</type>		<entity name="Effectivity_assignment">			<description>				An <b>Effectivity_assignment</b> assigns an Effectivity to product data.			</description>			<explicit name="assigned_effectivity">				<description>					the <b>Effectivity</b> to be assigned to product data.				</description>				<typename name="Effectivity"/>			</explicit>			<explicit name="role">				<description>					the purpose of the assignment.				</description>				<builtintype type="STRING"/>			</explicit>			<explicit name="context" optional="YES">				<description>					the optional context for the effectvity.				</description>				<typename name="effectivity_context_select"/>			</explicit>			<explicit name="items">				<description>					the product data to which the <b>Effectivity</b>is assigned.				</description>				<aggregate type="SET" lower="1" upper="?"/>				<typename name="effectivity_item"/>			</explicit>		</entity>	</schema></express>