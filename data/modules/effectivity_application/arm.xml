<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.0 2001-11-15 11:24:44+00 rob Exp rob $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.0"
    source="../data/modules/effectivity_application/arm.exp"/>

  <schema name="Effectivity_application_arm">
    <interface
      kind="use"
      schema="End_item_identification_arm">
    </interface>

    <interface
      kind="use"
      schema="Product_version_arm">
    </interface>

    <interface
      kind="use"
      schema="Product_view_definition_arm">
    </interface>

    <interface
      kind="use"
      schema="Product_view_definition_structure_arm">
    </interface>

    <interface
      kind="use"
      schema="Effectivity_arm">
    </interface>

    <type name="effectivity_context_select">
      <select
        extensible="YES"
        selectitems="Configuration_item">
      </select>

    </type>
    <type name="effectivity_item">
      <select
        extensible="YES"
        selectitems="Product_view_definition Product_version View_definition_relationship">
      </select>

    </type>
    <entity
      name="Effectivity_assignment">
      <explicit
        name="assigned_effectivity">
        <typename
          name="Effectivity"/>
      </explicit>
      <explicit
        name="role">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="context"
        optional="YES">
        <typename
          name="effectivity_context_select"/>
      </explicit>
      <explicit
        name="items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="effectivity_item"/>
      </explicit>
    </entity>

  </schema>

</express>
