<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="mim_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1059">
   <application name="JSDAI" owner="LKSoft" source="effectivity_application_mim schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Effectivity_application_mim">
      <interface kind="use" schema="basic_attribute_schema">
         <interfaced.item name="object_role"/>
         <interfaced.item name="role_association"/>
         <interfaced.item name="role_select"/>
      </interface>
      <interface kind="use" schema="Effectivity_mim"/>
      <interface kind="use" schema="management_resources_schema">
         <interfaced.item name="effectivity_assignment"/>
      </interface>
      <type name="effectivity_item">
         <select extensible="YES" genericentity="YES"/>
      </type>
      <entity name="applied_effectivity_assignment" supertypes="effectivity_assignment">
         <explicit name="items">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="effectivity_item"/>
         </explicit>
      </entity>
      <entity name="applied_ineffectivity_assignment" supertypes="effectivity_assignment">
         <explicit name="items">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="effectivity_item"/>
         </explicit>
         <where expression="SIZEOF(['EFFECTIVITY_SCHEMA.LOT_EFFECTIVITY',&#10;                'EFFECTIVITY_SCHEMA.SERIAL_NUMBERED_EFFECTIVITY',&#10;                'PRODUCT_DEFINITION_SCHEMA.PRODUCT_DEFINITION_EFFECTIVITY'] *&#10;                     TYPEOF(SELF.assigned_effectivity)&#10;              ) = 0" label="WR1"/>
      </entity>
   </schema>
</express>
