<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.2 2001/11/21 15:38:19 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.2"
    source="../data/modules/elemental_geometric_shape/arm.exp"/>

  <schema name="Elemental_geometric_shape_arm">
    <interface
      kind="use"
      schema="Foundation_representation_arm">
    </interface>

    <interface
      kind="use"
      schema="Identification_assignment_arm">
    </interface>

    <entity
      name="Cartesian_coordinate_space">
      <explicit
        name="unit">
        <aggregate
          type="SET"
          lower="2"
          upper="?"/>
        <typename
          name="REAL"/>
      </explicit>
    </entity>

    <entity
      name="Detailed_geometric_model_element"
      supertypes="Representation_item">
    </entity>

    <entity
      name="Geometric_model"
      supertypes="Representation">
      <explicit
        name="is_defined_in">
        <typename
          name="Cartesian_coordinate_space"/>
      </explicit>
      <explicit
        name="model_id">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="version_id"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="role">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="elements">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="Detailed_geometric_model_element"/>
      </explicit>
      <explicit
        name="model_extent"
        optional="YES">
        <builtintype
          type="REAL"/>
      </explicit>
      <explicit
        name="accuracy"
        optional="YES">
        <builtintype
          type="REAL"/>
      </explicit>
      <explicit
        name="units"
        optional="YES">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="REAL"/>
      </explicit>
    </entity>

    <entity
      name="Template_instance"
      supertypes="Detailed_geometric_model_element">
      <explicit
        name="id">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="scale"
        optional="YES">
        <builtintype
          type="REAL"/>
      </explicit>
      <explicit
        name="template_definition">
        <typename
          name="Geometric_model"/>
      </explicit>
      <explicit
        name="transformation">
        <typename
          name="Transformation"/>
      </explicit>
    </entity>

    <entity
      name="Transformation">
    </entity>

  </schema>

</express>
