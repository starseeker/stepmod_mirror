<?xml version="1.0" encoding="utf-8"?><?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express
  SYSTEM "../../../dtd/express.dtd">
<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="">
   <application name="JSDAI" owner="LKSoft" source="elemental_geometric_shape_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Elemental_geometric_shape_arm">
      <interface kind="use" schema="Foundation_representation_arm"/>
      <interface kind="use" schema="Identification_assignment_arm"/>
      <type name="cartesian_transformation">
         <select selectitems="Cartesian_transformation_2d Cartesian_transformation_3d"/>
      </type>
      <type name="template_definition_select">
         <select extensible="YES" selectitems="Geometric_model"/>
      </type>
      <entity name="Axis_placement" super.expression="ONEOF (Axis_placement_2d, Axis_placement_3d)" supertypes="Detailed_geometric_model_element">
         <explicit name="origin">
            <typename name="Cartesian_point"/>
         </explicit>
         <explicit name="x_axis">
            <typename name="Direction"/>
         </explicit>
         <explicit name="y_axis">
            <typename name="Direction"/>
         </explicit>
         <derived expression="SIZEOF(origin.coordinates)" name="dim">
            <builtintype type="INTEGER"/>
         </derived>
         <where expression="dim &gt;1" label="WR1"/>
         <where expression="dim = SIZEOF(x_axis.coordinates)" label="WR2"/>
         <where expression="dim = SIZEOF(y_axis.coordinates)" label="WR3"/>
      </entity>
      <entity name="Axis_placement_2d" supertypes="Axis_placement"/>
      <entity name="Axis_placement_3d" supertypes="Axis_placement"/>
      <entity name="Axis_placement_mapping">
         <explicit name="source">
            <typename name="Axis_placement"/>
         </explicit>
         <explicit name="target">
            <typename name="Axis_placement"/>
         </explicit>
         <where expression="source\Axis_placement.dim = target\Axis_placement.dim" label="WR1"/>
      </entity>
      <entity name="Cartesian_point" supertypes="Detailed_geometric_model_element">
         <explicit name="coordinates">
            <aggregate lower="1" type="LIST" upper="3"/>
            <typename name="length_measure"/>
         </explicit>
      </entity>
      <entity name="Cartesian_transformation_2d" supertypes="Detailed_geometric_model_element">
         <explicit name="multiplication_matrix">
            <aggregate lower="1" type="ARRAY" upper="2"/>
            <typename name="Direction"/>
         </explicit>
         <explicit name="translation">
            <typename name="Cartesian_point"/>
         </explicit>
         <where expression="SIZEOF(multiplication_matrix[1]\Direction.coordinates)=2" label="WR1"/>
         <where expression="SIZEOF(multiplication_matrix[2]\Direction.coordinates)=2" label="WR2"/>
         <where expression="SIZEOF(translation.coordinates)=2" label="WR3"/>
      </entity>
      <entity name="Cartesian_transformation_3d" supertypes="Detailed_geometric_model_element">
         <explicit name="multiplication_matrix">
            <aggregate lower="1" type="ARRAY" upper="3"/>
            <typename name="Direction"/>
         </explicit>
         <explicit name="translation">
            <typename name="Cartesian_point"/>
         </explicit>
         <where expression="SIZEOF(multiplication_matrix[1]\Direction.coordinates)=3" label="WR1"/>
         <where expression="SIZEOF(multiplication_matrix[2]\Direction.coordinates)=3" label="WR2"/>
         <where expression="SIZEOF(multiplication_matrix[3]\Direction.coordinates)=3" label="WR3"/>
         <where expression="SIZEOF(translation.coordinates)=3" label="WR4"/>
      </entity>
      <entity abstract.supertype="YES" name="Detailed_geometric_model_element" super.expression="ONEOF (Cartesian_point, Direction, Axis_placement, Cartesian_transformation_2d, Cartesian_transformation_3d, Geometric_placement_operation)" supertypes="Representation_item"/>
      <entity name="Direction" supertypes="Detailed_geometric_model_element">
         <explicit name="coordinates">
            <aggregate lower="2" type="LIST" upper="3"/>
            <typename name="length_measure"/>
         </explicit>
      </entity>
      <entity name="Geometric_coordinate_space" supertypes="Numerical_representation_context">
         <explicit name="dimension_count">
            <builtintype type="INTEGER"/>
         </explicit>
         <where expression="dimension_count &gt;0" label="WR1"/>
      </entity>
      <entity name="Geometric_model" supertypes="Representation">
         <explicit name="version_id" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="model_extent" optional="YES">
            <typename name="length_measure"/>
         </explicit>
         <explicit name="context_of_items">
            <typename name="Geometric_coordinate_space"/>
            <redeclaration entity-ref="Representation"/>
         </explicit>
         <explicit name="items">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Detailed_geometric_model_element"/>
            <redeclaration entity-ref="Representation"/>
         </explicit>
      </entity>
      <entity name="Geometric_operator_transformation" supertypes="Geometric_placement_operation">
         <explicit name="target">
            <typename name="cartesian_transformation"/>
         </explicit>
      </entity>
      <entity name="Geometric_placement" supertypes="Geometric_placement_operation">
         <explicit name="target">
            <typename name="Axis_placement"/>
         </explicit>
      </entity>
      <entity abstract.supertype="YES" name="Geometric_placement_operation" super.expression="ONEOF (Geometric_placement, Geometric_operator_transformation)" supertypes="Detailed_geometric_model_element">
         <explicit name="source">
            <typename name="Axis_placement"/>
         </explicit>
         <explicit name="template_definition">
            <typename name="template_definition_select"/>
         </explicit>
      </entity>
   </schema>
</express>