<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.16 2002/07/05 11:10:27 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="mim_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.16"
    source="../data/modules/elemental_geometric_shape/mim.exp"/>

  <schema name="Elemental_geometric_shape_mim">
    <interface
      kind="use"
      schema="Foundation_representation_mim">
    </interface>

    <interface
      kind="use"
      schema="Identification_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="Value_with_unit_mim">
    </interface>

    <interface
      kind="use"
      schema="geometry_schema">
      <interfaced.item
        name="cartesian_point"/>
      <interfaced.item
        name="direction"/>
      <interfaced.item
        name="axis2_placement_2d"/>
      <interfaced.item
        name="axis2_placement_3d"/>
      <interfaced.item
        name="geometric_representation_context"/>
      <interfaced.item
        name="geometric_representation_item"/>
      <interfaced.item
        name="cartesian_transformation_operator_2d"/>
      <interfaced.item
        name="cartesian_transformation_operator_3d"/>
    </interface>

    <interface
      kind="use"
      schema="product_property_representation_schema">
      <interfaced.item
        name="shape_representation"/>
      <interfaced.item
        name="shape_representation_relationship"/>
    </interface>

    <interface
      kind="use"
      schema="representation_schema">
      <interfaced.item
        name="item_defined_transformation"/>
      <interfaced.item
        name="value_representation_item"/>
    </interface>

    <type name="representation_identification_item">
      <select
        basedon="identification_item"
        selectitems="shape_representation">
      </select>

    </type>
  </schema>

</express>
