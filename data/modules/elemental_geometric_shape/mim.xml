<?xml version="1.0"?>
<!-- $Id: mim.xml,v 1.3 2002/01/16 14:45:45 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/01/16 14:45:45 $"
  rcs.revision="$Revision: 1.3 $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.6"
    source="../data/modules/elemental_geometric_shape/mim.exp"/>

  <schema name="Elemental_geometric_shape_mim">
    <interface
      kind="use"
      schema="Foundation_representation_mim">
    </interface>

    <interface
      kind="use"
      schema="Identification_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="basic_attribute_schema">
      <interfaced.item
        name="description_attribute"/>
      <interfaced.item
        name="id_attribute"/>
    </interface>

    <interface
      kind="use"
      schema="geometry_schema">
      <interfaced.item
        name="cartesian_point"/>
      <interfaced.item
        name="placement"/>
      <interfaced.item
        name="axis1_placement"/>
      <interfaced.item
        name="axis2_placement_2d"/>
      <interfaced.item
        name="axis2_placement_3d"/>
      <interfaced.item
        name="geometric_representation_context"/>
      <interfaced.item
        name="geometric_representation_item"/>
      <interfaced.item
        name="cartesian_transformation_operator"/>
      <interfaced.item
        name="cartesian_transformation_operator_2d"/>
      <interfaced.item
        name="cartesian_transformation_operator_3d"/>
    </interface>

    <interface
      kind="use"
      schema="measure_schema">
      <interfaced.item
        name="global_unit_assigned_context"/>
      <interfaced.item
        name="length_measure"/>
      <interfaced.item
        name="length_measure_with_unit"/>
    </interface>

    <interface
      kind="use"
      schema="product_property_representation_schema">
      <interfaced.item
        name="shape_representation"/>
      <interfaced.item
        name="shape_representation_relationship"/>
    </interface>

    <interface
      kind="use"
      schema="representation_schema">
      <interfaced.item
        name="functionally_defined_transformation"/>
      <interfaced.item
        name="item_defined_transformation"/>
      <interfaced.item
        name="representation_relationship_with_transformation"/>
      <interfaced.item
        name="value_representation_item"/>
      <interfaced.item
        name="uncertainty_measure_with_unit"/>
      <interfaced.item
        name="mapped_item"/>
    </interface>

    <type name="representation_identification_item">
<description>
A representation_identification_item extends identification assignment to
representation instances.  
</description>
      <select
        basedon="identification_item"
        selectitems="representation">
      </select>

    </type>
  </schema>

</express>
