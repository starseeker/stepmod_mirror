<?xml version="1.0" encoding="utf-8"?>
  <?xml-stylesheet 
  type="text/xsl" 
  href="../../../xsl/descriptions.xsl"?>
<?DOCTYPE 
  express SYSTEM "../../../dtd/description.dtd"?>
<ext_descriptions module_directory="event" schema_file="arm.xml" rcs.date="$Date: $" rcs.revision="$Revision: $">


  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Event_arm 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
   <ext_description linkend="Event_arm"/>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Event_arm  Type: event_item 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Event_arm.event_item">
				This select is a place holder for assignments of event to elements of product data.
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Event_arm  Type: event_organization_or_person_in_organization_item 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Event_arm.event_organization_or_person_in_organization_item">
				This select extends the assignments of an organization or a person in an organization to Event.
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Event_arm  Type: event_date_or_date_time_item 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Event_arm.event_date_or_date_time_item">
				This select is a extends the assignments of a date or a date and time to <b>Event</b>.
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Event_arm  Entity: Event 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Event_arm.Event">
				An <b>Event</b> is the fact of the existence of a state at some point in time.
				<note>
					The point in time of an event may not be known.
				</note>
			
   </ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Event_arm  Entity: Event  Attribute: id 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Event_arm.Event.id">
					the identification of the event assigned by the organization.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Event_arm  Entity: Event  Attribute: name 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Event_arm.Event.name">
					words by which the event is known.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Event_arm  Entity: Event  Attribute: description 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Event_arm.Event.description">
					text which describes the event.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Event_arm  Entity: Event  Attribute: identifer 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Event_arm.Event.identifer">
					the organization (or person in an organization) that assigned and owns the event id.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Event_arm  Entity: Event  Attribute: planned_date 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Event_arm.Event.planned_date">
					the optional date and/ or time when the event is planned to occur.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Event_arm  Entity: Event  Attribute: actual_date 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Event_arm.Event.actual_date">
					the optional date and/ or time when the event occured.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Event_arm  Entity: Event_assignment 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Event_arm.Event_assignment">
				An <b>Event_assignment</b> assigns an Event to product data.
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Event_arm  Entity: Event_assignment  Attribute: assigned_event 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Event_arm.Event_assignment.assigned_event">
					the <b>Event</b> to be assigned to product data.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Event_arm  Entity: Event_assignment  Attribute: role 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Event_arm.Event_assignment.role">
					the purpose of the assignment.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Event_arm  Entity: Event_assignment  Attribute: items 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Event_arm.Event_assignment.items">
					the product data to which the <b>Event</b> is assigned.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Event_arm  Entity: Event_relationship 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Event_arm.Event_relationship">
				An <b>Event_relationship</b> specifies that two events are related. This is a foundation concept which may be refined in other modules. This concept can be used to define sequencing.
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Event_arm  Entity: Event_relationship  Attribute: relation_type 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Event_arm.Event_relationship.relation_type">
					an optional type descriptor for the relationship.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Event_arm  Entity: Event_relationship  Attribute: description 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Event_arm.Event_relationship.description">
					an optional description of the relationship.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Event_arm  Entity: Event_relationship  Attribute: event1 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Event_arm.Event_relationship.event1">
					the <b>Event</b> to be related to <b>event2</b>.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Event_arm  Entity: Event_relationship  Attribute: event2 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Event_arm.Event_relationship.event2">
					the <b>Event</b> to be related to <b>event1</b>.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Event_arm  Entity: Relative_event 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Event_arm.Relative_event">
				A <b>Relative_event</b> is an Event that takes place at a time offset from the time that another Event takes place.
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Event_arm  Entity: Relative_event  Attribute: related_event 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Event_arm.Relative_event.related_event">
					the <b>Event</b> this <b>Event</b> is based on.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Event_arm  Entity: Relative_event  Attribute: offset 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Event_arm.Relative_event.offset">
					the difference in time between the two events.
				</ext_description>
</ext_descriptions>