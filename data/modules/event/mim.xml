<?xml version="1.0"?><!-- edited with XML Spy v3.5 (http://www.xmlspy.com) by Michael Ward (Eurostep Limited) --><!-- $Id: mim.xml,v 1.4 2002/02/13 14:22:06 robbod Exp $ --><?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?><!DOCTYPE express SYSTEM "../../../dtd/express.dtd"><express language_version="2" rcs.date="$Date: 2002/02/13 14:22:06 $" rcs.revision="$Revision: 1.4 $">	<application name="express2xml.js" owner="Eurostep Limited" url="http://www.eurostep.com" version="1.6" source="../data/modules/event/mim.exp"/>	<schema name="Event_mim">		<interface kind="use" schema="Date_time_assignment_mim"/>		<interface kind="use" schema="Person_organisation_assignment_mim"/>		<interface kind="use" schema="date_time_schema">			<interfaced.item name="event_occurrence"/>			<interfaced.item name="relative_event_occurrence"/>			<interfaced.item name="event_occurrence_relationship"/>		</interface>		<interface kind="use" schema="management_resources_schema">			<interfaced.item name="event_occurrence_assignment"/>		</interface>		<interface kind="use" schema="measure_schema">			<interfaced.item name="time_measure_with_unit"/>			<interfaced.item name="time_measure"/>		</interface>		<type name="event_occurrence_item">			<description>				An <b>event_occurrence_item</b> specifies those objects to which an <b>event_occurrence</b> may be assigned.			</description>			<select extensible="YES"/>		</type>		<type name="event_occurrence_date_item">			<description>				An <b>event_occurrence_date_item</b> extends date assignment to <b>event_occurrence</b> instances.			</description>			<select basedon="date_item" selectitems="event_occurrence"/>		</type>		<type name="event_occurrence_date_and_time_item">			<description>				An <b>event_occurrence_date_and_time_item</b> extends date and time assignment to <b>event_occurrence</b> instances. 			</description>			<select basedon="date_and_time_item" selectitems="event_occurrence"/>		</type>		<type name="event_occurrence_organization_item">			<description>				An <b>event_occurrence_organization_item</b> extends organization assignment to <b>event_occurrence</b> instances.			</description>			<select basedon="organization_item" selectitems="event_occurrence"/>		</type>		<type name="event_occurrence_person_and_organization_item">			<description>				An <b>event_occurrence_person_and_organization_item</b> extends person and organization assignment to <b>event_occurrence</b> instances.			</description>			<select basedon="person_and_organization_item" selectitems="event_occurrence"/>		</type>		<entity name="applied_event_occurrence_assignment" supertypes="event_occurrence_assignment">			<explicit name="items">				<aggregate type="SET" lower="1" upper="?"/>				<typename name="event_occurrence_item"/>			</explicit>		</entity>	</schema></express>