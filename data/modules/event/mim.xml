<?xml version="1.0"?>
<!-- $Id: mim.xml,v 1.3 2002/01/16 14:45:45 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/01/16 14:45:45 $"
  rcs.revision="$Revision: 1.3 $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.6"
    source="../data/modules/event/mim.exp"/>

  <schema name="Event_mim">
    <interface
      kind="use"
      schema="Date_time_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="Person_organisation_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="date_time_schema">
      <interfaced.item
        name="event_occurrence"/>
      <interfaced.item
        name="relative_event_occurrence"/>
      <interfaced.item
        name="event_occurrence_relationship"/>
    </interface>

    <interface
      kind="use"
      schema="management_resources_schema">
      <interfaced.item
        name="event_occurrence_assignment"/>
    </interface>

    <interface
      kind="use"
      schema="measure_schema">
      <interfaced.item
        name="time_measure_with_unit"/>
      <interfaced.item
        name="time_measure"/>
    </interface>

    <type name="event_occurrence_item">
      <select
        extensible="YES">
      </select>

    </type>
    <type name="event_occurrence_date_item">
      <select
        basedon="date_item"
        selectitems="event_occurrence">
      </select>

    </type>
    <type name="event_occurrence_date_and_time_item">
      <select
        basedon="date_and_time_item"
        selectitems="event_occurrence">
      </select>

    </type>
    <type name="event_occurrence_organization_item">
      <select
        basedon="organization_item"
        selectitems="event_occurrence">
      </select>

    </type>
    <type name="event_occurrence_person_and_organization_item">
      <select
        basedon="person_and_organization_item"
        selectitems="event_occurrence">
      </select>

    </type>
    <entity
      name="applied_event_occurrence_assignment"
      supertypes="event_occurrence_assignment">
      <explicit
        name="items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="event_occurrence_item"/>
      </explicit>
    </entity>

  </schema>

</express>
