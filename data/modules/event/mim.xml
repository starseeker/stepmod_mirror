<?xml version="1.0"?>
<!-- $Id: mim.xml,v 1.8 2002/04/02 17:55:32 goset1 Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/04/02 17:55:32 $"
  rcs.revision="$Revision: 1.8 $"
  description.file="mim_descriptions.xml">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.9"
    source="../data/modules/event/mim.exp"/>

  <schema name="Event_mim">
    <interface
      kind="use"
      schema="Date_time_assignment_mim">
    </interface>
    <interface
      kind="use"
      schema="Value_with_unit_mim">
    </interface>
    <interface
      kind="use"
      schema="date_time_schema">
      <interfaced.item
        name="event_occurrence"/>
      <interfaced.item
        name="relative_event_occurrence"/>
      <interfaced.item
        name="event_occurrence_relationship"/>
    </interface>

    <interface
      kind="use"
      schema="management_resources_schema">
      <interfaced.item
        name="event_occurrence_assignment"/>
    </interface>

    <type name="event_occurrence_item">
      <select
        extensible="YES"
        selectitems="">
      </select>

    </type>
    <type name="event_occurrence_date_item">
      <select
        basedon="date_item"
        selectitems="event_occurrence">
      </select>

    </type>
    <type name="event_occurrence_date_and_time_item">
      <select
        basedon="date_and_time_item"
        selectitems="event_occurrence">
      </select>

    </type>
    <entity
      name="applied_event_occurrence_assignment"
      supertypes="event_occurrence_assignment">
      <explicit
        name="items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="event_occurrence_item"/>
      </explicit>
    </entity>

  </schema>

</express>
