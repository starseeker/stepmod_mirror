	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: Experience_arm
   <express_ref linkend="experience:arm:Experience_arm"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<ext_description linkend="Experience_arm"/>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_arm  Type: defined_methods 
   <express_ref linkend="experience:arm:Experience_arm.defined_methods"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Experience_arm.defined_methods"/>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_arm  Type: experience_classified_select 
   <express_ref linkend="experience:arm:Experience_arm.experience_classified_select"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Experience_arm.experience_classified_select"/>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_arm  Type: experience_date_or_date_time_item 
   <express_ref linkend="experience:arm:Experience_arm.experience_date_or_date_time_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Experience_arm.experience_date_or_date_time_item">
		<example number="1">start date.</example>
		<example number="2">end date.</example>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_arm  Type: experience_instance_property_assignment_select 
   <express_ref linkend="experience:arm:Experience_arm.experience_instance_property_assignment_select"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Experience_arm.experience_instance_property_assignment_select">
		The assignment of properties that define the experience instance.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_arm  Type: person_or_organization_or_person_in_organization_select 
   <express_ref linkend="experience:arm:Experience_arm.person_or_organization_or_person_in_organization_select"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Experience_arm.person_or_organization_or_person_in_organization_select">
		 The person, organization or person in an organization that gains the experience.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_arm  Entity: Experience_gained  
   <express_ref linkend="experience:arm:Experience_arm.Experience_gained"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Experience_arm.Experience_gained">An <b>Experience_gained</b> is a relationship between an  
<express_ref linkend="experience:arm:Experience_arm.Experience_instance"/> and a person or organization.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_arm  Entity: Experience_gained  Attribute: experience_of 
   <express_ref linkend="experience:arm:Experience_arm.Experience_gained.experience_of"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Experience_arm.Experience_gained.experience_of">
		the <express_ref linkend="experience:arm:Experience_arm.Experience_instance"/> undergone by a person or organization.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_arm  Entity: Experience_gained  Attribute: gained_by 
   <express_ref linkend="experience:arm:Experience_arm.Experience_gained.gained_by"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Experience_arm.Experience_gained.gained_by">
		the person or organization that undergoes the <express_ref linkend="experience:arm:Experience_arm.Experience_instance"/>.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_arm  Entity: Experience_gained  Attribute: role 
   <express_ref linkend="experience:arm:Experience_arm.Experience_gained.role"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Experience_arm.Experience_gained.role">
          <p>
            the text that specifies the function performed by the
            person or organization during the process of gaining the <express_ref
            linkend="experience:arm:Experience_arm.Experience_instance"/>.
          The value of the attribute need not be specified. 
        </p>
		<example number="1">inspector</example>
		<example number="2">manager</example>
		<example number="3">mechanic</example>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_arm  Entity: Experience_instance  
   <express_ref linkend="experience:arm:Experience_arm.Experience_instance"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Experience_arm.Experience_instance"> An <b>Experience_instance</b>
		is a particular eposide of contact with and/or observation of facts or events which contributes to the accumulation of knowledge or skill.
		<example number="1">Changing the exhaust system on a car.</example>
		<example number="2">2 years work on same type of milling machine.</example>
		<example number="3">100 flying hours in a Tornado jet.</example>
		<example number="4">5 years as Workshop Manager.</example>
		The nature, duration and worth of the <b>Experience_instance</b> can be described using assigned properties or by referring to activities or tasks.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_arm  Entity: Experience_instance  Attribute: consists_of 
   <express_ref linkend="experience:arm:Experience_arm.Experience_instance.consists_of"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Experience_arm.Experience_instance.consists_of">
     the <express_ref
     linkend="activity:arm:Activity_arm.Activity"/> or <express_ref
   linkend="resource_as_realized:arm:Resource_as_realized_arm.Resource_as_realized"/>
   that provides the content of the <b>Experience_instance</b>.	
   The value of the attribute need not be specified. 
</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_arm  Entity: Experience_instance  Attribute: is_defined_by 
   <express_ref linkend="experience:arm:Experience_arm.Experience_instance.is_defined_by"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Experience_arm.Experience_instance.is_defined_by">
		 an experience instance contributes to the definition of the experience gained.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_arm  Entity: Experience_instance  Attribute: description 
   <express_ref linkend="experience:arm:Experience_arm.Experience_instance.description"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Experience_arm.Experience_instance.description">
		the text that characterizes the experience instance.
The value of the attribute need not be specified. 
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_arm  Entity: Experience_instance  Attribute: role 
   <express_ref linkend="experience:arm:Experience_arm.Experience_instance.role"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Experience_arm.Experience_instance.role">
		the role in which the experience instance was performed.
		<example number="1">inspector</example>
		<example number="2">manager</example>
		<example number="3">mechanic</example>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_arm  Entity: Experience_type_relationship 
   <express_ref linkend="experience:arm:Experience_arm.Experience_type_relationship"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Experience_arm.Experience_type_relationship">An <b>Experience_type_relationship</b> is used to relate a compound experience to its component experiences.
		<example>Ten years programming experience made up of five years C++ programming experience and five years Java programming experience.</example>
	</ext_description>
	
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_arm  Entity: Experience_type_relationship  Attribute: compound_experience 
   <express_ref linkend="experience:arm:Experience_arm.Experience_type_relationship.compound_experience"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Experience_arm.Experience_type_relationship.compound_experience">
		the <express_ref linkend="experience:arm:Experience_arm.Experience_type"/>  that the <b>Experience_type_relationship</b> identifies as comprising other <express_ref linkend="experience:arm:Experience_arm.Experience_type"/> objects.
	</ext_description>
	
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_arm  Entity: Experience_type_relationship  Attribute: component_experience 
   <express_ref linkend="experience:arm:Experience_arm.Experience_type_relationship.component_experience"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Experience_arm.Experience_type_relationship.component_experience">
		the <express_ref linkend="experience:arm:Experience_arm.Experience_type"/>  that the <b>Experience_type_relationship</b> identifies as a component of another <express_ref linkend="experience:arm:Experience_arm.Experience_type"/>.
	</ext_description>


<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_arm  Entity: Experience_type  
   <express_ref linkend="experience:arm:Experience_arm.Experience_type"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Experience_arm.Experience_type">An <b>Experience_type</b> is a 
		category or class of experience.
		<example number="1">welding</example>
		<example number="2">mechanical design</example>
		<example number="3">flying</example>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_arm  Entity: Experience_type  Attribute: description 
   <express_ref linkend="experience:arm:Experience_arm.Experience_type.description"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Experience_arm.Experience_type.description">
     the text that characterizes the <b>Experience_type</b>.
   The value of the attribute need not be specified. 
 </ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_arm  Entity: Experience_type  Attribute: name 
   <express_ref linkend="experience:arm:Experience_arm.Experience_type.name"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Experience_arm.Experience_type.name">
		the word or words by which the <b>Experience_type</b> is known.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_arm  Entity: Experience_type  Attribute: consists_of 
   <express_ref linkend="experience:arm:Experience_arm.Experience_type.consists_of"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Experience_arm.Experience_type.consists_of">
		the <express_ref
linkend="activity_method:arm:Activity_method_arm.Activity_method"/> or
<express_ref
linkend="required_resource:arm:Required_resource_arm.Required_resource"/>
that provides the content of the <b>Experience_type</b>.	
The value of the attribute need not be specified. 	
	</ext_description>
</ext_descriptions>
