<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express language_version="2" rcs.date="$Date: $" rcs.revision="$Revision: $" description.file="mim_descriptions.xml">
	<application name="GraphicalEXPRESS" owner="Eurostep" url="www.eurostep.com" version="1.3.14" source="experience_mim_v016.vsd"/>
	<schema name="Experience_mim">
		<interface kind="use" schema="action_schema">
			<interfaced.item name="action_method"/>
		</interface>
		<interface kind="use" schema="Activity_method_mim"/>
		<interface kind="use" schema="Classification_assignment_mim"/>
		<interface kind="use" schema="Date_time_assignment_mim"/>
		<interface kind="use" schema="experience_schema">
			<interfaced.item name="experience"/>
			<interfaced.item name="experience_type"/>
		</interface>
		<interface kind="use" schema="management_resources_schema">
			<interfaced.item name="experience_assignment"/>
			<interfaced.item name="experience_type_assignment"/>
		</interface>
		<interface kind="use" schema="Person_organization_mim"/>
		<interface kind="use" schema="person_organization_schema">
			<interfaced.item name="organization"/>
			<interfaced.item name="person"/>
			<interfaced.item name="person_and_organization"/>
		</interface>
		<interface kind="use" schema="process_property_schema">
			<interfaced.item name="action_resource_requirement"/>
		</interface>
		<interface kind="use" schema="product_property_definition_schema">
			<interfaced.item name="characterized_object"/>
		</interface>
		<interface kind="use" schema="Property_assignment_mim"/>
		<interface kind="use" schema="Required_resource_mim"/>
		<type name="experience_date_and_time_item">
			<select extensible="YES" genericentity="YES" basedon="date_and_time_item" selectitems="applied_experience_assignment experience"/>
			<graphic.element page="2" xcoord="453" ycoord="396"/>
		</type>
		<type name="experience_date_item">
			<select extensible="YES" genericentity="YES" basedon="date_item" selectitems="applied_experience_assignment experience"/>
			<graphic.element page="2" xcoord="453" ycoord="177"/>
		</type>
		<type name="experience_item">
			<select extensible="YES" genericentity="YES" selectitems="action_method action_resource_requirement"/>
			<graphic.element page="2" xcoord="170" ycoord="113"/>
		</type>
		<type name="experience_type_classification_item">
			<select extensible="YES" genericentity="YES" basedon="classification_item" selectitems="experience_type"/>
			<graphic.element page="2" xcoord="170" ycoord="722"/>
		</type>
		<type name="experience_type_item">
			<select extensible="YES" genericentity="YES" selectitems="experience organization person person_and_organization"/>
			<graphic.element page="2" xcoord="170" ycoord="524"/>
		</type>
		<entity name="applied_experience_assignment" supertypes="experience_assignment">
			<explicit name="items">
				<aggregate type="SET" lower="1" upper="?"/>
				<typename name="experience_item"/>
			</explicit>
			<graphic.element page="2" xcoord="170" ycoord="177"/>
		</entity>
		<entity name="applied_experience_type_assignment" supertypes="experience_type_assignment">
			<explicit name="items">
				<aggregate type="SET" lower="1" upper="?"/>
				<typename name="experience_type_item"/>
			</explicit>
			<graphic.element page="2" xcoord="170" ycoord="588"/>
		</entity>
		<entity name="characterized_experience" supertypes="characterized_object experience">
			<graphic.element page="2" xcoord="453" ycoord="559"/>
		</entity>
		<graphic.element page="1" xcoord="304" ycoord="644"/>
	</schema>
</express>
