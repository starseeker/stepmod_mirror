  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Experience_mim.applied_experience_type_assignment">
     An <b>applied_experience_type_assignment</b> is a type of
     <express_ref 
       linkend="management_resources_schema:ir_express:management_resources_schema.experience_type_assignment"/>
     that associates an instance of
     <express_ref linkend="experience_schema:ir_express:experience_schema.experience_type"/>,
     with one or more 
     <express_ref
       linkend="experience:mim:Experience_mim.experience_type_item"/>
     instances. 
   </ext_description>

   <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_mim  Entity: applied_experience_type_assignment  Attribute: items 
   <express_ref linkend="experience:mim:Experience_mim.applied_experience_type_assignment.items"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Experience_mim.applied_experience_type_assignment.items">
     the set of one or more instances of entity datatypes listed in 
     <express_ref
       linkend="experience:mim:Experience_mim.experience_type_item"/>,
     that are associated with the 
     <express_ref linkend="experience_schema:ir_express:experience_schema.experience_type"/>,
     information.
   </ext_description>

   <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_mim  Entity: characterized_experience  
   <express_ref linkend="experience:mim:Experience_mim.characterized_experience"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Experience_mim.characterized_experience">
     A <b>characterized_experience</b> 
   is a type of 
   <express_ref 
     linkend="product_property_definition_schema:ir_express:product_property_definition_schema.characterized_object"/>
   and 
   <express_ref linkend="experience_schema:ir_express:experience_schema.experience"/>
   that serves to assign 
   <b>property_definition</b> 
   instances to 
   <express_ref linkend="experience_schema:ir_express:experience_schema.experience"/>
   instances.
 </ext_description>
