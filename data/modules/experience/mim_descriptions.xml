	<ext_description linkend="Experience_mim.applied_experience_type_assignment.items">
		the set of one or more instances of entity datatypes listed in <b>experience_type_item</b>, that are associated with the <b>experience_type</b> information.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_mim  Entity: characterized_experience  
   <express_ref linkend="experience:mim:Experience_mim.characterized_experience"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Experience_mim.characterized_experience">
		A <b>characterized_experience</b> is a type of <b>experience</b> and <b>characterized_object</b> that serves to assign <b>property_definition</b> instances to <b>experience</b> instances.
	</ext_description>
