	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_mim  Entity: applied_experience_assignment  
   <express_ref linkend="experience:mim:Experience_mim.applied_experience_assignment"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Experience_mim.applied_experience_assignment">
     An <b>applied_experience_assignment</b> is a type of
     <express_ref linkend="management_resources_schema:ir_express:management_resources_schema.experience_assignment"/>
     that associates an instance of
     <express_ref linkend="experience_schema:ir_express:experience_schema.experience"/>
     with one or more 
     <express_ref linkend="experience:mim:Experience_mim.experience_item"/>
     instances.  
   </ext_description>

   <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_mim  Entity: applied_experience_assignment  Attribute: items 
   <express_ref linkend="experience:mim:Experience_mim.applied_experience_assignment.items"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Experience_mim.applied_experience_assignment.items">
     the set of one or more instances of entity data types cited in 
     <express_ref linkend="experience:mim:Experience_mim.experience_item"/>,
     that are associated with the 
     <express_ref linkend="experience_schema:ir_express:experience_schema.experience"/>
     information.
 </ext_description>
 
 <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Experience_mim  Entity: applied_experience_type_assignment  
