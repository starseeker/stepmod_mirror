SCHEMA Expression_arm;

USE FROM Generic_expression_arm;
USE FROM Numeric_expression_arm;
USE FROM Boolean_expression_arm;
USE FROM String_expression_arm;

ENTITY Expression;
ABSTRACT SUPERTYPE OF (ONEOF (Numeric_expression,
				Boolean_expression,
				String_expression));
	SUBTYPE OF (Generic_expression);
END_ENTITY;

END_SCHEMA;
