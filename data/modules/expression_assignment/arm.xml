<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<?xml-stylesheet type="text/xsl"
href="../../../xsl/express.xsl" ?>
<express
   language_version="2"
   rcs.date="$Date: 2006/04/11 21:48:24 $"
   rcs.revision="$Revision: 1.1 $"
	 	 description.file="arm_descriptions.xml">
  <schema name="Expression_assignment_arm">
   <interface
      kind="use"
      schema="Expression_arm">
    </interface>
        <type name="expression_assignment_item"> 
      <select
      extensible="YES"
      genericentity="YES"
        />
    </type>

    <entity
      name="Expression_assignment"
      >

      <explicit
        name="expression">
        <typename name="Expression"/>
      </explicit>
      <explicit
        name="items">
        <aggregate type="SET"
          lower="1"
          upper="?"/>
        <typename name="expression_assignment_item"/>
      </explicit>

    </entity>
  </schema>
</express>
