<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1667">
   <application name="JSDAI" owner="LKSoft" source="extended_basic_geometry_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Extended_basic_geometry_arm">
      <interface kind="use" schema="Basic_geometry_arm"/>
      <interface kind="use" schema="Geometric_model_relationship_arm"/>
      <entity name="Detailed_geometric_model_element_relationship">
         <explicit name="item_1">
            <typename name="Detailed_geometric_model_element"/>
         </explicit>
         <explicit name="item_2">
            <typename name="Detailed_geometric_model_element"/>
         </explicit>
         <where expression="item_1 &lt;&gt; item_2" label="WR1"/>
      </entity>
      <entity name="Geometric_model_element_relationship" supertypes="Detailed_geometric_model_element_relationship Detailed_geometric_model_element"/>
      <entity name="Vector" supertypes="Detailed_geometric_model_element">
         <explicit name="orientation">
            <typename name="Direction"/>
         </explicit>
         <explicit name="magnitude">
            <typename name="length_measure"/>
         </explicit>
         <where expression="magnitude &gt;= 0.0" label="WR1"/>
      </entity>
      <subtype.constraint entity="Detailed_geometric_model_element" name="alternate_extended_geometry_items" super.expression="ONEOF (Direction, Axis_placement, Cartesian_transformation_2d, Cartesian_transformation_3d, Curve, Geometric_model_element_relationship, Surface, Vector)"/>
   </schema>
</express>
