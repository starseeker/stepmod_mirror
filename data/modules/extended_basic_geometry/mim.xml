<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date: 2007-05-29 06:06:08 -0700 (An, 29 Geg 2007) $" rcs.revision="$Revision: 1040 $" description.file="mim_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="extended_basic_geometry_mim schema_instance"/>
   <schema name="Extended_basic_geometry_mim">
      <interface kind="use" schema="Basic_geometry_mim"/>
      <interface kind="use" schema="representation_schema">
         <interfaced.item name="definitional_representation_relationship"/>
         <interfaced.item name="mapped_item"/>
         <interfaced.item name="representation_item_relationship"/>
         <interfaced.item name="representation_map"/>
      </interface>
      <interface kind="use" schema="product_property_representation_schema">
         <interfaced.item name="shape_representation_relationship"/>
      </interface>
      <interface kind="use" schema="geometry_schema">
         <interfaced.item name="vector"/>
      </interface>
      <interface kind="reference" schema="support_resource_schema">
         <interfaced.item name="bag_to_set"/>
      </interface>
      <entity name="geometric_model_element_relationship" supertypes="geometric_representation_item representation_item_relationship">
         <explicit name="relating_representation_item">
            <typename name="geometric_representation_item"/>
            <redeclaration entity-ref="representation_item_relationship"/>
         </explicit>
         <explicit name="related_representation_item">
            <typename name="geometric_representation_item"/>
            <redeclaration entity-ref="representation_item_relationship"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="relating_representation_item"/>
            <unique.attribute attribute="related_representation_item"/>
         </unique>
         <where label="WR1" expression="SELF\representation_item_relationship.relating_representation_item :&lt;&gt;: &#10;       SELF\representation_item_relationship.related_representation_item"/>
      </entity>
   </schema>
</express>
