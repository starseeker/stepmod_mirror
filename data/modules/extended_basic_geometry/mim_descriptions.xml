</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: geometric_model_element_relationship
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Extended_basic_geometry_mim.geometric_model_element_relationship">
	A <b>geometric_model_element_relationship</b> is a type of
	<express_ref linkend="geometry_schema:ir_express:geometry_schema.geometric_representation_item"/>
	and a type of
	<express_ref linkend="representation_schema:ir_express:representation_schema.representation_item_relationship"/>
	that implements the ARM concept of
	<express_ref linkend="Extended_basic_geometry:arm:Extended_basic_geometry_arm.Geometric_model_element_relationship"/>.
</ext_description>

<!-- relating_representation_item -->
<ext_description linkend="Extended_basic_geometry_mim.geometric_model_element_relationship.relating_representation_item">
	an attribute inherited from the
	<express_ref linkend="representation_schema:ir_express:representation_schema.representation_item_relationship"/>
	shall be redeclared as the
	<express_ref linkend="geometry_schema:ir_express:geometry_schema.geometric_representation_item"/>
	for the <b>geometric_model_element_relationship</b>.
</ext_description>

<!-- related_representation_item -->
<ext_description linkend="Extended_basic_geometry_mim.geometric_model_element_relationship.related_representation_item">
	an attribute inherited from the
	<express_ref linkend="representation_schema:ir_express:representation_schema.representation_item_relationship"/>
	shall be redeclared as the
	<express_ref linkend="geometry_schema:ir_express:geometry_schema.geometric_representation_item"/>
	for the <b>geometric_model_element_relationship</b>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
UR: geometric_model_element_relationship.ur:UR1
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Extended_basic_geometry_mim.geometric_model_element_relationship.ur:UR1">
	The combination of <b>relating_representation_item</b> and <b>related_representation_item</b>
	shall be unique within a population of <b>geometric_model_element_relationship</b>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
UR: geometric_model_element_relationship.wr:WR1
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Extended_basic_geometry_mim.geometric_model_element_relationship.wr:WR1">
	The <b>relating_representation_item</b> shall not be <b>related_representation_item</b>. 
</ext_description>

</ext_descriptions>
