<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date: 2007-07-20 02:54:28 -0700 (Pn, 20 Lie 2007) $" rcs.revision="$Revision: 1319 $" description.file="mim_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="extended_date_mim schema_instance"/>
   <schema name="Extended_date_mim">
      <interface kind="use" schema="Date_time_mim"/>
      <interface kind="use" schema="date_time_schema">
         <interfaced.item name="date"/>
         <interfaced.item name="day_in_month_number"/>
         <interfaced.item name="ordinal_date"/>
         <interfaced.item name="week_of_year_and_day_date"/>
      </interface>
      <interface kind="reference" schema="date_time_schema">
         <interfaced.item name="leap_year"/>
      </interface>
      <entity name="calendar_month_year" supertypes="calendar_date">
         <derived name="day_component" expression="1">
            <typename name="day_in_month_number"/>
            <redeclaration entity-ref="calendar_date"/>
         </derived>
      </entity>
   </schema>
</express>
