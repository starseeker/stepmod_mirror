         <explicit name="constrained_physical_feature" optional="NO">
            <typename name="physical_feature_or_part_template"/>
         </explicit>
      </entity>
      <entity name="Dimension_related_positional_boundary_definition" supertypes="Positional_boundary_definition" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="characterized_boundary_member_definitions" optional="NO">
            <aggregate type="SET" lower="1" upper="?" unique="NO" optional="NO"/>
            <typename name="Positional_boundary_member_definition"/>
         </explicit>
         <explicit name="defined_positional_boundary" optional="NO">
            <typename name="Dimension_related_positional_boundary"/>
            <redeclaration entity-ref="Positional_boundary_definition"/>
         </explicit>
      </entity>
      <entity name="Dimension_set" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="associated_dimensions" optional="NO">
            <aggregate type="SET" lower="1" upper="?" unique="NO" optional="NO"/>
