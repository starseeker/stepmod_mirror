         <explicit name="tolerance_value" optional="NO">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="associated_dimension" optional="NO">
            <typename name="Size_dimension"/>
         </explicit>
         <inverse name="boundary_definition" entity="Dimension_related_positional_boundary_definition" attribute="characterized_boundary_member_definitions"/>
      </entity>
