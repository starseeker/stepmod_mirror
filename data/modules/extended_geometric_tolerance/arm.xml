         </explicit>
      </entity>
      <entity name="Geometric_tolerance_with_applied_material_condition_property" supertypes="Geometric_tolerance" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="applied_material_condition_property_name" optional="NO">
            <typename name="material_condition_property_names"/>
         </explicit>
      </entity>
      <entity name="Limit_dimension_representation" supertypes="Shape_dimension_representation" abstract.supertype="NO" abstract.entity="NO">
