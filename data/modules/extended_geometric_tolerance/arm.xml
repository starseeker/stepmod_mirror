         <inverse name="included_datum_targets" entity="Datum_target_usage_in_datum_target_set_relationship" attribute="defined_datum_target_set">
            <inverse.aggregate type="SET" lower="1" upper="?"/>
         </inverse>
      </entity>
      <entity name="Datum_target_usage_in_datum_target_set_relationship" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="defined_datum_target_set" optional="NO">
            <typename name="Datum_target_set"/>
         </explicit>
         <explicit name="used_datum_target" optional="NO">
            <typename name="Datum_target"/>
         </explicit>
         <explicit name="datum_target_number" optional="NO">
            <builtintype type="INTEGER" fixed="NO"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="used_datum_target"/>
            <unique.attribute attribute="defined_datum_target_set"/>
         </unique>
         <unique label="UR2">
            <unique.attribute attribute="datum_target_number"/>
            <unique.attribute attribute="defined_datum_target_set"/>
         </unique>
      </entity>
      <entity name="Derived_non_feature_shape_element" abstract.supertype="YES" supertypes="Non_feature_shape_element" super.expression="ONEOF (Apex, Centre_of_symmetry, Linear_extension, Geometric_intersection, Geometric_alignment, Tangent_shape_element)" abstract.entity="NO">
         <inverse name="deriving_relationships" entity="Shape_element_deriving_relationship" attribute="relating">
            <inverse.aggregate type="SET" lower="1" upper="?"/>
         </inverse>
      </entity>
      <entity name="Diametrical_boundary_size_characteristic" supertypes="Boundary_size_characteristic" abstract.supertype="NO" abstract.entity="NO">
