         </explicit>
         <inverse name="elements_of" entity="Datum_based_vector_orientation" attribute="direction_components">
            <inverse.aggregate type="SET" lower="1" upper="?"/>
         </inverse>
      </entity>
      <entity name="Edge_segment_vertex" supertypes="Datum_point" abstract.supertype="NO" abstract.entity="NO"/>
      <entity name="Externally_defined_dimension_representation" supertypes="Shape_dimension_representation" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="dimension_value" optional="YES">
            <typename name="Value_with_unit"/>
         </explicit>
         <inverse name="definitions" entity="External_source_identification" attribute="item">
            <inverse.aggregate type="SET" lower="1" upper="?"/>
         </inverse>
      </entity>
      <entity name="Geometric_alignment" supertypes="Derived_non_feature_shape_element" abstract.supertype="NO" abstract.entity="NO">
