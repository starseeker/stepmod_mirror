         <explicit name="is_applied_to" optional="NO">
            <typename name="Composite_unit_shape_element"/>
            <redeclaration entity-ref="Size_dimension"/>
         </explicit>
      <where label="IP1"/>
      </entity>
      <entity name="Directed_axis" supertypes="Non_feature_shape_element" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="axis_to_be_oriented" optional="NO">
            <typename name="Centre_axis"/>
         </explicit>
         <explicit name="start_shape_element" optional="NO">
            <typename name="Shape_element"/>
         </explicit>
         <explicit name="end_shape_element" optional="NO">
            <typename name="Shape_element"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="axis_to_be_oriented"/>
            <unique.attribute attribute="start_shape_element"/>
         </unique>
         <where label="WR1" expression="NOT EXISTS(SELF\Shape_element.description)"/>
      <where label="IP1"/>
      </entity>
      <entity name="Direction_element" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="direction" optional="NO">
            <typename name="Datum_based_direction_vector"/>
         </explicit>
         <explicit name="angle_value" optional="NO">
            <typename name="Angle_data_element"/>
