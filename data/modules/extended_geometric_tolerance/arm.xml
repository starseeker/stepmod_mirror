            <aggregate type="SET" lower="1" upper="?" unique="NO" optional="NO"/>
            <typename name="Geometric_dimension"/>
         </explicit>
      </entity>
      <entity name="Dimension_related_positional_boundary" supertypes="Positional_boundary" abstract.supertype="NO" abstract.entity="NO">
