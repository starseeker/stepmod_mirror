<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="extended_geometric_tolerance_arm schema_instance"/>
   <schema name="Extended_geometric_tolerance_arm">
      <interface kind="use" schema="Requirement_decomposition_arm"/>
      <interface kind="use" schema="Default_tolerance_arm"/>
      <interface kind="use" schema="Dimension_tolerance_arm"/>
      <interface kind="use" schema="Geometric_tolerance_arm"/>
      <interface kind="use" schema="Non_feature_shape_element_arm"/>
      <interface kind="reference" schema="Requirement_decomposition_arm">
         <interfaced.item name="get_rvd"/>
      </interface>
      <type name="centre_type">
         <enumeration items="centred_on_arc centred_on_radial" extensible="NO"/>
      </type>
      <type name="descriptive_or_geometric_orientation">
         <select selectitems="measure_orientation Direction" genericentity="NO" extensible="NO"/>
      </type>
      <type name="dimension_qualifiers">
         <enumeration items="free_state average" extensible="NO"/>
      </type>
      <type name="dimensional_location_or_dimensional_size">
         <select selectitems="Size_dimension Location_dimension" genericentity="NO" extensible="NO"/>
      </type>
      <type name="egt_external_identification_item">
         <select extensible="YES" genericentity="YES" basedon="external_identification_item" selectitems="Externally_defined_dimension_representation"/>
      </type>
      <type name="egt_requirement_assignment_item">
         <select extensible="YES" genericentity="YES" basedon="requirement_assignment_item" selectitems="Restraint_condition Statistical_dimensional_location Statistical_dimensional_size Statistical_geometric_tolerance"/>
      </type>
      <type name="geometric_or_dimension_select">
         <select selectitems="Geometric_tolerance Geometric_dimension" genericentity="NO" extensible="NO"/>
      </type>
      <type name="geometric_tolerance_qualifiers">
         <enumeration items="pitch_diameter minor_diameter each_element major_diameter each_radial_element not_concave not_convex free_state" extensible="NO"/>
      </type>
      <type name="locating_or_refinement_linear_profile_tolerance">
         <select selectitems="Line_profile_tolerance Composite_linear_profile_refinement_tolerance" genericentity="NO" extensible="NO"/>
      </type>
      <type name="locating_or_refinement_surface_profile_tolerance">
         <select selectitems="Surface_profile_tolerance Composite_surface_profile_refinement_tolerance" genericentity="NO" extensible="NO"/>
      </type>
      <type name="measure_orientation">
         <enumeration items="reversed normal" extensible="NO"/>
      </type>
      <type name="offset_types">
         <enumeration items="towards_material away_from_material" extensible="NO"/>
      </type>
      <type name="orientation_select">
         <select selectitems="Datum_axis_related_orientation Viewing_plane_based_orientation Datum_based_vector_orientation" genericentity="NO" extensible="NO"/>
      </type>
      <type name="physical_feature_or_part_template">
         <select extensible="YES" genericentity="YES" selectitems="Datum_feature"/>
      </type>
      <type name="restrained_item_select">
         <select selectitems="Single_datum physical_feature_or_part_template" genericentity="NO" extensible="NO"/>
      </type>
      <type name="true_profile_representations">
         <select extensible="YES" selectitems="Dimension_set Document_definition Func" genericentity="NO"/>
      </type>
      <entity name="Apex" supertypes="Derived_non_feature_shape_element" abstract.supertype="NO" abstract.entity="NO">
         <where label="IP1"/>
         <where label="IP2"/>
      </entity>
      <entity name="Boundary_size_characteristic" abstract.supertype="YES" super.expression="ONEOF (Diametrical_boundary_size_characteristic, Opposing_boundary_set_size_characteristic)" abstract.entity="NO">
         <explicit name="tolerance_value" optional="NO">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="maximum_tolerance_value" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
      </entity>
      <entity name="Centre_axis" supertypes="Centre_of_symmetry" abstract.supertype="NO" abstract.entity="NO">
         <where label="WR1" expression="NOT EXISTS(SELF\Shape_element.description)"/>
      <where label="IP1"/>
      </entity>
      <entity name="Centre_of_symmetry" abstract.supertype="YES" supertypes="Derived_non_feature_shape_element" super.expression="ONEOF (Centre_axis, Centre_plane, Centre_point)" abstract.entity="NO"/>
      <entity name="Centre_plane" supertypes="Centre_of_symmetry" abstract.supertype="NO" abstract.entity="NO">
         <where label="WR1" expression="NOT EXISTS(SELF\Shape_element.description)"/>
      <where label="IP1"/>
      </entity>
      <entity name="Centre_point" supertypes="Centre_of_symmetry" abstract.supertype="NO" abstract.entity="NO">
         <where label="WR1" expression="NOT EXISTS(SELF\Shape_element.description)"/>
      <where label="IP1"/>
      </entity>
      <entity name="Composite_linear_profile_refinement_tolerance" supertypes="Line_profile_tolerance" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="refined_control" optional="NO">
            <typename name="locating_or_refinement_linear_profile_tolerance"/>
         </explicit>
         <where label="WR1" expression="NOT EXISTS(SELF\Geometric_tolerance.name)"/>
      </entity>
      <entity name="Composite_surface_profile_refinement_tolerance" supertypes="Surface_profile_tolerance" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="refined_control" optional="NO">
            <typename name="locating_or_refinement_surface_profile_tolerance"/>
         </explicit>
         <where label="WR1" expression="NOT EXISTS(SELF\Geometric_tolerance.name)"/>
      </entity>
      <entity name="Conical_tolerance_zone_boundary" supertypes="Tolerance_zone_boundary" abstract.supertype="NO" abstract.entity="NO">
         <inverse name="boundary_and_surface_relationships" entity="Conical_tolerance_zone_boundary_and_surface_relationship" attribute="sized_tolerance_zone_boundary">
            <inverse.aggregate type="SET" lower="2" upper="2"/>
         </inverse>
         <where label="WR1" expression="NOT EXISTS(SELF\Shape_element.description)"/>
      </entity>
      <entity name="Conical_tolerance_zone_boundary_and_surface_relationship" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="sized_tolerance_zone_boundary" optional="NO">
            <typename name="Conical_tolerance_zone_boundary"/>
         </explicit>
         <explicit name="terminating_surface" optional="NO">
            <typename name="physical_feature_or_part_template"/>
         </explicit>
         <inverse name="size_characteristic" entity="Conical_tolerance_zone_boundary_diametrical_size_characteristic" attribute="sized_boundary_at_surface"/>
      </entity>
      <entity name="Conical_tolerance_zone_boundary_diametrical_size_characteristic" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="sized_boundary_at_surface" optional="NO">
            <typename name="Conical_tolerance_zone_boundary_and_surface_relationship"/>
         </explicit>
         <explicit name="tolerance_value" optional="NO">
            <typename name="Length_data_element"/>
         </explicit>
      </entity>
      <entity name="Conical_tolerance_zone_definition" supertypes="Tolerance_zone_definition" abstract.supertype="NO" abstract.entity="NO"/>
      <entity name="Datum_axis" supertypes="Datum_shape_element" abstract.supertype="NO" abstract.entity="NO">
         <where label="WR1" expression="NOT EXISTS(SELF\Shape_element.description)"/>
      </entity>
      <entity name="Datum_axis_and_tolerance_zone_orienting_relationship" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="oriented_tolerance_zone" optional="NO">
            <typename name="Tolerance_zone"/>
         </explicit>
         <explicit name="orienting_datum_axis" optional="NO">
            <typename name="Datum_axis"/>
         </explicit>
         <inverse name="zone_orientation" entity="Datum_axis_related_orientation" attribute="orienting_relationship"/>
      </entity>
      <entity name="Datum_axis_related_orientation" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="orienting_relationship" optional="NO">
            <typename name="Datum_axis_and_tolerance_zone_orienting_relationship"/>
         </explicit>
         <explicit name="orientation_type" optional="NO">
            <typename name="centre_type"/>
         </explicit>
      </entity>
      <entity name="Datum_based_direction_vector" abstract.supertype="NO" abstract.entity="NO">
