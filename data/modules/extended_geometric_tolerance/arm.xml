         <explicit name="dimension" optional="NO">
            <typename name="dimensional_location_or_dimensional_size"/>
         </explicit>
         <explicit name="auxiliary_dimension_representation" optional="NO">
            <builtintype type="BOOLEAN" fixed="NO"/>
         </explicit>
      </entity>
      <entity name="Dimensional_size_based_on_opposing_boundaries" supertypes="Size_dimension" abstract.supertype="NO" abstract.entity="NO">
