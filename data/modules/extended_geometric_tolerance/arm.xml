            <typename name="Value_with_unit"/>
         </explicit>
         <explicit name="lower_dimension_value" optional="NO">
            <typename name="Value_with_unit"/>
         </explicit>
      </entity>
      <entity name="Linear_extension" supertypes="Derived_non_feature_shape_element" abstract.supertype="NO" abstract.entity="NO">
