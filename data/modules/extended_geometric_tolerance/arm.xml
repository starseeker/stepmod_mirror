         <explicit name="vector_of" optional="NO">
            <typename name="Datum_plane"/>
         </explicit>
         <explicit name="positive_direction_vector" optional="NO">
            <builtintype type="BOOLEAN" fixed="NO"/>
         </explicit>
      </entity>
      <entity name="Datum_based_length_measure" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="measure" optional="NO">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="measure_orientation" optional="NO">
            <typename name="descriptive_or_geometric_orientation"/>
         </explicit>
         <explicit name="of_datum" optional="NO">
            <typename name="Datum_plane"/>
         </explicit>
      </entity>
      <entity name="Datum_based_vector_orientation" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="direction_components" optional="NO">
            <aggregate type="SET" lower="2" upper="2" unique="NO" optional="NO"/>
            <typename name="Direction_element"/>
         </explicit>
      </entity>
      <entity name="Datum_difference" supertypes="Shape_element_relationship" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="precedent_datum" optional="NO">
            <typename name="Datum_shape_element"/>
            <redeclaration entity-ref="Shape_element_relationship" old_name="relating"/>
         </explicit>
         <explicit name="subsequent_datum" optional="NO">
            <typename name="Datum_shape_element"/>
            <redeclaration entity-ref="Shape_element_relationship" old_name="related"/>
         </explicit>
      </entity>
      <entity name="Datum_difference_based_characteristic" supertypes="Representation_item" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="of_datum_difference" optional="NO">
            <typename name="Datum_difference"/>
         </explicit>
      </entity>
      <entity name="Datum_feature" supertypes="Shape_element" super.expression="Datum_target_set" abstract.supertype="NO" abstract.entity="NO"/>
      <entity name="Datum_plane" supertypes="Datum_shape_element" abstract.supertype="NO" abstract.entity="NO">
         <where label="WR1" expression="NOT EXISTS(SELF\Shape_element.description)"/>
      </entity>
      <entity name="Datum_point" supertypes="Datum_shape_element" abstract.supertype="NO" abstract.entity="NO"/>
      <entity name="Datum_shape_element" supertypes="Datum Non_feature_shape_element" abstract.supertype="NO" abstract.entity="NO"/>
