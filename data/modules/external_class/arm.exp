SCHEMA External_class_arm;

USE FROM Class_arm;

TYPE class_source_select = EXTENSIBLE SELECT 
  (External_class_library);
END_TYPE;

ENTITY External_class
SUBTYPE OF (Class);
  external_source : class_source_select;
END_ENTITY;

ENTITY External_class_library;
  id : STRING;
  name : STRING;
  description : OPTIONAL STRING;
  library_type : STRING;
  location : STRING;
END_ENTITY;

END_SCHEMA;
