<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.22 2002/08/14 06:43:41 goset1 Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.22"
    source="../data/modules/external_item_identification_assignment/arm.exp"/>

  <schema name="External_item_identification_assignment_arm">
    <type name="external_identification_item">
      <select
        extensible="YES"
        genericentity="YES">
      </select>

    </type>
    <entity
      name="External_item_identification"
      supertypes="External_source_assignment">
      <explicit
        name="external_id">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="External_source_identification">
      <explicit
        name="source_id">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="source_type">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="item">
        <typename
          name="external_identification_item"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

  </schema>

</express>
