<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="external_library_arm schema_instance"/>
   <schema name="External_library_arm">
      <interface kind="use" schema="External_properties_arm"/>
      <interface kind="use" schema="External_source_arm"/>
      <interface kind="use" schema="Plib_class_reference_arm"/>
      <type name="el_external_identification_item">
         <select extensible="YES" genericentity="YES" basedon="external_identification_item" selectitems="User_defined_library_or_dictionary"/>
      </type>
      <entity name="Iso15926_4_rdl" supertypes="Known_source"/>
      <entity name="Known_source" abstract.supertype="YES" supertypes="External_source" super.expression="ONEOF (Iso15926_4_rdl, Otd_catalogue, Otd_dictionary, Plib_dictionary, Plib_library)"/>
      <entity name="Otd_catalogue" supertypes="Known_source"/>
      <entity name="Otd_dictionary" supertypes="Known_source"/>
      <entity name="Plib_dictionary" supertypes="Known_source"/>
      <entity name="Plib_library" supertypes="Known_source"/>
      <entity name="User_defined_library_or_dictionary" supertypes="External_source">
         <inverse name="source" entity="External_source_identification" attribute="item"/>
      </entity>
      <subtype.constraint name="external_source_subtypes" entity="External_source" super.expression="ONEOF (Known_source, User_defined_library_or_dictionary)"/>
   </schema>
</express>
