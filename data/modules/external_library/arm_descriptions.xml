<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: External_library_arm
   <express_ref linkend="external_library:arm:External_library_arm"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="External_library_arm">
</ext_description>
<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: External_library_arm  Entity: External_library  
<express_ref linkend="external_library:arm:External_library_arm.External_library"/>
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++  -->
<ext_description linkend="External_library_arm.External_library">
	An <b>External_library</b> is a type of
	<express_ref linkend="External_library:arm:External_library_arm.External_source"/>
	that identifies a library outside of ISO 10303.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: External_library_arm  Entity: External_source
<express_ref linkend="external_library:arm:External_library_arm.External_library"/>
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++  -->
<ext_description linkend="External_library_arm.External_source">
	<p>
	An <b>External_source</b> is an identification of a source of
	product data that is not the EXPRESS schema to which the exchange conforms.
	</p>
	<note>
		The product data may conform to some other part of this International Standard.
	</note>
	<p>
	An <b>External_source</b> may be a
	<express_ref linkend="External_library:arm:External_library_arm.Known_source"/>
	or a
	<express_ref linkend="External_library:arm:External_library_arm.User_defined_library_or_dictionary"/>.
	</p>
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: External_library_arm  Entity: External_source  Attribute: id 
<express_ref linkend="external_library:arm:External_library_arm.External_source.id"/>
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="External_library_arm.External_source.id">
   the identification of the <b>External_source</b>.        
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: External_library_arm  Entity: External_source  Attribute: description 
   <express_ref linkend="external_library:arm:External_library_arm.External_source.description"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="External_library_arm.External_source.description">
    the text that characterizes the <b>External_source</b>.
    The value of this attribute need not be specified.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: External_library_arm  Entity: Iso15926_4_rdl
<express_ref linkend="external_library:arm:External_library_arm.Iso15926_4_rdl"/>
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++  -->
<ext_description linkend="External_library_arm.Iso15926_4_rdl">
	An <b>Iso15926_4_rdl</b> is a type of
	<express_ref linkend="External_library:arm:External_library_arm.Known_source"/>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: External_library_arm  Entity: Known_source
<express_ref linkend="external_library:arm:External_library_arm.Known_source"/>
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++  -->
<ext_description linkend="External_library_arm.Known_source">
	A <b>Known_source</b> is a type of
	<express_ref linkend="External_library:arm:External_library_arm.External_source"/>.
	A <b>Known_source</b> is a 
	<express_ref linkend="External_library:arm:External_library_arm.Iso15926_4_rdl"/>
	or a
	<express_ref linkend="External_library:arm:External_library_arm.Otd_catalogue"/>
	or a
	<express_ref linkend="External_library:arm:External_library_arm.Otd_dictionary"/>
	or a
	<express_ref linkend="External_library:arm:External_library_arm.Plib_dictionary"/>
	or a
	<express_ref linkend="External_library:arm:External_library_arm.Plib_library"/>.
</ext_description>

