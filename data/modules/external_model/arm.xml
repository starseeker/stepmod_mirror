<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.16 2002/07/05 11:10:27 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.16"
    source="../data/modules/external_model/arm.exp"/>

  <schema name="External_model_arm">
    <interface
      kind="use"
      schema="Elemental_geometric_shape_arm">
    </interface>

    <interface
      kind="use"
      schema="File_identification_arm">
    </interface>

    <entity
      name="External_geometric_model"
      supertypes="Geometric_model">
      <explicit
        name="items">
        <redeclaration
          entity-ref="Representation"/>
          <aggregate
            type="SET"
            lower="1"
            upper="1"/>
          <typename
            name="Axis_placement"/>
        </explicit>
        <explicit
          name="external_file">
          <typename
            name="Digital_file"/>
        </explicit>
        <where
          label="WR1"
          expression="SELF\Representation.context_of_items.dimension_count = 3">
        </where>
        <where
          label="WR2"
          expression="SELF\Representation.context_of_items.kind = 'external'">
        </where>
      </entity>

    </schema>

  </express>
