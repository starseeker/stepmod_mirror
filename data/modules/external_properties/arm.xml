<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express language_version="2" description.file="arm_descriptions.xml" rcs.date="$Date: 2002/09/13 12:19:44 $" rcs.revision="$Revision: 1.1 $">
	<application name="express2xml.js" owner="Eurostep Limited" url="http://www.eurostep.com" version="1.22" source="../data/modules/external_properties/arm.exp"/>
	<schema name="External_properties_arm">
		<interface kind="use" schema="Independent_property_arm"/>
		<interface kind="use" schema="Plib_class_reference_arm"/>
		<type name="external_property_select">
			<select selectitems="Plib_property_reference External_library_property"/>
		</type>
		<entity name="External_library_property" supertypes="Independent_property">
			<explicit name="external_id">
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="library_type">
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="description" optional="YES">
				<builtintype type="STRING"/>
			</explicit>
		</entity>
		<entity name="Plib_property_reference" supertypes="Independent_property">
			<explicit name="code">
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="version">
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="name_scope">
				<typename name="Plib_class_reference"/>
			</explicit>
		</entity>
		<subtype.constraint name="plib_or_external" entity="Independent_property" super.expression="ONEOF(Plib_property_reference, External_library_property)"/>
	</schema>
</express>
