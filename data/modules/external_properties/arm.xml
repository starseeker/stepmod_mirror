<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date: 2007-05-30 07:12:20 -0700 (Tr, 30 Geg 2007) $" rcs.revision="$Revision: 1075 $" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="external_properties_arm schema_instance"/>
   <schema name="External_properties_arm">
      <interface kind="use" schema="External_library_arm"/>
      <interface kind="use" schema="Identification_assignment_arm"/>
      <interface kind="use" schema="Independent_property_arm"/>
      <interface kind="use" schema="Plib_class_reference_arm"/>
      <type name="external_property_select">
         <select selectitems="Plib_property_reference External_library_property"/>
      </type>
      <entity name="External_library_property" supertypes="Independent_property">
         <explicit name="external_id">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="source">
            <typename name="External_source"/>
         </explicit>
         <derived name="library_type" expression="source.id">
            <builtintype type="STRING"/>
         </derived>
      </entity>
      <entity name="Plib_property_reference" supertypes="Independent_property">
         <explicit name="code">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="version">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="name_scope">
            <typename name="Plib_class_reference"/>
         </explicit>
      </entity>
      <subtype.constraint name="plib_or_external" entity="Independent_property" super.expression="ONEOF (Plib_property_reference, External_library_property)"/>
   </schema>
</express>
