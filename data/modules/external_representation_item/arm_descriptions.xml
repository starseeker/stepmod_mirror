  -->
	<ext_description linkend="External_representation_item_arm.External_library">
An <b>External_library</b> is the identification of an external library.
</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: External_representation_item_arm  Entity: External_library  Attribute: id 
   <express_ref linkend="external_representation_item:arm:External_representation_item_arm.External_library.id"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="External_representation_item_arm.External_library.id">
       the identifier for the <b>External_library</b>.        
      </ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: External_representation_item_arm  Entity: External_library  Attribute: description 
   <express_ref linkend="external_representation_item:arm:External_representation_item_arm.External_library.description"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="External_representation_item_arm.External_library.description">
        the text that provides further information about the <b>External_library</b>.
      
      The value of this attribute need not be specified.
    </ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: External_representation_item_arm  Entity: External_representation_item  
   <express_ref linkend="external_representation_item:arm:External_representation_item_arm.External_representation_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="External_representation_item_arm.External_representation_item">
        An <b>External_representation_item</b> is a type of <express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation_item"/> whose value is defined in an external library.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: External_representation_item_arm  Entity: External_representation_item  Attribute: id 
   <express_ref linkend="external_representation_item:arm:External_representation_item_arm.External_representation_item.id"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="External_representation_item_arm.External_representation_item.id">
       the identifier for the <b>External_representation_item</b>.        
      </ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: External_representation_item_arm  Entity: External_representation_item  Attribute: external_source 
   <express_ref linkend="external_representation_item:arm:External_representation_item_arm.External_representation_item.external_source"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="External_representation_item_arm.External_representation_item.external_source">
	the identification of the external library that provides the value of the <express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation_item"/>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: External_representation_item_arm  Entity: External_string_representation_item  
