<ext_description linkend="External_representation_item_mim">
</ext_description><!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: External_representation_item_mim  Entity: externally_defined_representation_item  
   <express_ref linkend="external_representation_item:mim:External_representation_item_mim.externally_defined_representation_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="External_representation_item_mim.externally_defined_representation_item">
        An <b>externally_defined_representation_item</b> is a type of 
        <express_ref linkend="representation_schema:ir_express:representation_schema.representation_item"/> 
        and 
        <express_ref linkend="external_reference_schema:ir_express:external_reference_schema.externally_defined_item"/>
        that implements the ARM concept of
        <express_ref linkend="External_representation_item:arm:External_representation_item_arm.External_representation_item"/>.
        An <b>externally_defined_representation_item</b> value is defined in an external library.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: External_representation_item_mim  Entity: externally_defined_string  
   <express_ref linkend="external_representation_item:mim:External_representation_item_mim.externally_defined_string"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="External_representation_item_mim.externally_defined_string">
        An <b>externally_defined_string</b> is a type of 
        <express_ref linkend="external_representation_item:mim:External_representation_item_mim.externally_defined_representation_item"/> 
        that implements the ARM concept of
        <express_ref linkend="External_representation_item:arm:External_representation_item_arm.External_string_representation_item"/>.
        An <b>externally_defined_string</b> is a string defined in an external library.
</ext_description>

<ext_description linkend="External_representation_item_mim.externally_defined_representation">
        An <b>externally_defined_representation</b> is a type of 
        <express_ref linkend="external_reference_schema:ir_express:external_reference_schema.externally_defined_item"/>
        and 
        <express_ref linkend="representation_schema:ir_express:representation_schema.representation"/> 
        that implements the ARM concept of
        <express_ref linkend="External_representation_item:arm:External_representation_item_arm.External_representation_item"/>.
        An <b>externally_defined_representation</b> value is defined in an external library.
</ext_description>
<ext_description linkend="External_representation_item_mim.externally_defined_representation.items">
	the items shall be of type
	<express_ref linkend="External_representation_item:arm:External_representation_item_arm.External_representation_item"/>.
</ext_description>

