<!-- +++++++++++ External_source +++++++++++ -->
<ext_description linkend="External_source_arm.External_source">
	<p>
	An <b>External_source</b> is an identification of a source of
	product data that is not the EXPRESS schema to which the exchange conforms.
	</p>
	<note>
		The product data may conform to some other part of this International Standard.
	</note>
</ext_description>

<!-- id -->
<ext_description linkend="External_source_arm.External_source.id">
   the identification of the <b>External_source</b>.        
</ext_description>

<!-- description -->
<ext_description linkend="External_source_arm.External_source.description">
    the text that characterizes the <b>External_source</b>.
    The value of this attribute need not be specified.
</ext_description>

