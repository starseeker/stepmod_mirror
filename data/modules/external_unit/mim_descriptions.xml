	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: External_unit_mim  Entity: externally_defined_conversion_based_unit  
   <express_ref linkend="external_unit:mim:External_unit_mim.externally_defined_conversion_based_unit"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="External_unit_mim.externally_defined_conversion_based_unit">
	    An <b>externally_defined_conversion_based_unit</b> is a type of 
	    <express_ref linkend="measure_schema:ir_express:measure_schema.conversion_based_unit"/> 
	    and 
	    <express_ref linkend="external_reference_schema:ir_express:external_reference_schema.externally_defined_item"/> 
	    that implements the ARM concept of
	    <express_ref linkend="External_unit:arm:External_unit_arm.External_conversion_based_unit"/>.
	    In an <b>externally_defined_conversion_based_unit</b> the meaning of the <express_ref linkend="measure_schema:ir_express:measure_schema.context_dependent_unit"/> is given in an external library. 
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: External_unit_mim  Entity: externally_defined_context_dependent_unit  
   <express_ref linkend="external_unit:mim:External_unit_mim.externally_defined_context_dependent_unit"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="External_unit_mim.externally_defined_context_dependent_unit">
        An <b>externally_defined_context_dependent_unit</b> is a type of 
        <express_ref linkend="measure_schema:ir_express:measure_schema.context_dependent_unit"/> 
        and 
        <express_ref linkend="external_reference_schema:ir_express:external_reference_schema.externally_defined_item"/> 
        that implements the ARM concept of
        <express_ref linkend="External_unit:arm:External_unit_arm.External_context_dependent_unit"/>. 
        In an <b>externally_defined_context_dependent_unit</b> the meaning of the 
        <express_ref linkend="measure_schema:ir_express:measure_schema.context_dependent_unit"/> 
        is given in an external library. 
	</ext_description>
</ext_descriptions>
