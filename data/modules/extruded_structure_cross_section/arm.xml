<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="">
   <application name="JSDAI" owner="LKSoft" source="extruded_structure_cross_section_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Extruded_structure_cross_section_arm">
      <interface kind="use" schema="Physical_unit_usage_view_arm"/>
      <interface kind="use" schema="Component_feature_arm"/>
      <type name="cross_sectional_constituent_element">
         <select selectitems="Cross_sectional_outer_component_feature Cross_sectional_group_shape_element"/>
      </type>
      <type name="twist_direction_enum">
         <enumeration items="clockwise counter_clockwise"/>
      </type>
      <entity name="Cross_sectional_group_shape_element" super.expression="ONEOF (Cross_sectional_group_shape_element_with_tubular_cover, Cross_sectional_group_shape_element_with_lacing, Twisted_cross_sectional_group_shape_element)" supertypes="Shape_element_group">
         <explicit name="max_cross_section_diameter" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="min_cross_section_diameter" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="minimum_bend_radius" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="items">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="cross_sectional_constituent_element"/>
            <redeclaration entity-ref="Shape_element_group"/>
         </explicit>
      </entity>
      <entity name="Cross_sectional_group_shape_element_with_lacing" supertypes="Cross_sectional_group_shape_element">
         <explicit name="lacing_material" optional="YES">
            <typename name="Product_occurrence"/>
         </explicit>
      </entity>
      <entity name="Cross_sectional_group_shape_element_with_tubular_cover" supertypes="Cross_sectional_group_shape_element">
         <explicit name="cover">
            <typename name="Component_feature"/>
         </explicit>
      </entity>
      <entity name="Cross_sectional_outer_component_feature" supertypes="Component_feature">
         <explicit name="definition">
            <typename name="Cross_sectional_outer_part_shape_element"/>
            <redeclaration entity-ref="Component_feature"/>
         </explicit>
      </entity>
      <entity name="Cross_sectional_outer_part_shape_element" supertypes="Part_feature">
         <explicit name="max_cross_section_diameter" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="min_cross_section_diameter" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="minimum_bend_radius" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="SELF\Shape_element.containing_shape"/>
         </unique>
      </entity>
      <entity name="Shape_element_group" supertypes="Shape_element">
         <explicit name="items">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Shape_element"/>
         </explicit>
         <where expression="TRUE" label="WR1"/>
         <where expression="NOT (SELF IN items)" label="WR2"/>
      </entity>
      <entity name="Twisted_cross_sectional_group_shape_element" supertypes="Cross_sectional_group_shape_element">
         <explicit name="twist_period" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="twist_direction" optional="YES">
            <typename name="twist_direction_enum"/>
         </explicit>
      </entity>
   </schema>
</express>
