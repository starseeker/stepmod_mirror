<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?><!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1827">
   <application name="JSDAI" owner="LKSoft" source="extruded_structure_cross_section_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Extruded_structure_cross_section_arm">
      <interface kind="use" schema="General_design_connectivity_arm"/>
      <interface kind="use" schema="Value_with_unit_extension_arm"/>
      <type name="cross_sectional_constituent_element">
         <select selectitems="Cross_sectional_part_shape_element Cross_sectional_occurrence_shape_element"/>
      </type>
      <type name="outer_or_inner_boundary_enum">
         <enumeration items="outer_boundary inner_boundary"/>
      </type>
      <type name="part_view_definition_or_cross_sectional_part_shape_element_select">
         <select selectitems="Cross_sectional_part_shape_element Part_view_definition"/>
      </type>
      <type name="twist_direction_enum">
         <enumeration items="clockwise counter_clockwise"/>
      </type>
      <entity name="Cross_sectional_alternative_part_shape_element" supertypes="Cross_sectional_part_shape_element">
         <explicit name="items">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="cross_sectional_constituent_element"/>
         </explicit>
      </entity>
      <entity name="Cross_sectional_group_shape_element" super.expression="ONEOF (Cross_sectional_group_shape_element_with_tubular_cover, Cross_sectional_group_shape_element_with_lacing, Twisted_cross_sectional_group_shape_element)" supertypes="Cross_sectional_part_shape_element">
         <explicit name="items">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="cross_sectional_constituent_element"/>
         </explicit>
      </entity>
      <entity name="Cross_sectional_group_shape_element_with_lacing" supertypes="Cross_sectional_group_shape_element">
         <explicit name="lacing_material" optional="YES">
            <typename name="Product_occurrence"/>
         </explicit>
      </entity>
      <entity name="Cross_sectional_group_shape_element_with_tubular_cover" supertypes="Cross_sectional_group_shape_element">
         <explicit name="cover">
            <typename name="Occurrence_shape_feature"/>
         </explicit>
      </entity>
      <entity name="Cross_sectional_occurrence_shape_element" supertypes="Occurrence_shape_element">
         <explicit name="definition">
            <typename name="cross_sectional_constituent_element"/>
            <redeclaration entity-ref="Occurrence_shape_element"/>
         </explicit>
      </entity>
      <entity name="Cross_sectional_part_shape_element" super.expression="ONEOF (Cross_sectional_group_shape_element, Cross_sectional_alternative_part_shape_element)" supertypes="General_part_feature">
         <explicit name="associated_definition">
            <typename name="part_view_definition_or_cross_sectional_part_shape_element_select"/>
            <redeclaration entity-ref="Shape_element"/>
         </explicit>
         <explicit name="max_cross_section_diameter" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="min_cross_section_diameter" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="minimum_bend_radius" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="outer_or_inner_boundary" optional="YES">
            <typename name="outer_or_inner_boundary_enum"/>
         </explicit>
         <where expression="EXISTS(outer_or_inner_boundary) XOR ('PART_VIEW_DEFINITION_ARM.PART_VIEW_DEFINITION' IN TYPEOF(associated_definition))" label="WR1"/>
      </entity>
      <entity name="Twisted_cross_sectional_group_shape_element" supertypes="Cross_sectional_group_shape_element">
         <explicit name="twist_period" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="twist_direction" optional="YES">
            <typename name="twist_direction_enum"/>
         </explicit>
      </entity>
   </schema>
</express>
