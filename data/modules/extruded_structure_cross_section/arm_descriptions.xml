<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Extruded_structure_cross_section_arm  Enumeration: twist_direction_enum 
   <express_ref linkend="extruded_structure_cross_section:arm:Extruded_structure_cross_section_arm.twist_direction_enum"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Extruded_structure_cross_section_arm.twist_direction_enum">
	specifies the twist direction on whether it is right-hand or left-hand.
</ext_description>
<ext_description linkend="Extruded_structure_cross_section_arm.twist_direction_enum.clockwise">
	the direction is right-hand. 
</ext_description>
<ext_description linkend="Extruded_structure_cross_section_arm.twist_direction_enum.counter_clockwise">
	the direction is left-hand. 
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Extruded_structure_cross_section_arm  Entity: Cross_sectional_group_shape_element  
   <express_ref linkend="extruded_structure_cross_section:arm:Extruded_structure_cross_section_arm.Cross_sectional_group_shape_element"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Extruded_structure_cross_section_arm.Cross_sectional_group_shape_element">
	A <b>Cross_sectional_group_shape_element</b> is a type of
	<express_ref linkend="extruded_structure_cross_section:arm:Extruded_structure_cross_section_arm.Cross_sectional_part_shape_element"/>
	that defines a hierarchical grouping of other
	<express_ref linkend="extruded_structure_cross_section:arm:Extruded_structure_cross_section_arm.Cross_sectional_part_shape_element"/>s.
</ext_description>
<ext_description linkend="Extruded_structure_cross_section_arm.Cross_sectional_group_shape_element.items">
	defines the items that belong to this group.
</ext_description>



<ext_description linkend="Extruded_structure_cross_section_arm.Cross_sectional_part_shape_element">
	A <b>Cross_sectional_part_shape_element</b> is a type of
	<express_ref linkend="part_shape:arm:Part_shape_arm.Part_shape_element"/>
	that defines the cross sectional shape of an group of extruded objects perpendicular to the local centre line.
	The shape of the cross section is perpendicular to the local centre line in the geometric representation.
</ext_description>
<ext_description linkend="Extruded_structure_cross_section_arm.Cross_sectional_part_shape_element.max_cross_section_diameter">
	defines the maximum diameter of the cross section.
	The value of this attribute need not be specified.
</ext_description>
<ext_description linkend="Extruded_structure_cross_section_arm.Cross_sectional_part_shape_element.min_cross_section_diameter">
	defines the minimum diameter of the cross section.
	The value of this attribute need not be specified.
</ext_description>
<ext_description linkend="Extruded_structure_cross_section_arm.Cross_sectional_part_shape_element.minimum_bend_radius">
	defines the minimum allowed bend radius.
	The value of this attribute need not be specified.
</ext_description>


<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Extruded_structure_cross_section_arm  Entity: Cross_sectional_group_shape_element_with_lacing  
   <express_ref linkend="extruded_structure_cross_section:arm:Extruded_structure_cross_section_arm.Cross_sectional_group_shape_element_with_lacing"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Extruded_structure_cross_section_arm.Cross_sectional_group_shape_element_with_lacing">
	A <b>Cross_sectional_group_shape_element_with_lacing</b> is a type of
	<express_ref linkend="extruded_structure_cross_section:arm:Extruded_structure_cross_section_arm.Cross_sectional_group_shape_element"/>
	where the group is established by lacing.
</ext_description>
<ext_description linkend="Extruded_structure_cross_section_arm.Cross_sectional_group_shape_element_with_lacing.lacing_material">
	defines the material that is used for lacing.
	The value of this attribute need not be specified.
</ext_description>
    
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Extruded_structure_cross_section_arm  Entity: Cross_sectional_group_shape_element_with_tubular_cover  
   <express_ref linkend="extruded_structure_cross_section:arm:Extruded_structure_cross_section_arm.Cross_sectional_group_shape_element_with_tubular_cover"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Extruded_structure_cross_section_arm.Cross_sectional_group_shape_element_with_tubular_cover">
	A <b>Cross_sectional_group_shape_element_with_tubular_cover</b> is a type of
	<express_ref linkend="extruded_structure_cross_section:arm:Extruded_structure_cross_section_arm.Cross_sectional_group_shape_element"/>
	where the group members are enclosed in a tubular cover. 
</ext_description>
<ext_description linkend="Extruded_structure_cross_section_arm.Cross_sectional_group_shape_element_with_tubular_cover.cover">
	defines the material of the tubular cover.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Extruded_structure_cross_section_arm  Entity: Cross_sectional_occurrence_shape_element  
   <express_ref linkend="extruded_structure_cross_section:arm:Extruded_structure_cross_section_arm.Cross_sectional_occurrence_shape_element"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Extruded_structure_cross_section_arm.Cross_sectional_occurrence_shape_element">
	A <b>Cross_sectional_occurrence_shape_element</b> is a type of
	<express_ref linkend="general_design_connectivity:arm:General_design_connectivity_arm.Occurrence_shape_element"/>
	that represents the cross section shape that is perpendicular to a given centre-line.
</ext_description>
<ext_description linkend="Extruded_structure_cross_section_arm.Cross_sectional_occurrence_shape_element.definition">
	an inherited attribute that shall be of type
	<express_ref linkend="extruded_structure_cross_section:arm:Extruded_structure_cross_section_arm.Cross_sectional_part_shape_element"/>.
</ext_description>


<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Extruded_structure_cross_section_arm  Entity: Cross_sectional_alternative_part_shape_elementt  
   <express_ref linkend="extruded_structure_cross_section:arm:Extruded_structure_cross_section_arm.Cross_sectional_alternative_part_shape_element"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Extruded_structure_cross_section_arm.Cross_sectional_alternative_part_shape_element">
	<p>
	A <b>Cross_sectional_alternative_part_shape_element</b> is a type of
	<express_ref linkend="extruded_structure_cross_section:arm:Extruded_structure_cross_section_arm.Cross_sectional_part_shape_element"/>
	that is an alternative between one or more 
	<express_ref linkend="extruded_structure_cross_section:arm:Extruded_structure_cross_section_arm.Cross_sectional_occurrence_shape_element"/>
	or 
	<express_ref linkend="extruded_structure_cross_section:arm:Extruded_structure_cross_section_arm.Cross_sectional_group_shape_element"/>
	for the same purpose. At one particular cross section of a <b>Cross_sectional_alternative_part_shape_element</b> at one position 
	exactly one of these alternatives should be present. 
	</p>
	<example>
		A splice connecting two wires at a 
		<express_ref linkend="wiring_harness_assembly_design:arm:Wiring_harness_assembly_design_arm.Harness_node"/> 
		where each 
		<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Wire_occurrence"/>
		has the same purpose and does not change the cross section of the 
		<express_ref linkend="wiring_harness_assembly_design:arm:Wiring_harness_assembly_design_arm.Harness_segment"/>.
	</example>
</ext_description>
<ext_description linkend="Extruded_structure_cross_section_arm.Cross_sectional_alternative_part_shape_element.items">
	the possible alternatives of
	<express_ref linkend="extruded_structure_cross_section:arm:Extruded_structure_cross_section_arm.Cross_sectional_occurrence_shape_element"/>
	or 
	<express_ref linkend="extruded_structure_cross_section:arm:Extruded_structure_cross_section_arm.Cross_sectional_group_shape_element"/>
	for a <b>Cross_sectional_alternative_part_shape_element</b>.	
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Extruded_structure_cross_section_arm  Entity: Shape_element_group  
   <express_ref linkend="extruded_structure_cross_section:arm:Extruded_structure_cross_section_arm.Shape_element_group"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Extruded_structure_cross_section_arm.Shape_element_group">
	A <b>Shape_element_group</b> is a type of
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element"/>
	that is a grouping of one or more other
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element"/>s.
</ext_description>
<ext_description linkend="Extruded_structure_cross_section_arm.Shape_element_group.items">
	specifies the members of the group.
</ext_description>
<ext_description linkend="Extruded_structure_cross_section_arm.Shape_element_group.wr:WR1">
	all members shall have the same 
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element.associated_definition"/>
	as the
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element.associated_definition"/>
	of the <b>Shape_element_group</b>.
</ext_description>
<ext_description linkend="Extruded_structure_cross_section_arm.Shape_element_group.wr:WR2">
	a <b>Shape_element_group</b> shall not be a member of itself.
</ext_description>


<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Extruded_structure_cross_section_arm  Entity: Twisted_cross_sectional_group_shape_element  
   <express_ref linkend="extruded_structure_cross_section:arm:Extruded_structure_cross_section_arm.Twisted_cross_sectional_group_shape_element"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Extruded_structure_cross_section_arm.Twisted_cross_sectional_group_shape_element">
	A <b>Twisted_cross_sectional_group_shape_element</b> is a type of
	<express_ref linkend="extruded_structure_cross_section:arm:Extruded_structure_cross_section_arm.Cross_sectional_group_shape_element"/>
	where all the members are twisted together. 
</ext_description>
<ext_description linkend="Extruded_structure_cross_section_arm.Twisted_cross_sectional_group_shape_element.twist_period">
	specifies the length of lay.
	The value of this attribute need not be specified.
</ext_description>
<ext_description linkend="Extruded_structure_cross_section_arm.Twisted_cross_sectional_group_shape_element.twist_direction">
	specifies the direction of lay on whether it is right-hand or left-hand.
	The value of this attribute need not be specified.
</ext_description>

</ext_descriptions>
