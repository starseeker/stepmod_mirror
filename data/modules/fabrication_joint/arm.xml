<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:08" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="fabrication_joint_arm schema_instance"/>
   <schema name="FABRICATION_JOINT_ARM">
      <interface kind="use" schema="LAND_ARM"/>
      <interface kind="use" schema="LAYERED_INTERCONNECT_MODULE_WITH_PRINTED_COMPONENT_DESIGN_ARM"/>
      <interface kind="use" schema="SEQUENTIAL_LAMINATE_ASSEMBLY_DESIGN_ARM"/>
      <type name="fabricated_feature_select">
         <select selectitems="component_termination_passage_join_terminal conductive_interconnect_element_terminal embedded_component_terminal land_join_terminal non_functional_land_join_terminal printed_component_join_terminal via_terminal"/>
      </type>
      <entity name="connection_zone_based_fabrication_joint" supertypes="fabrication_joint">
         <explicit name="zone_1">
            <typename name="connection_zone_in_design_view"/>
         </explicit>
         <explicit name="zone_2">
            <typename name="connection_zone_in_design_view"/>
         </explicit>
         <where label="wr1" expression=" zone_1 :&lt;&gt;: zone_2 "/>
      </entity>
      <entity name="fabrication_joint">
         <explicit name="fabrication_features_1">
            <typename name="fabricated_feature_select"/>
         </explicit>
         <explicit name="fabrication_features_2">
            <typename name="fabricated_feature_select"/>
         </explicit>
         <explicit name="auxiliary_joint_material" optional="YES">
            <typename name="stratum_feature_template_component"/>
         </explicit>
         <explicit name="associated_interconnect_module">
            <typename name="interconnect_module"/>
         </explicit>
         <where label="wr1" expression=" fabrication_features_1 &lt;&gt; fabrication_features_2 "/>
         <where label="wr2" expression=" fabrication_features_1 &lt;&gt; auxiliary_joint_material "/>
         <where label="wr3" expression=" fabrication_features_2 &lt;&gt; auxiliary_joint_material "/>
      </entity>
      <entity name="passage_terminal_based_fabrication_joint_link">
         <explicit name="associated_terminal">
            <typename name="fabricated_feature_select"/>
         </explicit>
         <explicit name="precedent_point">
            <typename name="fabrication_joint"/>
         </explicit>
         <explicit name="subsequent_point">
            <typename name="fabrication_joint"/>
         </explicit>
         <unique label="ur1">
            <unique.attribute entity-ref="passage_terminal_based_fabrication_joint_link" attribute="associated_terminal"/>
            <unique.attribute entity-ref="passage_terminal_based_fabrication_joint_link" attribute="precedent_point"/>
            <unique.attribute entity-ref="passage_terminal_based_fabrication_joint_link" attribute="subsequent_point"/>
         </unique>
         <where label="wr1" expression=" precedent_point &lt;&gt; subsequent_point "/>
         <where label="wr2" expression=" precedent_point . fabrication_features_2 = associated_terminal "/>
         <where label="wr3" expression=" subsequent_point . fabrication_features_1 = associated_terminal "/>
         <where label="wr4" expression=" sizeof ( typeof ( associated_terminal ) * [ 'layered_interconnect_module_design_arm.' + 'component_termination_passage_join_terminal' , 'layered_interconnect_module_design_arm.' + 'via_terminal' ] ) = 1 "/>
      </entity>
   </schema>
</express>
