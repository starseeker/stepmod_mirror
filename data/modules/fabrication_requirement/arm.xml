<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$">
   <application name="JSDAI" owner="LKSoft" source="fabrication_requirement_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Fabrication_requirement_arm">
      <interface kind="use" schema="Fabrication_technology_arm"/>
      <type name="metal_or_dielectric">
         <enumeration items="over_finished_conductor over_finished_dielectric"/>
      </type>
      <entity name="Fabrication_thickness_requirement" supertypes="Predefined_requirement_view_definition">
         <explicit name="characterized_class">
            <typename name="metal_or_dielectric"/>
         </explicit>
         <explicit name="characterized_class_range">
            <typename name="Range_characteristic"/>
         </explicit>
         <explicit name="tolerance">
            <typename name="Tolerance_characteristic"/>
         </explicit>
         <where expression="SELF\Product_view_definition.initial_context.life_cycle_stage = 'test'" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Product_view_definition.id)" label="WR2"/>
      </entity>
      <entity name="Layout_land_width_tolerance_requirement" supertypes="Predefined_requirement_view_definition">
         <explicit name="mating_feature_pitch_class">
            <typename name="Pitch_class"/>
         </explicit>
         <explicit name="maximum_negative_deviation">
            <typename name="Length_data_element"/>
         </explicit>
         <where expression="SELF\Product_view_definition.initial_context.life_cycle_stage = 'test'" label="WR1"/>
      </entity>
      <entity name="Layout_line_width_tolerance_requirement" supertypes="Predefined_requirement_view_definition">
         <explicit name="characterized_stratum_technology">
            <typename name="Stratum_technology"/>
         </explicit>
         <explicit name="tolerance_line_width_class">
            <typename name="Line_width_class"/>
         </explicit>
         <explicit name="maximum_negative_deviation">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="maximum_positive_deviation">
            <typename name="Length_data_element"/>
         </explicit>
         <where expression="SELF\Product_view_definition.initial_context.life_cycle_stage = 'test'" label="WR1"/>
      </entity>
      <entity name="Line_width_class">
         <explicit name="class_member">
            <typename name="Range_characteristic"/>
         </explicit>
         <explicit name="class_name">
            <builtintype type="STRING"/>
         </explicit>
         <where expression="SIZEOF(QUERY(np &lt;* class_member\Representation.items | NOT ('VALUE_WITH_UNIT_EXTENSION_ARM.LENGTH_DATA_ELEMENT' IN TYPEOF(np)))) = 0" label="WR1"/>
      </entity>
      <entity name="Pitch_class">
         <explicit name="class_member">
            <typename name="Range_characteristic"/>
         </explicit>
         <explicit name="class_name">
            <builtintype type="STRING"/>
         </explicit>
         <where expression="SIZEOF(QUERY(np &lt;* class_member\Representation.items | NOT ('VALUE_WITH_UNIT_EXTENSION_ARM.LENGTH_DATA_ELEMENT' IN TYPEOF(np)))) = 0" label="WR1"/>
      </entity>
      <subtype.constraint entity="Predefined_requirement_view_definition" name="fr_predefined_requirement_view_definition_subtypes" super.expression="ONEOF (Layout_line_width_tolerance_requirement, Layout_land_width_tolerance_requirement, Fabrication_thickness_requirement)"/>
   </schema>
</express>
