      <function name="sts_vertex_degree_less_than_two">
         <parameter name="input">
            <aggregate type="SET" lower="0" upper="?" unique="NO" optional="NO"/>
            <typename name="Stratum_technology_occurrence_link"/>
         </parameter>
         <builtintype type="BOOLEAN" fixed="NO"/>
         <algorithm>LOCAL
   psto : BAG OF INTEGER := [];
   ssto : BAG OF INTEGER := [];
   sto : SET OF Stratum_technology_occurrence := get_stack(input);
   i : INTEGER := 0;
   j : INTEGER := 0;
   pass : BOOLEAN := TRUE;
 END_LOCAL;
  REPEAT i := 1 TO SIZEOF(sto) BY 1;
   REPEAT j := 1 TO SIZEOF(input) BY 1;
     IF (input[j].precedent_sto :=: sto[i]) THEN 
      psto[i] := psto[i] + 1;
      IF (psto[i] = 2) THEN 
       pass := FALSE; 
       ESCAPE; 
      END_IF;
     END_IF;  
     IF (input[j].subsequent_sto :=: sto[i]) THEN 
      ssto[i] := ssto[i] + 1;  
      IF (ssto[i] = 2) THEN 
       pass := FALSE; 
       ESCAPE; 
      END_IF;
     END_IF;  
   END_REPEAT;                                     
  END_REPEAT;                                     
 RETURN(pass);</algorithm>
      </function>
