<where label="WR6" expression="(TYPEOF(as_finished_passage_extent) = 'CHARACTERISTIC_ARM.STATISTICAL_TOLERANCE_CHARACTERISTIC') OR&#10;        (TYPEOF(as_finished_passage_extent) = 'CHARACTERISTIC_ARM.SYMMETRICAL_TOLERANCE_CHARACTERISTIC') OR&#10;        (SIZEOF(QUERY(it &lt;* as_finished_passage_extent\Representation.items |&#10;          NOT('VALUE_WITH_UNIT_EXTENSION_ARM.LENGTH_DATA_ELEMENT' IN TYPEOF(as_finished_passage_extent\Representation.items))&#10;         )) = 0)"/>
<where label="WR7" expression="NOT(TYPEOF(as_finished_passage_extent) = 'CHARACTERISTIC_ARM.SYMMETRICAL_TOLERANCE_CHARACTERISTIC') OR&#10;        (SIZEOF(QUERY(it &lt;* as_finished_passage_extent\Representation.items |&#10;        ('VALUE_WITH_UNIT_EXTENSION_ARM.LENGTH_DATA_ELEMENT' IN TYPEOF(as_finished_passage_extent\Representation.items))&#10;           )) = 1)"/>
<where label="WR8" expression="(TYPEOF(as_finished_deposition_thickness) = 'CHARACTERISTIC_ARM.STATISTICAL_TOLERANCE_CHARACTERISTIC') OR&#10;        (TYPEOF(as_finished_deposition_thickness) = 'CHARACTERISTIC_ARM.SYMMETRICAL_TOLERANCE_CHARACTERISTIC') OR&#10;        (SIZEOF(QUERY(it &lt;* as_finished_deposition_thickness\Representation.items |&#10;          NOT('VALUE_WITH_UNIT_EXTENSION_ARM.LENGTH_DATA_ELEMENT' IN TYPEOF(as_finished_deposition_thickness\Representation.items))&#10;         )) = 0)"/>
<where label="WR9" expression="NOT(TYPEOF(as_finished_deposition_thickness) = 'CHARACTERISTIC_ARM.SYMMETRICAL_TOLERANCE_CHARACTERISTIC') OR&#10;        (SIZEOF(QUERY(it &lt;* as_finished_deposition_thickness\Representation.items |&#10;        ('VALUE_WITH_UNIT_EXTENSION_ARM.LENGTH_DATA_ELEMENT' IN TYPEOF(as_finished_deposition_thickness\Representation.items))&#10;           )) = 1)"/>
