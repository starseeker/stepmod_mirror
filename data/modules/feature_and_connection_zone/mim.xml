   <schema name="Feature_and_connection_zone_mim">
      <interface kind="use" schema="Shape_feature_mim"/>
      <interface kind="use" schema="Surface_conditions_mim"/>
      <entity name="connection_zone_interface_plane_relationship" supertypes="shape_aspect shape_aspect_relationship">
         <unique label="UR1">
            <unique.attribute attribute="name" entity-ref="shape_aspect"/>
         </unique>
      </entity>
      <entity name="feature_definition_with_connection_area" supertypes="shape_feature_definition"/>
      <entity name="location_shape_representation" supertypes="shape_representation">
         <where expression="SIZEOF(SELF.items)= 1" label="WR1"/>
         <where expression="SIZEOF(QUERY(i &lt;* SELF.items | 'GEOMETRY_SCHEMA.' + 'POINT' IN TYPEOF(i)))= 1" label="WR2"/>
      </entity>
   </schema>
</express>
