<express language_version="2" rcs.date="$Date: 2007/08/02 17:35:11 $" rcs.revision="$Revision: 1.16 $" description.file="mim_descriptions.xml">
<application name="GraphicalEXPRESS" owner="Eurostep" url="www.eurostep.com" version="1.4.0" source="mim.vsd" />
<schema name="File_identification_mim">
<interface kind="use" schema="document_schema">
<interfaced.item name="document" />
<interfaced.item name="document_representation_type" />
</interface>
<interface kind="use" schema="External_item_identification_assignment_mim">
</interface>
<interface kind="use" schema="Identification_assignment_mim">
</interface>
<interface kind="use" schema="product_property_definition_schema">
<interfaced.item name="characterized_object" />
</interface>
<type name="file_identification_item">
<select basedon="identification_item" selectitems="document_file" />
<graphic.element page="2" xcoord="77" ycoord="28" />
</type>
<type name="file_location_select">
<select basedon="external_identification_item" selectitems="document_file" />
<graphic.element page="2" xcoord="375" ycoord="28" />
</type>
<entity name="document_file" supertypes="characterized_object document">
<where label="WR1" expression="SELF\characterized_object.name = ''">
</where>
<where label="WR2" expression=" NOT EXISTS(SELF\characterized_object.description)">
</where>
<where label="WR3" expression="SIZEOF(QUERY(drt &lt;* SELF\document.representation_types | drt.name IN ['digital', 'physical'])) = 1">
</where>
<graphic.element page="2" xcoord="212" ycoord="106" />
</entity>
<graphic.element page="1" xcoord="162" ycoord="106" />
</schema>
</express>
