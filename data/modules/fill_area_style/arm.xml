<?xml version="1.0" encoding="utf-8"?><?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express
  SYSTEM "../../../dtd/express.dtd">
<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="">
   <application name="JSDAI" owner="LKSoft" source="fill_area_style_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Fill_area_style_arm">
      <interface kind="use" schema="Basic_curve_arm"/>
      <interface kind="use" schema="Styled_curve_arm"/>
      <interface kind="use" schema="Value_with_unit_extension_arm"/>
      <interface kind="use" schema="Extended_basic_geometry_arm"/>
      <entity name="Externally_defined_hatch_style" supertypes="Hatch_style"/>
      <entity name="Externally_defined_tile_style" supertypes="Tile_style"/>
      <entity abstract.supertype="YES" name="Hatch_style" super.expression="ONEOF (Externally_defined_hatch_style, User_defined_hatch_style)" supertypes="Detailed_geometric_model_element"/>
      <entity name="Solid_fill_area_style">
         <explicit name="fill_colour">
            <typename name="Colour"/>
         </explicit>
      </entity>
      <entity abstract.supertype="YES" name="Tile_style" super.expression="ONEOF (Externally_defined_tile_style, User_defined_tile_style)" supertypes="Detailed_geometric_model_element"/>
      <entity name="User_defined_hatch_style" supertypes="Hatch_style">
         <explicit name="hatch_line_appearance">
            <typename name="Curve_appearance"/>
         </explicit>
         <explicit name="start_of_next_hatch_line">
            <typename name="Vector"/>
         </explicit>
         <explicit name="point_of_reference_hatch_line">
            <typename name="Cartesian_point"/>
         </explicit>
         <explicit name="pattern_start">
            <typename name="Cartesian_point"/>
         </explicit>
         <explicit name="hatch_line_angle">
            <typename name="Angle_data_element"/>
         </explicit>
         <where expression="'STYLED_CURVE_ARM.SOLID_CURVE_FONT' IN TYPEOF(SELF.hatch_line_appearance.font)" label="WR1"/>
      </entity>
      <entity name="User_defined_tile_style" supertypes="Tile_style">
         <explicit name="first_direction_repeat_factor">
            <typename name="Vector"/>
         </explicit>
         <explicit name="second_direction_repeat_factor">
            <typename name="Vector"/>
         </explicit>
         <explicit name="tiling_scale">
            <builtintype type="INTEGER"/>
         </explicit>
      </entity>
      <subtype.constraint entity="Detailed_geometric_model_element" name="fas_detailed_geometric_model_element_subtypes" super.expression="ONEOF (Hatch_style, Tile_style)"/>
   </schema>
</express>