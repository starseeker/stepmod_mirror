<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date: 2010/09/10 10:51:05 $" rcs.revision="$Revision: 1.9 $" reference="ISO/TS 10303-1381">
   <application name="JSDAI" owner="LKSoft" source="finite_element_analysis_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Finite_element_analysis_arm">
      <interface kind="use" schema="Analysis_characterized_arm"/>
      <interface kind="use" schema="Elemental_geometric_shape_arm"/>
      <interface kind="use" schema="Finite_elements_arm"/>
      <type name="constraint_definition_reference">
         <select selectitems="Detailed_geometric_model_element Fea_group Node"/>
      </type>
      <type name="definition_reference">
         <select selectitems="Detailed_geometric_model_element Element Fea_group Node"/>
      </type>
      <type name="fe_analysis_approval_item">
         <select basedon="analysis_approval_item" extensible="YES" genericentity="YES" selectitems="Fe_analysis Fe_analysis_results"/>
      </type>
      <entity name="Analysis_message" supertypes="Fe_analysis_state_definition">
         <explicit name="message_level">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="quality">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="Analysis_report_representation" super.expression="Graphical_representation ANDOR Tabular_representation">
         <explicit name="control" optional="YES">
            <typename name="Fe_analysis"/>
         </explicit>
         <explicit name="model_ref">
            <typename name="Fea_model"/>
         </explicit>
         <explicit name="result" optional="YES">
            <typename name="Fe_analysis_results"/>
         </explicit>
      </entity>
      <entity name="Calculated_state" supertypes="Fe_analysis_state"/>
      <entity name="Constraint" super.expression="ONEOF (Single_point_constraint, Linear_constraint_equation, Nodal_degree_of_freedom_reduction)">
         <explicit name="identification">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="steps">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Fe_analysis_control_step"/>
         </explicit>
      </entity>
      <entity name="Element_field_variable_definition" supertypes="Fe_analysis_state_definition"/>
      <entity name="Element_nodal_freedom_actions" supertypes="Fe_analysis_state_definition"/>
      <entity name="Fe_analysis">
         <explicit name="intended_analysis_code">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="model_ref">
            <typename name="Fea_model"/>
         </explicit>
      </entity>
      <entity name="Fe_analysis_control_step" super.expression="ONEOF (Modes_and_frequencies_control_step, Static_control_step)">
         <explicit name="analysis">
            <typename name="Fe_analysis"/>
         </explicit>
         <explicit name="identification">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="initial_input_state">
            <typename name="Fe_analysis_state"/>
         </explicit>
         <explicit name="sequence">
            <builtintype type="INTEGER"/>
         </explicit>
      </entity>
      <entity name="Fe_analysis_results"/>
      <entity name="Fe_analysis_results_step" super.expression="ONEOF (Modes_and_frequencies_results_step, Static_results_step)">
         <explicit name="control">
            <typename name="Fe_analysis_control_step"/>
         </explicit>
         <explicit name="result">
            <typename name="Fe_analysis_results"/>
         </explicit>
      </entity>
      <entity name="Fe_analysis_state" super.expression="ONEOF (Calculated_state, Linearly_superimposed_state, Output_request_state, Specified_state)">
         <explicit name="definitions">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Fe_analysis_state_definition"/>
         </explicit>
      </entity>
      <entity name="Fe_analysis_state_definition" super.expression="ONEOF (Analysis_message, Element_field_variable_definition, Element_nodal_freedom_actions, Linear_constraint_equation_value, Nodal_freedom_definitions, Single_point_constraint_values)">
         <explicit name="ref">
            <typename name="definition_reference"/>
         </explicit>
      </entity>
      <entity name="Graphical_representation" supertypes="Analysis_report_representation">
         <explicit name="graphical_filename">
             <typename name="Document_definition"/>
         </explicit>
      </entity>
      <entity name="Linear_constraint_equation" supertypes="Constraint">
         <explicit name="freedoms_and_coefficients_nodes">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="constraint_definition_reference"/>
         </explicit>
      </entity>
      <entity name="Linear_constraint_equation_value" supertypes="Fe_analysis_state_definition">
         <explicit name="equation">
            <typename name="Linear_constraint_equation"/>
         </explicit>
      </entity>
      <entity name="Linearly_superimposed_state" supertypes="Fe_analysis_state"/>
      <entity name="Modes_and_frequencies_control_step" supertypes="Fe_analysis_control_step">
         <explicit name="final_input_state">
            <typename name="Fe_analysis_state"/>
         </explicit>
         <explicit name="frequency_range">
            <aggregate lower="1" type="LIST" upper="2"/>
            <builtintype type="REAL"/>
         </explicit>
         <explicit name="number_of_modes">
            <builtintype type="INTEGER"/>
         </explicit>
      </entity>
      <entity name="Modes_and_frequencies_results_step" supertypes="Fe_analysis_results_step">
         <explicit name="resulting_states">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Calculated_state"/>
         </explicit>
      </entity>
      <entity name="Nodal_degree_of_freedom_reduction" supertypes="Constraint">
         <explicit name="node">
            <typename name="constraint_definition_reference"/>
         </explicit>
      </entity>
      <entity name="Nodal_freedom_definitions" supertypes="Fe_analysis_state_definition"/>
      <entity name="Output_request_state" supertypes="Fe_analysis_state">
         <explicit name="steps">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Fe_analysis_control_step"/>
         </explicit>
      </entity>
      <entity name="Single_point_constraint" supertypes="Constraint">
         <explicit name="required_node">
            <typename name="constraint_definition_reference"/>
         </explicit>
      </entity>
      <entity name="Single_point_constraint_values" supertypes="Fe_analysis_state_definition">
         <explicit name="element">
            <typename name="Single_point_constraint"/>
         </explicit>
      </entity>
      <entity name="Specified_state" supertypes="Fe_analysis_state"/>
      <entity name="Static_control_step" supertypes="Fe_analysis_control_step">
         <explicit name="final_input_state">
            <typename name="Fe_analysis_state"/>
         </explicit>
      </entity>
      <entity name="Static_results_step" supertypes="Fe_analysis_results_step">
         <explicit name="resulting_state">
            <typename name="Calculated_state"/>
         </explicit>
      </entity>
      <entity name="Tabular_representation" supertypes="Analysis_report_representation">
         <explicit name="tabular_filename">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
   </schema>
</express>
