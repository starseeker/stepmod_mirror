<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$">
   <application name="JSDAI" owner="LKSoft" source="finite_elements_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Finite_elements_arm">
      <interface kind="use" schema="Analysis_characterized_arm"/>
      <interface kind="use" schema="Analysis_identification_arm"/>
      <interface kind="use" schema="Elemental_geometric_shape_arm"/>
      <interface kind="use" schema="Shape_property_assignment_arm"/>
      <interface kind="use" schema="Value_with_unit_arm"/>
      <type name="definition_element">
         <select selectitems="Curve_element Shape_element Surface_element Volume_element"/>
      </type>
      <type name="element_property_select">
         <select selectitems="Curve_property Surface_property"/>
      </type>
      <type name="fe_model_approval_item">
         <select basedon="analysis_approval_item" extensible="YES" genericentity="YES" selectitems="Fea_model"/>
      </type>
      <entity name="Curve_element" supertypes="Element"/>
      <entity name="Curve_property">
         <explicit name="defined_elements">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Curve_element"/>
         </explicit>
      </entity>
      <entity name="Curve_section_properties">
         <explicit name="property">
            <typename name="Curve_property"/>
         </explicit>
      </entity>
      <entity name="Damping_matrix" supertypes="Matrix"/>
      <entity name="Directionally_explicit_element" supertypes="Element">
         <explicit name="associated_matrix">
            <typename name="Matrix"/>
         </explicit>
      </entity>
      <entity name="Element" super.expression="ONEOF (Volume_element, Surface_element, Substructure_element, Curve_element, Point_element, Directionally_explicit_element, Explicit_element)">
         <explicit name="description" optional="YES">
            <typename name="Element_description"/>
         </explicit>
         <explicit name="element_identification">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="model_ref">
            <typename name="Fea_model"/>
         </explicit>
         <explicit name="node_list">
            <aggregate lower="1" type="LIST" upper="?"/>
            <typename name="Node"/>
         </explicit>
      </entity>
      <entity name="Element_description">
         <explicit name="description">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="Element_property_geometric_relationship">
         <explicit name="geometry_ref">
            <typename name="Detailed_geometric_model_element"/>
         </explicit>
         <explicit name="section_ref">
            <typename name="element_property_select"/>
         </explicit>
      </entity>
      <entity name="Element_shape_aspect"/>
      <entity name="Element_shape_relationship">
         <explicit name="element_aspect">
            <typename name="Element_shape_aspect"/>
         </explicit>
         <explicit name="element_reference">
            <typename name="Element"/>
         </explicit>
         <explicit name="role">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="shape">
            <typename name="Detailed_geometric_model_element"/>
         </explicit>
      </entity>
      <entity name="Explicit_element" supertypes="Element">
         <explicit name="associated_matrix">
            <typename name="Matrix"/>
         </explicit>
      </entity>
      <entity name="Fea_group">
         <explicit name="description">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="elements" optional="YES">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Element"/>
         </explicit>
         <explicit name="group_identification">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="nodes" optional="YES">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Node"/>
         </explicit>
      </entity>
      <entity name="Fea_group_relationship">
         <explicit name="relating_group">
            <typename name="Fea_group"/>
         </explicit>
         <explicit name="related_group">
            <typename name="Fea_group"/>
         </explicit>
      </entity>
      <entity name="Fea_model">
         <explicit name="analysis_type">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="cae_filename" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="creating_software">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="definition">
            <typename name="Analysis_discipline_definition"/>
         </explicit>
         <explicit name="description">
            <typename name="Fea_model_description"/>
         </explicit>
         <explicit name="identification">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="intended_analysis_code">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="Fea_model_description">
         <explicit name="description">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="Mass_matrix" supertypes="Matrix"/>
      <entity name="Matrix" super.expression="ONEOF (Mass_matrix, Stiffness_matrix, Damping_matrix)"/>
      <entity name="Nodal_results_coordinate_system"/>
      <entity name="Node">
         <explicit name="description" optional="YES">
            <typename name="Node_description"/>
         </explicit>
         <explicit name="location">
            <typename name="Detailed_geometric_model_element"/>
         </explicit>
         <explicit name="model_ref">
            <typename name="Fea_model"/>
         </explicit>
         <explicit name="node_identification">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="results_coordinate_space" optional="YES">
            <typename name="Nodal_results_coordinate_system"/>
         </explicit>
      </entity>
      <entity name="Node_description">
         <explicit name="description">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="Node_shape_relationship">
         <explicit name="node_reference">
            <typename name="Node"/>
         </explicit>
         <explicit name="role">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="shape">
            <typename name="Detailed_geometric_model_element"/>
         </explicit>
      </entity>
      <entity name="Point_element" supertypes="Element">
         <explicit name="associated_matrix">
            <typename name="Matrix"/>
         </explicit>
      </entity>
      <entity name="Stiffness_matrix" supertypes="Matrix"/>
      <entity name="Substructure_element" supertypes="Element">
         <explicit name="substructure_model_ref">
            <typename name="Fea_model"/>
         </explicit>
      </entity>
      <entity name="Substructure_node_relationship">
         <explicit name="relating_node">
            <typename name="Node"/>
         </explicit>
         <explicit name="related_node">
            <typename name="Node"/>
         </explicit>
      </entity>
      <entity name="Surface_element" supertypes="Element"/>
      <entity name="Surface_property">
         <explicit name="defined_elements">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Surface_element"/>
         </explicit>
      </entity>
      <entity name="Surface_thickness">
         <explicit name="property">
            <typename name="Surface_property"/>
         </explicit>
         <explicit name="thickness">
            <typename name="length_measure"/>
         </explicit>
      </entity>
      <entity name="Volume_element" supertypes="Element"/>
   </schema>
</express>
