<ext_description linkend="Footprint_definition_arm">
</ext_description>

<!-- +++++++++++ Breakout_footprint_definition +++++++++++ -->
<ext_description linkend="Footprint_definition_arm.Breakout_footprint_definition">
	A <b>Breakout_footprint_definition</b> is a type of 
	<express_ref linkend="Footprint_definition:arm:Footprint_definition_arm.Generic_footprint_definition"/>.
</ext_description>

<!-- reference_footprint -->
<ext_description linkend="Footprint_definition_arm.Breakout_footprint_definition.reference_footprint">
	specifies a role of the 
	<express_ref linkend="Footprint_definition:arm:Footprint_definition_arm.Footprint_definition"/>
	for the <b>Breakout_footprint_definition</b>.
</ext_description>

<!-- breakout_traces -->
<ext_description linkend="Footprint_definition_arm.Breakout_footprint_definition.breakout_traces">
	specifies an inverse relationship that specifies that the existence of the <b>Breakout_footprint_definition</b> is dependent on the existence of the  
	<express_ref linkend="Footprint_definition:arm:Footprint_definition_arm.Stratum_feature_template_location_in_breakout_footprint_definition"/> 
	that specifies the <b>Breakout_footprint_definition</b> as its 
	<express_ref linkend="Footprint_definition:arm:Footprint_definition_arm.Stratum_feature_template_location_in_breakout_footprint_definition.assembly"/>.  
	There shall be one or more  
	<express_ref linkend="Footprint_definition:arm:Footprint_definition_arm.Stratum_feature_template_location_in_breakout_footprint_definition"/> 
	for a <b>Breakout_footprint_definition</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Footprint_definition_arm.Breakout_footprint_definition.wr:WR1">
	The 
	<express_ref linkend="layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.Multi_stratum_structured_template.location"/>
	of <b>Breakout_footprint_definition</b> shall be same as location of its <b>reference_footprint</b>.
</ext_description>

<!-- IP1 -->
<ext_description linkend="Footprint_definition_arm.Breakout_footprint_definition.wr:IP1">
	The domain of the geometry of the <b>Breakout_footprint_definition</b> shall be the same domain as that of the <b>reference_footprint</b>.
</ext_description>

<!-- IP2 -->
<ext_description linkend="Footprint_definition_arm.Breakout_footprint_definition.wr:IP2">
	Each member of <b>breakout_traces</b> shall have a domain that overlaps with a 
	<express_ref linkend="Land:arm:Land_arm.Land"/> 
	in the <b>reference_footprint</b>.
</ext_description>

<!-- IP3 -->
<ext_description linkend="Footprint_definition_arm.Breakout_footprint_definition.wr:IP3">
	Each member of <b>breakout_traces</b> shall have a domain that overlaps with a 
	<express_ref linkend="Land:arm:Land_arm.Land"/> 
	in the 
	<express_ref linkend="Footprint_definition:arm:Footprint_definition_arm.Generic_footprint_definition.padstacks">padstack</express_ref>
	inherited from 
	<express_ref linkend="Footprint_definition:arm:Footprint_definition_arm.Generic_footprint_definition"/>.
	
</ext_description>

<!-- +++++++++++ Breakout_footprint_definition_shape_model +++++++++++ -->
<ext_description linkend="Footprint_definition_arm.Breakout_footprint_definition_shape_model">
	A <b>Breakout_footprint_definition_shape_model</b> is a type of
	<express_ref linkend="Layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.Structured_template_planar_shape_model"/>.
</ext_description>

<!-- shape_characterized_definition -->
<ext_description linkend="Footprint_definition_arm.Breakout_footprint_definition_shape_model.shape_characterized_definition">
	specifies a role of the
	<express_ref linkend="Footprint_definition:arm:Footprint_definition_arm.Breakout_footprint_definition"/>
	for the <b>Breakout_footprint_definition_shape_model</b>. There shall exist exactly one 
	<express_ref linkend="Footprint_definition:arm:Footprint_definition_arm.Breakout_footprint_definition"/>
	for the <b>Breakout_footprint_definition_shape_model</b>.
</ext_description>

<!-- reference_shape -->
<ext_description linkend="Footprint_definition_arm.Breakout_footprint_definition_shape_model.reference_shape">
	specifies a role of the
	<express_ref linkend="Footprint_definition:arm:Footprint_definition_arm.Footprint_definition_shape_model"/>
	for the <b>Breakout_footprint_definition_shape_model</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Footprint_definition_arm.Breakout_footprint_definition_shape_model.wr:WR1">
The <express_ref linkend="footprint_definition:arm:Footprint_definition_arm.Footprint_definition_shape_model.shape_characterized_definition"/>
of <b>reference_shape</b> shall be same footprint as one referenced by
<express_ref linkend="footprint_definition:arm:Footprint_definition_arm.Breakout_footprint_definition.reference_footprint"/>
of <express_ref linkend="footprint_definition:arm:Footprint_definition_arm.Breakout_footprint_definition_shape_model.shape_characterized_definition"/>
of <b>Breakout_footprint_definition_shape_model</b>.
</ext_description>


<!-- +++++++++++ Footprint_definition +++++++++++ -->
<ext_description linkend="Footprint_definition_arm.Footprint_definition">
<p>
	A <b>Footprint_definition</b> is a type of 
	<express_ref linkend="Footprint_definition:arm:Footprint_definition_arm.Generic_footprint_definition"/>.  
</p>
<example number="1">
	<p>
	<module_ref linkend="footprint_definition:4_entities:figure:lpt4">Figure 1</module_ref> illustrates two categories of complex land patterns.  
	For a specific technology implementation in an 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Interconnect_module"/>, 
	all parameters would be assigned specific values, in addition to the explicit geometry and material descriptions otherwise provided.
	</p>
	</example>
	<figure number="1" id="lpt4">
	<title>Complex technology specific land pattern definition</title>
	<img src="lpt_4.gif"/></figure>
	<example number="2">
	<p>
	<module_ref linkend="footprint_definition:4_entities:figure:lpt5">Figure 2</module_ref> illustrates two categories of simple land patterns.  
	For a specific technology implementation in an 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Interconnect_module"/>, 
	all parameters would be assigned specific values, in addition to the explicit geometry and material descriptions otherwise provided.
	</p>
	</example>
	<figure number="2" id="lpt5">
	<title>Simple technology specific land pattern definition</title>
	<img src="lpt_5.gif"/>
	</figure>
	<!--  
	A <b>Footprint_definition</b> is a mating reference layout item for a specific  
	<express_ref linkend="Package:arm:Package_arm.Package"/> 
	<b>shape</b>.  It identifies the reference  
	<express_ref linkend="Package:arm:Package_arm.Package"/> 
	<b>shape</b>, and a reference breakout.
	The reference breakout is only significant for the metal pattern that is on the top layer.  The specific  
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Stratum"/> 
	stackup for the reference breakout is of no significance.  The  
	<express_ref linkend="Package:arm:Package_arm.Package"/> 
	is identified by a query on <b>shape</b> and then on the  
	<express_ref linkend="Physical_unit_2d_shape:arm:Physical_unit_2d_shape_arm.Physical_unit_planar_shape_model"/> 
	that references a  
	<express_ref linkend="Package:arm:Package_arm.Package"/>.  
	The location of the footprint is derived from the location of the  
	<express_ref linkend="Layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.Padstack_definition"/> 
	that contribute to the <b>Footprint_definition</b>, and all  
	<express_ref linkend="Layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.Padstack_definition"/> 
	that contribute to the <b>Footprint_definition</b> shall have the same location.
	The fact that a <b>Footprint_definition</b> is a for a bottom arrangement affects only the vertical stackup, that is what 
	<express_ref linkend="Layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.Padstack_definition">Padstack_definitions</express_ref> 
	contribute to the <b>Footprint_definition</b>.  The horizontal geometrical arrangement of the <b>padstack</b>s is identical for the top and bottom arrangements.
	The location of terminals that mate with the footprint in the case of a  
	<express_ref linkend="Package:arm:Package_arm.Package"/> shall be consistent with the
	location of the elements of the footprint definition. The location of terminals of a printed part template
	shall be consistent with the location of the elements of the footprint definition.
	

	<note number="1">
		Mirroring of the geometrical arrangement of the <b>padstack</b>s in the footprint is accomplished using the mirror function in the 
		<express_ref linkend="Physical_unit_2d_design_view:arm:Physical_unit_2d_design_view_arm.Component_2d_location"/> 
		that locates the occurrence of the <b>Footprint_definition</b> in the design.
	</note>
	<note number="2">
		It is expected that Vias and  
		<express_ref linkend="Land:arm:Land_arm.Via_dependent_land"/> 
		and  
		<express_ref linkend="Land:arm:Land_arm.Land"/> 
		will be used to support the reference layout, as well as copper <b>shape</b>s to suport getting from the mating contact to the breakout terminal.
		In the case of top surface breakout patterns, the join terminals of the reference layout which are typically Via terminals provide the capability to define the standard connection to the pattern.  
		It is the usual case that there is only layout metal on the top layer.
	</note>
	
	<note number="3">
		It is not required or expected that the <b>padstack_reference_implementation</b> will be the same member of  
		<express_ref linkend="Layered_interconnect_simple_template:arm:Layered_interconnect_simple_template_arm.Reference_packaged_part_interconnect_implementation"/> 
		that is the assembly for the <b>reference_breakout</b>. 
		The purpose of the <b>padstack_reference_implementation</b> is to facilitate the rule requiring a consistent 
		<express_ref linkend="Layered_interconnect_simple_template:arm:Layered_interconnect_simple_template_arm.Reference_packaged_part_interconnect_implementation"/> 
		among all the 
		<express_ref linkend="Layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.Padstack_definition"/> 
		that contribute to the <b>Footprint_definition</b>.
	</note>
	
	<note number="3">
		While it is the case that in general a DERIVEd attribute may not be return a result, in the case of <b>footprint_location</b> and <b>padstack_reference_implementation</b>, 
		it is required that they be calculable since the attributes they reference are mandatory.
	</note>
	<note number="4">
		A  <express_ref linkend="Printed_physical_layout_template:arm:Printed_physical_layout_template_arm.Printed_part_template"/> 
		may be referenced by a <b>Footprint_definition</b> in the case where the <b>Footprint_definition</b> is providing material stackup and layout information for the 
		<express_ref linkend="Printed_physical_layout_template:arm:Printed_physical_layout_template_arm.Printed_part_template"/>.
	</note>
	<note number="5">
		The primary use for the reference to  
		<express_ref linkend="Printed_physical_layout_template:arm:Printed_physical_layout_template_arm.Printed_part_template"/> 
		is for support of edge connectors.
	</note>

	<example>
		In the case of an edge connector definition, the complete definition when footprint definition is used would consist of a
		<express_ref linkend="Printed_physical_layout_template:arm:Printed_physical_layout_template_arm.Printed_connector_template"/> 
		in association with a <b>Footprint_definition</b>. The <b>Footprint_definition</b> provides stackup information and the connector template provides
		terminal location and identification.  The identification that a <b>Footprint_definition</b> is used will be accomplished by a reference from a 
		<express_ref linkend="Layered_interconnect_module_with_printed_component_design:arm:Layered_interconnect_module_with_printed_component_design_arm.Printed_connector_component"/> 
		to a  
		<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Footprint_occurrence"/> 
		which will reference the footprint definition.
	</example>
	-->
</ext_description>


<!-- reference_package -->
<ext_description linkend="Footprint_definition_arm.Footprint_definition.reference_package">
<p>
	specifies a role of the 
	<express_ref linkend="Physical_unit_usage_view:arm:Physical_unit_usage_view_arm.Part_usage_view"/> 
	for the <b>Footprint_definition</b>.
	The value of this attribute need not be specified.
</p>
</ext_description>

<!-- WR1 -->
<ext_description linkend="Footprint_definition_arm.Footprint_definition.wr:WR1">
All <express_ref linkend="part_template_shape_with_parameters:arm:Part_template_shape_with_parameters_arm.Geometric_template.shapes"/>
of <b>Footprint_definition</b> shall be of type
<express_ref linkend="footprint_definition:arm:Footprint_definition_arm.Footprint_definition_shape_model"/>.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Footprint_definition_arm.Footprint_definition.wr:WR2">
	The <b>Footprint_definition</b> shall not be composed of other <b>Footprint_definitions</b>.
</ext_description>

<!-- +++++++++++ Footprint_definition_shape_model +++++++++++ -->
<ext_description linkend="Footprint_definition_arm.Footprint_definition_shape_model">
	A <b>Footprint_definition_shape_model</b> is a type of 
	<express_ref linkend="Layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.Structured_template_planar_shape_model"/>. 
	A <b>Footprint_definition_shape_model</b> is the planar shape of a 
	<express_ref linkend="Footprint_definition:arm:Footprint_definition_arm.Footprint_definition"/>.
	A <b>Footprint_definition_shape_model</b> may reference either a shape of a 
	<express_ref linkend="Part_view_definition:arm:Part_view_definition_arm.Part_view_definition"/> 
	or a shape of a 
	<express_ref linkend="Printed_physical_layout_template:arm:Printed_physical_layout_template_arm.Printed_part_template"/> 
	as a causal shape, with which the <b>Footprint_definition_shape_model</b> is to be compatible.
</ext_description>

<!-- reference_shape -->
<ext_description linkend="Footprint_definition_arm.Footprint_definition_shape_model.reference_shape">
	specifies the 
	<express_ref linkend="Shape_parameters:arm:Shape_parameters_arm.physical_unit_shape_model_select"/> 
	for the <b>Footprint_definition_shape_model</b>.
	The value of this attribute need not be specified.
</ext_description>

<!-- shape_characterized_definition -->
<ext_description linkend="Footprint_definition_arm.Footprint_definition_shape_model.shape_characterized_definition">
	specifies a role of the 
	<express_ref linkend="Footprint_definition:arm:Footprint_definition_arm.Footprint_definition"/> 
	for the <b>Footprint_definition_shape_model</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Footprint_definition_arm.Footprint_definition_shape_model.wr:WR1">
	If the <b>reference_shape</b> is a 
	<express_ref linkend="Physical_unit_3d_shape:arm:Physical_unit_3d_shape_arm.Physical_unit_3d_shape_model"/> 
	or is a 
	<express_ref linkend="Physical_unit_2d_shape:arm:Physical_unit_2d_shape_arm.Physical_unit_planar_shape_model"/> 
	and if the
	<express_ref linkend="Physical_unit_shape_with_parameters:arm:Physical_unit_shape_with_parameters_arm.Physical_unit_shape_model.shape_characterized_definition"/> 
	attribute of <b>reference_shape</b> references a
	<express_ref linkend="Physical_unit_usage_view:arm:Physical_unit_usage_view_arm.Part_usage_view"/> 
	and if <b>shape_characterized_definition</b> is a
	<express_ref linkend="Footprint_definition:arm:Footprint_definition_arm.Footprint_definition"/>, 
	then that 
	<express_ref linkend="Physical_unit_usage_view:arm:Physical_unit_usage_view_arm.Part_usage_view"/> 
	shall also be referenced by the
	<express_ref linkend="Footprint_definition:arm:Footprint_definition_arm.Footprint_definition.reference_package"/> 
	attribute of the <b>shape_characterized_definition</b>.	
</ext_description>

<!-- WR2 -->
<ext_description linkend="Footprint_definition_arm.Footprint_definition_shape_model.wr:WR2">
	If the <b>reference_shape</b> is a 
	<express_ref linkend="Physical_unit_3d_shape:arm:Physical_unit_3d_shape_arm.Physical_unit_3d_shape_model"/> or a  
	<express_ref linkend="Physical_unit_2d_shape:arm:Physical_unit_2d_shape_arm.Physical_unit_planar_shape_model"/>, 
	then only a  <express_ref linkend="Physical_unit_usage_view:arm:Physical_unit_usage_view_arm.Part_usage_view"/> 
	shall be referenced by the <b>reference_shape</b>.
</ext_description>

<!-- WR3 -->
<ext_description linkend="Footprint_definition_arm.Footprint_definition_shape_model.wr:WR3">
	If the <b>reference_shape</b> is a 
	<express_ref linkend="Part_template_3d_shape:arm:Part_template_3d_shape_arm.Part_template_3d_shape_model"/>, 
	then only a  
	<express_ref linkend="Printed_physical_layout_template:arm:Printed_physical_layout_template_arm.Printed_part_template"/> 
	shall be referenced by the <b>reference_shape</b>.
</ext_description>

<!-- WR4 -->
<ext_description linkend="Footprint_definition_arm.Footprint_definition_shape_model.wr:WR4">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.description"/>
	shall not be populated.
</ext_description>

<!-- +++++++++++ Generic_footprint_definition +++++++++++ -->
<ext_description linkend="Footprint_definition_arm.Generic_footprint_definition">
	A <b>Generic_footprint_definition</b> is a type of 
	<express_ref linkend="Layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.Multi_stratum_structured_template"/>.
	A <b>Generic_footprint_definition</b> is either a 
	<express_ref linkend="Footprint_definition:arm:Footprint_definition_arm.Footprint_definition"/>
	or a 
	<express_ref linkend="Footprint_definition:arm:Footprint_definition_arm.Breakout_footprint_definition"/>.
</ext_description>

<!-- padstack_based_location -->
<ext_description linkend="Footprint_definition_arm.Generic_footprint_definition.padstack_based_location">
	specifies the 
	<express_ref linkend="Layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.template_arrangement"/> 
	that is evaluated using function 
	<express_ref linkend="Footprint_definition:arm:Footprint_definition_arm.get_template_arrangement"/> 
	with <b>padstacks</b> as an argument.
</ext_description>

<!-- padstacks -->
<ext_description linkend="Footprint_definition_arm.Generic_footprint_definition.padstacks">
<p>
	specifies the role of 
	<express_ref linkend="Layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.Template_location_in_structured_template"/> 
	for the <b>Generic_footprint_definition</b>. 
	There shall be at least one 
	<express_ref linkend="Layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.Template_location_in_structured_template"/> 
	provided.
</p>
</ext_description>

<!-- single_stratum_components -->
<ext_description linkend="Footprint_definition_arm.Generic_footprint_definition.single_stratum_components">
	specifies the role of the 
	<express_ref linkend="Layered_interconnect_simple_template:arm:Layered_interconnect_simple_template_arm.Single_stratum_template"/>
	for the <b>Generic_footprint_definition</b>. 
	The 
	<express_ref linkend="Layered_interconnect_simple_template:arm:Layered_interconnect_simple_template_arm.Single_stratum_template">Single_stratum_templates</express_ref> 
	are optional and need not be provided for the <b>Generic_footprint_definition</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Footprint_definition_arm.Generic_footprint_definition.wr:WR1">
The <express_ref linkend="layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.Multi_stratum_structured_template.location"/>
of <express_ref linkend="layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.Multi_stratum_structured_template">Multi_stratum_structured_templates</express_ref>
composing <b>Generic_footprint_definition</b> shall be either <express_ref linkend="layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.template_arrangement.symmetrical"/>
or same as one of <b>Generic_footprint_definition</b>.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Footprint_definition_arm.Generic_footprint_definition.wr:WR2">
The 
<express_ref linkend="layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.Multi_stratum_structured_template.location"/> 
shall be <express_ref linkend="layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.template_arrangement.top"/> if and only if the padstack location is <express_ref linkend="layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.template_arrangement.top"/>.
</ext_description>

<!-- WR3 -->
<ext_description linkend="Footprint_definition_arm.Generic_footprint_definition.wr:WR3">
The 
<express_ref linkend="layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.Multi_stratum_structured_template.location"/> 
shall be <express_ref linkend="layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.template_arrangement.bottom"/> if and only if the padstack location is <express_ref linkend="layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.template_arrangement.bottom"/>.
</ext_description>

<!-- WR4 -->
<ext_description linkend="Footprint_definition_arm.Generic_footprint_definition.wr:WR4">
The 
<express_ref linkend="layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.Multi_stratum_structured_template.location"/>
shall be <express_ref linkend="layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.template_arrangement.symmetrical"/> if and only if the padstack location is <express_ref linkend="layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.template_arrangement.symmetrical"/>.
</ext_description>

<!-- WR5 -->
<ext_description linkend="Footprint_definition_arm.Generic_footprint_definition.wr:WR5">
	The 
	<express_ref linkend="layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.Multi_stratum_structured_template.location"/> 
	shall be 
	<express_ref linkend="layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.template_arrangement.swappable"/> 
	if and only if the padstack location is 
	<express_ref linkend="layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.template_arrangement.swappable"/>.
</ext_description>

<!-- WR6 -->
<ext_description linkend="Footprint_definition_arm.Generic_footprint_definition.wr:WR6">
	If there are no <b>single_stratum_components</b>, then the 
	<express_ref linkend="layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.Multi_stratum_structured_template.location"/> 
	inherited from the 
	<express_ref linkend="layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.Multi_stratum_structured_template"/> 
	shall be equal to the calculated 
	<b>padstack_based_location</b>. 
	
</ext_description>


<!-- +++++++++++ Part_feature_based_template_location +++++++++++ -->
<ext_description linkend="Footprint_definition_arm.Part_feature_based_template_location">
	A <b>Part_feature_based_template_location</b> is a type of
	<express_ref linkend="Layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.Template_location_in_structured_template"/>.
</ext_description>

<!-- all_reference_feature -->
<ext_description linkend="Footprint_definition_arm.Part_feature_based_template_location.all_reference_feature">
	specifies a role of the
	<express_ref linkend="Physical_unit_usage_view:arm:Physical_unit_usage_view_arm.Part_feature"/>
	for the <b>Part_feature_based_template_location</b>.
	There shall exist more than one
	<express_ref linkend="Physical_unit_usage_view:arm:Physical_unit_usage_view_arm.Part_feature"/>
	for the <b>Part_feature_based_template_location</b>.
</ext_description>

<!-- reference_feature -->
<ext_description linkend="Footprint_definition_arm.Part_feature_based_template_location.reference_feature">
	specifies a role of the
	<express_ref linkend="Physical_unit_usage_view:arm:Physical_unit_usage_view_arm.Part_feature"/>
	for the <b>Part_feature_based_template_location</b>.
</ext_description>

<!-- reference_designation -->
<ext_description linkend="Footprint_definition_arm.Part_feature_based_template_location.reference_designation">
	an attribute inherited from the 
	<express_ref linkend="Layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.Template_location_in_structured_template"/>
	shall be redeclared as a string 
	for the <b>Part_feature_based_template_location</b>.
</ext_description>

