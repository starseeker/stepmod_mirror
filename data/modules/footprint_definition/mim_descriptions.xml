<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: breakout_footprint_definition
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Footprint_definition_mim.breakout_footprint_definition">
	A <b>breakout_footprint_definition</b> is a type of
	<express_ref linkend="Footprint_definition:mim:Footprint_definition_mim.generic_footprint_definition"/>
	that implements the ARM concepts of
	<express_ref linkend="Footprint_definition:arm:Footprint_definition_arm.Breakout_footprint_definition"/>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: footprint_definition
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Footprint_definition_mim.footprint_definition">
<p>
	A <b>footprint_definition</b> is a type of 
	<express_ref linkend="Footprint_definition:mim:Footprint_definition_mim.generic_footprint_definition"/> 
	that implements the ARM concepts of
	<express_ref linkend="Footprint_definition:arm:Footprint_definition_arm.Footprint_definition"/>.
</p></ext_description>


<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: generic_footprint_definition
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Footprint_definition_mim.generic_footprint_definition">
	A <b>generic_footprint_definition</b> is a type of 
	<express_ref linkend="Layered_interconnect_complex_template:mim:Layered_interconnect_complex_template_mim.multi_stratum_structured_template"/>
	that implements the ARM concepts of
	<express_ref linkend="Footprint_definition:arm:Footprint_definition_arm.Generic_footprint_definition"/>.
	A <b>generic_footprint_definition</b> is one of a
	<express_ref linkend="Footprint_definition:mim:Footprint_definition_mim.footprint_definition"/>
	or a
	<express_ref linkend="Footprint_definition:mim:Footprint_definition_mim.breakout_footprint_definition"/>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Constraint: multi_stratum_structured_template_subtypes
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Footprint_definition_mim.multi_stratum_structured_template_subtypes">
	A <b>multi_stratum_structured_template_subtypes</b> constraint specifies a constraint that applies to instances of subtypes of
	<express_ref linkend="Layered_interconnect_complex_template:mim:Layered_interconnect_complex_template_mim.multi_stratum_structured_template"/>.
</ext_description>

