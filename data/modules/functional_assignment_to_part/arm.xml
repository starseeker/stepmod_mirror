<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:08" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="functional_assignment_to_part_arm schema_instance"/>
   <schema name="FUNCTIONAL_ASSIGNMENT_TO_PART_ARM">
      <interface kind="use" schema="NETWORK_FUNCTIONAL_USAGE_VIEW_ARM"/>
      <interface kind="use" schema="PHYSICAL_UNIT_USAGE_VIEW_ARM"/>
      <entity name="functional_usage_view_to_part_terminal_assignment">
         <explicit name="physical_usage_view_terminal">
            <typename name="part_terminal"/>
         </explicit>
         <explicit name="functional_usage_view_terminal">
            <typename name="scalar_terminal_definition"/>
         </explicit>
         <unique label="ur1">
            <unique.attribute entity-ref="functional_usage_view_to_part_terminal_assignment" attribute="functional_usage_view_terminal"/>
            <unique.attribute entity-ref="functional_usage_view_to_part_terminal_assignment" attribute="physical_usage_view_terminal"/>
         </unique>
      </entity>
      <entity name="part_connected_terminals_definition">
         <explicit name="name">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="associated_definition">
            <typename name="part_usage_view"/>
         </explicit>
         <explicit name="connected_terminals">
            <aggregate type="SET" lower="2" upper="?"/>
            <typename name="part_terminal"/>
         </explicit>
         <unique label="ur1">
            <unique.attribute entity-ref="part_connected_terminals_definition" attribute="name"/>
            <unique.attribute entity-ref="part_connected_terminals_definition" attribute="associated_definition"/>
         </unique>
         <where label="wr1" expression=" sizeof ( query ( ct &lt;* connected_terminals | not ( associated_definition = ct \ shape_element . containing_shape ) ) ) = 0 "/>
      </entity>
      <entity name="part_terminal" abstract.supertype="YES" supertypes="part_feature">
         <inverse name="connection_requirement" entity="part_connected_terminals_definition" attribute="connected_terminals">
            <inverse.aggregate type="SET" lower="0" upper="1"/>
         </inverse>
         <inverse name="functional_usage_view_terminal" entity="functional_usage_view_to_part_terminal_assignment" attribute="physical_usage_view_terminal">
            <inverse.aggregate type="SET" lower="0" upper="1"/>
         </inverse>
      </entity>
   </schema>
</express>
