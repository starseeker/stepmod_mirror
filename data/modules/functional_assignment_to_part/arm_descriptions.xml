  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Functional_assignment_to_part_arm">
</ext_description>

<!-- +++++++++++ fatp_part_connected_terminals_definition_domain_enumeration +++++++++++ -->
<ext_description linkend="Functional_assignment_to_part_arm.fatp_part_connected_terminals_definition_domain_enumeration">
	A <b>fatp_part_connected_terminals_definition_domain_enumeration</b> lists the values of domain types for a
	<express_ref linkend="functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_terminal"/>
	or a
	<express_ref linkend="cable:arm:Cable_arm.Wire_identification"/>.
</ext_description>

<ext_description linkend="Functional_assignment_to_part_arm.fatp_part_connected_terminals_definition_domain_enumeration.electrical">
	specifies the domain type is electrical;
</ext_description>

<ext_description linkend="Functional_assignment_to_part_arm.fatp_part_connected_terminals_definition_domain_enumeration.thermal">
	specifies the domain type is thermal;
</ext_description>

<ext_description linkend="Functional_assignment_to_part_arm.fatp_part_connected_terminals_definition_domain_enumeration.optical">
	specifies the domain type is optical;
</ext_description>

<ext_description linkend="Functional_assignment_to_part_arm.fatp_part_connected_terminals_definition_domain_enumeration.magnetic">
	specifies the domain type is magnetic.
</ext_description>

<!-- +++++++++++ interface_or_join_terminal_enumeration  +++++++++++ -->
<ext_description linkend="Functional_assignment_to_part_arm.interface_or_join_terminal_enumeration">
	A <b>interface_or_join_terminal_enumeration</b> defines the possible connection types for a
	<express_ref linkend="functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_terminal"/>.
</ext_description>

<ext_description linkend="Functional_assignment_to_part_arm.interface_or_join_terminal_enumeration.join_terminal">
	specifies the connection type is to join at the same immediate assembly level.
</ext_description>

<ext_description linkend="Functional_assignment_to_part_arm.interface_or_join_terminal_enumeration.interface_terminal">
	specifies the connection type is to interface to the next higher assembly level.
</ext_description>

<ext_description linkend="Functional_assignment_to_part_arm.terminal_joint_type">
	The <b>terminal_joint_type</b> provides pre-defined
	<express_ref linkend="functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_terminal.joint_type"/>s 
	for 
	<express_ref linkend="functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_terminal"/>s.
</ext_description>

<!-- +++++++++++ Part_terminal +++++++++++ -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_terminal">
	A <b>Part_terminal</b> is a type of 
	<express_ref linkend="Physical_unit_usage_view:arm:Physical_unit_usage_view_arm.Part_feature"/>.  
</ext_description>

<ext_description linkend="Functional_assignment_to_part_arm.Part_terminal.domain_type">
	the text that provides further information about terminal type for the <b>Part_terminal</b>. 
	The value of this attribute need not be specified.
</ext_description>

<ext_description linkend="Functional_assignment_to_part_arm.Part_terminal.interface_or_join_terminal">
	specifies on whether the <b>Part_terminal</b> is intended to join at the immediate assembly level 
	or on whether it is intended to interface to the next higher assembly level.
</ext_description>

<ext_description linkend="Functional_assignment_to_part_arm.Part_terminal.functional_usage_view_terminal">
	specifies an inverse relationship that specifies that the existence of the  <b>Part_terminal</b> 
	is dependent on the existence of the 
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Functional_usage_view_to_part_terminal_assignment"/> 
	that specifies the <b>Part_terminal</b>
	as its 
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Functional_usage_view_to_part_terminal_assignment.physical_usage_view_terminal"/>. 
	There shall be no more than one  
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Functional_usage_view_to_part_terminal_assignment"/>
	for a particular <b>Part_terminal</b>.
</ext_description>

<ext_description linkend="Functional_assignment_to_part_arm.Part_terminal.joint_type">
	specifies the intended kind of joint of an occurrence of this <b>Part_terminal</b> with another <b>Part_terminal</b> by an
	<express_ref linkend="assembly_technology:arm:Assembly_technology_arm.Assembly_joint"/>.
	The value of this attribute need not be specified.
</ext_description>

<!-- connection_requirement -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_terminal.connection_requirement">
	specifies an inverse relationship that specifies that the existence of the  <b>Part_terminal</b> 
	is dependent on the existence of the 
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_definition"/> 
	that specifies the <b>Part_terminal</b>
	as its 
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_definition.connected_terminals"/>. 
	There shall be no more than one  
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_definition"/>
	for a particular <b>Part_terminal</b>.
</ext_description>
<!-- +++++++++++ Part_connected_terminals_definition +++++++++++ -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_definition"><p>

 A <b>Part_connected_terminals_definition</b> is the association between two or more <express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_terminal"/> that are connected internally to the part.  This object serves as a formal requirement statement for connections implemented by a product.  It is a topological abstraction in that it does not provide any information stating the variation of properties with respect to product or environmental variability.
</p><note>
This object is included to support the case where multiple terminals of a part are connected internally (e.g., VCC, Ground) to facilitate mapping between the physical terminal definition and the functional terminal definition.
</note>
</ext_description>
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_definition.name">
the words by which the <b>Part_connected_terminals_definition</b> is known.
</ext_description>
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_definition.connected_terminals">
specifies a role of the <express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_terminal"/> for the <b>Part_connected_terminals_definition</b>. 
There shall be two or more  <express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_terminal"/> for a <b>Part_connected_terminals_definition</b>.
</ext_description>
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_definition.associated_definition">
specifies the <express_ref linkend="Physical_unit_usage_view:arm:Physical_unit_usage_view_arm.Part_usage_view"/> that is the product definition for the 
<b>Part_connected_terminals_definition</b>.
</ext_description>

<!-- domain -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_definition.domain">
	specifies an inverse relationship that specifies that the existence of the <b>Part_connected_terminals_definition</b> 
	is dependent on the existence of the 
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_definition_domain"/> 
	that specifies the <b>Part_connected_terminals_definition</b> as its 
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_definition_domain.associated_definition"/>. 
	There shall be no more than one 
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_definition_domain"/> 
	for a particular <b>Part_connected_terminals_definition</b>.
</ext_description>

<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_definition.wr:WR1">
All terminals specified by the <b>connected_terminals</b> shall be on the <b>associated_definition</b>.
</ext_description>
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_definition.wr:WR2">
All terminals specified by the member of this object shall be mapped to the same terminal of the functional 
definition.
</ext_description>
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_definition.ur:UR1">
The combination of <b>name</b> and <b>associated_definition</b> shall be unique within a population of <b>Part_connected_terminals_definition</b>s.
</ext_description>

<!-- +++++++++++ Part_connected_terminals_definition_domain +++++++++++ -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_definition_domain">
	A <b>Part_connected_terminals_definition_domain</b> is is the application of a domain name to a
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_definition"/>
	to support cases where domains other than pure electrical are required.
	If a <b>Part_connected_terminals_definition_domain</b> is not applied to a
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_definition"/>,
	the
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_definition.domain"/>
	of the
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_definition"/>
	shall be interpreted to be electrical.
</ext_description>

<!-- domain_type -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_definition_domain.domain_type">
	specifies the role of the
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.fatp_part_connected_terminals_definition_domain_enumeration"/>
	for the <b>Part_connected_terminals_definition_domain</b>.
</ext_description>

<!-- associated_definition -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_definition_domain.associated_definition">
	specifies the role of the
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_definition"/>
	for the <b>Part_connected_terminals_definition_domain</b>.
</ext_description>

<!-- +++++++++++ Part_connected_terminals_element +++++++++++ -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_element">
	A <b>Part_connected_terminals_element</b> is a the object that describes a link between
	two nodes for the graph representation. 
	A node may be referenced by more than one link.
	A node may be either a  terminal or a topological junction.
</ext_description>

<!-- name -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_element.name">
	the words by which the <b>Part_connected_terminals_element</b> is known.
</ext_description>

<!-- start_terminus -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_element.start_terminus">
	specifies a role of the
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.fatp_termination_or_junction"/>
	for the <b>Part_connected_terminals_element</b>.
</ext_description>

<!-- end_terminus -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_element.end_terminus">
	specifies a role of the
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.fatp_termination_or_junction"/>
	for the <b>Part_connected_terminals_element</b>.
</ext_description>

<!-- connectivity_context -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_element.connectivity_context">
	specifies a role of the
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_definition"/>
	for the <b>Part_connected_terminals_element</b>.
</ext_description>

<!-- UR1 -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_element.ur:UR1">
	The combination of <b>name</b> and <b>connectivity_context</b> shall be unique within a population of 
	<b>Part_connected_terminals_element</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_element.wr:WR1">
	The <b>start_terminus</b> shall not be equal to the <b>end_terminus</b>.
</ext_description>

<!-- +++++++++++ Part_connected_terminals_layout_topology_requirement_assignment +++++++++++ -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_layout_topology_requirement_assignment">
	A <b>Part_connected_terminals_layout_topology_requirement_assignment</b> is a type of
	<express_ref linkend="Requirement_assignment:arm:Requirement_assignment_arm.Requirement_assignment"/>.
</ext_description>

<!-- assigned_requirement -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_layout_topology_requirement_assignment.assigned_requirement">
	an attribute inherited from the
	<express_ref linkend="Requirement_assignment:arm:Requirement_assignment_arm.Requirement_assignment"/>
	shall be redeclared as the
	<express_ref linkend="physical_connectivity_layout_topology_requirement:arm:Physical_connectivity_layout_topology_requirement_arm.Physical_connectivity_layout_topology_requirement"/>
	for the <b>Part_connected_terminals_layout_topology_requirement_assignment</b>.
</ext_description>

<!-- assigned_to -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_layout_topology_requirement_assignment.assigned_to">
	an attribute inherited from the
	<express_ref linkend="Requirement_assignment:arm:Requirement_assignment_arm.Requirement_assignment"/>
	shall be redeclared as the
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_definition"/>
	for the <b>Part_connected_terminals_layout_topology_requirement_assignment</b>.
</ext_description>

<!-- +++++++++++ Part_connected_terminals_structure_definition +++++++++++ -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_structure_definition">
	A <b>Part_connected_terminals_structure_definition</b> is a type of
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_definition"/>
	that has a structure.
	Structure included supports assignment of path based properties to a 
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_definition"/> 
	for routing or validation purposes. 
	The structure is a graph and may be a tree if there are no redundant arcs, but that is not a requirement. 
	The arcs in the graph are the 
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_element"/> 
	referenced by the <b>structural_element</b> attribute. 
	The nodes in the graph may be either a terminal or a topological junction. 
</ext_description>

<!-- structural_terminal_nodes -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_structure_definition.structural_terminal_nodes">
	specifies a role of the
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_terminal"/>
	for the <b>Part_connected_terminals_structure_definition</b>.
	There shall exist zero or more
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_terminal">Part_terminals</express_ref>
	for the <b>Part_connected_terminals_structure_definition</b>.
</ext_description>

<!-- structural_junction_nodes -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_structure_definition.structural_junction_nodes">
	specifies a role of the
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_topological_junction"/>
	for the <b>Part_connected_terminals_structure_definition</b>.
	There shall exist zero or more
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_topological_junction">Part_connected_terminals_topological_junctions</express_ref>
	for the <b>Part_connected_terminals_structure_definition</b>.
</ext_description>

<!-- tree_structure -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_structure_definition.tree_structure">
	specifies a LOGICAL value.
</ext_description>

<!-- structural_element -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_structure_definition.structural_element">
	specifies an inverse relationship that specifies that the existence of the <b>Part_connected_terminals_structure_definition</b> 
	is dependent on the existence of the 
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_element"/> 
	that specifies the <b>Part_connected_terminals_structure_definition</b> as its 
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_element.connectivity_context"/>. 
	There shall be no more than one 
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_element"/> 
	for a particular <b>Part_connected_terminals_structure_definition</b>.
</ext_description>

<!--  WR1-->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_structure_definition.wr:WR1">
	<p>
		Each member of <b>structural_terminal_nodes</b> shall be in the set of
		<express_ref linkend="Physical_connectivity_definition:arm:Physical_connectivity_definition_arm.Physical_connectivity_definition.associated_terminals"/> 
		inherited from P
		<express_ref linkend="Physical_connectivity_definition:arm:Physical_connectivity_definition_arm.Physical_connectivity_definition"/>. 	
	</p>
</ext_description>

<!-- WR2 -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_structure_definition.wr:WR2">
	<p>
		The size of the set of <b>structural_terminal_nodes</b> shall be the size of the set of 
		<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_definition.connected_terminals"/> 
		inherited from 
		<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_definition"/>. 
	</p>
</ext_description>

<!-- +++++++++++ Part_connected_terminals_topological_junction +++++++++++ -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_topological_junction">
	A <b>Part_connected_terminals_topological_junction</b> is used when at least three
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_terminal"/> 
	are connected and the explicit topology must be stated.
</ext_description>

<!-- name -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_topological_junction.name">
	the words by which the <b>Part_connected_terminals_topological_junction</b> is known.
</ext_description>

<!-- scope -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_topological_junction.scope">
	specifies a role of the
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_structure_definition"/>
	for the <b>Part_connected_terminals_topological_junction</b>.
</ext_description>

<!-- UR1 -->
<ext_description linkend="Functional_assignment_to_part_arm.Part_connected_terminals_topological_junction.ur:UR1">
	The combination of <b>name</b> and <b>scope</b> shall be unique within a population of 
	<b>Part_connected_terminals_topological_junction</b>.
</ext_description>


<!-- +++++++++++ Functional_usage_view_to_part_terminal_assignment +++++++++++ -->
<ext_description linkend="Functional_assignment_to_part_arm.Functional_usage_view_to_part_terminal_assignment">

 A 
<b>Functional_usage_view_to_part_terminal_assignment</b> is the association between a <express_ref linkend="Functional_usage_view:arm:Functional_usage_view_arm.Scalar_terminal_definition"/>
and a <express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_terminal"/>. This is the fundamental mechanism for mapping between 
the functional view and the physical view of a design or library item.
</ext_description>
<ext_description linkend="Functional_assignment_to_part_arm.Functional_usage_view_to_part_terminal_assignment.functional_usage_view_terminal">
specifies a role of the <express_ref linkend="Functional_usage_view:arm:Functional_usage_view_arm.Scalar_terminal_definition"/> for the <b>Functional_usage_view_to_part_terminal_assignment</b>.
</ext_description>
<ext_description linkend="Functional_assignment_to_part_arm.Functional_usage_view_to_part_terminal_assignment.physical_usage_view_terminal">
specifies a role of the <express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_terminal"/> for the <b>Functional_usage_view_to_part_terminal_assignment</b>.
</ext_description>
<ext_description linkend="Functional_assignment_to_part_arm.Functional_usage_view_to_part_terminal_assignment.ur:UR1">
The combination of <b>functional_usage_view_terminal</b> and <b>physical_usage_view_terminal</b> shall be unique within a population of <b>Functional_usage_view_to_part_terminal_assignment</b>.
</ext_description>

<!-- +++++++++++ fatp_get_pt +++++++++++ -->
<ext_description linkend="Functional_assignment_to_part_arm.fatp_get_pt">
	The <b>fatp_get_pt</b> function returns the set of 
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_terminal"/> 
	referenced by the set of 
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_element"/> 
	input to the function.
</ext_description>

<!-- input -->
<ext_description linkend="Functional_assignment_to_part_arm.fatp_get_pt.input">
	the specified set of
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_element">Part_connected_terminals_elements</express_ref>.
</ext_description>

<!-- +++++++++++ fatp_get_tj +++++++++++ -->
<ext_description linkend="Functional_assignment_to_part_arm.fatp_get_tj">
	The <b>fatp_get_tj</b> function returns the set of
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_topological_junction"/> 
	referenced by the set of
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_element"/> 
	input to the function.
</ext_description>

<!-- input -->
<ext_description linkend="Functional_assignment_to_part_arm.fatp_get_tj.input">
	the specified set of
	<express_ref linkend="Functional_assignment_to_part:arm:Functional_assignment_to_part_arm.Part_connected_terminals_element">Part_connected_terminals_elements</express_ref>.
</ext_description>

</ext_descriptions>
