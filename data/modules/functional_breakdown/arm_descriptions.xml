<ext_descriptions module_directory="functional_breakdown" schema_file="arm.xml" describe.selects="YES" rcs.date="$Date: 2002/12/12 14:17:56 $" rcs.revision="$Revision: 1.2 $">
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: Functional_breakdown_arm
   <express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<ext_description linkend="Functional_breakdown_arm"/>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Functional_breakdown_arm  Entity: Functional_breakdown  
   <express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_breakdown"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Functional_breakdown_arm.Functional_breakdown">
		<p>
		    A <b>Functional_breakdown</b> is a type of 
		  <express_ref linkend="product_breakdown:arm:Product_breakdown_arm.Breakdown"/>
		  that identifies a partitioning of a product into a set of related functional
		  elements so as to form explicit, parent-child views that comprise the
		  product elements.  
  		</p>
		<p>
		    The parent-child view is represented by 
		    <express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_element_usage"/>
		    instances relating the functional elements in the breakdown which are represented by  
		    <express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_element"/>s.
		</p>
		<example>
    			A functional breakdown provides a decomposition of an aircraft in terms
    			of high-level functional processes such as flight, taxiing and at rest all
    			the way down to low-level processes such as detect onboard fuel level, move
   			 tail rudder and provide standard tow attachment point. 
  		</example>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Functional_breakdown_arm  Entity: Functional_breakdown_context  
   <express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_breakdown_context"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Functional_breakdown_arm.Functional_breakdown_context">
		<p>
			A 
			<b>
				Functional_breakdown_context
			</b> 
			identifies the membership relationship between a 
			<express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_element"/> 
			and a 
			<express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_breakdown"/> 
			of which the functional element is a member.
		</p>
		<example>
			A function 'provide load lifting capability' is member of the functional breakdown of a helicopter.
		</example>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Functional_breakdown_arm  Entity: Functional_breakdown_context  Attribute: breakdown 
   <express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_breakdown_context.breakdown"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Functional_breakdown_arm.Functional_breakdown_context.breakdown">
		the version of the Functional_breakdown of which the Functional_breakdown_element is a member.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Functional_breakdown_arm  Entity: Functional_breakdown_context  Attribute: breakdown_element 
   <express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_breakdown_context.breakdown_element"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Functional_breakdown_arm.Functional_breakdown_context.breakdown_element">
		the view of the version of the Functional_breakdown_element that is a member of the Functional_breakdown.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Functional_breakdown_arm  Entity: Functional_breakdown_version  
   <express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_breakdown_version"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Functional_breakdown_arm.Functional_breakdown_version">
		<p>
			A 
			<b>Functional_breakdown_version</b> 
			is a type of 
			<express_ref linkend="product_breakdown:arm:Product_breakdown_arm.Breakdown_version"/> 
			that identifies a version of a 
			<express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_breakdown"/>
			.
		</p>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Functional_breakdown_arm  Entity: Functional_breakdown_version  Attribute: of_product 
   <express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_breakdown_version.of_product"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Functional_breakdown_arm.Functional_breakdown_version.of_product">
		the Functional_breakdown_version shall only be a version of a 
		<express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_breakdown"/>
		.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Functional_breakdown_arm  Entity: Functional_element  
   <express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_element"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Functional_breakdown_arm.Functional_element">
		<p>
			A 
			<b>
				Functional_element
			</b> 
			is a type of 
			<express_ref linkend="product_breakdown:arm:Product_breakdown_arm.Breakdown_element"/> 
			that identifies the elements in one or more 
			<express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_breakdown"/> 
			objects.
		</p>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Functional_breakdown_arm  Entity: Functional_element_definition  
   <express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_element_definition"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Functional_breakdown_arm.Functional_element_definition">
		<p>
			A 
			<b>Functional_breakdown_element_definition</b> 
			is a type of 
			<express_ref linkend="product_breakdown:arm:Product_breakdown_arm.Breakdown_element_definition"/> 
			that identifies a view of a version (
			<express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_element_version"/>
			) of a 
			<express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_element"/>
			.
		</p>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Functional_breakdown_arm  Entity: Functional_element_definition  Attribute: defined_version 
   <express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_element_definition.defined_version"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Functional_breakdown_arm.Functional_element_definition.defined_version">
		the Functional_element_definition shall only be a view of a 
		<express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_element"/>
		.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Functional_breakdown_arm  Entity: Functional_element_usage  
   <express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_element_usage"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Functional_breakdown_arm.Functional_element_usage">
		<p>
			A 
			<b>
				Functional_element_usage
			</b> 
			identifies a relationship between a  parent and child 
			<express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_element"/>
			.
		</p>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Functional_breakdown_arm  Entity: Functional_element_usage  Attribute: child_element 
   <express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_element_usage.child_element"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Functional_breakdown_arm.Functional_element_usage.child_element">
		the child functional element of the relationship.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Functional_breakdown_arm  Entity: Functional_element_usage  Attribute: parent_element 
   <express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_element_usage.parent_element"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Functional_breakdown_arm.Functional_element_usage.parent_element">
		the parent functional element of the relationship.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Functional_breakdown_arm  Entity: Functional_element_version  
   <express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_element_version"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Functional_breakdown_arm.Functional_element_version">
		<p>
			A 
			<b>
				Functional_element_version
			</b> 
			is a type of 
			<express_ref linkend="product_breakdown:arm:Product_breakdown_arm.Breakdown_element_version"/> 
			that identifies a version of a 
			<express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_element"/>
			.
		</p>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Functional_breakdown_arm  Entity: Functional_element_version  Attribute: of_product 
   <express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_element_version.of_product"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Functional_breakdown_arm.Functional_element_version.of_product">
		the Functional_element_version shall only be a version of a 
		<express_ref linkend="functional_breakdown:arm:Functional_breakdown_arm.Functional_element"/>
		.
	</ext_description>
</ext_descriptions>
