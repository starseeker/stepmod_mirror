    source_id = 'ISO-TS-10303-1099') AND (SELF\
    Tensor_order_and_symmetry_by_library_reference.item_id IN ['TO001', 'TO002'
    , 'TO003', 'TO004', 'TO005', 'TO006', 'TO007', 'TO008', 'TO009']);
