<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:08" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="functional_decomposition_to_design_arm schema_instance"/>
   <schema name="FUNCTIONAL_DECOMPOSITION_TO_DESIGN_ARM">
      <interface kind="use" schema="CHARACTERISTIC_ARM"/>
      <interface kind="use" schema="DESIGN_MANAGEMENT_ARM"/>
      <interface kind="use" schema="NETWORK_FUNCTIONAL_DESIGN_VIEW_ARM"/>
      <interface kind="use" schema="PHYSICAL_UNIT_DESIGN_VIEW_ARM"/>
      <entity name="component_swap_specification" supertypes="change_design_object_management_relationship">
         <explicit name="current_design_object">
            <typename name="design_functional_unit_allocation_to_assembly_component"/>
            <redeclaration entity-ref="change_design_object_management_relationship"/>
         </explicit>
         <explicit name="previous_design_object">
            <typename name="design_functional_unit_allocation_to_assembly_component"/>
            <redeclaration entity-ref="change_design_object_management_relationship"/>
         </explicit>
      </entity>
      <entity name="design_composition_path">
         <explicit name="path_element">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="functional_unit"/>
         </explicit>
         <explicit name="design_functional_unit">
            <typename name="functional_unit"/>
         </explicit>
      </entity>
      <entity name="design_functional_unit_allocation_to_assembly_component">
         <explicit name="functional_design_definition_path">
            <typename name="design_composition_path"/>
         </explicit>
         <explicit name="implementation">
            <typename name="assembly_component"/>
         </explicit>
         <explicit name="global_swappable">
            <builtintype type="BOOLEAN"/>
         </explicit>
         <explicit name="local_swappable">
            <builtintype type="BOOLEAN"/>
         </explicit>
         <explicit name="of_property" optional="YES">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="coordinated_characteristic"/>
         </explicit>
         <explicit name="path_alias">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="swap_code">
            <builtintype type="STRING"/>
         </explicit>
         <unique label="ur1">
            <unique.attribute entity-ref="design_functional_unit_allocation_to_assembly_component" attribute="implementation"/>
            <unique.attribute entity-ref="design_functional_unit_allocation_to_assembly_component" attribute="path_alias"/>
         </unique>
      </entity>
      <entity name="design_functional_unit_allocation_to_reference_functional_unit">
         <explicit name="reference_definition_path">
            <typename name="reference_composition_path"/>
         </explicit>
         <explicit name="design_definition_path">
            <typename name="design_composition_path"/>
         </explicit>
      </entity>
      <entity name="gate_path_swap_specification" supertypes="change_design_object_management_relationship">
         <explicit name="current_design_object">
            <typename name="design_functional_unit_allocation_to_reference_functional_unit"/>
            <redeclaration entity-ref="change_design_object_management_relationship"/>
         </explicit>
         <explicit name="previous_design_object">
            <typename name="design_functional_unit_allocation_to_reference_functional_unit"/>
            <redeclaration entity-ref="change_design_object_management_relationship"/>
         </explicit>
      </entity>
      <entity name="reference_composition_path">
         <explicit name="path_element">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="functional_unit"/>
         </explicit>
         <explicit name="reference_functional_unit">
            <typename name="functional_unit"/>
         </explicit>
      </entity>
      <entity name="reference_functional_unit_assignment_to_part">
         <explicit name="functional_design_definition_path">
            <typename name="reference_composition_path"/>
         </explicit>
         <explicit name="implementation">
            <typename name="part_view_definition"/>
         </explicit>
         <explicit name="of_property" optional="YES">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="coordinated_characteristic"/>
         </explicit>
         <explicit name="path_alias">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="swap_code" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <unique label="ur1">
            <unique.attribute entity-ref="reference_functional_unit_assignment_to_part" attribute="implementation"/>
            <unique.attribute entity-ref="reference_functional_unit_assignment_to_part" attribute="path_alias"/>
         </unique>
      </entity>
      <entity name="terminal_swap_specification" supertypes="change_design_object_management_relationship">
         <explicit name="previous_design_object">
            <typename name="functional_unit_terminal_node_assignment"/>
            <redeclaration entity-ref="change_design_object_management_relationship"/>
         </explicit>
         <explicit name="current_design_object">
            <typename name="functional_unit_terminal_node_assignment"/>
            <redeclaration entity-ref="change_design_object_management_relationship"/>
         </explicit>
      </entity>
      <subtype.constraint name="change_design_object_management_relationship_subtypes" entity="change_design_object_management_relationship" super.expression="ONEOF (terminal_swap_specification, gate_path_swap_specification, component_swap_specification)"/>
   </schema>
</express>
