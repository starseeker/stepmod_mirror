<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:08" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="functional_decomposition_with_nodal_representation_to_packaged_mapping_arm schema_instance"/>
   <schema name="FUNCTIONAL_DECOMPOSITION_WITH_NODAL_REPRESENTATION_TO_PACKAGED_MAPPING_ARM">
      <interface kind="use" schema="FUNCTIONAL_DECOMPOSITION_TO_DESIGN_ARM"/>
      <interface kind="use" schema="PART_EXTERNAL_REFERENCE_ARM"/>
      <interface kind="use" schema="PHYSICAL_CONNECTIVITY_DEFINITION_ARM"/>
      <type name="design_composition_path_or_root">
         <select selectitems="design_composition_path functional_unit_network_definition"/>
      </type>
      <entity name="aggregate_connectivity_requirement">
         <explicit name="connectivity_requirement">
            <typename name="functional_unit_network_node_definition"/>
         </explicit>
         <explicit name="design_definition_path">
            <typename name="design_composition_path_or_root"/>
         </explicit>
         <unique label="ur1">
            <unique.attribute entity-ref="aggregate_connectivity_requirement" attribute="design_definition_path"/>
            <unique.attribute entity-ref="aggregate_connectivity_requirement" attribute="connectivity_requirement"/>
         </unique>
         <where label="wr1" expression=" not ( 'functional_decomposition_with_nodal_representation_to_packaged_mapping_arm.functional_unit_network_definition' in typeof ( design_definition_path ) ) or ( connectivity_requirement . associated_functional_unit_definition = design_definition_path ) "/>
         <where label="wr2" expression=" not ( 'functional_decomposition_with_nodal_representation_to_packaged_mapping_arm.design_composition_path' in typeof ( design_definition_path ) ) or ( connectivity_requirement . associated_functional_unit_definition = design_definition_path . design_functional_unit . definition ) "/>
      </entity>
      <entity name="connectivity_allocation">
         <explicit name="implementation">
            <typename name="physical_connectivity_definition"/>
         </explicit>
         <explicit name="connectivity_requirement">
            <typename name="aggregate_connectivity_requirement"/>
         </explicit>
         <unique label="ur1">
            <unique.attribute entity-ref="connectivity_allocation" attribute="implementation"/>
         </unique>
      </entity>
   </schema>
</express>
