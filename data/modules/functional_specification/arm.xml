<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-02-24T18:49:25" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="functional_specification_arm schema_instance"/>
   <schema name="Functional_specification_arm">
      <interface kind="use" schema="External_item_identification_assignment_arm"/>
      <interface kind="use" schema="Network_functional_design_view_arm"/>
      <interface kind="use" schema="Signal_arm"/>
      <interface kind="use" schema="Specification_document_arm"/>
      <type name="fs_external_identification_item">
         <select extensible="YES" genericentity="YES" basedon="external_identification_item" selectitems="Functional_specification"/>
      </type>
      <entity name="Functional_specification">
         <explicit name="associated_definition">
            <typename name="Functional_specification_definition"/>
         </explicit>
         <explicit name="reference_signal">
            <typename name="Signal_specification"/>
         </explicit>
         <explicit name="characterizing_functional_unit">
            <typename name="Functional_unit"/>
         </explicit>
         <explicit name="characterizing_signal">
            <typename name="Signal_specification"/>
         </explicit>
         <inverse name="characteristic_category" entity="External_source_identification" attribute="item"/>
         <where label="WR1" expression="characterizing_functional_unit.composed_network = associated_definition"/>
      </entity>
      <entity name="Functional_specification_definition" supertypes="Functional_unit_network_definition">
         <inverse name="composing_specification" entity="Functional_specification" attribute="associated_definition">
            <inverse.aggregate type="SET" lower="1" upper="?"/>
         </inverse>
      </entity>
      <entity name="Signal_specification">
         <explicit name="measurement_condition">
            <typename name="Test_specification"/>
         </explicit>
         <explicit name="measurement_access_mechanism">
            <typename name="Functional_unit_terminal"/>
         </explicit>
         <explicit name="reference_access_mechanism" optional="YES">
            <typename name="Functional_unit_terminal"/>
         </explicit>
         <explicit name="minimum_characteristic">
            <typename name="Signal_property"/>
         </explicit>
         <explicit name="maximum_characteristic">
            <typename name="Signal_property"/>
         </explicit>
         <explicit name="characterization_network">
            <typename name="Functional_unit_network_definition"/>
         </explicit>
      </entity>
   </schema>
</express>
