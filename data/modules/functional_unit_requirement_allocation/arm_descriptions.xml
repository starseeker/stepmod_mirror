<!-- +++++++++++ Functional_unit_requirement_allocation +++++++++++ -->
<ext_description linkend="Functional_unit_requirement_allocation_arm.Functional_unit_requirement_allocation">
	A <b>Functional_unit_requirement_allocation</b> is a type of 
	<express_ref linkend="Requirement_assignment:arm:Requirement_assignment_arm.Requirement_assignment"/>. 
	A <b>Functional_unit_requirement_allocation</b> is the relationship between the 
	<express_ref linkend="Requirement_decomposition:arm:Requirement_decomposition_arm.Requirement_definition_property"/> 
	and the 
	<express_ref linkend="Network_functional_design_view:arm:Network_functional_design_view_arm.Functional_unit"/> 
	that satisfies the  
	<express_ref linkend="Requirement_decomposition:arm:Requirement_decomposition_arm.Requirement_definition_property"/>.
</ext_description>

<!-- assigned_to -->
<ext_description linkend="Functional_unit_requirement_allocation_arm.Functional_unit_requirement_allocation.assigned_to">
	an attribute inherited from the  
	<express_ref linkend="Requirement_assignment:arm:Requirement_assignment_arm.Requirement_assignment"/> 
	supertype specifies the 
	<express_ref linkend="Network_functional_design_view:arm:Network_functional_design_view_arm.Functional_unit"/> 
	that satisfies the 
	<express_ref linkend="Requirement_decomposition:arm:Requirement_decomposition_arm.Requirement_definition_property"/> 
	for the <b>Functional_unit_requirement_allocation</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Functional_unit_requirement_allocation_arm.Functional_unit_requirement_allocation.wr:WR1">
	The
	<express_ref linkend="Requirement_assignment:arm:Requirement_assignment_arm.Requirement_assignment.description"/>
	shall not be populated.
</ext_description>

