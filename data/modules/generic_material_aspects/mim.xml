   <schema name="Generic_material_aspects_mim">
      <interface kind="use" schema="Independent_property_representation_mim"/>
      <interface kind="use" schema="Product_view_definition_mim"/>
      <interface kind="use" schema="material_property_definition_schema">
         <interfaced.item name="material_designation"/>
         <interfaced.item name="material_designation_characterization"/>
