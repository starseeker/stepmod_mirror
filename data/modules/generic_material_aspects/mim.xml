   <schema name="Generic_material_aspects_mim">
      <interface kind="use" schema="Independent_property_representation_mim"/>
      <interface kind="use" schema="material_property_definition_schema">
         <interfaced.item name="material_designation"/>
         <interfaced.item name="material_designation_characterization"/>
         <interfaced.item name="material_property"/>
      </interface>
      <interface kind="use" schema="material_property_representation_schema">
         <interfaced.item name="data_environment"/>
         <interfaced.item name="material_property_representation"/>
      </interface>
      <entity name="general_material_property" supertypes="general_property">
         <where label="WR1" expression="SIZEOF( QUERY( gpa &lt;* USEDIN(SELF,'GENERIC_MATERIAL_ASPECTS_MIM.GENERAL_PROPERTY_ASSOCIATION.BASE_DEFINITION') | &#10;        NOT ('GENERIC_MATERIAL_ASPECTS_MIM.MATERIAL_PROPERTY' IN TYPEOF(gpa.derived_definition)) )) = 0"/>
      </entity>
   </schema>
