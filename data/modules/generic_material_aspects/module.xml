 <mapping_table>
   <ae entity="Applied_independent_material_property" extensible="NO">
      <aimelt xml:space="preserve">material_property</aimelt>
      <source xml:space="preserve">ISO 10303-45</source>
      <refpath xml:space="preserve">material_property &lt;=
      property_definition = derived_property_select
			derived_property_select &lt;- general_property_association.derived_definition</refpath>
      <aa attribute="base_independent_property" assertion_to="General_material_property">
         <aimelt xml:space="preserve">PATH</aimelt>
         <refpath xml:space="preserve">material_property &lt;=
property_definition
property_definition = derived_property_select
derived_property_select &lt;- 
general_property_association.derived_definition
general_property_association.base_definition -&gt; general_property
general_property =&gt; general_material_property
</refpath>
      </aa>
   </ae>
 
   		<ae entity="Data_environment" extensible="NO">
			<aimelt xml:space="preserve">data_environment</aimelt>
			<source xml:space="preserve">ISO 10303-45</source>
			<aa attribute="environment_name">
				<aimelt xml:space="preserve">data_environment.name</aimelt>
				<source xml:space="preserve">ISO 10303-45</source>
			</aa>
			<aa attribute="description">
				<aimelt xml:space="preserve">data_environment.description</aimelt>
				<source xml:space="preserve">ISO 10303-45</source>
			</aa>
			<aa attribute="environment_characterization" assertion_to="Independent_property_representation">
				<aimelt xml:space="preserve">PATH</aimelt>
				<refpath xml:space="preserve">
	 data_environment.elements -&gt; property_definition_representation
	 property_definition_representation.definition -&gt; represented_definition 
represented_definition = general_property 
</refpath>
			</aa>
			<aa attribute="environment_characterization" assertion_to="Property_representation">
				<aimelt xml:space="preserve">PATH</aimelt>
				<refpath xml:space="preserve">
	 data_environment.elements -&gt; property_definition_representation
	 {property_definition_representation.definition -&gt; represented_definition 
represented_definition = property_definition}
</refpath>
			</aa>
		</ae>
		<ae entity="Material_identification" extensible="NO">
			<aimelt xml:space="preserve">material_designation</aimelt>
			<source xml:space="preserve">ISO 10303-45</source>
			<aa attribute="material_name">
				<aimelt xml:space="preserve">material_designation.name</aimelt>
				<source xml:space="preserve">ISO 10303-45</source>
			</aa>
			<aa attribute="items" assertion_to="material_item_select">
				<aimelt xml:space="preserve">PATH</aimelt>
				<refpath xml:space="preserve">
					material_designation.definitions[i] -&gt; characterized_definition
				</refpath>
			</aa>
		</ae>
   <ae entity="Material_property" extensible="YES">
      <aimelt xml:space="preserve">material_property</aimelt>
      <source xml:space="preserve">ISO 10303-45</source>
   </ae>
		
		<ae entity="Material_property_association" extensible="NO">
			<aimelt xml:space="preserve">material_designation_characterization</aimelt>
			<source xml:space="preserve">ISO 10303-45</source>
			<aa attribute="described_material" assertion_to="Material_identification">
				<aimelt xml:space="preserve">PATH</aimelt>
				<refpath xml:space="preserve">material_designation_characterization.designation -&gt; material_designation</refpath>
			</aa>
			<aa attribute="associated_property_value" assertion_to="Material_property_value_representation">
				<aimelt xml:space="preserve">PATH</aimelt>
				<refpath xml:space="preserve">material_designation_characterization.property -&gt; characterized_material_property
characterized_material_property = material_property_representation
material_property_representation &lt;= property_definition_representation
property_definition_representation.used_representation -&gt; representation
</refpath>
			</aa>
			<aa attribute="definitional">
				<alt_map id="1">
					<description>If definitional is true</description>
					<aimelt xml:space="preserve">material_designation_characterization.name</aimelt>
					<source xml:space="preserve">ISO 10303-45</source>
					<refpath xml:space="preserve">{material_designation_characterization.name = 'definitional'}
</refpath>
				</alt_map>
				<alt_map id="2">
					<description>If definitional is false</description>
					<aimelt xml:space="preserve">material_designation_characterization.name</aimelt>
					<source xml:space="preserve">ISO 10303-45</source>
					<refpath xml:space="preserve">{material_designation_characterization.name = 'non definitional'}
</refpath>
				</alt_map>
				<alt_map id="3">
					<description>If definitional is not specified</description>
					<aimelt xml:space="preserve">material_designation_characterization.name</aimelt>
					<source xml:space="preserve">ISO 10303-45</source>
					<refpath xml:space="preserve">{material_designation_characterization.name = ''}
</refpath>
				</alt_map>
			</aa>
		</ae>
		<ae entity="Material_property_value_representation" extensible="NO">
			<aimelt xml:space="preserve">representation</aimelt>
			<source xml:space="preserve">ISO 10303-43</source>
