      <type name="axial_geometry_element">
         <select extensible="YES" genericentity="YES" selectitems="Circle Circular_cone Circular_cylinder Conical_surface Cylindrical_surface Line Plane Point Solid_of_revolution Sphere Spherical_surface Surface_of_revolution Toroidal_surface Torus"/>
      </type>
      <type name="curve_or_surface">
