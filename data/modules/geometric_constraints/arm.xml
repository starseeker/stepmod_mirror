         <where expression="(NOT (offset_direction_constrained = TRUE)&#10;    AND ((offset_type = curve_2d_offset) &#10;    OR (offset_type = surface_offset)))" label="WR2"/>
         <where expression="offset_value &gt; 0.0" label="WR3"/>
