<!-- +++++++++++ axis_placement_2d_3d_mapping +++++++++++ -->
<ext_description linkend="Geometric_model_2d_3d_relationship_mim.axis_placement_2d_3d_mapping">
	An <b>axis_placement_2d_3d_mapping</b> is a type of
	<express_ref linkend="representation_schema:ir_express:representation_schema.item_defined_transformation"/>
	that implements the ARM concept of
	<express_ref linkend="Geometric_model_2d_3d_relationship:arm:Geometric_model_2d_3d_relationship_arm.Axis_placement_2d_3d_mapping"/>.
</ext_description>

<!-- transform_item_1 -->
<ext_description linkend="Geometric_model_2d_3d_relationship_mim.axis_placement_2d_3d_mapping.transform_item_1">
	an attribute inherited from the
	<express_ref linkend="representation_schema:ir_express:representation_schema.item_defined_transformation"/>
	shall be redeclared as the
	<express_ref linkend="geometry_schema:ir_express:geometry_schema.axis2_placement_2d"/>
	for the <b>axis_placement_2d_3d_mapping</b>.
</ext_description>

<!-- transform_item_2 -->
<ext_description linkend="Geometric_model_2d_3d_relationship_mim.axis_placement_2d_3d_mapping.transform_item_2">
	an attribute inherited from the
	<express_ref linkend="representation_schema:ir_express:representation_schema.item_defined_transformation"/>
	shall be redeclared as the
	<express_ref linkend="geometry_schema:ir_express:geometry_schema.axis2_placement_3d"/>
	for the <b>axis_placement_2d_3d_mapping</b>.
</ext_description>

<!-- +++++++++++ geometric_relationship_with_2d_3d_placement_transformation +++++++++++ -->
<ext_description linkend="Geometric_model_2d_3d_relationship_mim.geometric_relationship_with_2d_3d_placement_transformation">
	A <b>geometric_relationship_with_2d_3d_placement_transformation</b> is a type of
	<express_ref linkend="product_property_representation_schema:ir_express:product_property_representation_schema.shape_representation_relationship"/>
	and a type of
	<express_ref linkend="representation_schema:ir_express:representation_schema.representation_relationship_with_transformation"/>.
</ext_description>

<!-- transformation_operator -->
<ext_description linkend="Geometric_model_2d_3d_relationship_mim.geometric_relationship_with_2d_3d_placement_transformation.transformation_operator">
	an attribute inherited from the
	<express_ref linkend="representation_schema:ir_express:representation_schema.representation_relationship_with_transformation"/>
	shall be redeclared as the
	<express_ref linkend="Geometric_model_2d_3d_relationship:mim:Geometric_model_2d_3d_relationship_mim.axis_placement_2d_3d_mapping"/>
	for the <b>geometric_relationship_with_2d_3d_placement_transformation</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Geometric_model_2d_3d_relationship_mim.geometric_relationship_with_2d_3d_placement_transformation.wr:WR1">
	The <b>geometric_relationship_with_2d_3d_placement_transformation</b> shall not participate in cycles.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Geometric_model_2d_3d_relationship_mim.geometric_relationship_with_2d_3d_placement_transformation.wr:WR2">
	A <b>geometric_relationship_with_2d_3d_placement_transformation</b> shall not be a 
	<express_ref linkend="representation_schema:ir_express:representation_schema.definitional_representation_relationship"/>.
</ext_description>

<!-- +++++++++++ acyclic_representation_relationship +++++++++++ -->
<ext_description linkend="Geometric_model_2d_3d_relationship_mim.acyclic_representation_relationship">
	The <b>acyclic_representation_relationship</b> function checks if specified
	<express_ref linkend="representation_schema:ir_express:representation_schema.representation_relationship"/>
	is not participating in cycle with regards to specified
	<express_ref linkend="representation_schema:ir_express:representation_schema.representation">representations</express_ref>.
</ext_description>

<!-- relation -->
<ext_description linkend="Geometric_model_2d_3d_relationship_mim.acyclic_representation_relationship.relation">
	the specified <express_ref linkend="representation_schema:ir_express:representation_schema.representation_relationship"/>.
</ext_description>

<!-- relatives -->
<ext_description linkend="Geometric_model_2d_3d_relationship_mim.acyclic_representation_relationship.relatives">
	the specified <express_ref linkend="representation_schema:ir_express:representation_schema.representation">representations</express_ref>.
</ext_description>

<!-- specific_relation -->
<ext_description linkend="Geometric_model_2d_3d_relationship_mim.acyclic_representation_relationship.specific_relation">
	the specified type of 
	<express_ref linkend="representation_schema:ir_express:representation_schema.representation_relationship">Representation_relationships</express_ref>
	to be considered.
</ext_description>

