<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.25 2002/11/06 00:59:57 thendrix Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.25"
    source="../data/modules/geometric_tolerance/arm.exp"/>

  <schema name="Geometric_tolerance_arm">
    <interface
      kind="use"
      schema="Derived_shape_element_arm">
    </interface>

    <interface
      kind="use"
      schema="Elemental_geometric_shape_arm">
    </interface>

    <interface
      kind="use"
      schema="Measure_representation_arm">
    </interface>

    <interface
      kind="use"
      schema="Shape_property_assignment_arm">
    </interface>

    <interface
      kind="use"
      schema="Value_with_unit_arm">
    </interface>

    <type name="datum_representation_select">
      <select
        selectitems="Datum_target_set Shape_element Derived_geometry">
      </select>

    </type>
    <entity
      name="Angularity_tolerance"
      supertypes="Geometric_tolerance">
      <explicit
        name="reference_datum">
        <aggregate
          type="SET"
          lower="1"
          upper="2"/>
        <typename
          name="Datum"/>
      </explicit>
    </entity>

    <entity
      name="Circular_runout_tolerance"
      supertypes="Geometric_tolerance">
      <explicit
        name="angle">
        <builtintype
          type="REAL"/>
      </explicit>
      <explicit
        name="reference_datum">
        <aggregate
          type="SET"
          lower="1"
          upper="2"/>
        <typename
          name="Datum"/>
      </explicit>
    </entity>

    <entity
      name="Coaxiality_tolerance"
      supertypes="Geometric_tolerance">
      <explicit
        name="reference_datum">
        <aggregate
          type="SET"
          lower="1"
          upper="2"/>
        <typename
          name="Datum"/>
      </explicit>
    </entity>

    <entity
      name="Compound_datum"
      supertypes="Datum">
      <explicit
        name="made_up_by">
        <aggregate
          type="SET"
          lower="2"
          upper="?"/>
        <typename
          name="Single_datum"/>
      </explicit>
    </entity>

    <entity
      name="Concentricity_tolerance"
      supertypes="Geometric_tolerance">
      <explicit
        name="reference_datum">
        <aggregate
          type="SET"
          lower="1"
          upper="2"/>
        <typename
          name="Datum"/>
      </explicit>
    </entity>

    <entity
      name="Cylindricity_tolerance"
      supertypes="Geometric_tolerance">
    </entity>

    <entity
      name="Datum"
      abstract.supertype="YES">
      <explicit
        name="precedence">
        <builtintype
          type="INTEGER"/>
      </explicit>
    </entity>

    <entity
      name="Datum_target">
      <explicit
        name="id">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="Datum_target_set">
      <explicit
        name="rule_description">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="target">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="Datum_target"/>
      </explicit>
    </entity>

    <entity
      name="Flatness_tolerance"
      supertypes="Geometric_tolerance">
    </entity>

    <entity
      name="Geometric_tolerance"
      abstract.supertype="YES"
      super.expression="(ONEOF (	Angularity_tolerance,	Concentricity_tolerance,	Circularity_tolerance,	Circular_runout_tolerance,	Coaxiality_tolerance,	Cylindricity_tolerance,	Flatness_tolerance,	Line_profile_tolerance,	Parallelism_tolerance,	Perpendicularity_tolerance,	Position_tolerance,	Roundness_tolerance,	Straightness_tolerance,	Surface_profile_tolerance,	Symmetry_tolerance,	Total_runout_tolerance	))">
      <explicit
        name="applied_to">
        <typename
          name="Shape_element"/>
      </explicit>
      <explicit
        name="modification">
        <typename
          name="Tolerance_condition"/>
      </explicit>
      <explicit
        name="name"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="qualifying_note"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="segment_size"
        optional="YES">
        <typename
          name="Value_with_unit"/>
      </explicit>
      <explicit
        name="significant_digits"
        optional="YES">
        <builtintype
          type="INTEGER"/>
      </explicit>
      <explicit
        name="tolerance_value">
        <typename
          name="Value_with_unit"/>
      </explicit>
      <explicit
        name="value_determination"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <where
        label="WR1"
        expression="(NOT (EXISTS(segment_size))) OR   ('GEOMETRIC_TOLERANCE_ARM.LENGTH_MEASURE' IN TYPEOF(segment_size.value_component))">
      </where>
      <where
        label="WR2"
        expression="(NOT (EXISTS(tolerance_value))) OR   ('GEOMETRIC_TOLERANCE_ARM.LENGTH_MEASURE' IN TYPEOF(tolerance_value.value_component))">
      </where>
      <where
        label="WR3"
        expression="(NOT (EXISTS(significant_digits))) OR   (significant_digits &gt; 0)">
      </where>
    </entity>

    <entity
      name="Geometric_tolerance_relationship">
      <explicit
        name="related">
        <typename
          name="Geometric_tolerance"/>
      </explicit>
      <explicit
        name="relating">
        <typename
          name="Geometric_tolerance"/>
      </explicit>
      <explicit
        name="relation_type">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="Line_profile_tolerance"
      supertypes="Geometric_tolerance">
      <explicit
        name="affected_plane">
        <typename
          name="Reference_plane"/>
      </explicit>
      <explicit
        name="reference_datum">
        <aggregate
          type="SET"
          lower="0"
          upper="3"/>
        <typename
          name="Datum"/>
      </explicit>
    </entity>

    <entity
      name="Parallelism_tolerance"
      supertypes="Geometric_tolerance">
      <explicit
        name="affected_plane">
        <typename
          name="Reference_plane"/>
      </explicit>
      <explicit
        name="reference_datum">
        <aggregate
          type="SET"
          lower="1"
          upper="2"/>
        <typename
          name="Datum"/>
      </explicit>
    </entity>

    <entity
      name="Perpendicularity_tolerance"
      supertypes="Geometric_tolerance">
      <explicit
        name="affected_plane">
        <typename
          name="Reference_plane"/>
      </explicit>
      <explicit
        name="reference_datum">
        <aggregate
          type="SET"
          lower="1"
          upper="3"/>
        <typename
          name="Datum"/>
      </explicit>
    </entity>

    <entity
      name="Placed_target"
      abstract.supertype="YES"
      super.expression="(ONEOF(Target_circular_area, Target_circular_line, Target_point, Target_rectangle, Target_straight_line))"
      supertypes="Datum_target">
      <explicit
        name="defined_in">
        <typename
          name="Geometric_coordinate_space"/>
      </explicit>
      <explicit
        name="parameter_reference">
        <typename
          name="Axis_placement"/>
      </explicit>
    </entity>

    <entity
      name="Position_tolerance"
      supertypes="Geometric_tolerance">
      <explicit
        name="affected_plane">
        <typename
          name="Reference_plane"/>
      </explicit>
      <explicit
        name="reference_datum">
        <aggregate
          type="SET"
          lower="0"
          upper="3"/>
        <typename
          name="Datum"/>
      </explicit>
    </entity>

    <entity
      name="Projection"
      supertypes="Tolerance_zone_definition">
      <explicit
        name="projection_end">
        <typename
          name="Shape_element"/>
      </explicit>
      <explicit
        name="projection_length">
        <typename
          name="Value_with_unit"/>
      </explicit>
    </entity>

    <entity
      name="Reference_plane">
    </entity>

    <entity
      name="Roundness_tolerance"
      supertypes="Geometric_tolerance">
    </entity>

    <entity
      name="Single_datum"
      supertypes="Datum">
      <explicit
        name="datum_name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="defined_by">
        <typename
          name="datum_representation_select"/>
      </explicit>
      <explicit
        name="modification">
        <typename
          name="Tolerance_condition"/>
      </explicit>
    </entity>

    <entity
      name="Straightness_tolerance"
      supertypes="Geometric_tolerance">
      <explicit
        name="affected_plane">
        <typename
          name="Reference_plane"/>
      </explicit>
    </entity>

    <entity
      name="Surface_profile_tolerance"
      supertypes="Geometric_tolerance">
      <explicit
        name="reference_datum">
        <aggregate
          type="SET"
          lower="0"
          upper="3"/>
        <typename
          name="Datum"/>
      </explicit>
    </entity>

    <entity
      name="Symmetry_tolerance"
      supertypes="Geometric_tolerance">
      <explicit
        name="affected_plane">
        <typename
          name="Reference_plane"/>
      </explicit>
      <explicit
        name="reference_datum">
        <aggregate
          type="SET"
          lower="1"
          upper="3"/>
        <typename
          name="Datum"/>
      </explicit>
    </entity>

    <entity
      name="Target_area"
      supertypes="Datum_target">
    </entity>

    <entity
      name="Target_circular_area"
      supertypes="Placed_target">
      <explicit
        name="diameter">
        <typename
          name="Numerical_item_with_unit"/>
      </explicit>
    </entity>

    <entity
      name="Target_circular_line"
      supertypes="Placed_target">
      <explicit
        name="diameter">
        <typename
          name="Numerical_item_with_unit"/>
      </explicit>
    </entity>

    <entity
      name="Target_point"
      supertypes="Placed_target">
    </entity>

    <entity
      name="Target_rectangle"
      supertypes="Placed_target">
      <explicit
        name="target_length">
        <typename
          name="Numerical_item_with_unit"/>
      </explicit>
      <explicit
        name="target_width">
        <typename
          name="Numerical_item_with_unit"/>
      </explicit>
    </entity>

    <entity
      name="Target_straight_line"
      supertypes="Placed_target">
    </entity>

    <entity
      name="Tolerance_condition">
      <explicit
        name="condition">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="Tolerance_zone">
      <explicit
        name="form_type">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="zone_for">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="Geometric_tolerance"/>
      </explicit>
    </entity>

    <entity
      name="Tolerance_zone_definition">
      <explicit
        name="defining">
        <typename
          name="Tolerance_zone"/>
      </explicit>
      <explicit
        name="first_element">
        <typename
          name="Shape_element"/>
      </explicit>
      <explicit
        name="second_element">
        <typename
          name="Shape_element"/>
      </explicit>
    </entity>

    <entity
      name="Total_runout_tolerance"
      supertypes="Geometric_tolerance">
      <explicit
        name="angle">
        <builtintype
          type="REAL"/>
      </explicit>
      <explicit
        name="reference_datum">
        <aggregate
          type="SET"
          lower="1"
          upper="2"/>
        <typename
          name="Datum"/>
      </explicit>
    </entity>

  </schema>

</express>
