<?xml version="1.0"?>
<!-- $Id: arm.xml,v 1.8 2002/05/29 16:17:40 darla Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/05/29 16:17:40 $"
  rcs.revision="$Revision: 1.8 $"
  description.file="arm_descriptions.xml">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.9"
    source="../data/modules/identification_assignment/arm.exp"/>

  <schema name="Identification_assignment_arm">

    <type name="identification_item">
      <select
        extensible="YES"
        selectitems="">
      </select>
    </type>

    <entity name="Identification_assignment">
      <explicit
        name="identifier">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="role"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="identification_item"/>
      </explicit>
    </entity>

  </schema>

</express>
