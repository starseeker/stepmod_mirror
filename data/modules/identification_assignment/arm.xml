<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/05/28 12:45:22 $"
  rcs.revision="$Revision: 1.7 $"
  description.file="arm_descriptions.xml">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.9"
    source="../data/modules/identification_assignment/arm.exp"/>

  <schema name="Identification_assignment_arm">
    <interface
      kind="use"
      schema="Person_organization_assignment_arm">
    </interface>

    <type name="identification_item">
      <select
        extensible="YES"
        selectitems="">
      </select>

    </type>
    <type name="identification_organization_or_person_in_organization_item">
      <select
        basedon="organization_or_person_in_organization_item"
        selectitems="Identification_assignment">
      </select>

    </type>
    <entity
      name="Identification_assignment">
      <explicit
        name="identifier">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="role"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="identification_item"/>
      </explicit>
      <inverse
        name="owner"
        entity="organization_or_person_in_organization_assignment"
        attribute="items">
        <inverse.aggregate
          type="SET"
          lower="0"
          upper="1"/>
      </inverse>
      <where
        label="WR1"
        expression="SIZEOF(QUERY(person&lt;*|person\Organization_or_person_in_organization_assignment.role &lt;&gt; 'identification owner'">
      </where>
    </entity>

  </schema>

</express>
