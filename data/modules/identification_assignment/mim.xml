<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/01/16 14:54:03 $"
  rcs.revision="$Revision: 1.3 $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.6"
    source="../data/modules/identification_assignment/mim.exp"/>

  <schema name="Identification_assignment_mim">
    <interface
      kind="use"
      schema="management_resources_schema">
      <interfaced.item
        name="identification_assignment"/>
    </interface>

    <interface
      kind="use"
      schema="Person_organisation_assignment_mim">
    </interface>

    <type name="identification_item">
      <description>An <b>identification_item</b> specifies those objects to which an <b>identification</b> may be assigned.</description>
      <select
        extensible="YES"
        selectitems="">
      </select>

    </type>
    <type name="identification_organization_item">
      <description>An <b>identification_organization_item</b> extends organization assignment to <b>applied_identification_assignment</b> instances.</description>
      <select
        basedon="organization_item"
        selectitems="applied_identification_assignment">
      </select>

    </type>
    <type name="identification_person_and_organization_item">
      <description>A <b>identification_person_and_organization_item</b> extends person and organization assignment to <b>applied_identification_assignment</b> instances.</description>
      <select
        basedon="person_and_organization_item"
        selectitems="applied_identification_assignment">
      </select>

    </type>
    <entity
      name="applied_identification_assignment"
      supertypes="identification_assignment">
      <description></description>
      <explicit
        name="items">
      <description></description>
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="identification_item"/>
      </explicit>
    </entity>

  </schema>

</express>
