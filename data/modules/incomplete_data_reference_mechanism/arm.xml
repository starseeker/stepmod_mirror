<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.26"
    source="../data/modules/incomplete_data_reference_mechanism/arm.exp"/>

  <schema name="Incomplete_data_reference_mechanism_arm">
    <interface
      kind="use"
      schema="Classification_assignment_arm">
    </interface>

    <interface
      kind="use"
      schema="Document_definition_arm">
    </interface>

    <interface
      kind="use"
      schema="File_identification_arm">
    </interface>

    <interface
      kind="use"
      schema="Part_view_definition_arm">
    </interface>

    <type name="idrm_marked_item">
      <select
        extensible="YES"
        basedon="classification_item"
        selectitems="Digital_document_definition Digital_file Part_view_definition">
      </select>

    </type>
    <entity
      name="Incomplete_reference_marking"
      supertypes="Classification_assignment">
      <where
        label="WR1"
        expression="SIZEOF (QUERY( item &lt;* SELF\Classification_assignment.items | 	NOT ('INCOMPLETE_DATA_REFERENCE_MECHANISM_ARM.IDRM_MARKED_ITEM' IN TYPEOF(item) ) )) = 0">
      </where>
      <where
        label="WR2"
        expression="SELF\Classification_assignment.assigned_class.id = 'reference'">
      </where>
    </entity>

  </schema>

</express>
