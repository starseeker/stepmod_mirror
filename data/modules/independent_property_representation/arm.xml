<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/01/16 14:24:56 $"
  rcs.revision="$Revision: 1.3 $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.6"
    source="../data/modules/independent_property_representation/arm.exp"/>

  <schema name="Independent_property_representation_arm">
    <interface
      kind="use"
      schema="Foundation_representation_arm">
    </interface>

    <interface
      kind="use"
      schema="Independent_property_arm">
    </interface>

    <entity
      name="Independent_property_representation">
      <description>
        An Independent_property_representation specifies the association of an
        <b>Independent_property</b> with a Representation. 
      </description>
      <explicit
        name="property">
        <description>
          The <b>Independent_property</b> that is being associated to a
        Representation.
        </description>
        <typename
          name="Independent_property"/>
      </explicit>
      <explicit
        name="rep">
        <description>
          The Representation being associated to an
          <b>Independent_property</b>. 
        </description>
        <typename
          name="Representation"/>
      </explicit>
    </entity>

  </schema>

</express>


