<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express language_version="2" description.file="arm_descriptions.xml" rcs.date="$Date: 2003/02/03 08:28:29 $" rcs.revision="$Revision: 1.1 $">
	<application name="express2xml.js" owner="Eurostep Limited" url="http://www.eurostep.com" version="1.26" source="../data/modules/inertia_characteristics/arm.exp"/>
	<schema name="Inertia_characteristics_arm">
		<interface kind="use" schema="Elemental_geometric_shape_arm"/>
		<interface kind="use" schema="Shape_property_assignment_arm"/>
		<interface kind="use" schema="Value_with_unit_arm"/>
		<constant name="schema_name" expression="'INERTIA_CHARACTERISTICS_ARM'">
			<builtintype type="STRING"/>
		</constant>
		<entity name="Centre_of_mass" supertypes="Shape_dependent_property_representation">
			<where label="WR1" expression="SIZEOF(SELF\Shape_dependent_property_representation.property_representation.items)=1"/>
			<where label="WR2" expression="SIZEOF(QUERY (it &lt;* SELF\Shape_dependent_property_representation.property_representation.items |   schema_name + '.CARTESIAN_POINT' IN TYPEOF(it) ))=1"/>
			<where label="WR3" expression="schema_name + '.GEOMETRIC_COORDINATE_SPACE' IN 	TYPEOF(SELF\Shape_dependent_property_representation.property_representation.context_of_items)"/>
		</entity>
		<entity name="Inertia_matrix_item" supertypes="Representation_item">
			<explicit name="inertia_values">
				<aggregate type="ARRAY" lower="1" upper="6"/>
				<builtintype type="REAL"/>
			</explicit>
			<explicit name="common_unit">
				<typename name="Unit"/>
			</explicit>
		</entity>
		<entity name="Moments_of_inertia" supertypes="Shape_dependent_property_representation">
			<where label="WR1" expression="SIZEOF(QUERY (it &lt;* SELF\Shape_dependent_property_representation.property_representation.items |   schema_name + '.AXIS_PLACEMENT'IN TYPEOF(it) ))=1"/>
			<where label="WR2" expression="SIZEOF(QUERY (it &lt;* SELF\Shape_dependent_property_representation.property_representation.items |   schema_name + '.INERTIA_MATRIX_ITEM' IN TYPEOF(it) ))=1"/>
			<where label="WR3" expression="schema_name + '.GEOMETRIC_COORDINATE_SPACE' IN 	TYPEOF(SELF\Shape_dependent_property_representation.property_representation.context_of_items)"/>
		</entity>
	</schema>
</express>
