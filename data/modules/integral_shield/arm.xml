<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-10-04T18:07:43" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="integral_shield_arm schema_instance"/>
   <schema name="Integral_shield_arm">
      <interface kind="use" schema="Layered_interconnect_module_with_printed_component_design_arm"/>
      <interface kind="use" schema="Requirement_assignment_arm"/>
      <type name="integral_or_routed_shield">
         <select basedon="requirement_assignment_item" selectitems="Routed_shield Integral_shield"/>
      </type>
      <type name="laminate_shieldable_item">
         <select selectitems="Component_external_reference Component_feature Component_feature_external_reference Component_group_external_reference Generic_physical_network Physical_connectivity_definition Physical_connectivity_definition_external_reference Stratum_feature_template_component"/>
      </type>
      <entity name="Component_group_external_reference">
         <explicit name="name">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="associated_definition">
            <builtintype type="STRING"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="name"/>
            <unique.attribute attribute="associated_definition"/>
         </unique>
      </entity>
      <entity name="Integral_shield" supertypes="Laminate_component">
         <explicit name="shielded_item">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="laminate_shieldable_item"/>
         </explicit>
      </entity>
      <entity name="Physical_connectivity_definition_external_reference">
         <explicit name="name">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="associated_definition">
            <builtintype type="STRING"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="name"/>
            <unique.attribute attribute="associated_definition"/>
         </unique>
      </entity>
      <entity name="Routed_shield" supertypes="Routed_printed_component">
         <explicit name="shielded_item">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="laminate_shieldable_item"/>
         </explicit>
      </entity>
   </schema>
</express>
