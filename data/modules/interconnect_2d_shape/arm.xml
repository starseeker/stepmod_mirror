<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:09" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="interconnect_2d_shape_arm schema_instance"/>
   <schema name="INTERCONNECT_2D_SHAPE_ARM">
      <interface kind="use" schema="FABRICATION_JOINT_ARM"/>
      <interface kind="use" schema="LAYERED_INTERCONNECT_MODULE_WITH_DESIGN_INTENT_MODIFICATIONS_ARM"/>
      <interface kind="use" schema="PHYSICAL_UNIT_2D_DESIGN_VIEW_ARM"/>
      <type name="design_intent_modification_planar_shape_select">
         <select selectitems="design_intent_modification_styled_area_representation design_intent_modification_planar_shape"/>
      </type>
      <entity name="component_2d_embedded_location" supertypes="component_2d_location">
         <explicit name="precedent_surface">
            <typename name="adjacent_stratum_surface_embedded_component_surface_definition"/>
         </explicit>
         <explicit name="subsequent_surface">
            <typename name="adjacent_stratum_surface_embedded_component_surface_definition"/>
         </explicit>
         <explicit name="reference_terminal">
            <typename name="fabrication_joint"/>
         </explicit>
         <unique label="ur1">
            <unique.attribute entity-ref="component_2d_embedded_location" attribute="precedent_surface"/>
            <unique.attribute entity-ref="component_2d_embedded_location" attribute="reference_terminal"/>
            <unique.attribute entity-ref="component_2d_embedded_location" attribute="subsequent_surface"/>
         </unique>
         <where label="wr1" expression=" precedent_surface &lt;&gt; subsequent_surface "/>
         <where label="wr2" expression=" self \ component_2d_location . substrate_location = false "/>
      </entity>
      <entity name="design_intent_modification_planar_position">
         <explicit name="design_shape">
            <typename name="physical_unit_planar_shape"/>
         </explicit>
         <explicit name="transformation">
            <typename name="axis_placement_2d"/>
         </explicit>
         <explicit name="feature_shape">
            <typename name="design_intent_modification_planar_shape_select"/>
         </explicit>
      </entity>
      <entity name="design_intent_modification_planar_shape" supertypes="planar_projected_shape">
         <explicit name="represented_design_intent_modification">
            <typename name="design_intent_modification"/>
         </explicit>
         <where label="wr1" expression=" ( sizeof ( usedin ( self , 'layered_2d_shape_arm.' + 'reference_geometric_representation_relationship.' + 'related_feature_shape' ) ) = 0 ) "/>
      </entity>
      <entity name="design_intent_modification_styled_area_representation">
         <explicit name="derived_planar_shape" optional="YES">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="design_intent_modification_planar_shape"/>
         </explicit>
         <explicit name="represented_design_intent_modification">
            <typename name="design_intent_modification"/>
         </explicit>
         <explicit name="shape_styled_area_representation">
            <typename name="fill_area"/>
         </explicit>
      </entity>
   </schema>
</express>
