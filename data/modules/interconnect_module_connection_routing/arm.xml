<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-01-26T09:44:21" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="interconnect_module_connection_routing_arm schema_instance"/>
   <schema name="Interconnect_module_connection_routing_arm">
      <interface kind="use" schema="Layered_interconnect_module_with_design_intent_modifications_arm"/>
      <interface kind="use" schema="Fabrication_joint_arm"/>
      <interface kind="use" schema="Physical_connectivity_definition_arm"/>
      <interface kind="use" schema="Interconnect_module_with_macros_arm"/>
      <type name="imcr_termination_or_junction">
         <select extensible="YES" genericentity="YES" basedon="termination_or_junction" selectitems="Inter_stratum_join_relationship Explicitly_located_layer_connection_point"/>
      </type>
      <type name="map_link">
         <select selectitems="Conductive_interconnect_element_terminal_link Passage_terminal_based_fabrication_joint_link"/>
      </type>
      <type name="terminal_or_inter_stratum_feature">
         <select selectitems="Embedded_component_terminal Inter_stratum_feature Interconnect_module_macro_component_join_terminal Interconnect_module_terminal Printed_component_join_terminal"/>
      </type>
      <type name="trace_junction_or_via">
         <select extensible="YES" genericentity="YES"/>
      </type>
      <entity name="Dependently_located_layer_connection_point" supertypes="Layer_connection_point">
         <explicit name="associated_design_object">
            <typename name="terminal_or_inter_stratum_feature"/>
         </explicit>
         <explicit name="reference_zone" optional="YES">
            <typename name="Connection_zone_in_design_view"/>
         </explicit>
      </entity>
      <entity name="Dielectric_crossover_area" supertypes="Stratum_concept_relationship">
         <explicit name="relating_concept">
            <typename name="Stratum_feature"/>
         </explicit>
         <explicit name="related_concept">
            <typename name="Stratum_feature"/>
         </explicit>
         <explicit name="resulting_dielectric_crossover">
            <typename name="Stratum_feature"/>
         </explicit>
      </entity>
      <entity name="Direct_stratum_component_join_implementation">
         <explicit name="implementing_joint">
            <typename name="Fabrication_joint"/>
         </explicit>
         <explicit name="topological_requirement">
            <typename name="Stratum_embedded_component_join_relationship"/>
         </explicit>
      </entity>
      <entity name="Explicitly_located_layer_connection_point" supertypes="Layer_connection_point"/>
      <entity name="Indirect_stratum_component_join_implementation" supertypes="Plated_inter_stratum_feature">
         <explicit name="indirect_topological_requirement">
            <typename name="Stratum_embedded_component_join_relationship"/>
         </explicit>
         <explicit name="implementing_joint">
            <typename name="Fabrication_joint"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute entity-ref="Indirect_stratum_component_join_implementation" attribute="indirect_topological_requirement"/>
            <unique.attribute entity-ref="Indirect_stratum_component_join_implementation" attribute="implementing_joint"/>
         </unique>
      </entity>
      <entity name="Inter_stratum_join_implementation" abstract.supertype="YES" super.expression="ONEOF (Physical_network_supporting_stratum_feature_conductive_join, Physical_network_supporting_inter_stratum_feature)">
         <explicit name="topological_requirement">
            <typename name="Inter_stratum_join_relationship"/>
         </explicit>
      </entity>
      <entity name="Join_two_physical_connectivity_definition_supporting_inter_stratum_feature" supertypes="Physical_network_supporting_inter_stratum_feature">
         <explicit name="joined_definitions">
            <aggregate type="SET" lower="2" upper="2"/>
            <typename name="Physical_connectivity_definition"/>
         </explicit>
      </entity>
      <entity name="Junction_vertex_allocation">
         <explicit name="implementation_point">
            <typename name="trace_junction_or_via"/>
         </explicit>
         <explicit name="required_vertex">
            <typename name="Topological_junction"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute entity-ref="Junction_vertex_allocation" attribute="required_vertex"/>
         </unique>
      </entity>
      <entity name="Physical_connectivity_abstraction_map">
         <explicit name="name_space_context_based_definition">
            <typename name="Physical_connectivity_element"/>
         </explicit>
         <explicit name="geometric_context_based_definition">
            <typename name="map_link"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute entity-ref="Physical_connectivity_abstraction_map" attribute="name_space_context_based_definition"/>
            <unique.attribute entity-ref="Physical_connectivity_abstraction_map" attribute="geometric_context_based_definition"/>
         </unique>
      </entity>
      <entity name="Physical_network_supporting_inter_stratum_feature" supertypes="Plated_inter_stratum_feature Inter_stratum_join_implementation" super.expression="Join_two_physical_connectivity_definition_supporting_inter_stratum_feature"/>
      <entity name="Physical_network_supporting_stratum_feature_conductive_join" supertypes="Stratum_feature_conductive_join Inter_stratum_join_implementation"/>
      <entity name="Stratum_feature_conductive_join" supertypes="Stratum_concept_relationship" super.expression="Physical_network_supporting_stratum_feature_conductive_join">
         <explicit name="relating_concept">
            <typename name="Stratum_feature_template_component"/>
         </explicit>
         <explicit name="related_concept">
            <typename name="Stratum_feature_template_component"/>
         </explicit>
         <where label="WR1" expression="related_concept.stratum_feature_implementation.resident_stratum :&lt;&gt;: relating_concept.stratum_feature_implementation.resident_stratum"/>
      </entity>
      <subtype.constraint name="layer_connection_point_subtypes" entity="Layer_connection_point" super.expression="ONEOF (Explicitly_located_layer_connection_point, Dependently_located_layer_connection_point)"/>
      <subtype.constraint name="Stratum_concept_relationship_subtypes" entity="Stratum_concept_relationship" super.expression="ONEOF (Stratum_feature_conductive_join, Dielectric_crossover_area)"/>
   </schema>
</express>
