<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="interconnect_module_connection_routing_arm schema_instance"/>
   <schema name="Interconnect_module_connection_routing_arm">
      <interface kind="use" schema="Fabrication_joint_arm"/>
      <interface kind="use" schema="Interconnect_module_with_macros_arm"/>
      <type name="imcr_termination_or_junction">
         <select extensible="YES" genericentity="YES" basedon="termination_or_junction" selectitems="Inter_stratum_join_relationship Explicitly_located_layer_connection_point"/>
      </type>
      <type name="map_link">
         <select selectitems="Conductive_interconnect_element_terminal_link Passage_terminal_based_fabrication_joint_link" genericentity="NO" extensible="NO"/>
      </type>
      <type name="terminal_or_inter_stratum_feature">
         <select selectitems="Embedded_physical_component_terminal Inter_stratum_feature Interconnect_module_macro_component_join_terminal Interconnect_module_terminal Printed_component_join_terminal" genericentity="NO" extensible="NO"/>
      </type>
      <type name="trace_junction_or_via">
         <select extensible="YES" selectitems="Explicitly_located_layer_connection_point" genericentity="NO"/>
      </type>
      <entity name="Dependently_located_layer_connection_point" supertypes="Layer_connection_point" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="associated_design_object" optional="NO">
            <typename name="terminal_or_inter_stratum_feature"/>
         </explicit>
         <explicit name="reference_zone" optional="YES">
            <typename name="Connection_zone_in_design_view"/>
         </explicit>
      <where label="IP1"/>
      </entity>
      <entity name="Dielectric_crossover_area" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="relating_concept" optional="NO">
            <typename name="Stratum_feature"/>
         </explicit>
         <explicit name="related_concept" optional="NO">
            <typename name="Stratum_feature"/>
         </explicit>
         <explicit name="resulting_dielectric_crossover" optional="NO">
            <typename name="Stratum_feature"/>
         </explicit>
         <explicit name="resulting_shape" optional="YES">
            <typename name="Geometric_model"/>
         </explicit>
         <inverse name="technology_requirement" entity="Requirement_assignment" attribute="assigned_to">
            <inverse.aggregate type="SET" lower="1" upper="?"/>
         </inverse>
      </entity>
      <entity name="Direct_stratum_component_join_implementation" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="implementing_joint" optional="NO">
            <typename name="Fabrication_joint"/>
         </explicit>
         <explicit name="topological_requirement" optional="NO">
            <typename name="Stratum_embedded_physical_component_join_relationship"/>
         </explicit>
      </entity>
      <entity name="Explicitly_located_layer_connection_point" supertypes="Layer_connection_point" abstract.supertype="NO" abstract.entity="NO"/>
      <entity name="Indirect_stratum_component_join_implementation" supertypes="Plated_inter_stratum_feature" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="indirect_topological_requirement" optional="NO">
            <typename name="Stratum_embedded_physical_component_join_relationship"/>
         </explicit>
         <explicit name="implementing_joint" optional="NO">
            <typename name="Fabrication_joint"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="indirect_topological_requirement"/>
            <unique.attribute attribute="implementing_joint"/>
         </unique>
      </entity>
      <entity name="Inter_stratum_join_implementation" abstract.supertype="YES" super.expression="ONEOF (Physical_network_supporting_stratum_feature_conductive_join, Physical_network_supporting_inter_stratum_feature)" abstract.entity="NO">
         <explicit name="topological_requirement" optional="NO">
            <typename name="Inter_stratum_join_relationship"/>
         </explicit>
      </entity>
      <entity name="Join_two_physical_connectivity_definition_supporting_inter_stratum_feature" supertypes="Physical_network_supporting_inter_stratum_feature" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="joined_definitions" optional="NO">
            <aggregate type="SET" lower="2" upper="2" unique="NO" optional="NO"/>
            <typename name="Physical_connectivity_definition"/>
         </explicit>
      </entity>
      <entity name="Junction_vertex_allocation" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="implementation_point" optional="NO">
            <typename name="trace_junction_or_via"/>
         </explicit>
         <explicit name="required_vertex" optional="NO">
            <typename name="Topological_junction"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="required_vertex"/>
         </unique>
      </entity>
      <entity name="Physical_connectivity_abstraction_map" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="name_space_context_based_definition" optional="NO">
            <typename name="Physical_connectivity_element"/>
         </explicit>
         <explicit name="geometric_context_based_definition" optional="NO">
            <typename name="map_link"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="name_space_context_based_definition"/>
            <unique.attribute attribute="geometric_context_based_definition"/>
         </unique>
      </entity>
      <entity name="Physical_network_supporting_inter_stratum_feature" supertypes="Plated_inter_stratum_feature Inter_stratum_join_implementation" super.expression="Join_two_physical_connectivity_definition_supporting_inter_stratum_feature" abstract.supertype="NO" abstract.entity="NO"/>
      <entity name="Physical_network_supporting_stratum_feature_conductive_join" supertypes="Stratum_feature_conductive_join Inter_stratum_join_implementation" abstract.supertype="NO" abstract.entity="NO"/>
      <entity name="Stratum_feature_conductive_join" super.expression="Physical_network_supporting_stratum_feature_conductive_join" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="relating_concept" optional="NO">
            <typename name="Stratum_feature_template_component"/>
         </explicit>
         <explicit name="related_concept" optional="NO">
            <typename name="Stratum_feature_template_component"/>
         </explicit>
         <explicit name="resulting_shape" optional="YES">
            <typename name="Geometric_model"/>
         </explicit>
         <inverse name="technology_requirement" entity="Requirement_assignment" attribute="assigned_to">
            <inverse.aggregate type="SET" lower="1" upper="?"/>
         </inverse>
         <where label="WR1" expression="related_concept.stratum_feature_implementation.resident_stratum :&lt;&gt;: relating_concept.stratum_feature_implementation.resident_stratum"/>
      </entity>
      <subtype.constraint name="layer_connection_point_subtypes" entity="Layer_connection_point" super.expression="ONEOF (Explicitly_located_layer_connection_point, Dependently_located_layer_connection_point)" abstract.supertype="NO"/>
   </schema>
</express>
