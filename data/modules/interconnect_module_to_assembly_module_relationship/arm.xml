<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date: 2007-03-21 03:48:09 -0700 (Tr, 21 Kov 2007) $" rcs.revision="$Revision: 23 $" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="interconnect_module_to_assembly_module_relationship_arm schema_instance"/>
   <schema name="Interconnect_module_to_assembly_module_relationship_arm">
      <interface kind="use" schema="Interconnect_module_connection_routing_arm"/>
      <interface kind="use" schema="Layered_interconnect_module_design_arm"/>
      <type name="component_based_message_type">
         <enumeration items="component_reference_designation terminal_designation"/>
      </type>
      <type name="component_or_component_feature">
         <select selectitems="Assembly_component Component_external_reference Component_feature Component_feature_external_reference"/>
      </type>
      <type name="imtar_bare_die_component_or_packaged_component">
         <select extensible="YES" genericentity="YES" basedon="bare_die_component_or_packaged_component" selectitems="Packaged_component Bare_die_component"/>
      </type>
      <type name="imtar_bare_die_terminal_or_packaged_part_terminal">
         <select extensible="YES" genericentity="YES" basedon="bare_die_terminal_or_packaged_part_terminal" selectitems="Bare_die_terminal Packaged_part_terminal"/>
      </type>
      <type name="imtar_documented_element_select">
         <select extensible="YES" basedon="documented_element_select" selectitems="component_or_component_feature"/>
      </type>
      <type name="imtar_requirement_assignment_item">
         <select extensible="YES" genericentity="YES" basedon="requirement_assignment_item" selectitems="Component_feature_to_layout_feature_relationship"/>
      </type>
      <type name="imtar_restriction_basis_item">
         <select extensible="YES" basedon="restriction_basis_item" selectitems="component_or_component_feature"/>
      </type>
      <entity name="Assembly_requirement_for_interconnect_relationship" supertypes="Part_definition_relationship">
         <explicit name="relating_view">
            <typename name="Assembly_module"/>
            <redeclaration entity-ref="View_definition_relationship"/>
         </explicit>
         <explicit name="related_view">
            <typename name="Interconnect_module"/>
            <redeclaration entity-ref="View_definition_relationship"/>
         </explicit>
      </entity>
      <entity name="Component_based_annotation_symbol_placement_relationship" supertypes="Component_feature_to_layout_feature_relationship">
         <explicit name="related_stratum_concept">
            <typename name="special_symbol_component"/>
            <redeclaration entity-ref="Component_feature_to_layout_feature_relationship"/>
         </explicit>
      </entity>
      <entity name="Component_based_annotation_text_relationship" supertypes="Component_feature_to_layout_feature_relationship">
         <explicit name="related_stratum_concept">
            <typename name="Laminate_text_string_component"/>
            <redeclaration entity-ref="Component_feature_to_layout_feature_relationship"/>
         </explicit>
         <explicit name="message_type">
            <typename name="component_based_message_type"/>
         </explicit>
      </entity>
      <entity name="Component_feature_to_layout_feature_relationship" super.expression="ONEOF (Component_based_annotation_symbol_placement_relationship, Component_based_annotation_text_relationship)">
         <explicit name="relating_feature">
            <typename name="component_or_component_feature"/>
         </explicit>
         <explicit name="related_stratum_concept">
            <typename name="stratum_concept"/>
         </explicit>
         <inverse name="requirement" entity="Requirement_assignment" attribute="assigned_to">
            <inverse.aggregate type="SET" lower="0" upper="1"/>
         </inverse>
      </entity>
   </schema>
</express>
