<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date: 2015/08/28 17:41:03 $" rcs.revision="$Revision: 1.55 $" reference="ISO/TS 10303-1687">
   <application name="JSDAI" owner="LKSoft" source="interconnect_module_with_macros_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Interconnect_module_with_macros_arm">
      <interface kind="use" schema="Layered_interconnect_module_design_arm"/>
      <type name="imwm_terminal_or_inter_stratum_feature">
         <select basedon="terminal_or_inter_stratum_feature" selectitems="Interconnect_module_macro_component_join_terminal"/>
      </type>
      <entity name="Interconnect_module_macro_component" supertypes="Physical_component Single_instance">
         <explicit name="derived_from">
            <typename name="Layered_interconnect_module_design_view"/>
            <redeclaration entity-ref="Definition_based_product_occurrence"/>
         </explicit>
         <inverse attribute="associated_definition" entity="Interconnect_module_macro_component_join_terminal" name="join_access_mechanisms">
            <inverse.aggregate lower="1" type="SET" upper="?"/>
         </inverse>
         <where expression="SIZEOF(QUERY(immcjt &lt;* join_access_mechanisms | immcjt\Component_feature.definition\Shape_element.associated_definition :&lt;&gt;: SELF\Definition_based_product_occurrence.derived_from\Part_design_view.usage_view)) = 0" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Product_view_definition.additional_characterization)" label="WR2"/>
      </entity>
      <entity name="Interconnect_module_macro_component_join_terminal" supertypes="Physical_component_terminal">
         <explicit name="definition">
            <typename name="Interconnect_module_macro_terminal"/>
            <redeclaration entity-ref="Component_feature"/>
         </explicit>
         <explicit name="associated_component">
            <typename name="Interconnect_module_macro_component"/>
            <redeclaration entity-ref="Component_feature" old_name="associated_definition"/>
         </explicit>
      </entity>
   </schema>
</express>
