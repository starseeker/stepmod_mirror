<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:09" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="interconnect_module_with_macros_arm schema_instance"/>
   <schema name="INTERCONNECT_MODULE_WITH_MACROS_ARM">
      <interface kind="use" schema="ASSEMBLY_MODULE_WITH_INTERCONNECT_COMPONENT_ARM"/>
      <interface kind="use" schema="COMPONENT_FEATURE_ARM"/>
      <interface kind="use" schema="INTERCONNECT_MODULE_USAGE_VIEW_ARM"/>
      <interface kind="use" schema="LAYERED_INTERCONNECT_MODULE_DESIGN_ARM"/>
      <interface kind="use" schema="PHYSICAL_UNIT_DESIGN_VIEW_ARM"/>
      <entity name="interconnect_module_macro_component" supertypes="interconnect_module_component">
         <explicit name="design_definition">
            <typename name="interconnect_module"/>
         </explicit>
         <inverse name="join_access_mechanisms" entity="interconnect_module_macro_component_join_terminal" attribute="associated_component">
            <inverse.aggregate type="SET" lower="1" upper="?"/>
         </inverse>
         <where label="wr1" expression=" sizeof ( query ( immcjt &lt;* join_access_mechanisms | immcjt \ component_feature . definition \ shape_element . containing_shape :&lt;&gt;: design_definition \ part_design_view . usage_view ) ) = 0 "/>
      </entity>
      <entity name="interconnect_module_macro_component_join_terminal" supertypes="interconnect_module_component_terminal">
         <explicit name="definition">
            <typename name="interconnect_module_join_terminal"/>
            <redeclaration entity-ref="component_feature"/>
         </explicit>
         <explicit name="associated_component">
            <typename name="interconnect_module_macro_component"/>
            <redeclaration entity-ref="component_feature"/>
         </explicit>
      </entity>
   </schema>
</express>
