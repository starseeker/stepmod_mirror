</ext_description>

<!-- +++++++++++ Interconnect_module_macro_component +++++++++++ -->
<ext_description linkend="Interconnect_module_with_macros_arm.Interconnect_module_macro_component">
	A <b>Interconnect_module_macro_component</b> is a type of 
	<express_ref linkend="Assembly_component:arm:Assembly_component_arm.Physical_component"/>
	and a type of
	<express_ref linkend="product_occurrence:arm:Product_occurrence_arm.Single_instance"/>.
</ext_description>

<!-- join_access_mechanisms -->
<ext_description linkend="Interconnect_module_with_macros_arm.Interconnect_module_macro_component.join_access_mechanisms">
	an attribute is an inverse relationship that specifies that the existence of the <b>Interconnect_module_macro_component</b> 
	is dependent on the existence of the  
	<express_ref linkend="Interconnect_module_with_macros:arm:Interconnect_module_with_macros_arm.Interconnect_module_macro_component_join_terminal"/> 
	that specifies the <b>Interconnect_module_macro_component</b> as its 
	<express_ref linkend="Interconnect_module_with_macros:arm:Interconnect_module_with_macros_arm.Interconnect_module_macro_component_join_terminal.associated_component"/>. 
	There shall be one or more 
	<express_ref linkend="Interconnect_module_with_macros:arm:Interconnect_module_with_macros_arm.Interconnect_module_macro_component_join_terminal">Interconnect_module_macro_component_join_terminals</express_ref> 
	for a particular <b>Interconnect_module_macro_component</b>.
</ext_description>

<!-- derived_from -->
<ext_description linkend="Interconnect_module_with_macros_arm.Interconnect_module_macro_component.derived_from">
	specifies the 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Layered_interconnect_module_design_view"/> 
	for the <b>Interconnect_module_macro_component</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Interconnect_module_with_macros_arm.Interconnect_module_macro_component.wr:WR1">
	The 
	<express_ref linkend="Interconnect_module_usage_view:arm:Interconnect_module_usage_view_arm.Interconnect_module_usage_view"/>
	referenced by the 
	<express_ref linkend="physical_unit_design_view:arm:Physical_unit_design_view_arm.Part_design_view.usage_view"/> 
	of <b>derived_from</b> shall be the same 
	<express_ref linkend="Interconnect_module_usage_view:arm:Interconnect_module_usage_view_arm.Interconnect_module_usage_view"/>
	referenced by the 
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element.associated_definition"/>
	which is referenced by the 
	<express_ref linkend="Component_feature:arm:Component_feature_arm.Component_feature.definition"/>
	which is referenced by the <b>join_access_mechanisms</b>.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Interconnect_module_with_macros_arm.Interconnect_module_macro_component.wr:WR2">
	The
	<express_ref linkend="Product_view_definition:arm:Product_view_definition_arm.Product_view_definition.additional_characterization"/>
	shall not be populated.
</ext_description>

<!-- +++++++++++ Interconnect_module_macro_component_join_terminal +++++++++++ -->
<ext_description linkend="Interconnect_module_with_macros_arm.Interconnect_module_macro_component_join_terminal">

An <b>Interconnect_module_macro_component_join_terminal</b> is a type of 
<express_ref linkend="Physical_component_feature:arm:Physical_component_feature_arm.Physical_component_terminal"/>.
</ext_description>
<ext_description linkend="Interconnect_module_with_macros_arm.Interconnect_module_macro_component_join_terminal.associated_component">
specifies a role of the <express_ref linkend="Interconnect_module_with_macros:arm:Interconnect_module_with_macros_arm.Interconnect_module_macro_component"/> for the
<b>Interconnect_module_macro_component_join_terminal</b>.
</ext_description>
<ext_description linkend="Interconnect_module_with_macros_arm.Interconnect_module_macro_component_join_terminal.definition">
specifies a role of the <express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Interconnect_module_macro_terminal"/> for the 
<b>Interconnect_module_macro_component_join_terminal</b>.
</ext_description>
</ext_descriptions>

