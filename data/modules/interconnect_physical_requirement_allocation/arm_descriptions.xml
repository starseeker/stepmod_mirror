<!-- +++++++++++  Dependent_electrical_isolation_removal_template +++++++++++ -->
<ext_description linkend="Interconnect_physical_requirement_allocation_arm.Dependent_electrical_isolation_removal_template">
<p>
  A <b>Dependent_electrical_isolation_removal_template</b> is a type of
  <express_ref linkend="Interconnect_physical_requirement_allocation:arm:Interconnect_physical_requirement_allocation_arm.Electrical_isolation_removal_template"/>
  that has an existence dependency on a member of <express_ref linkend="Interconnect_physical_requirement_allocation:arm:Interconnect_physical_requirement_allocation_arm.land_physical_template_or_inter_stratum_feature_template"/>.
</p>
<note>
  A  <b>Dependent_electrical_isolation_removal_template</b> is often used when design intent traceability is desired or where constraint graphs are to be 
  provided.
</note>
</ext_description>

<!-- associated_item -->
<ext_description linkend="Interconnect_physical_requirement_allocation_arm.Dependent_electrical_isolation_removal_template.associated_item">
	specifies a role of the 
	<express_ref linkend="Interconnect_physical_requirement_allocation:arm:Interconnect_physical_requirement_allocation_arm.land_physical_template_or_inter_stratum_feature_template"/> 
	for the <b>Dependent_electrical_isolation_removal_template</b>.
</ext_description>
<!-- WR1 -->
  <ext_description linkend="Interconnect_physical_requirement_allocation_arm.Dependent_electrical_isolation_removal_template.wr:WR1">
    When a <b>Dependent_electrical_isolation_removal_template</b> is included in a 
    <express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Padstack_definition"/> by a member of
    <express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Dependent_template_location_in_padstack_definition"/>, the 
    <express_ref linkend="Interconnect_physical_requirement_allocation:arm:Interconnect_physical_requirement_allocation_arm.Dependent_electrical_isolation_removal_template.associated_item"/>
    shall be included in the construction of the <express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Padstack_definition"/>
    and the assembly relationship 
    (<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Template_location_in_structured_template"/>)
    of the 
    <express_ref linkend="Interconnect_physical_requirement_allocation:arm:Interconnect_physical_requirement_allocation_arm.Dependent_electrical_isolation_removal_template.associated_item"/>
    to the <express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Padstack_definition"/>
    shall be the 
    <express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Dependent_template_location_in_padstack_definition.reference_location"/>
    for the
    <express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Dependent_template_location_in_padstack_definition"/>.
  </ext_description>
  
<!-- +++++++++++ Dependent_thermal_isolation_removal_template +++++++++++ -->
<ext_description linkend="Interconnect_physical_requirement_allocation_arm.Dependent_thermal_isolation_removal_template">
