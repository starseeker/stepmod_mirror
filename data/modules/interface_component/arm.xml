      <interface kind="use" schema="PHYSICAL_UNIT_DESIGN_VIEW_ARM"/>
      <entity name="interface_component" supertypes="physical_component">
         <inverse name="access_mechanism" entity="physical_component_interface_terminal" attribute="associated_component">
