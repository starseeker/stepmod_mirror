        <builtintype type="STRING"
        />
      </explicit>
    </entity>

    <entity name="Applied_activity_assignment"
    >
      <explicit
        name="assigned_activity">
        <typename name="Activity"/>
      </explicit>
      <explicit
        name="items">
        <aggregate type="SET"
          lower="1"
          upper="?"
        />
        <typename name="activity_item"/>
      </explicit>
      <explicit
        name="role">
        <builtintype type="STRING"
        />
      </explicit>
    </entity>

    <entity name="Activity_actual"
      supertypes="Activity"
    >
    </entity>

    <entity name="Activity_happening"
      supertypes="Activity_relationship"
    >
      <explicit
        name="relating_activity">
        <typename name="Activity_actual"/>
        <redeclaration
          entity-ref="Activity_relationship"/>
      </explicit>
      <derived
        name="actual"
        expression="SELF\Activity_relationship.relating_activity">
        <typename name="Activity_actual"/>
      </derived>
      <derived
        name="predicted"
        expression="SELF\Activity_relationship.related_activity">
        <typename name="Activity"/>
      </derived>
      <where
        label="WR1"
        expression="NOT (&apos;ACTIVITY_AS_REALIZED.ACTIVITY_ACTUAL&apos; IN TYPEOF(predicted))">
      </where>
    </entity>

    <entity name="Activity_method"
    >
      <explicit
        name="name">
        <builtintype type="STRING"
        />
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype type="STRING"
        />
      </explicit>
      <explicit
        name="consequence"
        optional="YES">
        <builtintype type="STRING"
        />
      </explicit>
      <explicit
        name="purpose">
        <builtintype type="STRING"
        />
      </explicit>
    </entity>

