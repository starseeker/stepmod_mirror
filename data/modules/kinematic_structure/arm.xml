         <where expression="SIZEOF( QUERY(lr &lt;* SELF\Kinematic_pair.associated_link_representations_2 | &#10;    NOT('KINEMATIC_STRUCTURE_ARM.RIGID_LINK_REPRESENTATION' IN TYPEOF(lr)))) = 0" label="WR2"/>
