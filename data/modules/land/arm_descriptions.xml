	<express_ref linkend="Layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.Dependent_thermal_isolation_removal_template"/>
	for the <b>Dependent_thermal_isolation_removal_component</b>.
</ext_description>

<!-- associated_land -->
<ext_description linkend="Land_arm.Dependent_thermal_isolation_removal_component.associated_land">
	specifies a role of the
	<express_ref linkend="Land:arm:Land_arm.Land"/>
	for the <b>Dependent_thermal_isolation_removal_component</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Land_arm.Dependent_thermal_isolation_removal_component.wr:WR1">
	The <b>associated_land</b>\<express_ref linkend="product_occurrence:arm:Product_occurrence_arm.Definition_based_product_occurrence"/>.<express_ref linkend="Land:arm:Land_arm.Land.derived_from"/>
	shall be equal to
	<b>derived_from</b>.<express_ref linkend="Layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.Dependent_thermal_isolation_removal_template.associated_item"/>.
