++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="land_mim">
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: contact_size_dependent_land
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Land_mim.contact_size_dependent_land">
	A <b>contact_size_dependent_land</b> is a type of
	<express_ref linkend="Land:mim:Land_mim.land"/>
	that implements the ARM concept of
	<express_ref linkend="Land:arm:Land_arm.Contact_size_dependent_land"/>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: dependent_thermal_isolation_removal_component
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Land_mim.dependent_thermal_isolation_removal_component">
	A <b>dependent_thermal_isolation_removal_component</b> is a type of 
	<express_ref linkend="Layered_interconnect_module_design:mim:Layered_interconnect_module_design_mim.material_removal_structured_component"/> 
	that implements the ARM concept of 
	<express_ref linkend="Land:arm:Land_arm.Dependent_thermal_isolation_removal_component"/>.
</ext_description>


<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: inter_stratum_feature_dependent_land
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Land_mim.inter_stratum_feature_dependent_land">
	An <b>inter_stratum_feature_dependent_land</b> is a type of
	<express_ref linkend="Land:mim:Land_mim.land"/>
	that implements the ARM concept of
	<express_ref linkend="Land:arm:Land_arm.Inter_stratum_feature_dependent_land"/>.
	An <b>inter_stratum_feature_dependent_land</b> is one of a
	<express_ref linkend="Land:mim:Land_mim.plated_passage_dependent_land"/>
	or an
	<express_ref linkend="Land:mim:Land_mim.unsupported_passage_dependent_land"/>.	
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: land
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Land_mim.land"><p>
	A <b>land</b> is a type of 
	<express_ref linkend="Layered_interconnect_module_design:mim:Layered_interconnect_module_design_mim.stratum_feature_template_component"/> 
	that implements the ARM concepts of:
	</p>
	<ul>
		<li> <express_ref linkend="Land:arm:Land_arm.Contact_size_dependent_land"/>;</li>
		<li> <express_ref linkend="Land:arm:Land_arm.Land"/>.</li>
	</ul>
	A <b>land</b> may be a
	<express_ref linkend="Land:mim:Land_mim.contact_size_dependent_land"/>
	or an
	<express_ref linkend="Land:mim:Land_mim.inter_stratum_feature_dependent_land"/>.
</ext_description>

<!-- +++++++++++ land_with_join_terminal  +++++++++++ -->
<ext_description linkend="Land_mim.land_with_join_terminal">
	A <b>land_with_join_terminal</b> is a type of
	<express_ref linkend="Land:mim:Land_mim.land"/>
	and a type of
	<express_ref linkend="Layered_interconnect_module_design:mim:Layered_interconnect_module_design_mim.laminate_component_join_terminal"/>
	that implements the ARM concept of
	<express_ref linkend="Land:arm:Land_arm.Land_with_join_terminal"/>.
</ext_description>

<!-- of_shape -->
<ext_description linkend="Land_mim.land_with_join_terminal.of_shape">
	an attribute inherited from the 
	<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.shape_aspect"/> 
	shall be redeclared as the 
	<express_ref linkend="Land:mim:Land_mim.land_with_join_terminal"/> 
	for the <b>land_with_join_terminal</b>.
	
</ext_description>


<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: plated_passage_dependent_land
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Land_mim.plated_passage_dependent_land">
	A <b>plated_passage_dependent_land</b> is a type of
	<express_ref linkend="Land:mim:Land_mim.inter_stratum_feature_dependent_land"/>
	that implements the ARM concept of
	<express_ref linkend="Land:arm:Land_arm.Plated_passage_dependent_land"/>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: unsupported_passage_dependent_land
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Land_mim.unsupported_passage_dependent_land">
	A <b>unsupported_passage_dependent_land</b> is a type of
	<express_ref linkend="Land:mim:Land_mim.inter_stratum_feature_dependent_land"/>
	that implements the ARM concept of
	<express_ref linkend="Land:arm:Land_arm.Unsupported_passage_dependent_land"/>.
</ext_description>


</ext_descriptions>
