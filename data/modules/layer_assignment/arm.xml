<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date: 2007-05-07 06:26:16 -0700 (Pr, 07 Geg 2007) $" rcs.revision="$Revision: 680 $" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="layer_assignment_arm schema_instance"/>
   <schema name="Layer_assignment_arm">
      <interface kind="use" schema="Foundation_representation_arm"/>
      <interface kind="use" schema="Group_arm"/>
      <type name="structuring_method_select">
         <select extensible="YES" selectitems="Group Layer"/>
      </type>
      <entity name="Layer">
         <explicit name="id">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="description">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="layered_elements">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="Representation_item"/>
         </explicit>
      </entity>
      <entity name="Structuring_method_hierarchy">
         <explicit name="relating">
            <typename name="structuring_method_select"/>
         </explicit>
         <explicit name="related">
            <typename name="Group"/>
         </explicit>
      </entity>
   </schema>
</express>
