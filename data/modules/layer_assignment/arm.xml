<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.22"
    source="../data/modules/layer_assignment/arm.exp"/>

  <schema name="Layer_assignment_arm">
    <interface
      kind="use"
      schema="Foundation_representation_arm">
    </interface>

    <entity
      name="Layer">
      <explicit
        name="description">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="id">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="layered_elements">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="Representation_item"/>
      </explicit>
    </entity>

  </schema>

</express>
