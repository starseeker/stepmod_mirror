   <schema name="Layered_2d_shape_arm">
      <interface kind="use" schema="Constructive_solid_geometry_2d_arm"/>
      <interface kind="use" schema="Wireframe_2d_arm"/>
      <interface kind="use" schema="Shape_feature_arm"/>
      <type name="part_template_or_physical_unit_2d_shape_select">
         <select extensible="YES" genericentity="YES"/>
      </type>
      <entity name="Planar_projected_shape_model" supertypes="Planar_shape_model">
         <where expression="NOT EXISTS(SELF\Representation.name)" label="WR1"/>
      </entity>
      <entity abstract.supertype="YES" name="Planar_shape_model" super.expression="Planar_projected_shape_model" supertypes="Geometric_model">
         <where expression="SELF\Geometric_model.context_of_items.dimension_count = 2" label="WR1"/>
      </entity>
   </schema>
</express>
