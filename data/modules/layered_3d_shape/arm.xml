<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:09" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="layered_3d_shape_arm schema_instance"/>
   <schema name="LAYERED_3D_SHAPE_ARM">
      <interface kind="use" schema="ADVANCED_BOUNDARY_REPRESENTATION_ARM"/>
      <interface kind="use" schema="CURVE_SWEPT_SOLID_ARM"/>
      <interface kind="use" schema="EXTENDED_ELEMENTAL_GEOMETRIC_SHAPE_ARM"/>
      <interface kind="use" schema="GEOMETRICALLY_BOUNDED_SURFACE_ARM"/>
      <interface kind="use" schema="PART_TEMPLATE_3D_SHAPE_ARM"/>
      <interface kind="use" schema="LAYERED_2D_SHAPE_ARM"/>
      <interface kind="use" schema="PHYSICAL_UNIT_3D_SHAPE_ARM"/>
      <type name="l3ds_part_template_or_physical_unit_shape_select">
         <select extensible="YES" genericentity="YES" basedon="part_template_or_physical_unit_shape_select" selectitems="part_template_or_physical_unit_3d_shape_select"/>
      </type>
      <type name="part_template_or_physical_unit_3d_shape_select">
         <select extensible="YES" genericentity="YES" selectitems="part_template_3d_shape physical_unit_3d_shape"/>
      </type>
      <type name="three_dimensional_shape">
         <select selectitems="advanced_brep_shape_representation curve_swept_solid geometrically_bounded_surface_shape_representation"/>
      </type>
      <entity name="part_template_or_physical_unit_3d_shape_representation_relationship" supertypes="geometric_model_relationship">
         <explicit name="principal_part_shape">
            <typename name="part_template_or_physical_unit_3d_shape_select"/>
            <redeclaration entity-ref="geometric_model_relationship" old_name="rep_1"/>
         </explicit>
         <explicit name="related_part_shape">
            <typename name="part_template_or_physical_unit_3d_shape_select"/>
            <redeclaration entity-ref="geometric_model_relationship" old_name="rep_2"/>
         </explicit>
         <unique label="ur1">
            <unique.attribute entity-ref="part_template_or_physical_unit_3d_shape_representation_relationship" attribute="principal_part_shape"/>
            <unique.attribute entity-ref="part_template_or_physical_unit_3d_shape_representation_relationship" attribute="related_part_shape"/>
         </unique>
         <where label="wr1" expression=" principal_part_shape :&lt;&gt;: related_part_shape "/>
      </entity>
      <entity name="shape_definition_3d_intersection" supertypes="geometric_model_relationship">
         <explicit name="orientation_intersection_plane">
            <typename name="axis_placement_3d"/>
         </explicit>
         <explicit name="planar_shape_origin">
            <typename name="axis_placement_2d"/>
         </explicit>
         <explicit name="surface_intersection_result">
            <typename name="planar_shape"/>
            <redeclaration entity-ref="geometric_model_relationship" old_name="rep_1"/>
         </explicit>
         <explicit name="projected_volume">
            <typename name="geometric_model"/>
            <redeclaration entity-ref="geometric_model_relationship" old_name="rep_2"/>
         </explicit>
         <where label="wr1" expression=" not ( 'layered_3d_shape_arm.planar_shape' in typeof ( projected_volume ) ) "/>
      </entity>
      <entity name="shape_definition_3d_plane_projection" supertypes="geometric_model_relationship">
         <explicit name="viewing_plane">
            <typename name="axis_placement_3d"/>
         </explicit>
         <explicit name="planar_shape_origin">
            <typename name="axis_placement_2d"/>
         </explicit>
         <explicit name="surface_projection_result">
            <typename name="planar_shape"/>
            <redeclaration entity-ref="geometric_model_relationship" old_name="rep_1"/>
         </explicit>
         <explicit name="projected_volume">
            <typename name="geometric_model"/>
            <redeclaration entity-ref="geometric_model_relationship" old_name="rep_2"/>
         </explicit>
         <where label="wr1" expression=" not ( 'layered_3d_shape_arm.planar_shape' in typeof ( projected_volume ) ) "/>
      </entity>
      <subtype.constraint name="l3ds_geometric_representation_relationship_subtypes" entity="geometric_model_relationship" super.expression="ONEOF (shape_definition_3d_intersection, shape_definition_3d_plane_projection)"/>
   </schema>
</express>
