<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: multi_stratum_structured_template
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Layered_interconnect_complex_template_mim.multi_stratum_structured_template">
	A <b>multi_stratum_structured_template</b> is a type of 
	<express_ref linkend="Layered_interconnect_complex_template:mim:Layered_interconnect_complex_template_mim.structured_template"/> 
	that implements the ARM concept of
	<express_ref linkend="Layered_interconnect_complex_template:arm:Layered_interconnect_complex_template_arm.Multi_stratum_structured_template"/>.
	A <b>multi_stratum_structured_template</b> may be either a
	<express_ref linkend="Layered_interconnect_complex_template:mim:Layered_interconnect_complex_template_mim.padstack_definition"/>
	or a
	<express_ref linkend="Layered_interconnect_complex_template:mim:Layered_interconnect_complex_template_mim.multi_stratum_special_symbol_template"/>,
	or a
	<express_ref linkend="Layered_interconnect_complex_template:mim:Layered_interconnect_complex_template_mim.stratum_stack_dependent_template"/>,
	or a
	<express_ref linkend="Layered_interconnect_complex_template:mim:Layered_interconnect_complex_template_mim.padstack_definition"/>
	and a
	<express_ref linkend="Layered_interconnect_complex_template:mim:Layered_interconnect_complex_template_mim.stratum_stack_dependent_template"/>,
	or a
	<express_ref linkend="Layered_interconnect_complex_template:mim:Layered_interconnect_complex_template_mim.multi_stratum_special_symbol_template"/>
	and a
	<express_ref linkend="Layered_interconnect_complex_template:mim:Layered_interconnect_complex_template_mim.stratum_stack_dependent_template"/>.
</ext_description>

