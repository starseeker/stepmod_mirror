<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="layered_interconnect_module_2d_design_arm schema_instance"/>
   <schema name="Layered_interconnect_module_2d_design_arm">
      <interface kind="use" schema="Layered_interconnect_module_design_arm"/>
      <entity name="Located_2d_inter_stratum_extent" supertypes="Inter_stratum_extent">
         <explicit name="location">
            <typename name="Cartesian_point"/>
         </explicit>
      </entity>
      <entity name="Stratum_feature_planar_shape_model" supertypes="Single_area_csg_2d_model">
         <explicit name="shape_characterized_component">
            <typename name="Stratum_feature"/>
         </explicit>
         <where label="WR1" expression="NOT EXISTS(SELF\Representation.description)"/>
      </entity>
      <entity name="Stratum_interconnect_module_planar_position">
         <explicit name="stratum_component">
            <typename name="Interconnect_module_stratum_assembly_relationship"/>
         </explicit>
         <explicit name="stratum_shape">
            <typename name="Stratum_planar_shape_model"/>
         </explicit>
         <explicit name="interconnect_module_shape">
            <typename name="Physical_unit_planar_shape_model"/>
         </explicit>
         <explicit name="transformation">
            <typename name="Axis_placement_2d"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="stratum_component"/>
         </unique>
      </entity>
      <entity name="Stratum_planar_shape_model" supertypes="Planar_projected_shape_model">
         <explicit name="shape_characterized_component">
            <typename name="Stratum"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="shape_characterized_component"/>
         </unique>
         <where label="WR1" expression="NOT EXISTS(SELF\Representation.description)"/>
      </entity>
   </schema>
</express>
