<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:09" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="layered_interconnect_module_3d_design_arm schema_instance"/>
   <schema name="LAYERED_INTERCONNECT_MODULE_3D_DESIGN_ARM">
      <interface kind="use" schema="PHYSICAL_UNIT_3D_SHAPE_ARM"/>
      <interface kind="use" schema="STRATUM_NON_PLANAR_SHAPE_ARM"/>
      <type name="stratum_feature_3d_shape_select">
         <select selectitems="stratum_feature_non_planar_2d_shape stratum_feature_3d_shape"/>
      </type>
      <entity name="stratum_3d_shape" supertypes="feature_shape_definition">
         <explicit name="shape_characterized_stratum">
            <typename name="stratum"/>
         </explicit>
         <where label="wr1" expression=" not ( 'layered_interconnect_module_3d_design_arm.planar_shape' in typeof ( self ) ) "/>
      </entity>
      <entity name="stratum_feature_3d_shape" supertypes="feature_shape_definition">
         <explicit name="represented_stratum_feature">
            <typename name="stratum_feature"/>
         </explicit>
      </entity>
      <entity name="stratum_feature_in_stratum_3d_position">
         <explicit name="stratum_shape">
            <typename name="stratum_3d_shape"/>
         </explicit>
         <explicit name="transformation">
            <typename name="axis_placement_3d"/>
         </explicit>
         <explicit name="feature_shape">
            <typename name="stratum_feature_3d_shape_select"/>
         </explicit>
      </entity>
      <entity name="stratum_interconnect_module_3d_position">
         <explicit name="stratum_component">
            <typename name="interconnect_module_stratum_assembly_relationship"/>
         </explicit>
         <explicit name="stratum_shape">
            <typename name="stratum_3d_shape"/>
         </explicit>
         <explicit name="interconnect_module_shape">
            <typename name="physical_unit_3d_shape"/>
         </explicit>
         <explicit name="transformation">
            <typename name="axis_placement_3d"/>
         </explicit>
         <unique label="ur1">
            <unique.attribute entity-ref="stratum_interconnect_module_3d_position" attribute="stratum_component"/>
         </unique>
      </entity>
      <entity name="stratum_surface_in_stratum_3d_position">
         <explicit name="stratum_shape">
            <typename name="stratum_3d_shape"/>
         </explicit>
         <explicit name="transformation">
            <typename name="axis_placement_3d"/>
         </explicit>
         <explicit name="surface_shape">
            <typename name="stratum_surface_shape_select"/>
         </explicit>
      </entity>
   </schema>
</express>
