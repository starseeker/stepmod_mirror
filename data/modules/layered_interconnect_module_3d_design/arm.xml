<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="layered_interconnect_module_3d_design_arm schema_instance"/>
   <schema name="Layered_interconnect_module_3d_design_arm">
      <interface kind="use" schema="Physical_unit_3d_shape_arm"/>
      <interface kind="use" schema="Stratum_non_planar_shape_arm"/>
      <type name="stratum_feature_3d_shape_select">
         <select selectitems="Stratum_feature_non_planar_2d_shape_model Stratum_feature_3d_shape_model"/>
      </type>
      <entity name="Stratum_3d_shape_model" supertypes="Geometric_model">
         <explicit name="shape_characterized_stratum">
            <typename name="Stratum"/>
         </explicit>
         <where label="WR1" expression="NOT ('LAYERED_INTERCONNECT_MODULE_3D_DESIGN_ARM.PLANAR_SHAPE_MODEL' IN TYPEOF(SELF))"/>
         <where label="WR2" expression="NOT EXISTS(SELF\Representation.name)"/>
         <where label="WR3" expression="NOT EXISTS(SELF\Representation.description)"/>
         <where label="WR4" expression="SIZEOF(USEDIN(SELF, 'SHAPE_PROPERTY_ASSIGNMENT_ARM.SHAPE_DESCRIPTION_ASSOCIATION.REPRESENTATION'))=0"/>
      </entity>
      <entity name="Stratum_feature_3d_shape_model" supertypes="Geometric_model">
         <explicit name="represented_stratum_feature">
            <typename name="Stratum_feature"/>
         </explicit>
         <where label="WR1" expression="NOT EXISTS(SELF\Representation.name)"/>
         <where label="WR2" expression="NOT EXISTS(SELF\Representation.description)"/>
         <where label="WR3" expression="SIZEOF(USEDIN(SELF, 'SHAPE_PROPERTY_ASSIGNMENT_ARM.SHAPE_DESCRIPTION_ASSOCIATION.REPRESENTATION'))=0"/>
      </entity>
      <entity name="Stratum_interconnect_module_3d_position">
         <explicit name="stratum_component">
            <typename name="Interconnect_module_stratum_assembly_relationship"/>
         </explicit>
         <explicit name="stratum_shape">
            <typename name="Stratum_3d_shape_model"/>
         </explicit>
         <explicit name="interconnect_module_shape">
            <typename name="Physical_unit_3d_shape_model"/>
         </explicit>
         <explicit name="transformation">
            <typename name="Axis_placement_3d"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="stratum_component"/>
         </unique>
      </entity>
   </schema>
</express>
