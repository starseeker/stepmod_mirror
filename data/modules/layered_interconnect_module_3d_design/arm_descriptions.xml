<ext_description linkend="Layered_interconnect_module_3d_design_arm">
</ext_description>
<!-- +++++++++++ Stratum_interconnect_module_3d_position +++++++++++ -->
<ext_description linkend="Layered_interconnect_module_3d_design_arm.Stratum_interconnect_module_3d_position">
	A <b>Stratum_interconnect_module_3d_position</b> is the position of the 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Stratum"/>
	model in the 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Interconnect_module"/> 
	model in three dimensions.
</ext_description>

<!-- transformation -->
<ext_description linkend="Layered_interconnect_module_3d_design_arm.Stratum_interconnect_module_3d_position.transformation">
specifies a role of the <express_ref linkend="Extended_basic_geometry:arm:Extended_basic_geometry_arm.Axis_placement_3d"/>
for the <b>Stratum_interconnect_module_3d_position</b>.
</ext_description>

<!-- interconnect_module_shape -->
<ext_description linkend="Layered_interconnect_module_3d_design_arm.Stratum_interconnect_module_3d_position.interconnect_module_shape">
specifies the <express_ref linkend="Physical_unit_3d_shape:arm:Physical_unit_3d_shape_arm.Physical_unit_3d_shape_model"/> that is the <express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Interconnect_module"/> model for the 
<b>Stratum_interconnect_module_3d_position</b>.
</ext_description>

<!-- stratum_shape -->
<ext_description linkend="Layered_interconnect_module_3d_design_arm.Stratum_interconnect_module_3d_position.stratum_shape">
specifies the <express_ref linkend="Layered_interconnect_module_3d_design:arm:Layered_interconnect_module_3d_design_arm.Stratum_3d_shape_model"/> for 
the <express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Stratum"/> model for the <b>Stratum_interconnect_module_3d_position</b>.
</ext_description>

<!-- stratum_component -->
<ext_description linkend="Layered_interconnect_module_3d_design_arm.Stratum_interconnect_module_3d_position.stratum_component">
specifies a role of the <express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Interconnect_module_stratum_assembly_relationship"/> for the 
<b>Stratum_interconnect_module_3d_position</b>.
</ext_description>

<!-- UR1 -->
<ext_description linkend="Layered_interconnect_module_3d_design_arm.Stratum_interconnect_module_3d_position.ur:UR1">
<p>
The <b>stratum_component</b> shall be unique within a population of <b>Stratum_interconnect_module_3d_position</b>s.
</p>
</ext_description>


<!-- +++++++++++ Stratum_feature_3d_shape_model ++++++++++ -->
<ext_description linkend="Layered_interconnect_module_3d_design_arm.Stratum_feature_3d_shape_model">
	A <b>Stratum_feature_3d_shape_model</b> is a type of 
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model"/>.
	A <b>Stratum_feature_3d_shape_model</b> is the representation of a 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Stratum_feature"/> 
	using a closed volume. 
</ext_description>

<!-- shape_characterized_component -->
<ext_description linkend="Layered_interconnect_module_3d_design_arm.Stratum_feature_3d_shape_model.shape_characterized_component">
	specifies a role of a 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Stratum_feature"/> 
	for the <b>Stratum_feature_3d_shape_model</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Layered_interconnect_module_3d_design_arm.Stratum_feature_3d_shape_model.wr:WR1">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.name"/>
	shall not be populated.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Layered_interconnect_module_3d_design_arm.Stratum_feature_3d_shape_model.wr:WR2">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.description"/>
	shall not be populated.
</ext_description>

<!-- WR3 -->
<ext_description linkend="Layered_interconnect_module_3d_design_arm.Stratum_feature_3d_shape_model.wr:WR3">
The <b>Stratum_feature_3d_shape_model</b> shall not be used in role of <express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_description_association.representation"/>
by any <express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_description_association"/>.
</ext_description>

<!-- +++++++++++ Stratum_3d_shape_model ++++++++++ -->
<ext_description linkend="Layered_interconnect_module_3d_design_arm.Stratum_3d_shape_model">
	A <b>Stratum_3d_shape_model</b> is a type of 
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model"/>.
	A <b>Stratum_3d_shape_model</b> is the three-dimensional shape of a 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Stratum"/>.
</ext_description>

<!-- shape_characterized_component -->
<ext_description linkend="Layered_interconnect_module_3d_design_arm.Stratum_3d_shape_model.shape_characterized_component">
	specifies a role of the 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Stratum"/> 
	for the <b>Stratum_3d_shape_model</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Layered_interconnect_module_3d_design_arm.Stratum_3d_shape_model.wr:WR1">
<p>
	The <b>Stratum_3d_shape_model</b> shall not be a 
	<express_ref linkend="layered_2d_shape:arm:Layered_2d_shape_arm.Planar_shape_model"/>.
</p>
</ext_description>

<!-- WR2 -->
<ext_description linkend="Layered_interconnect_module_3d_design_arm.Stratum_3d_shape_model.wr:WR2">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.name"/>
	shall not be populated.
</ext_description>

<!-- WR3 -->
<ext_description linkend="Layered_interconnect_module_3d_design_arm.Stratum_3d_shape_model.wr:WR3">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.description"/>
	shall not be populated.
</ext_description>

<!-- WR4 -->
<ext_description linkend="Layered_interconnect_module_3d_design_arm.Stratum_3d_shape_model.wr:WR4">
The <b>Stratum_3d_shape_model</b> shall not be used in role of <express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_description_association.representation"/>
by any <express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_description_association"/>.
</ext_description>

<!-- +++++++++++ in_3d_assembly_context +++++++++++ -->
<ext_description linkend="Layered_interconnect_module_3d_design_arm.in_3d_assembly_context">
	The <b>in_3d_assembly_context</b> function returns TRUE only if specified 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Area_component"/> 
	has at least one 
	<express_ref linkend="Physical_unit_3d_design_view:arm:Physical_unit_3d_design_view_arm.Assembly_component_3d_shape_model"/>  
	referencing it as
	<express_ref linkend="Physical_unit_3d_design_view:arm:Physical_unit_3d_design_view_arm.Assembly_component_3d_shape_model.shape_characterized_component"/>  
	and if all such 
	<express_ref linkend="Physical_unit_3d_design_view:arm:Physical_unit_3d_design_view_arm.Assembly_component_3d_shape_model">Assembly_component_3d_shape_models</express_ref>
	share 
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model.context_of_items"/> 
	with at least one 
	<express_ref linkend="Physical_unit_3d_shape:arm:Physical_unit_3d_shape_arm.Physical_unit_3d_shape_model"/> 
	of assembly they are assembled into. Otherwise function returns FALSE.
</ext_description>

<!-- comp -->
<ext_description linkend="Layered_interconnect_module_3d_design_arm.in_3d_assembly_context.comp">
	the specified
	<express_ref linkend="Physical_unit_design_view:arm:Physical_unit_design_view_arm.Assembly_component"/>.
</ext_description>

<!-- +++++++++++ no_3d_location_for_component +++++++++++ -->
<ext_description linkend="Layered_interconnect_module_3d_design_arm.no_3d_location_for_component">
	The <b>no_3d_location_for_component</b> function returns TRUE only if there is no
	<express_ref linkend="Physical_unit_3d_design_view:arm:Physical_unit_3d_design_view_arm.Component_3d_location"/> 
	given for specified component. Otherwise it returns FALSE.
</ext_description>

<!-- comp -->
<ext_description linkend="Layered_interconnect_module_3d_design_arm.no_3d_location_for_component.comp">
	the specified
	<express_ref linkend="Physical_unit_design_view:arm:Physical_unit_design_view_arm.Assembly_component"/>.
</ext_description>

