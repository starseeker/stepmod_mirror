<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:09" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="layered_interconnect_module_3d_shape_arm schema_instance"/>
   <schema name="LAYERED_INTERCONNECT_MODULE_3D_SHAPE_ARM">
      <interface kind="use" schema="LAYERED_INTERCONNECT_MODULE_DESIGN_ARM"/>
      <interface kind="use" schema="PART_TEMPLATE_3D_SHAPE_ARM"/>
      <type name="material_or_specification">
         <select selectitems="requirement_definition_property material_identification"/>
      </type>
      <entity name="cutout" supertypes="inter_stratum_feature" super.expression="ONEOF (physical_connectivity_interrupting_cutout, plated_cutout, partially_plated_cutout)"/>
      <entity name="cutout_edge_segment" supertypes="inter_stratum_feature" super.expression="plated_cutout_edge_segment">
         <explicit name="composed_cutout">
            <typename name="cutout"/>
         </explicit>
         <explicit name="start_vertex">
            <typename name="edge_segment_vertex"/>
         </explicit>
         <explicit name="end_vertex">
            <typename name="edge_segment_vertex"/>
         </explicit>
         <explicit name="derived_from">
            <typename name="inter_stratum_feature_edge_segment_template"/>
            <redeclaration entity-ref="inter_stratum_feature"/>
         </explicit>
         <where label="wr1" expression=" start_vertex :&lt;&gt;: end_vertex "/>
      </entity>
      <entity name="dielectric_material_passage" supertypes="inter_stratum_feature">
         <explicit name="feature_material" optional="YES">
            <typename name="material_or_specification"/>
         </explicit>
         <explicit name="precedent_passage">
            <typename name="cutout"/>
         </explicit>
      </entity>
      <entity name="edge_segment_cross_section" supertypes="shape_definition_3d_intersection">
         <explicit name="surface_intersection_result">
            <typename name="edge_shape"/>
            <redeclaration entity-ref="shape_definition_3d_intersection"/>
         </explicit>
         <explicit name="projected_volume">
            <typename name="part_template_3d_shape"/>
            <redeclaration entity-ref="shape_definition_3d_intersection"/>
         </explicit>
         <derived name="cross_section_reference" expression=" self \ shape_definition_3d_intersection . projected_volume \ part_template_3d_shape . shape_characterized_part_template \ inter_stratum_feature_edge_segment_template . start_vertex ">
            <typename name="edge_segment_vertex"/>
         </derived>
         <derived name="associated_template" expression=" self \ shape_definition_3d_intersection . projected_volume \ part_template_3d_shape . shape_characterized_part_template ">
            <typename name="inter_stratum_feature_edge_segment_template"/>
         </derived>
         <inverse name="referencing_template" entity="inter_stratum_feature_edge_segment_template" attribute="cross_section"/>
         <where label="wr1" expression=" associated_template :=: referencing_template "/>
      </entity>
      <entity name="inter_stratum_feature_edge_segment_template" supertypes="inter_stratum_feature_template">
         <explicit name="composed_edge">
            <typename name="inter_stratum_feature_edge_template"/>
         </explicit>
         <explicit name="start_vertex">
            <typename name="edge_segment_vertex"/>
         </explicit>
         <explicit name="end_vertex">
            <typename name="edge_segment_vertex"/>
         </explicit>
         <explicit name="reference_path">
            <typename name="trimmed_curve"/>
         </explicit>
         <explicit name="cross_section" optional="YES">
            <typename name="edge_segment_cross_section"/>
         </explicit>
         <where label="wr1" expression=" exists ( cross_section ) xor ( sizeof ( usedin ( self , 'layered_interconnect_module_3d_shape_arm.part_template_3d_shape.' + 'shape_characterized_part_template' ) ) &gt; 0 ) "/>
         <where label="wr2" expression=" exists ( reference_path . start_point ) and exists ( reference_path . end_point ) "/>
         <where label="wr3" expression=" not ( exists ( start_vertex \ datum_point . location_in_2d ) ) or ( reference_path . start_point :=: start_vertex \ datum_point . location_in_2d ) "/>
         <where label="wr4" expression=" not ( exists ( start_vertex \ datum_point . location_in_3d ) ) or ( reference_path . start_point :=: start_vertex \ datum_point . location_in_3d ) "/>
         <where label="wr5" expression=" not ( exists ( end_vertex \ datum_point . location_in_2d ) ) or ( reference_path . end_point :=: end_vertex \ datum_point . location_in_2d ) "/>
         <where label="wr6" expression=" not ( exists ( end_vertex \ datum_point . location_in_3d ) ) or ( reference_path . end_point :=: end_vertex \ datum_point . location_in_3d ) "/>
         <where label="wr7" expression=" start_vertex :&lt;&gt;: end_vertex "/>
      </entity>
      <entity name="inter_stratum_feature_edge_template" supertypes="inter_stratum_feature_template">
         <inverse name="edges" entity="inter_stratum_feature_edge_segment_template" attribute="composed_edge">
            <inverse.aggregate type="SET" lower="2" upper="?"/>
         </inverse>
      </entity>
      <entity name="interconnect_module_edge" supertypes="inter_stratum_feature" super.expression="ONEOF (partially_plated_interconnect_module_edge, plated_interconnect_module_edge)"/>
      <entity name="interconnect_module_edge_segment" supertypes="inter_stratum_feature" super.expression="plated_interconnect_module_edge_segment">
         <explicit name="composed_edge">
            <typename name="interconnect_module_edge"/>
         </explicit>
         <explicit name="start_vertex">
            <typename name="edge_segment_vertex"/>
         </explicit>
         <explicit name="end_vertex">
            <typename name="edge_segment_vertex"/>
         </explicit>
         <explicit name="derived_from">
            <typename name="inter_stratum_feature_edge_segment_template"/>
            <redeclaration entity-ref="inter_stratum_feature"/>
         </explicit>
         <where label="wr1" expression=" start_vertex :&lt;&gt;: end_vertex "/>
      </entity>
      <entity name="partially_plated_cutout" supertypes="cutout">
         <inverse name="plated_segment" entity="cutout_edge_segment" attribute="composed_cutout">
            <inverse.aggregate type="SET" lower="1" upper="?"/>
         </inverse>
         <inverse name="unplated_segment" entity="cutout_edge_segment" attribute="composed_cutout">
            <inverse.aggregate type="SET" lower="1" upper="?"/>
         </inverse>
      </entity>
      <entity name="partially_plated_interconnect_module_edge" supertypes="interconnect_module_edge">
         <inverse name="plated_segment" entity="interconnect_module_edge_segment" attribute="composed_edge">
            <inverse.aggregate type="SET" lower="1" upper="?"/>
         </inverse>
         <inverse name="unplated_segment" entity="interconnect_module_edge_segment" attribute="composed_edge">
            <inverse.aggregate type="SET" lower="1" upper="?"/>
         </inverse>
      </entity>
      <entity name="physical_connectivity_interrupting_cutout" supertypes="cutout">
         <explicit name="interrupted_connectivity_element">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="conductive_interconnect_element"/>
         </explicit>
      </entity>
      <entity name="plated_cutout" supertypes="cutout plated_inter_stratum_feature"/>
      <entity name="plated_cutout_edge_segment" supertypes="cutout_edge_segment plated_inter_stratum_feature">
         <explicit name="composed_cutout">
            <typename name="partially_plated_cutout"/>
            <redeclaration entity-ref="cutout_edge_segment"/>
         </explicit>
      </entity>
      <entity name="plated_interconnect_module_edge" supertypes="interconnect_module_edge plated_inter_stratum_feature"/>
      <entity name="plated_interconnect_module_edge_segment" supertypes="plated_inter_stratum_feature interconnect_module_edge_segment">
         <explicit name="composed_edge">
            <typename name="partially_plated_interconnect_module_edge"/>
            <redeclaration entity-ref="interconnect_module_edge_segment"/>
         </explicit>
      </entity>
      <subtype.constraint name="inter_stratum_feature_template_subtypes" entity="inter_stratum_feature_template" super.expression="ONEOF (component_termination_passage_template, via_template, unsupported_passage_template, inter_stratum_feature_edge_segment_template, inter_stratum_feature_edge_template)"/>
      <subtype.constraint name="plated_inter_stratum_feature_subtypes" entity="plated_inter_stratum_feature" super.expression="ONEOF (plated_cutout, plated_interconnect_module_edge, plated_cutout_edge_segment, plated_interconnect_module_edge_segment, plated_passage)"/>
   </schema>
</express>
