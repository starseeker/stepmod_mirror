    SUBTYPE OF (Laminate_component);
    	SELF\definition_based_product_occurrence.derived_from : OPTIONAL stratum_feature_template;
      probed_layout_item 																		: stratum_feature;
      probe_contact_zone 																		: connection_zone_in_design_view;
