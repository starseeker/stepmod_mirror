<!-- WR1 -->
<ext_description linkend="Layered_interconnect_module_with_printed_component_design_arm.Single_stratum_printed_component.wr:WR1">
	Each 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Stratum_feature"/> 
	referenced by <b>stratum_feature_implementation</b> shall specify the same 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Stratum"/> 
	in the role of 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Stratum_feature.resident_stratum"/> 
	as that 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Stratum"/>
	referenced by the first member of <b>stratum_feature_implementation</b>.
	
</ext_description>


