	<express_ref linkend="Layered_interconnect_module_with_printed_component_design:arm:Layered_interconnect_module_with_printed_component_design_arm.Printed_component"/>
	and a type of
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Structured_layout_component"/>.
</ext_description>

<!-- derived_from -->
<ext_description linkend="Layered_interconnect_module_with_printed_component_design_arm.Single_stratum_printed_component.derived_from">
	an attribute inherited from the
	<express_ref linkend="Layered_interconnect_module_with_printed_component_design:arm:Layered_interconnect_module_with_printed_component_design_arm.Printed_component"/>
	shall be redeclared as the
	<express_ref linkend="Printed_physical_layout_template:arm:Printed_physical_layout_template_arm.Single_stratum_printed_part_template"/>
