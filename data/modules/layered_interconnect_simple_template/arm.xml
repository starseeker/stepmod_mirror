<?xml version="1.0" encoding="utf-8"?><?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express
  SYSTEM "../../../dtd/express.dtd">
<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="">
   <application name="JSDAI" owner="LKSoft" source="layered_interconnect_simple_template_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Layered_interconnect_simple_template_arm">
      <interface kind="use" schema="Layered_2d_shape_arm"/>
      <interface kind="use" schema="Fill_area_style_arm"/>
      <interface kind="use" schema="Fabrication_technology_arm"/>
      <interface kind="use" schema="Text_representation_arm"/>
      <interface kind="use" schema="Part_template_shape_with_parameters_arm"/>
      <interface kind="reference" schema="Requirement_decomposition_arm">
         <interfaced.item name="get_rvd"/>
      </interface>
      <interface kind="reference" schema="Support_resource_arm">
         <interfaced.item name="bag_to_set"/>
      </interface>
      <type name="land_connection_zone_class">
         <enumeration items="surface_point_class edge_area_class edge_curve_class edge_point_class surface_area_class"/>
      </type>
      <type name="land_template_terminal_class">
         <enumeration items="surface_point_class edge_area_class edge_curve_class edge_point_class surface_area_class"/>
      </type>
      <type name="list_external_identification_item">
         <select basedon="external_identification_item" extensible="YES" genericentity="YES" selectitems="Land_physical_template"/>
      </type>
      <type name="list_usage_concept">
         <select basedon="usage_concept" extensible="YES" genericentity="YES" selectitems="Land_template_terminal"/>
      </type>
      <type name="ptswp_ptsm_definition">
         <select selectitems="Via_template Component_termination_passage_template"/>
      </type>
      <type name="ptswp_template_shape_model_context">
         <enumeration items="drilled_passage_context finished_passage_context unknown_context"/>
      </type>
      <entity name="Blind_passage_template" supertypes="Unsupported_passage_template">
         <explicit name="bottom_distance_from_datum_reference">
            <typename name="Shape_dimension_representation"/>
         </explicit>
         <explicit name="datum_reference_layer">
            <typename name="Stratum_technology_occurrence"/>
         </explicit>
         <explicit name="datum_reference_side">
            <typename name="Stratum_technology_occurrence_link"/>
         </explicit>
         <explicit name="of_measure_orientation">
            <typename name="measure_orientation"/>
         </explicit>
      </entity>
      <entity name="Closed_path_area_with_parameters" supertypes="Path_area_with_parameters">
         <explicit name="centreline">
            <typename name="closed_curve"/>
            <redeclaration entity-ref="Path_area_with_parameters"/>
         </explicit>
         <explicit name="parameters">
            <typename name="Closed_curve_style_parameters"/>
            <redeclaration entity-ref="Path_area_with_parameters"/>
         </explicit>
      </entity>
      <entity name="Component_termination_passage_template" supertypes="Continuous_template Inter_stratum_feature_template">
         <explicit name="of_passage_technology">
            <typename name="Default_component_termination_passage_definition"/>
            <redeclaration entity-ref="Inter_stratum_feature_template"/>
         </explicit>
         <inverse attribute="associated_definition" entity="Component_termination_passage_template_terminal" name="access_mechanisms">
            <inverse.aggregate lower="2" type="SET" upper="?"/>
         </inverse>
      </entity>
      <entity name="Component_termination_passage_template_interface_terminal" supertypes="Component_termination_passage_template_terminal"/>
      <entity name="Component_termination_passage_template_join_terminal" supertypes="Component_termination_passage_template_terminal">
         <explicit name="disallowed_inter_stratum_extent" optional="YES">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Stratum_technology_occurrence_link"/>
         </explicit>
      </entity>
      <entity abstract.supertype="YES" name="Component_termination_passage_template_terminal" super.expression="ONEOF (Component_termination_passage_template_interface_terminal, Component_termination_passage_template_join_terminal)" supertypes="Shape_feature">
         <explicit name="associated_definition">
            <typename name="Component_termination_passage_template"/>
            <redeclaration entity-ref="Shape_element" old_name="containing_shape"/>
         </explicit>
         <explicit name="connection_area" optional="YES">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Connection_zone_in_layout_template"/>
            <redeclaration entity-ref="Shape_feature"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="element_name" entity-ref="Shape_feature"/>
            <unique.attribute attribute="associated_definition"/>
         </unique>
         <where expression="NOT EXISTS(SELF\Shape_element.description)" label="WR1"/>
      </entity>
      <entity name="Connection_zone_in_layout_template" supertypes="Connection_zone Instanced_feature">
         <explicit name="containing_shape">
            <typename name="Template_definition"/>
            <redeclaration entity-ref="Shape_element"/>
         </explicit>
         <explicit name="connection_zone_category" optional="YES">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="land_connection_zone_class"/>
         </explicit>
      </entity>
      <entity name="Continuous_template" supertypes="Geometric_template">
         <where expression="SIZEOF(QUERY(s &lt;* SELF\Geometric_template.shapes |&#xA;          NOT &#xA;          (SIZEOF(['CONSTRUCTIVE_SOLID_GEOMETRY_2D_ARM.SINGLE_AREA_CSG_2D_MODEL'] * TYPEOF(s))&#xA;           = 1)&#xA;          )) = 0" label="WR1"/>
      </entity>
      <entity name="Counterbore_passage_template" supertypes="Unsupported_passage_template">
         <explicit name="smaller_passage">
            <typename name="Unsupported_passage_template"/>
         </explicit>
         <explicit name="larger_passage">
            <typename name="Blind_passage_template"/>
         </explicit>
      </entity>
      <entity name="Countersunk_passage_template" supertypes="Unsupported_passage_template">
         <explicit name="constant_diameter_passage">
            <typename name="Unsupported_passage_template"/>
         </explicit>
         <explicit name="tapered_passage">
            <typename name="Blind_passage_template"/>
         </explicit>
      </entity>
      <entity name="Default_attachment_size_based_land_physical_template" supertypes="Land_physical_template Shape_feature">
         <derived expression="SELF" name="containing_shape">
            <typename name="Default_attachment_size_based_land_physical_template"/>
            <redeclaration entity-ref="Shape_element"/>
         </derived>
         <derived expression="''" name="element_name">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="Shape_feature"/>
         </derived>
         <inverse attribute="associated_definition" entity="Land_template_interface_terminal" name="interface_access_mechanisms">
            <inverse.aggregate lower="1" type="SET" upper="?"/>
         </inverse>
         <where expression="NOT (SIZEOF(interface_access_mechanisms[1].connection_area) &gt; 0) OR &#xA;                 ('minimum attachment region size' IN &#xA;                  list_get_zone_name(interface_access_mechanisms[1].connection_area))" label="WR1"/>
         <where expression="NOT (SIZEOF(interface_access_mechanisms[1].connection_area) &gt; 1) OR &#xA;                 (SIZEOF(['maximum attachment region size','heel area', 'toe area'] * &#xA;                  list_get_zone_name(interface_access_mechanisms[1].connection_area)) = 1)" label="WR2"/>
         <where expression="NOT (SIZEOF(interface_access_mechanisms[1].connection_area) &gt; 2) OR &#xA;                 (SIZEOF(['maximum attachment region size','heel area', 'toe area'] * &#xA;                  list_get_zone_name(interface_access_mechanisms[1].connection_area)) = 2)" label="WR3"/>
         <where expression="NOT (SIZEOF(interface_access_mechanisms[1].connection_area) &gt; 3) OR &#xA;                 (SIZEOF(['maximum attachment region size','heel area', 'toe area'] * &#xA;                  list_get_zone_name(interface_access_mechanisms[1].connection_area)) = 3)" label="WR4"/>
         <where expression="NOT EXISTS(SELF\Shape_feature.connection_area)" label="WR5"/>
         <where expression="SIZEOF(interface_access_mechanisms) = 1" label="WR6"/>
         <where expression="SELF\Shape_element.id = SELF\Item_shape.id" label="WR7"/>
      </entity>
      <entity abstract.supertype="YES" name="Default_passage_based_land_physical_template" super.expression="ONEOF (Default_plated_passage_based_land_physical_template, Default_unsupported_passage_based_land_physical_template)" supertypes="Land_physical_template">
         <explicit name="of_passage_technology">
            <typename name="Passage_technology"/>
         </explicit>
      </entity>
      <entity name="Default_plated_passage_based_land_physical_template" supertypes="Default_passage_based_land_physical_template">
         <where expression="SELF\Default_passage_based_land_physical_template.of_passage_technology.plated_passage = TRUE" label="WR1"/>
         <where expression="NOT('FABRICATION_TECHNOLOGY_ARM.DEFAULT_VIA_DEFINITION' IN TYPEOF(SELF\Default_passage_based_land_physical_template.of_passage_technology)) OR&#xA;&#x9;        (SIZEOF(QUERY(ltjt &lt;* SELF\Land_physical_template.access_mechanisms | NOT('LAYERED_INTERCONNECT_SIMPLE_TEMPLATE_ARM.LAND_TEMPLATE_JOIN_TERMINAL' IN TYPEOF(ltjt)))) = 0)" label="WR2"/>
      </entity>
      <entity name="Default_trace_template" supertypes="Trace_template">
         <explicit name="of_stratum_technology">
            <typename name="Stratum_technology"/>
         </explicit>
      </entity>
      <entity name="Default_unsupported_passage_based_land_physical_template" supertypes="Default_passage_based_land_physical_template">
         <explicit name="of_passage_technology">
            <typename name="Default_unsupported_passage_definition"/>
            <redeclaration entity-ref="Default_passage_based_land_physical_template"/>
         </explicit>
      </entity>
      <entity name="Electrical_isolation_removal_template" supertypes="Material_removal_feature_template">
         <derived expression="get_rvd(SELF, 'electrical requirement')" name="electrical_isolation_spacing_requirement">
            <aggregate lower="0" type="SET" upper="?"/>
            <typename name="Requirement_view_definition"/>
         </derived>
         <where expression="SIZEOF(QUERY(eisr &lt;* electrical_isolation_spacing_requirement |&#xA;         NOT('INTERCONNECT_PHYSICAL_REQUIREMENT_ALLOCATION_ARM.ELECTRICAL_ISOLATION_REQUIREMENT'&#xA;        IN TYPEOF(eisr)) )) =0" label="WR1"/>
         <where expression="SIZEOF(QUERY(s &lt;* SELF\Geometric_template.shapes |&#xA;        ('LAYERED_INTERCONNECT_SIMPLE_TEMPLATE_ARM.PLANAR_CLOSED_PATH_SHAPE_MODEL_WITH_PARAMETERS' IN TYPEOF(s)))) = 0" label="WR2"/>
         <where expression="SIZEOF(QUERY(s &lt;* SELF\Geometric_template.shapes |&#xA;        NOT ('CONSTRUCTIVE_SOLID_GEOMETRY_2D_ARM.SINGLE_BOUNDARY_CSG_2D_MODEL' IN TYPEOF(s)))) = 0" label="WR3"/>
      </entity>
      <entity name="Fill_area_template" supertypes="Parametric_template">
         <explicit name="outer_boundary_style">
            <typename name="Closed_curve_style_parameters"/>
         </explicit>
         <explicit name="inner_boundary_style">
            <typename name="Closed_curve_style_parameters"/>
         </explicit>
      </entity>
      <entity name="Hatch_area_template" supertypes="Fill_area_template">
         <explicit name="fill_boundary_style">
            <typename name="Curve_style_parameters_with_ends"/>
         </explicit>
         <explicit name="hatch_pattern">
            <aggregate lower="1" type="SET" upper="4"/>
            <typename name="Hatch_line_element"/>
         </explicit>
         <explicit name="min_feature" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
      </entity>
      <entity name="Hatch_line_element" supertypes="Detailed_geometric_model_element">
         <explicit name="hatch_line">
            <typename name="Line"/>
         </explicit>
         <explicit name="offset">
            <typename name="Length_data_element"/>
         </explicit>
      </entity>
      <entity name="Inter_stratum_feature_edge_segment_template" supertypes="Inter_stratum_feature_template">
         <explicit name="composed_edge">
            <typename name="Inter_stratum_feature_edge_template"/>
         </explicit>
         <explicit name="start_vertex">
            <typename name="Edge_segment_vertex"/>
         </explicit>
         <explicit name="end_vertex">
            <typename name="Edge_segment_vertex"/>
         </explicit>
         <explicit name="reference_path">
            <typename name="Trimmed_curve"/>
         </explicit>
         <where expression="EXISTS(reference_path.start_point) AND &#xA;            EXISTS(reference_path.end_point)" label="WR1"/>
         <where expression="start_vertex :&lt;&gt;: end_vertex" label="WR2"/>
      </entity>
      <entity name="Inter_stratum_feature_edge_template" supertypes="Inter_stratum_feature_template">
         <inverse attribute="composed_edge" entity="Inter_stratum_feature_edge_segment_template" name="edges">
            <inverse.aggregate lower="2" type="SET" upper="?"/>
         </inverse>
      </entity>
      <entity name="Inter_stratum_feature_template" super.expression="ONEOF (Component_termination_passage_template, Via_template, Unsupported_passage_template, Inter_stratum_feature_edge_segment_template, Inter_stratum_feature_edge_template)" supertypes="Geometric_template">
         <explicit name="of_passage_technology">
            <typename name="Passage_technology"/>
         </explicit>
         <where expression="NOT EXISTS(SELF\Product_view_definition.name)" label="WR1"/>
      </entity>
      <entity name="Land_physical_template" super.expression="ONEOF (Default_passage_based_land_physical_template, Default_attachment_size_based_land_physical_template)" supertypes="Stratum_feature_template">
         <inverse attribute="containing_shape" entity="Land_template_terminal" name="access_mechanisms">
            <inverse.aggregate lower="1" type="SET" upper="?"/>
         </inverse>
         <inverse attribute="item" entity="External_source_identification" name="pre_defined_classification_code">
            <inverse.aggregate lower="0" type="SET" upper="1"/>
         </inverse>
         <where expression="SIZEOF(QUERY(ltjt &lt;* access_mechanisms | ('LAYERED_INTERCONNECT_SIMPLE_TEMPLATE_ARM.LAND_TEMPLATE_JOIN_TERMINAL' IN TYPEOF(ltjt)))) &gt; 0" label="WR1"/>
         <where expression="SIZEOF(QUERY(s &lt;* SELF\Geometric_template.shapes |&#xA;             NOT ('CONSTRUCTIVE_SOLID_GEOMETRY_2D_ARM.SINGLE_AREA_CSG_2D_MODEL' IN TYPEOF(s)))) = 0" label="WR2"/>
         <where expression="SIZEOF(SELF\Geometric_template.shapes) &gt; 0" label="WR3"/>
      </entity>
      <entity name="Land_template_interface_terminal" supertypes="Land_template_terminal">
         <explicit name="associated_definition">
            <typename name="Default_attachment_size_based_land_physical_template"/>
            <redeclaration entity-ref="Land_template_terminal"/>
         </explicit>
      </entity>
      <entity name="Land_template_join_terminal" supertypes="Land_template_terminal"/>
      <entity abstract.supertype="YES" name="Land_template_terminal" super.expression="ONEOF (Land_template_join_terminal, Land_template_interface_terminal)" supertypes="Shape_feature">
         <explicit name="associated_definition">
            <typename name="Land_physical_template"/>
            <redeclaration entity-ref="Shape_element" old_name="containing_shape"/>
         </explicit>
         <explicit name="connection_area" optional="YES">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Connection_zone_in_layout_template"/>
            <redeclaration entity-ref="Shape_feature"/>
         </explicit>
         <explicit name="terminal_category" optional="YES">
            <typename name="land_template_terminal_class"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="element_name" entity-ref="Shape_feature"/>
            <unique.attribute attribute="associated_definition"/>
         </unique>
         <where expression="NOT (EXISTS(SELF\Shape_feature.connection_area)) OR NOT EXISTS(terminal_category)" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Shape_element.description)" label="WR2"/>
         <where expression="SIZEOF(QUERY(ca &lt;* connection_area |&#xA;            NOT(associated_definition = ca\Connection_zone_in_layout_template.containing_shape)&#xA;            )) = 0" label="WR3"/>
      </entity>
      <entity name="Material_removal_feature_template" supertypes="Single_stratum_continuous_template">
         <where expression="NOT EXISTS(SELF\Product_view_definition.name)" label="WR1"/>
         <where expression="SIZEOF(SELF\Geometric_template.shapes) &gt; 0" label="WR2"/>
      </entity>
      <entity abstract.supertype="YES" name="Parametric_template" super.expression="ONEOF (Fill_area_template, Teardrop_template, Trace_template)" supertypes="Template_definition"/>
      <entity name="Part_string_template" supertypes="Template_definition">
         <explicit name="string_alignment">
            <typename name="text_alignment"/>
         </explicit>
      </entity>
      <entity name="Part_text_template" supertypes="Template_definition">
         <explicit name="of_character">
            <typename name="Character_glyph_symbol"/>
         </explicit>
         <where expression="SIZEOF(['CONSTRUCTIVE_SOLID_GEOMETRY_3D_ARM.CSG_MODEL', 'LAYERED_INTERCONNECT_SIMPLE_TEMPLATE_ARM.SOLID_CHARACTER_GLYPH_2D_SYMBOL'] *&#xA;           TYPEOF(of_character)) = 1" label="WR1"/>
      </entity>
      <entity name="Planar_closed_path_shape_model_with_parameters" supertypes="Planar_path_shape_model_with_parameters">
         <where expression="SIZEOF(QUERY(it &lt;* SELF\Representation.items |   &#xA;      ('LAYERED_INTERCONNECT_SIMPLE_TEMPLATE_ARM.CLOSED_PATH_AREA_WITH_PARAMETERS' IN TYPEOF(it))&#xA;                )) = 1" label="WR1"/>
      </entity>
      <entity name="Planar_path_shape_model_with_parameters" supertypes="Single_area_csg_2d_model">
         <where expression="SIZEOF(QUERY(it &lt;* SELF\Representation.items |&#xA;      ('EXTENDED_BASIC_GEOMETRY_ARM.AXIS_PLACEMENT_2D' IN TYPEOF(it))&#xA;                )) = 1" label="WR1"/>
         <where expression="SIZEOF(QUERY(it &lt;* SELF\Representation.items |   &#xA;      ('CONSTRUCTIVE_SOLID_GEOMETRY_2D_ARM.PATH_AREA_WITH_PARAMETERS' IN TYPEOF(it))&#xA;                )) = 1" label="WR2"/>
         <where expression="SIZEOF(SELF\Representation.items) = 2" label="WR3"/>
      </entity>
      <entity name="Plated_passage_template_shape_model" supertypes="Part_template_shape_model">
         <explicit name="shape_characterized_definition">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="ptswp_ptsm_definition"/>
            <redeclaration entity-ref="Part_template_shape_model"/>
         </explicit>
         <explicit name="data_qualification">
            <typename name="ptswp_template_shape_model_context"/>
         </explicit>
         <derived expression="shape_characterized_definition[1]" name="template">
            <typename name="Geometric_template"/>
         </derived>
         <where expression="SIZEOF(QUERY(scd &lt;* shape_characterized_definition |  &#xA;              NOT(scd\Inter_stratum_feature_template.of_passage_technology\Passage_technology.plated_passage)&#xA;              )) = 0" label="WR1"/>
         <where expression="SIZEOF(QUERY(scd &lt;* shape_characterized_definition |&#xA;              NOT(TYPEOF(scd) = TYPEOF(template))&#xA;              )) = 0" label="WR2"/>
      </entity>
      <entity abstract.supertype="YES" name="Single_stratum_continuous_template" super.expression="ONEOF (Stratum_feature_template, Material_removal_feature_template)" supertypes="Continuous_template Single_stratum_template"/>
      <entity abstract.supertype="YES" name="Single_stratum_template" supertypes="Template_definition"/>
      <entity name="Snowball_template" supertypes="Teardrop_template">
         <explicit name="snowball_start_distance">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="snowball_end_distance" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="snowball_initial_spacing" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="snowball_end_spacing" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="snowball_initial_radius">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="snowball_end_radius" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="snowball_quantity_per_trace">
            <builtintype type="INTEGER"/>
         </explicit>
         <where expression="NOT (snowball_quantity_per_trace &gt; 1) OR (EXISTS(snowball_initial_spacing) AND EXISTS(snowball_end_spacing) AND EXISTS(snowball_end_distance) AND EXISTS(snowball_end_radius))" label="WR1"/>
         <where expression="(SIZEOF(USEDIN(SELF,'PART_TEMPLATE_SHAPE_WITH_PARAMETERS_ARM.PART_TEMPLATE_SHAPE_MODEL.' + 'SHAPE_CHARACTERIZED_DEFINITION')) = 0)" label="WR2"/>
         <where expression="NOT EXISTS(SELF\Product_view_definition.additional_characterization)" label="WR3"/>
      </entity>
      <entity name="Solid_character_glyph_2d_symbol" supertypes="Character_glyph_symbol Csg_2d_model"/>
      <entity name="Stratum_feature_template" super.expression="Land_physical_template" supertypes="Single_stratum_continuous_template">
         <where expression="NOT EXISTS(SELF\Product_view_definition.name)" label="WR1"/>
      </entity>
      <entity name="Teardrop_by_angle_template" supertypes="Teardrop_template">
         <explicit name="teardrop_angle">
            <typename name="Angle_data_element"/>
         </explicit>
         <where expression="(SIZEOF(USEDIN(SELF,'PART_TEMPLATE_SHAPE_WITH_PARAMETERS_ARM.PART_TEMPLATE_SHAPE_MODEL.' + 'SHAPE_CHARACTERIZED_DEFINITION')) = 0)" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Product_view_definition.additional_characterization)" label="WR2"/>
      </entity>
      <entity name="Teardrop_by_length_template" supertypes="Teardrop_template">
         <explicit name="teardrop_taper_start_distance">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="teardrop_taper_end_distance">
            <typename name="Length_data_element"/>
         </explicit>
         <where expression="(SIZEOF(USEDIN(SELF,'PART_TEMPLATE_SHAPE_WITH_PARAMETERS_ARM.PART_TEMPLATE_SHAPE_MODEL.' + 'SHAPE_CHARACTERIZED_DEFINITION')) = 0)" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Product_view_definition.additional_characterization)" label="WR2"/>
      </entity>
      <entity name="Teardrop_template" super.expression="ONEOF (Snowball_template, Teardrop_by_angle_template, Teardrop_by_length_template)" supertypes="Parametric_template Single_stratum_template">
         <where expression="(SIZEOF(['LAYERED_INTERCONNECT_SIMPLE_TEMPLATE_ARM.SNOWBALL_TEMPLATE',&#xA;            'LAYERED_INTERCONNECT_SIMPLE_TEMPLATE_ARM.TEARDROP_BY_ANGLE_TEMPLATE',&#xA;            'LAYERED_INTERCONNECT_SIMPLE_TEMPLATE_ARM.TEARDROP_BY_LENGTH_TEMPLATE'] * TYPEOF(SELF)) = 1) XOR&#xA;            ('PRODUCT_IDENTIFICATION_EXTENSION_ARM.EXTERNALLY_DEFINED_VIEW_DEFINITION' IN TYPEOF(SELF))" label="WR1"/>
      </entity>
      <entity name="Tile_area_template" supertypes="Fill_area_template">
         <explicit name="fill_template">
            <typename name="Single_stratum_template"/>
         </explicit>
         <explicit name="start_placement">
            <typename name="Axis_placement_2d"/>
         </explicit>
         <explicit name="first_repeat_factor">
            <typename name="Vector"/>
         </explicit>
         <explicit name="second_repeat_factor">
            <typename name="Vector"/>
         </explicit>
      </entity>
      <entity name="Trace_template" super.expression="Default_trace_template" supertypes="Parametric_template">
         <explicit name="trace_style">
            <typename name="Curve_style_parameters"/>
         </explicit>
         <where expression="NOT EXISTS(SELF\Product_view_definition.name)" label="WR1"/>
      </entity>
      <entity name="Unsupported_passage_template" super.expression="ONEOF (Counterbore_passage_template, Countersunk_passage_template, Blind_passage_template)" supertypes="Continuous_template Inter_stratum_feature_template">
         <explicit name="of_passage_technology">
            <typename name="Default_unsupported_passage_definition"/>
            <redeclaration entity-ref="Inter_stratum_feature_template"/>
         </explicit>
      </entity>
      <entity name="Via_template" supertypes="Continuous_template Inter_stratum_feature_template">
         <explicit name="of_passage_technology">
            <typename name="Default_via_definition"/>
            <redeclaration entity-ref="Inter_stratum_feature_template"/>
         </explicit>
         <inverse attribute="containing_shape" entity="Via_template_terminal" name="access_mechanisms">
            <inverse.aggregate lower="0" type="SET" upper="?"/>
         </inverse>
      </entity>
      <entity name="Via_template_terminal" supertypes="Shape_feature">
         <explicit name="associated_definition">
            <typename name="Via_template"/>
            <redeclaration entity-ref="Shape_element" old_name="containing_shape"/>
         </explicit>
         <explicit name="disallowed_inter_stratum_extent" optional="YES">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Stratum_technology_occurrence_link"/>
         </explicit>
         <explicit name="connection_area" optional="YES">
            <aggregate lower="1" type="SET" upper="?"/>
