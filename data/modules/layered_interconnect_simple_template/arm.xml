      <entity abstract.entity="NO" abstract.supertype="NO" name="Default_unsupported_passage_based_land_physical_template" supertypes="Default_passage_based_land_physical_template">
         <explicit name="of_passage_technology" optional="NO">
            <typename name="Default_unsupported_passage_definition"/>
