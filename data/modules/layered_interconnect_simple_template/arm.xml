      <entity abstract.entity="NO" abstract.supertype="NO" name="Inter_stratum_feature_template" super.expression="ONEOF (Component_termination_passage_template, Via_template, Unsupported_passage_template, Inter_stratum_feature_edge_segment_template, Inter_stratum_feature_edge_template)" supertypes="Geometric_template">
         <explicit name="of_passage_technology" optional="NO">
            <typename name="Passage_technology"/>
         </explicit>
         <where expression="NOT EXISTS(SELF\Product_view_definition.name)" label="WR1"/>
