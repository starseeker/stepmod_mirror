      <entity abstract.entity="NO" abstract.supertype="NO" name="Hatch_line_element" supertypes="Detailed_geometric_model_element">
         <explicit name="hatch_line" optional="NO">
            <typename name="Line"/>
         </explicit>
         <explicit name="offset" optional="NO">
            <typename name="Length_data_element"/>
         </explicit>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Inter_stratum_feature_edge_segment_template" supertypes="Inter_stratum_feature_template">
         <explicit name="composed_edge" optional="NO">
            <typename name="Inter_stratum_feature_edge_template"/>
         </explicit>
         <explicit name="start_vertex" optional="NO">
            <typename name="Edge_segment_vertex"/>
         </explicit>
         <explicit name="end_vertex" optional="NO">
            <typename name="Edge_segment_vertex"/>
         </explicit>
         <explicit name="reference_path" optional="NO">
            <typename name="Trimmed_curve"/>
         </explicit>
         <where expression="EXISTS(reference_path.start_point) AND &#10;            EXISTS(reference_path.end_point)" label="WR1"/>
