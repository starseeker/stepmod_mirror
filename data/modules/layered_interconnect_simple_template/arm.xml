         <where expression="SIZEOF(QUERY(ltjt &lt;* access_mechanisms | ('LAYERED_INTERCONNECT_SIMPLE_TEMPLATE_ARM.LAND_TEMPLATE_JOIN_TERMINAL' IN TYPEOF(ltjt)))) &gt; 0" label="WR1"/>
         <where expression="SIZEOF(QUERY(s &lt;* SELF\Geometric_template.shapes |&#10;             NOT ('CONSTRUCTIVE_SOLID_GEOMETRY_2D_ARM.SINGLE_AREA_CSG_2D_MODEL' IN TYPEOF(s)))) = 0" label="WR2"/>
         <where expression="SIZEOF(SELF\Geometric_template.shapes) &gt; 0" label="WR3"/>
