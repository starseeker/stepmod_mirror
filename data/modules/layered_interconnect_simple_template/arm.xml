      <entity abstract.entity="NO" abstract.supertype="NO" name="Default_attachment_size_based_land_physical_template" supertypes="Land_physical_template Shape_element">
         <derived expression="SELF" name="containing_shape">
            <typename name="Default_attachment_size_based_land_physical_template"/>
            <redeclaration entity-ref="Shape_element"/>
         </derived>
         <derived expression="''" name="element_name">
            <builtintype fixed="NO" type="STRING"/>
            <redeclaration entity-ref="Shape_element"/>
         </derived>
         <derived expression="TRUE" name="product_definitional">
            <builtintype fixed="NO" type="BOOLEAN"/>
            <redeclaration entity-ref="Shape_element"/>
         </derived>
         <inverse attribute="containing_shape" entity="Land_template_interface_terminal" name="interface_access_mechanisms">
            <inverse.aggregate lower="1" type="SET" upper="?"/>
         </inverse>
         <where expression="NOT (SIZEOF(interface_access_mechanisms[1].connection_area) &gt; 0) OR &#10;                 ('minimum attachment region size' IN &#10;                  list_get_zone_name(interface_access_mechanisms[1].connection_area))" label="WR1"/>
         <where expression="NOT (SIZEOF(interface_access_mechanisms[1].connection_area) &gt; 1) OR &#10;                 (SIZEOF(['maximum attachment region size','heel area', 'toe area'] * &#10;                  list_get_zone_name(interface_access_mechanisms[1].connection_area)) = 1)" label="WR2"/>
         <where expression="NOT (SIZEOF(interface_access_mechanisms[1].connection_area) &gt; 2) OR &#10;                 (SIZEOF(['maximum attachment region size','heel area', 'toe area'] * &#10;                  list_get_zone_name(interface_access_mechanisms[1].connection_area)) = 2)" label="WR3"/>
         <where expression="NOT (SIZEOF(interface_access_mechanisms[1].connection_area) &gt; 3) OR &#10;                 (SIZEOF(['maximum attachment region size','heel area', 'toe area'] * &#10;                  list_get_zone_name(interface_access_mechanisms[1].connection_area)) = 3)" label="WR4"/>
         <where expression="SIZEOF(interface_access_mechanisms) = 1" label="WR5"/>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="YES" name="Default_passage_based_land_physical_template" super.expression="ONEOF (Default_plated_passage_based_land_physical_template, Default_unsupported_passage_based_land_physical_template)" supertypes="Land_physical_template">
         <explicit name="of_passage_technology" optional="NO">
            <typename name="Passage_technology"/>
         </explicit>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Default_plated_passage_based_land_physical_template" supertypes="Default_passage_based_land_physical_template">
         <where expression="SELF\Default_passage_based_land_physical_template.of_passage_technology.plated_passage = TRUE" label="WR1"/>
         <where expression="NOT('FABRICATION_TECHNOLOGY_ARM.DEFAULT_VIA_DEFINITION' IN TYPEOF(SELF\Default_passage_based_land_physical_template.of_passage_technology)) OR&#10;&#9;        (SIZEOF(QUERY(ltjt &lt;* SELF\Land_physical_template.access_mechanisms | NOT('LAYERED_INTERCONNECT_SIMPLE_TEMPLATE_ARM.LAND_TEMPLATE_JOIN_TERMINAL' IN TYPEOF(ltjt)))) = 0)" label="WR2"/>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Default_trace_template" supertypes="Trace_template">
