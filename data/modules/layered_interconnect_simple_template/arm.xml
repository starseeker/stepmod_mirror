         <where expression="NOT EXISTS(SELF\Product_view_definition.additional_characterization)" label="WR2"/>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Teardrop_by_length_template" supertypes="Teardrop_template">
         <explicit name="teardrop_taper_start_distance" optional="NO">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="teardrop_taper_end_distance" optional="NO">
            <typename name="Length_data_element"/>
         </explicit>
         <where expression="(SIZEOF(USEDIN(SELF,'PART_TEMPLATE_SHAPE_WITH_PARAMETERS_ARM.PART_TEMPLATE_SHAPE_MODEL.' + 'SHAPE_CHARACTERIZED_DEFINITION')) = 0)" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Product_view_definition.additional_characterization)" label="WR2"/>
