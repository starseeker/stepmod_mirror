      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Planar_path_shape_model_with_parameters" supertypes="Single_area_csg_2d_model">
         <where expression="SIZEOF(QUERY(it &lt;* SELF\Representation.items |&#10;      ('EXTENDED_BASIC_GEOMETRY_ARM.AXIS_PLACEMENT_2D' IN TYPEOF(it))&#10;                )) = 1" label="WR1"/>
         <where expression="SIZEOF(QUERY(it &lt;* SELF\Representation.items |   &#10;      ('CONSTRUCTIVE_SOLID_GEOMETRY_2D_ARM.PATH_AREA_WITH_PARAMETERS' IN TYPEOF(it))&#10;                )) = 1" label="WR2"/>
         <where expression="SIZEOF(SELF\Representation.items) = 2" label="WR3"/>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Plated_passage_template_shape_model" supertypes="Part_template_shape_model">
         <explicit name="shape_characterized_definition" optional="NO">
            <aggregate lower="1" optional="NO" type="SET" unique="NO" upper="?"/>
            <typename name="ptswp_ptsm_definition"/>
            <redeclaration entity-ref="Part_template_shape_model"/>
         </explicit>
         <explicit name="data_qualification" optional="NO">
            <typename name="ptswp_template_shape_model_context"/>
         </explicit>
         <derived expression="shape_characterized_definition[1]" name="template">
            <typename name="Geometric_template"/>
         </derived>
         <where expression="SIZEOF(QUERY(scd &lt;* shape_characterized_definition |  &#10;              NOT(scd\Inter_stratum_feature_template.of_passage_technology\Passage_technology.plated_passage)&#10;              )) = 0" label="WR1"/>
         <where expression="SIZEOF(QUERY(scd &lt;* shape_characterized_definition |&#10;              NOT(TYPEOF(scd) = TYPEOF(template))&#10;              )) = 0" label="WR2"/>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="YES" name="Single_stratum_continuous_template" super.expression="ONEOF (Stratum_feature_template, Material_removal_feature_template)" supertypes="Continuous_template Single_stratum_template"/>
      <entity abstract.entity="NO" abstract.supertype="YES" name="Single_stratum_template" supertypes="Template_definition"/>
