      <entity abstract.entity="NO" abstract.supertype="NO" name="Inter_stratum_feature_edge_template" supertypes="Inter_stratum_feature_template">
         <inverse attribute="composed_edge" entity="Inter_stratum_feature_edge_segment_template" name="edges">
