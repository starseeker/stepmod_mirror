<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="layered_interconnect_simple_template_arm schema_instance"/>
   <schema name="Layered_interconnect_simple_template_arm">
      <interface kind="use" schema="Layered_2d_shape_arm"/>
      <interface kind="use" schema="Fill_area_style_arm"/>
      <interface kind="use" schema="Fabrication_technology_arm"/>
      <interface kind="use" schema="Text_representation_arm"/>
      <interface kind="use" schema="Part_template_shape_with_parameters_arm"/>
      <interface kind="reference" schema="Characterizable_object_arm">
         <interfaced.item name="bag_to_set"/>
      </interface>
      <type name="extend_or_chord_2_extend_or_truncate_or_round">
         <enumeration items="truncate chord_2_extend round extend" extensible="NO"/>
      </type>
      <type name="extend_or_truncate">
         <enumeration items="truncate extend" extensible="NO"/>
      </type>
      <type name="land_connection_zone_class">
         <enumeration items="surface_point_class edge_area_class edge_curve_class edge_point_class surface_area_class" extensible="NO"/>
      </type>
      <type name="land_template_terminal_class">
         <enumeration items="surface_point_class edge_area_class edge_curve_class edge_point_class surface_area_class" extensible="NO"/>
      </type>
      <type name="list_external_identification_item">
         <select extensible="YES" genericentity="YES" basedon="external_identification_item" selectitems="Land_physical_template"/>
      </type>
      <type name="list_usage_concept">
         <select extensible="YES" genericentity="YES" basedon="usage_concept" selectitems="Land_template_terminal"/>
      </type>
      <type name="ptswp_ptsm_definition">
         <select selectitems="Via_template Component_termination_passage_template" genericentity="NO" extensible="NO"/>
      </type>
      <type name="ptswp_template_shape_model_context">
         <enumeration items="drilled_passage_context finished_passage_context unknown_context" extensible="NO"/>
      </type>
      <type name="square_or_round">
         <enumeration items="square round" extensible="NO"/>
      </type>
      <entity name="Blind_passage_template" supertypes="Unsupported_passage_template" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="bottom_distance_from_datum_reference" optional="NO">
            <typename name="Shape_dimension_representation"/>
         </explicit>
         <explicit name="datum_reference_layer" optional="NO">
            <typename name="Stratum_technology_occurrence"/>
         </explicit>
         <explicit name="datum_reference_side" optional="NO">
            <typename name="Stratum_technology_occurrence_link"/>
         </explicit>
         <explicit name="of_measure_orientation" optional="NO">
            <typename name="measure_orientation"/>
         </explicit>
      </entity>
      <entity name="Closed_curve_style_parameters" supertypes="Curve_style_parameters" abstract.supertype="NO" abstract.entity="NO"/>
      <entity name="Closed_path_area_with_parameters" supertypes="Path_area_with_parameters" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="centreline" optional="NO">
            <typename name="Closed_curve"/>
            <redeclaration entity-ref="Path_area_with_parameters"/>
         </explicit>
         <explicit name="parameters" optional="NO">
            <typename name="Closed_curve_style_parameters"/>
            <redeclaration entity-ref="Path_area_with_parameters"/>
         </explicit>
      </entity>
      <entity name="Component_termination_passage_template" supertypes="Continuous_template Inter_stratum_feature_template" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="of_passage_technology" optional="NO">
            <typename name="Default_component_termination_passage_definition"/>
            <redeclaration entity-ref="Inter_stratum_feature_template"/>
         </explicit>
         <inverse name="access_mechanisms" entity="Component_termination_passage_template_terminal" attribute="associated_definition">
            <inverse.aggregate type="SET" lower="2" upper="?"/>
         </inverse>
      </entity>
      <entity name="Component_termination_passage_template_interface_terminal" supertypes="Component_termination_passage_template_terminal" abstract.supertype="NO" abstract.entity="NO"/>
      <entity name="Component_termination_passage_template_join_terminal" supertypes="Component_termination_passage_template_terminal" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="disallowed_inter_stratum_extent" optional="YES">
            <aggregate type="SET" lower="1" upper="?" unique="NO" optional="NO"/>
            <typename name="Stratum_technology_occurrence_link"/>
         </explicit>
      </entity>
      <entity name="Component_termination_passage_template_terminal" abstract.supertype="YES" supertypes="Shape_feature" super.expression="ONEOF (Component_termination_passage_template_interface_terminal, Component_termination_passage_template_join_terminal)" abstract.entity="NO">
         <explicit name="associated_definition" optional="NO">
            <typename name="Component_termination_passage_template"/>
            <redeclaration entity-ref="Shape_element" old_name="containing_shape"/>
         </explicit>
         <explicit name="connection_area" optional="YES">
            <aggregate type="SET" lower="1" upper="?" unique="NO" optional="NO"/>
            <typename name="Connection_zone_in_layout_template"/>
            <redeclaration entity-ref="Shape_feature"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute entity-ref="Shape_feature" attribute="element_name"/>
            <unique.attribute attribute="associated_definition"/>
         </unique>
         <where label="WR1" expression="NOT EXISTS(SELF\Shape_element.description)"/>
      </entity>
      <entity name="Connection_zone_in_layout_template" supertypes="Connection_zone Instanced_feature" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="containing_shape" optional="NO">
            <typename name="Template_definition"/>
            <redeclaration entity-ref="Shape_element"/>
         </explicit>
         <explicit name="connection_zone_category" optional="YES">
            <aggregate type="SET" lower="1" upper="?" unique="NO" optional="NO"/>
            <typename name="land_connection_zone_class"/>
         </explicit>
      </entity>
      <entity name="Continuous_template" supertypes="Geometric_template" abstract.supertype="NO" abstract.entity="NO">
         <where label="WR1" expression="SIZEOF(QUERY(s &lt;* SELF\Geometric_template.shapes |&#10;          NOT &#10;          (SIZEOF(['CONSTRUCTIVE_SOLID_GEOMETRY_2D_ARM.SINGLE_AREA_CSG_2D_MODEL'] * TYPEOF(s))&#10;           = 1)&#10;          )) = 0"/>
      </entity>
      <entity name="Counterbore_passage_template" supertypes="Unsupported_passage_template" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="smaller_passage" optional="NO">
            <typename name="Unsupported_passage_template"/>
         </explicit>
         <explicit name="larger_passage" optional="NO">
            <typename name="Blind_passage_template"/>
         </explicit>
      </entity>
      <entity name="Countersunk_passage_template" supertypes="Unsupported_passage_template" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="constant_diameter_passage" optional="NO">
            <typename name="Unsupported_passage_template"/>
         </explicit>
         <explicit name="tapered_passage" optional="NO">
            <typename name="Blind_passage_template"/>
         </explicit>
      </entity>
      <entity name="Curve_style_parameters" abstract.supertype="YES" super.expression="ONEOF (Closed_curve_style_parameters, Curve_style_parameters_with_ends)" abstract.entity="NO">
         <explicit name="name" optional="NO">
            <builtintype type="STRING" fixed="NO"/>
         </explicit>
         <explicit name="corner_style" optional="NO">
            <typename name="extend_or_chord_2_extend_or_truncate_or_round"/>
         </explicit>
         <explicit name="curve_width" optional="NO">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="width_uncertainty" optional="NO">
            <typename name="Length_data_element"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="name"/>
         </unique>
      </entity>
      <entity name="Curve_style_parameters_with_ends" supertypes="Curve_style_parameters" abstract.supertype="NO" abstract.entity="NO">

         <explicit name="end_extension" optional="NO">
            <typename name="extend_or_truncate"/>
         </explicit>
         <explicit name="end_style" optional="NO">
            <typename name="square_or_round"/>
         </explicit>
      </entity>
      <entity name="Default_attachment_size_based_land_physical_template" supertypes="Land_physical_template Shape_feature" abstract.supertype="NO" abstract.entity="NO">
         <derived name="containing_shape" expression="SELF">
            <typename name="Default_attachment_size_based_land_physical_template"/>
            <redeclaration entity-ref="Shape_element"/>
         </derived>
         <derived name="element_name" expression="''">
            <builtintype type="STRING" fixed="NO"/>
            <redeclaration entity-ref="Shape_feature"/>
         </derived>
         <inverse name="interface_access_mechanisms" entity="Land_template_interface_terminal" attribute="associated_definition">
            <inverse.aggregate type="SET" lower="1" upper="?"/>
         </inverse>
         <where label="WR1" expression="NOT (SIZEOF(interface_access_mechanisms[1].connection_area) &gt; 0) OR &#10;                 ('minimum attachment region size' IN &#10;                  list_get_zone_name(interface_access_mechanisms[1].connection_area))"/>
         <where label="WR2" expression="NOT (SIZEOF(interface_access_mechanisms[1].connection_area) &gt; 1) OR &#10;                 (SIZEOF(['maximum attachment region size','heel area', 'toe area'] * &#10;                  list_get_zone_name(interface_access_mechanisms[1].connection_area)) = 1)"/>
         <where label="WR3" expression="NOT (SIZEOF(interface_access_mechanisms[1].connection_area) &gt; 2) OR &#10;                 (SIZEOF(['maximum attachment region size','heel area', 'toe area'] * &#10;                  list_get_zone_name(interface_access_mechanisms[1].connection_area)) = 2)"/>
         <where label="WR4" expression="NOT (SIZEOF(interface_access_mechanisms[1].connection_area) &gt; 3) OR &#10;                 (SIZEOF(['maximum attachment region size','heel area', 'toe area'] * &#10;                  list_get_zone_name(interface_access_mechanisms[1].connection_area)) = 3)"/>
         <where label="WR5" expression="NOT EXISTS(SELF\Shape_feature.connection_area)"/>
         <where label="WR6" expression="SIZEOF(interface_access_mechanisms) = 1"/>
         <where label="WR7" expression="SELF\Shape_element.id = SELF\Item_shape.id"/>
      </entity>
      <entity name="Default_passage_based_land_physical_template" abstract.supertype="YES" supertypes="Land_physical_template" super.expression="ONEOF (Default_plated_passage_based_land_physical_template, Default_unsupported_passage_based_land_physical_template)" abstract.entity="NO">
         <explicit name="of_passage_technology" optional="NO">
            <typename name="Passage_technology"/>
         </explicit>
      </entity>
      <entity name="Default_plated_passage_based_land_physical_template" supertypes="Default_passage_based_land_physical_template" abstract.supertype="NO" abstract.entity="NO">
         <where label="WR1" expression="SELF\Default_passage_based_land_physical_template.of_passage_technology.plated_passage = TRUE"/>
         <where label="WR2" expression="NOT('LAYERED_INTERCONNECT_SIMPLE_TEMPLATE_ARM.VIA_TEMPLATE' IN TYPEOF(SELF\Default_passage_based_land_physical_template.of_passage_technology)) OR&#10;         (SIZEOF(QUERY(ltjt &lt;* SELF\Land_physical_template.access_mechanisms | NOT('LAYERED_INTERCONNECT_SIMPLE_TEMPLATE_ARM.LAND_TEMPLATE_JOIN_TERMINAL' IN TYPEOF(ltjt)))) = 0)"/>
      </entity>
      <entity name="Default_trace_template" supertypes="Trace_template" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="of_stratum_technology" optional="NO">
            <typename name="Stratum_technology"/>
         </explicit>
      </entity>
      <entity name="Default_unsupported_passage_based_land_physical_template" supertypes="Default_passage_based_land_physical_template" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="of_passage_technology" optional="NO">
            <typename name="Default_unsupported_passage_definition"/>
            <redeclaration entity-ref="Default_passage_based_land_physical_template"/>
         </explicit>
      </entity>
      <entity name="Fill_area_template" supertypes="Parametric_template" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="outer_boundary_style" optional="NO">
            <typename name="Closed_curve_style_parameters"/>
         </explicit>
         <explicit name="inner_boundary_style" optional="NO">
            <typename name="Closed_curve_style_parameters"/>
         </explicit>
      </entity>
      <entity name="Hatch_area_template" supertypes="Fill_area_template" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="fill_boundary_style" optional="NO">
            <typename name="Curve_style_parameters_with_ends"/>
         </explicit>
         <explicit name="hatch_pattern" optional="NO">
            <aggregate type="SET" lower="1" upper="4" unique="NO" optional="NO"/>
            <typename name="Hatch_line_element"/>
         </explicit>
         <explicit name="min_feature" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
      </entity>
      <entity name="Hatch_line_element" supertypes="Detailed_geometric_model_element" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="hatch_line" optional="NO">
            <typename name="Line"/>
         </explicit>
         <explicit name="offset" optional="NO">
            <typename name="Length_data_element"/>
         </explicit>
      </entity>
      <entity name="Inter_stratum_feature_edge_segment_template" supertypes="Inter_stratum_feature_template" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="composed_edge" optional="NO">
            <typename name="Inter_stratum_feature_edge_template"/>
         </explicit>
         <explicit name="start_vertex" optional="NO">
            <typename name="Edge_segment_vertex"/>
         </explicit>
         <explicit name="end_vertex" optional="NO">
            <typename name="Edge_segment_vertex"/>
         </explicit>
         <explicit name="reference_path" optional="NO">
            <typename name="Trimmed_curve"/>
         </explicit>
         <where label="WR1" expression="EXISTS(reference_path.start_point) AND &#10;            EXISTS(reference_path.end_point)"/>
         <where label="WR2" expression="start_vertex :&lt;&gt;: end_vertex"/>
      </entity>
      <entity name="Inter_stratum_feature_edge_template" supertypes="Inter_stratum_feature_template" abstract.supertype="NO" abstract.entity="NO">
         <inverse name="edges" entity="Inter_stratum_feature_edge_segment_template" attribute="composed_edge">
            <inverse.aggregate type="SET" lower="2" upper="?"/>
         </inverse>
      </entity>
      <entity name="Inter_stratum_feature_template" supertypes="Geometric_template" super.expression="ONEOF (Component_termination_passage_template, Via_template, Unsupported_passage_template, Inter_stratum_feature_edge_segment_template, Inter_stratum_feature_edge_template)" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="of_passage_technology" optional="NO">
            <typename name="Passage_technology"/>
         </explicit>
         <where label="WR1" expression="NOT EXISTS(SELF\Product_view_definition.name)"/>
      </entity>
      <entity name="Land_physical_template" supertypes="Stratum_feature_template" super.expression="ONEOF (Default_passage_based_land_physical_template, Default_attachment_size_based_land_physical_template)" abstract.supertype="NO" abstract.entity="NO">
         <inverse name="access_mechanisms" entity="Land_template_terminal" attribute="containing_shape">
            <inverse.aggregate type="SET" lower="1" upper="?"/>
         </inverse>
         <inverse name="pre_defined_classification_code" entity="External_source_identification" attribute="item">
            <inverse.aggregate type="SET" lower="0" upper="1"/>
         </inverse>
         <where label="WR1" expression="SIZEOF(QUERY(ltjt &lt;* access_mechanisms | ('LAYERED_INTERCONNECT_SIMPLE_TEMPLATE_ARM.LAND_TEMPLATE_JOIN_TERMINAL' IN TYPEOF(ltjt)))) &gt; 0"/>
         <where label="WR2" expression="SIZEOF(QUERY(s &lt;* SELF\Geometric_template.shapes |&#10;             NOT ('CONSTRUCTIVE_SOLID_GEOMETRY_2D_ARM.SINGLE_AREA_CSG_2D_MODEL' IN TYPEOF(s)))) = 0"/>
         <where label="WR3" expression="SIZEOF(SELF\Geometric_template.shapes) &gt; 0"/>
      </entity>
      <entity name="Land_template_interface_terminal" supertypes="Land_template_terminal" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="associated_definition" optional="NO">
            <typename name="Default_attachment_size_based_land_physical_template"/>
            <redeclaration entity-ref="Land_template_terminal"/>
         </explicit>
      </entity>
      <entity name="Land_template_join_terminal" supertypes="Land_template_terminal" abstract.supertype="NO" abstract.entity="NO"/>
      <entity name="Land_template_terminal" abstract.supertype="YES" supertypes="Shape_feature" super.expression="ONEOF (Land_template_join_terminal, Land_template_interface_terminal)" abstract.entity="NO">
         <explicit name="associated_definition" optional="NO">
            <typename name="Land_physical_template"/>
            <redeclaration entity-ref="Shape_element" old_name="containing_shape"/>
         </explicit>
         <explicit name="connection_area" optional="YES">
            <aggregate type="SET" lower="1" upper="?" unique="NO" optional="NO"/>
            <typename name="Connection_zone_in_layout_template"/>
            <redeclaration entity-ref="Shape_feature"/>
         </explicit>
         <explicit name="terminal_category" optional="YES">
            <typename name="land_template_terminal_class"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute entity-ref="Shape_feature" attribute="element_name"/>
            <unique.attribute attribute="associated_definition"/>
         </unique>
         <where label="WR1" expression="NOT (EXISTS(SELF\Shape_feature.connection_area)) OR NOT EXISTS(terminal_category)"/>
         <where label="WR2" expression="NOT EXISTS(SELF\Shape_element.description)"/>
         <where label="WR3" expression="SIZEOF(QUERY(ca &lt;* connection_area |&#10;            NOT(associated_definition = ca\Connection_zone_in_layout_template.containing_shape)&#10;            )) = 0"/>
      <where label="IP1"/>
         <where label="IP2"/>
      </entity>
      <entity name="Material_removal_feature_template" supertypes="Single_stratum_continuous_template" abstract.supertype="NO" abstract.entity="NO">
         <where label="WR1" expression="NOT EXISTS(SELF\Product_view_definition.name)"/>
         <where label="WR2" expression="SIZEOF(SELF\Geometric_template.shapes) &gt; 0"/>
      <where label="IP1"/>
      </entity>
      <entity name="Parametric_template" abstract.supertype="YES" supertypes="Template_definition" super.expression="ONEOF (Fill_area_template, Teardrop_template, Trace_template)" abstract.entity="NO"/>
      <entity name="Part_string_template" supertypes="Template_definition" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="string_alignment" optional="NO">
            <typename name="text_alignment"/>
         </explicit>
      </entity>
      <entity name="Part_text_template" supertypes="Template_definition" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="of_character" optional="NO">
            <typename name="Character_glyph_symbol"/>
         </explicit>
         <where label="WR1" expression="SIZEOF(['CONSTRUCTIVE_SOLID_GEOMETRY_3D_ARM.CSG_MODEL', 'LAYERED_INTERCONNECT_SIMPLE_TEMPLATE_ARM.SOLID_CHARACTER_GLYPH_2D_SYMBOL'] *&#10;           TYPEOF(of_character)) = 1"/>
      </entity>
      <entity name="Path_area_with_parameters" supertypes="Detailed_geometric_model_element" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="centreline" optional="NO">
            <typename name="Curve"/>
         </explicit>
         <explicit name="parameters" optional="NO">
            <typename name="Curve_style_parameters"/>
         </explicit>
      </entity>
      <entity name="Planar_closed_path_shape_model_with_parameters" supertypes="Planar_path_shape_model_with_parameters" abstract.supertype="NO" abstract.entity="NO">
         <where label="WR1" expression="SIZEOF(QUERY(it &lt;* SELF\Representation.items |   &#10;      ('LAYERED_INTERCONNECT_SIMPLE_TEMPLATE_ARM.CLOSED_PATH_AREA_WITH_PARAMETERS' IN TYPEOF(it))&#10;                )) = 1"/>
      </entity>
      <entity name="Planar_path_shape_model_with_parameters" supertypes="Single_area_csg_2d_model" abstract.supertype="NO" abstract.entity="NO">
         <where label="WR1" expression="SIZEOF(QUERY(it &lt;* SELF\Representation.items |&#10;      ('EXTENDED_BASIC_GEOMETRY_ARM.AXIS_PLACEMENT_2D' IN TYPEOF(it))&#10;                )) = 1"/>
         <where label="WR2" expression="SIZEOF(QUERY(it &lt;* SELF\Representation.items |   &#10;      ('LAYERED_INTERCONNECT_SIMPLE_TEMPLATE_ARM.PATH_AREA_WITH_PARAMETERS' IN TYPEOF(it))&#10;                )) = 1"/>
         <where label="WR3" expression="SIZEOF(SELF\Representation.items) = 2"/>
      </entity>
      <entity name="Plated_passage_template_shape_model" supertypes="Part_template_shape_model" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="shape_characterized_definition" optional="NO">
            <aggregate type="SET" lower="1" upper="?" unique="NO" optional="NO"/>
            <typename name="ptswp_ptsm_definition"/>
            <redeclaration entity-ref="Part_template_shape_model"/>
         </explicit>
         <explicit name="data_qualification" optional="NO">
            <typename name="ptswp_template_shape_model_context"/>
         </explicit>
         <derived name="template" expression="shape_characterized_definition[1]">
            <typename name="Geometric_template"/>
         </derived>
         <where label="WR1" expression="SIZEOF(QUERY(scd &lt;* shape_characterized_definition |  &#10;              NOT(scd\Inter_stratum_feature_template.of_passage_technology\Passage_technology.plated_passage)&#10;              )) = 0"/>
         <where label="WR2" expression="SIZEOF(QUERY(scd &lt;* shape_characterized_definition |&#10;              NOT(TYPEOF(scd) = TYPEOF(template))&#10;              )) = 0"/>
      </entity>
      <entity name="Single_stratum_continuous_template" abstract.supertype="YES" supertypes="Continuous_template Single_stratum_template" super.expression="ONEOF (Stratum_feature_template, Material_removal_feature_template)" abstract.entity="NO"/>
      <entity name="Single_stratum_template" abstract.supertype="YES" supertypes="Template_definition" abstract.entity="NO"/>
      <entity name="Snowball_template" supertypes="Teardrop_template" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="snowball_start_distance" optional="NO">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="snowball_end_distance" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="snowball_initial_spacing" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="snowball_end_spacing" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="snowball_initial_radius" optional="NO">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="snowball_end_radius" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="snowball_quantity_per_trace" optional="NO">
            <builtintype type="INTEGER" fixed="NO"/>
         </explicit>
         <where label="WR1" expression="NOT (snowball_quantity_per_trace &gt; 1) OR (EXISTS(snowball_initial_spacing) AND EXISTS(snowball_end_spacing) AND EXISTS(snowball_end_distance) AND EXISTS(snowball_end_radius))"/>
         <where label="WR2" expression="(SIZEOF(USEDIN(SELF,'PART_TEMPLATE_SHAPE_WITH_PARAMETERS_ARM.PART_TEMPLATE_SHAPE_MODEL.' + 'SHAPE_CHARACTERIZED_DEFINITION')) = 0)"/>
         <where label="WR3" expression="NOT EXISTS(SELF\Product_view_definition.additional_characterization)"/>
      </entity>
      <entity name="Solid_character_glyph_2d_symbol" supertypes="Character_glyph_symbol Csg_2d_model" abstract.supertype="NO" abstract.entity="NO"/>
      <entity name="Stratum_feature_template" supertypes="Single_stratum_continuous_template" super.expression="Land_physical_template" abstract.supertype="NO" abstract.entity="NO">
         <where label="WR1" expression="NOT EXISTS(SELF\Product_view_definition.name)"/>
      </entity>
      <entity name="Teardrop_by_angle_template" supertypes="Teardrop_template" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="teardrop_angle" optional="NO">
            <typename name="Angle_data_element"/>
         </explicit>
         <where label="WR1" expression="(SIZEOF(USEDIN(SELF,'PART_TEMPLATE_SHAPE_WITH_PARAMETERS_ARM.PART_TEMPLATE_SHAPE_MODEL.' + 'SHAPE_CHARACTERIZED_DEFINITION')) = 0)"/>
         <where label="WR2" expression="NOT EXISTS(SELF\Product_view_definition.additional_characterization)"/>
      </entity>
      <entity name="Teardrop_by_length_template" supertypes="Teardrop_template" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="teardrop_taper_start_distance" optional="NO">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="teardrop_taper_end_distance" optional="NO">
            <typename name="Length_data_element"/>
         </explicit>
