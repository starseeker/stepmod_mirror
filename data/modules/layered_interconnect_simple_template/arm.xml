      <entity abstract.entity="NO" abstract.supertype="YES" name="Land_template_terminal" super.expression="ONEOF (Land_template_join_terminal, Land_template_interface_terminal)" supertypes="General_part_feature">
         <explicit name="containing_shape" optional="NO">
            <typename name="Land_physical_template"/>
            <redeclaration entity-ref="Shape_element"/>
         </explicit>
         <explicit name="connection_area" optional="YES">
            <aggregate lower="1" optional="NO" type="SET" unique="NO" upper="?"/>
            <typename name="Connection_zone_in_layout_template"/>
         </explicit>
         <explicit name="terminal_category" optional="YES">
            <typename name="land_template_terminal_class"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="SELF\Shape_element.element_name"/>
            <unique.attribute attribute="containing_shape"/>
         </unique>
         <where expression="NOT (EXISTS(connection_area)) OR NOT EXISTS(terminal_category)" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Shape_element.description)" label="WR2"/>
         <where expression="SIZEOF(QUERY(ca &lt;* connection_area |&#10;            NOT(containing_shape = ca\Shape_element.containing_shape)&#10;            )) = 0" label="WR3"/>
      <where label="IP1"/>
         <where label="IP2"/>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Material_removal_feature_template" supertypes="Single_stratum_continuous_template">
         <where expression="NOT EXISTS(SELF\Product_view_definition.name)" label="WR1"/>
         <where expression="SIZEOF(SELF\Geometric_template.shapes) &gt; 0" label="WR2"/>
      <where label="IP1"/>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="YES" name="Parametric_template" super.expression="ONEOF (Fill_area_template, Teardrop_template, Trace_template)" supertypes="Template_definition"/>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Part_string_template" supertypes="Template_definition">
