      <entity abstract.entity="NO" abstract.supertype="NO" name="Stratum_feature_template" super.expression="Land_physical_template" supertypes="Single_stratum_continuous_template">
         <where expression="NOT EXISTS(SELF\Product_view_definition.name)" label="WR1"/>
      </entity>
