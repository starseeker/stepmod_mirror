<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" 
  href="../../../xsl/express.xsl"
?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express language_version="2" rcs.date="$Date: $" rcs.revision="$Revision: $" description.file="arm_descriptions.xml">
	<application name="GraphicalEXPRESS" owner="Eurostep" url="www.eurostep.com" version="1.3.16" source="Location_arm_v018.vsd"/>
	<schema name="Location_arm">
		<interface kind="use" schema="Classification_assignment_arm">
</interface>
		<interface kind="use" schema="Person_organization_arm">
</interface>
		<interface kind="use" schema="Product_identification_arm">
</interface>
		<interface kind="use" schema="Product_version_arm">
</interface>
		<interface kind="use" schema="Value_with_unit_arm">
</interface>
		<type name="location_classification_item">
			<select extensible="YES" genericentity="YES" basedon="classification_item" selectitems="Location Location_relationship"/>
			<graphic.element page="2" xcoord="467" ycoord="786"/>
		</type>
		<type name="product_based_location_representation">
			<select extensible="YES" genericentity="YES" selectitems="Product Product_version"/>
			<graphic.element page="2" xcoord="170" ycoord="276"/>
		</type>
		<entity name="Address_based_location_representation" supertypes="Location_representation">
			<explicit name="postal_address">
				<typename name="Address"/>
			</explicit>
			<graphic.element page="2" xcoord="170" ycoord="333"/>
		</entity>
		<entity name="Global_location_representation" supertypes="Location_representation">
			<explicit name="altitude" optional="YES">
				<typename name="Value_with_unit"/>
			</explicit>
			<explicit name="geographical_area" optional="YES">
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="latitude">
				<typename name="Value_with_unit"/>
			</explicit>
			<explicit name="longitude">
				<typename name="Value_with_unit"/>
			</explicit>
			<graphic.element page="2" xcoord="170" ycoord="588"/>
		</entity>
		<entity name="Location">
			<explicit name="name">
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="description" optional="YES">
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="alternative_location_representations">
				<aggregate lower="0" upper="?" type="SET">
					<typename name="location_representation"/>
				</aggregate>
			</explicit>
			<graphic.element page="2" xcoord="205" ycoord="715"/>
		</entity>
		<entity name="Location_relationship">
			<explicit name="name">
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="description" optional="YES">
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="relating">
				<typename name="Location"/>
			</explicit>
			<explicit name="related">
				<typename name="Location"/>
			</explicit>
			<graphic.element page="2" xcoord="205" ycoord="786"/>
		</entity>
		<entity name="Location_representation" abstract.supertype="YES" super.expression="ONEOF(Address_based_location_representation, Global_location_representation, Organization_based_location_representation, Product_based_location_identification, Regional_grid_location_representation)">
			<graphic.element page="2" xcoord="141" ycoord="644"/>
		</entity>
		<entity name="Organization_based_location_representation" supertypes="Location_representation">
			<explicit name="location_identifications">
				<aggregate lower="0" upper="?" type="LIST">
					<typename name="Organizational_location_identification"/>
				</aggregate>
			</explicit>
			<explicit name="organization_for_location">
				<typename name="Organization"/>
			</explicit>
			<graphic.element page="2" xcoord="170" ycoord="460"/>
		</entity>
		<entity name="Organizational_location_identification">
			<explicit name="identification_type">
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="location_value">
				<builtintype type="STRING"/>
			</explicit>
			<graphic.element page="2" xcoord="170" ycoord="389"/>
		</entity>
		<entity name="Product_based_location_identification" supertypes="Location_representation">
			<explicit name="location_identification">
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="location_name" optional="YES">
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="referenced_product">
				<typename name="product_based_location_representation"/>
			</explicit>
			<graphic.element page="2" xcoord="170" ycoord="219"/>
		</entity>
		<entity name="Regional_coordinate">
			<explicit name="name">
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="coordinate_value">
				<typename name="Value_with_unit"/>
			</explicit>
			<explicit name="grid_system">
				<typename name="Regional_grid_location_representation"/>
			</explicit>
			<graphic.element page="2" xcoord="467" ycoord="106"/>
		</entity>
		<entity name="Regional_grid_location_representation" supertypes="Location_representation">
			<explicit name="name">
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="description" optional="YES">
				<builtintype type="STRING"/>
			</explicit>
			<graphic.element page="2" xcoord="170" ycoord="106"/>
		</entity>
		<graphic.element page="1" xcoord="205" ycoord="219"/>
	</schema>
</express>
