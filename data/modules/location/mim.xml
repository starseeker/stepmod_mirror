		</interface>
		<interface kind="use" schema="Value_with_unit_mim">
</interface>
		<interface kind="use" schema="Zonal_breakdown_mim">
</interface>
		<type name="location_classification_item">
			<select extensible="YES" genericentity="YES" basedon="classification_item" selectitems="location location_relationship"/>
			<graphic.element page="2" xcoord="155" ycoord="453"/>
		</type>
		<type name="location_representation_item">
			<select extensible="YES" genericentity="YES" selectitems="organizational_address person_and_organization_address product product_definition_formation"/>
			<graphic.element page="2" xcoord="283" ycoord="113"/>
		</type>
		<entity name="applied_location_representation_assignment" supertypes="location_representation_assignment">
			<explicit name="items">
                          <aggregate lower="1" upper="?" type="SET"/>
					<typename name="location_representation_item"/>
			</explicit>
			<graphic.element page="2" xcoord="155" ycoord="177"/>
		</entity>
		<entity name="characterized_location_object" supertypes="characterized_object location">
			<graphic.element page="2" xcoord="283" ycoord="318"/>
		</entity>
		<graphic.element page="1" xcoord="283" ycoord="744"/>
	</schema>
</express>
