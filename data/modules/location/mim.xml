<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express language_version="2" rcs.date="$Date: 2004/11/09 13:01:58 $" rcs.revision="$Revision: 1.10 $" description.file="mim_descriptions.xml">
	<application name="GraphicalEXPRESS" owner="Eurostep" url="www.eurostep.com" version="1.3.16" source="Location_mim_v030.vsd"/>
	<schema name="Location_mim">
		<interface kind="use" schema="Classification_assignment_mim">
</interface>
		<interface kind="use" schema="location_schema">
			<interfaced.item name="location"/>
			<interfaced.item name="location_relationship"/>
		</interface>
		<interface kind="use" schema="management_resources_schema">
			<interfaced.item name="location_representation_assignment"/>
		</interface>
		<interface kind="use" schema="Person_organization_mim">
</interface>
		<interface kind="use" schema="person_organization_schema">
			<interfaced.item name="organizational_address"/>
		</interface>
		<interface kind="use" schema="product_definition_schema">
			<interfaced.item name="product"/>
			<interfaced.item name="product_definition_formation"/>
		</interface>
		<interface kind="use" schema="Product_identification_mim">
</interface>
		<interface kind="use" schema="product_property_definition_schema">
			<interfaced.item name="characterized_definition"/>
			<interfaced.item name="characterized_object"/>
			<interfaced.item name="property_definition"/>
		</interface>
		<interface kind="use" schema="product_property_representation_schema">
			<interfaced.item name="property_definition_representation"/>
			<interfaced.item name="represented_definition"/>
		</interface>
		<interface kind="use" schema="Product_version_mim">
</interface>
		<interface kind="use" schema="qualified_measure_schema">
			<interfaced.item name="measure_representation_item"/>
		</interface>
		<interface kind="use" schema="representation_schema">
			<interfaced.item name="representation"/>
