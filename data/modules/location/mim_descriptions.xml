	<ext_description linkend="Location_mim"/>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Location_mim  Type: location_item 
   <express_ref linkend="location:mim:Location_mim.location_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Location_mim.location_item"/>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Location_mim  Entity: applied_location_assignment  
   <express_ref linkend="location:mim:Location_mim.applied_location_assignment"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Location_mim.applied_location_representation_assignment">
		An <b>applied_location_representation_assignment</b> is a type of <express_ref linkend="management_resources_schema:ir_express:management_resources_schema.location_representation_assignment"/>

 that relates a <express_ref linkend="location_schema:ir_express:location_schema.location"/>

 to instances of entity data types listed in <express_ref linkend="location:mim:Location_mim.location_representation_item"/>.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Location_mim  Entity: applied_location_assignment  Attribute: items 
   <express_ref linkend="location:mim:Location_mim.applied_location_assignment.items"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Location_mim.applied_location_representation_assignment.items">
		the set of items by which the <express_ref linkend="location_schema:ir_express:location_schema.location"/>

 is represented.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Location_mim  Entity: characterized_location_object  
   <express_ref linkend="location:mim:Location_mim.characterized_location_object"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Location_mim.characterized_location_object">
		A <b>characterized_location_object</b> is a type of <express_ref linkend="location_schema:ir_express:location_schema.location"/>

 and <express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.characterized_object"/>

 that serves to assign <express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.property_definition"/>

 instances to <express_ref linkend="location_schema:ir_express:location_schema.location"/>

 instances.
	</ext_description>
</ext_descriptions>
