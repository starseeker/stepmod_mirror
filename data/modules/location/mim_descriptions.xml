   Schema: Location_mim  Type: location_item 
   <express_ref linkend="location:mim:Location_mim.location_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Location_mim.location_item"/>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Location_mim  Entity: applied_location_assignment  
   <express_ref linkend="location:mim:Location_mim.applied_location_assignment"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Location_mim.applied_location_representation_assignment">
		An <b>applied_location_representation_assignment</b> is a type of <b>location__representation_assignment</b> that relates an <b>location </b> to instances of entity data types listed in <b>location_representation_item</b>.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Location_mim  Entity: applied_location_assignment  Attribute: items 
   <express_ref linkend="location:mim:Location_mim.applied_location_assignment.items"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Location_mim.applied_location_representation_assignment.items">
		the set of items by which the <b>location</b> is represented.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Location_mim  Entity: characterized_location_object  
   <express_ref linkend="location:mim:Location_mim.characterized_location_object"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Location_mim.characterized_location_object">
		A <b>characterized_location_object</b> is a type of <b>location</b> and <b>characterized_object</b> that serves to assign property_definition instances to <b>location</b> instances.
	</ext_description>
</ext_descriptions>
