<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express language_version="2" rcs.date="$Date: $" rcs.revision="$Revision: $" description.file="arm_descriptions.xml">
	<application name="GraphicalEXPRESS" owner="Eurostep" url="www.eurostep.com" version="1.3.14" source="location_assignment_arm_v008.vsd"/>
	<schema name="Location_assignment_arm">
		<interface kind="use" schema="Location_arm"/>
		<type name="location_assignment_select">
			<select extensible="YES" genericentity="YES"/>
			<graphic.element page="2" xcoord="92" ycoord="141"/>
		</type>
		<entity name="Location_assignment">
			<explicit name="description" optional="YES">
				<description>
          textual description of the meaning of this location assignment
        </description>
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="role" optional="YES">
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="entity_for_location">
				<description>
          Identifies the product, person, product_group, event or activity which is related to the specified location 
        </description>
				<typename name="location_assignment_select"/>
			</explicit>
			<explicit name="location_for_assignment">
				<description>
          defines the location which is subject of this assignment
        </description>
				<typename name="Location"/>
			</explicit>
			<graphic.element page="2" xcoord="298" ycoord="141"/>
		</entity>
		<graphic.element page="1" xcoord="99" ycoord="134"/>
	</schema>
</express>
