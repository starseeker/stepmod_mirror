   <express_ref 
     linkend="location_assignment:mim:Location_assignment_mim.applied_location_assignment"/>
     that serves to assign 
   <express_ref 
     linkend="product_property_definition_schema:ir_express:product_property_definition_schema.property_definition"/>
   instances to 
   <express_ref 
     linkend="location_assignment:mim:Location_assignment_mim.applied_location_assignment"/>
     instances. 
