<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: 2002/11/29 13:35:29 $"
  rcs.revision="$Revision: 1.2 $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.24"
    source="../data/modules/location_in_building/arm.exp"/>

  <schema name="Location_in_building_arm">
    <interface
      kind="use"
      schema="Building_component_arm">
    </interface>

    <interface
      kind="use"
      schema="Building_item_arm">
    </interface>

    <interface
      kind="use"
      schema="Building_structure_arm">
    </interface>

    <interface
      kind="use"
      schema="Elemental_geometric_shape_arm">
    </interface>

    <entity
      name="Component_position_in_element">
      <explicit
        name="positioned_item">
        <typename
          name="Building_element_component"/>
      </explicit>
      <explicit
        name="positioned_within">
        <typename
          name="Building_element"/>
      </explicit>
      <explicit
        name="location">
        <typename
          name="Axis_placement"/>
      </explicit>
    </entity>

    <entity
      name="Element_position_in_space">
      <explicit
        name="positioned_item">
        <typename
          name="Building_element"/>
      </explicit>
      <explicit
        name="positioned_within">
        <typename
          name="Space"/>
      </explicit>
      <explicit
        name="location">
        <typename
          name="Axis_placement"/>
      </explicit>
    </entity>

    <entity
      name="Item_position_in_section">
      <explicit
        name="positioned_item">
        <typename
          name="Building_item"/>
      </explicit>
      <explicit
        name="positioned_within">
        <typename
          name="Building_section"/>
      </explicit>
      <explicit
        name="location">
        <typename
          name="Axis_placement"/>
      </explicit>
    </entity>

    <entity
      name="Level_position_in_section">
      <explicit
        name="positioned_item">
        <typename
          name="Building_level"/>
      </explicit>
      <explicit
        name="positioned_within">
        <typename
          name="Building_section"/>
      </explicit>
      <explicit
        name="location">
        <typename
          name="Axis_placement"/>
      </explicit>
    </entity>

    <entity
      name="Section_position_in_building">
      <explicit
        name="positioned_item">
        <typename
          name="Building_section"/>
      </explicit>
      <explicit
        name="positioned_within">
        <typename
          name="Building"/>
      </explicit>
      <explicit
        name="location">
        <typename
          name="Axis_placement"/>
      </explicit>
    </entity>

  </schema>

</express>
