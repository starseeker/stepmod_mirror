</ext_description>

<!-- +++++++++++ face_shape_representation_relationship +++++++++++ -->
<ext_description linkend="Machining_features_mim.face_shape_representation_relationship">
	A <b>face_shape_representation_relationship</b> is a type of
	<express_ref linkend="representation_schema:ir_express:representation_schema.representation_relationship"/>
	that is the representation of several 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.face_shape_representation"/>
	and their relationship to one another.	
</ext_description>

<!-- WR1 -->
<ext_description linkend="Machining_features_mim.face_shape_representation_relationship.wr:WR1">
	The 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.face_shape_representation"/>
	shall have only 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.face_shape_representation"/>
	in its
	<express_ref linkend="representation_schema:ir_express:representation_schema.representation_relationship.rep_1"/>.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Machining_features_mim.face_shape_representation_relationship.wr:WR2">
	The 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.face_shape_representation"/>
	shall have only 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.face_shape_representation"/>
	in its
	<express_ref linkend="representation_schema:ir_express:representation_schema.representation_relationship.rep_2"/>.
</ext_description>

<!-- +++++++++++ chamfer_offset_requires_faces +++++++++++ -->
<ext_description linkend="Machining_features_mim.chamfer_offset_requires_faces">
	The <b>chamfer_offset_requires_faces</b> rule specifies that all instances of 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.chamfer_offset"/>
	require
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.face_shape_representation"/>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Machining_features_mim.chamfer_offset_requires_faces.wr:WR1">
	If the description of the 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.chamfer_offset"/>
	is 'first offset', the 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.chamfer_offset"/>
	shall have at most one one 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.face_shape_representation"/>
	with a name of 'first face shape' or if the description of the
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.chamfer_offset"/>
	is 'second offset', the 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.chamfer_offset"/>
	shall have at most one one 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.face_shape_representation"/>
	with a name of 'second face shape'.
</ext_description>

<!-- +++++++++++ chamfer_requires_faces +++++++++++ -->
<ext_description linkend="Machining_features_mim.chamfer_requires_faces">
	The <b>chamfer_requires_faces rule</b> specifies that all instances of 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.chamfer"/>
	require 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.face_shape_representation"/>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Machining_features_mim.chamfer_requires_faces.wr:WR1">
	The 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.chamfer"/>
	shall have exactly one 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.face_shape_representation"/>
	in the role of the chamfer face.
</ext_description>

<!-- +++++++++++ edge_round_requires_faces +++++++++++ -->
<ext_description linkend="Machining_features_mim.edge_round_requires_faces">
	The <b>edge_round_requires_faces</b> rule specifies that all instances of 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.edge_round"/>
	require 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.face_shape_representation"/>.
</ext_description>

<ext_description linkend="Machining_features_mim.edge_round_requires_faces.wr:WR1">
	The 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.edge_round"/>
	shall have exactly one one 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.face_shape_representation"/>
	with a name of 'edge round face'.	
</ext_description>

<ext_description linkend="Machining_features_mim.edge_round_requires_faces.wr:WR2">
	The 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.edge_round"/>
	shall have exactly one one 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.face_shape_representation"/>
	with a name of 'first face shape'.	
</ext_description>

<ext_description linkend="Machining_features_mim.edge_round_requires_faces.wr:WR3">
	The 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.edge_round"/>
	shall have exactly one one 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.face_shape_representation"/>
	with a name of 'second face shape'.	
</ext_description>

<!-- +++++++++++ machining_feature_life_cycle +++++++++++ -->
<ext_description linkend="Machining_features_mim.machining_feature_life_cycle">
	The <b>machining_feature_life_cycle</b> rule specifies that each instance of 
	<express_ref linkend="shape_aspect_definition_schema:ir_express:shape_aspect_definition_schema.instanced_feature"/>,
	shall be defined for the manufacturing planning stage of the part on which it is specified.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Machining_features_mim.machining_feature_life_cycle.wr:WR1">
	For each instance of 
	<express_ref linkend="shape_aspect_definition_schema:ir_express:shape_aspect_definition_schema.instanced_feature"/>,
	the 
	<express_ref linkend="application_context_schema:ir_express:application_context_schema.product_definition_context.life_cycle_stage"/>.
	of the 
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition"/>
	for which it is defined has a value of 'manufacturing planning'.
</ext_description>

<!-- +++++++++++ transition_feature_life_cycle +++++++++++ -->
<ext_description linkend="Machining_features_mim.transition_feature_life_cycle">
	The <b>transition_feature_life_cycle</b> rule specifies that each instance of 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.transition_feature"/>
	shall be defined for the manufacturing planning stage of the part on which it is specified.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Machining_features_mim.transition_feature_life_cycle.wr:WR1">
	For each instance of 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.transition_feature"/>,
	the 
	<express_ref linkend="application_context_schema:ir_express:application_context_schema.product_definition_context.life_cycle_stage"/>.
	of the 
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition"/>
	for which it is defined has a value of 'manufacturing planning'.
</ext_description>

<!-- +++++++++++ transition_feature_on_part_boundary +++++++++++ -->
<ext_description linkend="Machining_features_mim.transition_feature_on_part_boundary">
	The <b>transition_feature_on_part_boundary</b> rule specifies that each instance of 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.transition_feature"/>
	shall lie on the boundary of the part for which it is defined.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Machining_features_mim.transition_feature_on_part_boundary.wr:WR1">
	For each instance of 
	<express_ref linkend="aic_machining_feature:ir_express:aic_machining_feature.transition_feature"/>,
	<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.shape_aspect.product_definitional"/>
	shall have a value of true.
</ext_description>


</ext_descriptions>
