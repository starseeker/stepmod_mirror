<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Management_resource_information_arm  Type: mri_approval_item 
   <express_ref linkend="management_resource_information:arm:Management_resource_information_arm.mri_approval_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Management_resource_information_arm.mri_approval_item">
  Using <express_ref linkend="approval:arm:Approval_arm.Approval_assignment"/>, an 
  <express_ref linkend="approval:arm:Approval_arm.Approval"/> 
  can be assigned to the entity data types listed in 
  <b>mri_approval_item</b>. 
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Management_resource_information_arm  Type: mri_date_or_date_time_item 
   <express_ref linkend="management_resource_information:arm:Management_resource_information_arm.mri_date_or_date_time_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Management_resource_information_arm.mri_date_or_date_time_item">
  Using 
  <express_ref
    linkend="date_time_assignment:arm:Date_time_assignment_arm.Date_or_date_time_assignment"/>,
  a
  <express_ref linkend="date_time:arm:Date_time_arm.Calendar_date"/>
  or 
  <express_ref linkend="date_time:arm:Date_time_arm.Date_time"/>
  can be assigned to the entity data types listed in 
  <b>mri_date_or_date_time_item</b>.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Management_resource_information_arm  Type: mri_identification_item 
   <express_ref linkend="management_resource_information:arm:Management_resource_information_arm.mri_identification_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description
linkend="Management_resource_information_arm.mri_identification_item">
 Using 
 <express_ref
   linkend="identification_assignment:arm:Identification_assignment_arm.Identification_assignment"/>
   an identification can be assigned to the entity data types listed in 
   <b>mri_identification_item</b>.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Management_resource_information_arm  Type: mri_organization_or_person_in_organization_item 
   <express_ref linkend="management_resource_information:arm:Management_resource_information_arm.mri_organization_or_person_in_organization_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Management_resource_information_arm.mri_organization_or_person_in_organization_item">
  Using 
  <express_ref
linkend="person_organization_assignment:arm:Person_organization_assignment_arm.Organization_or_person_in_organization_assignment"/>,
 an
  <express_ref
    linkend="person_organization:arm:Person_organization_arm.Organization"/>
  or
  <express_ref linkend="person_organization:arm:Person_organization_arm.Person_in_organization"/>
  can be assigned to the entity data types listed in <b>mri_organization_or_person_in_organization_item</b>.

</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Management_resource_information_arm  Type: mri_string_select 
   <express_ref linkend="management_resource_information:arm:Management_resource_information_arm.mri_string_select"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description
  linkend="Management_resource_information_arm.mri_string_select">
  <p>
    The entities listed in <b>mri_string_select</b> specify data
  types to which a 
  <express_ref linkend="multi_linguism:arm:Multi_linguism_arm.Language"/>
  can be assigned by the 
  <express_ref linkend="multi_linguism:arm:Multi_linguism_arm.Language_indication"/>
  and 
  <express_ref linkend="multi_linguism:arm:Multi_linguism_arm.Attribute_translation_assignment"/>
  entity data types.
</p>
<p>
  The 
  <express_ref linkend="multi_linguism:arm:Multi_linguism_arm.Language_indication"/>
  entity data type identifies the primary language
  of a text attribute while the 
  <express_ref linkend="multi_linguism:arm:Multi_linguism_arm.Attribute_translation_assignment"/>
  entity data
  type enables to convey the translations of that attribute in various
  languages.  
</p>
</ext_description>
</ext_descriptions>
