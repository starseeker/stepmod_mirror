<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-02-28T09:40:59" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="manifold_subsurface_arm schema_instance"/>
   <schema name="Manifold_subsurface_arm">
      <interface kind="use" schema="Elemental_geometric_shape_arm"/>
      <interface kind="use" schema="Manifold_surface_arm"/>
      <type name="open_shell_or_subsurface">
         <select selectitems="Manifold_surface_shape_representation Manifold_subsurface_shape"/>
      </type>
      <entity name="Manifold_subsurface_shape" supertypes="Geometric_model">
         <explicit name="parent_surface">
            <typename name="open_shell_or_subsurface"/>
         </explicit>
         <where label="WR1" expression="SELF :&lt;&gt;: parent_surface"/>
         <where label="WR2" expression="SELF\Geometric_model.context_of_items :=: parent_surface\Geometric_model.context_of_items"/>
      </entity>
   </schema>
</express>
