</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Manufacturing_configuration_effectivity_arm  Constant: schema_name 
   <express_ref linkend="manufacturing_configuration_effectivity:arm:Manufacturing_configuration_effectivity_arm.schema_name"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
