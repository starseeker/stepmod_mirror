<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<?xml-stylesheet type="text/xsl"
href="../../../xsl/express.xsl" ?>
<express
   language_version="2"
   rcs.date="$Date: 2002/12/24 16:44:30 $"
   rcs.revision="$Revision: 1.2 $">
  <schema name="Maths_value_arm">
<type name="maths_atom">
<select selectitems="maths_boolean maths_complex maths_integer maths_real" />
<description>
A <b>maths_value</b> that is a single Boolean, real integer or complex value.
</description>
<graphic.element page="2"/>
</type>
<type name="maths_boolean">
<builtintype type="BOOLEAN" />
<description>
A single Boolean value.
</description>
<graphic.element page="2"/>
</type>
<type name="maths_integer">
<builtintype type="INTEGER" />
<description>
A single integer value.
</description>
<graphic.element page="2"/>
</type>
<type name="maths_complex">
<aggregate type="LIST" lower="2" upper="2"/>
<builtintype type="REAL"/>
<description>
A single Boolean value.
</description>
<graphic.element page="2"/>
</type>
<type name="maths_real">
<builtintype type="REAL" />
<description>
A single real value.
</description>
<graphic.element page="2"/>
</type>
<type name="maths_tuple">
<aggregate type="LIST" lower="0" upper="?" />
<typename name="maths_value"/>
<description>
A tuple of maths values.
</description>
<graphic.element page="2"/>
</type>
<type name="maths_value">
<select extensible="YES" selectitems="maths_atom maths_tuple" />
<description>
A thing that is a member of a <b>maths_space</b>.
</description>
<graphic.element page="2"/>
</type>
<graphic.element page="1"/>
</schema>
</express>


