<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1815">
   <application name="JSDAI" owner="LKSoft" source="mating_structure_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Mating_structure_arm">
      <interface kind="use" schema="Item_definition_structure_arm"/>
      <interface kind="use" schema="Assembly_technology_arm"/>
      <type name="mating_type">
         <enumeration extensible="YES" items="bolted_joint brazing clasping clinching clipping doweling flanging foam_injection gluing laser_welding press_fit riveting sewing spot_welding stapling welding soldering friction_welding"/>
      </type>
      <entity name="Mated_part_relationship">
         <explicit name="mating_joints" optional="YES">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Assembly_joint"/>
         </explicit>
         <explicit name="mating_material" optional="YES">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Definition_based_product_occurrence"/>
         </explicit>
         <explicit name="mating_types" optional="YES">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="mating_type"/>
         </explicit>
         <explicit name="relating">
            <typename name="Assembled_part_association"/>
         </explicit>
         <explicit name="related">
            <typename name="Assembled_part_association"/>
         </explicit>
         <where expression="relating.relating_view :=: related.relating_view" label="WR1"/>
         <where expression="SIZEOF(QUERY(mj &lt;* mating_joints | NOT(mj.associated_assembly :=: relating.relating_view)))= 0" label="WR2"/>
      </entity>
   </schema>
</express>
