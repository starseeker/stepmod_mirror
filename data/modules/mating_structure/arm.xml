<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="">
   <application name="JSDAI" owner="LKSoft" source="mating_structure_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Mating_structure_arm">
      <interface kind="use" schema="Item_definition_structure_arm"/>
      <interface kind="use" schema="Assembly_technology_arm"/>
      <entity name="Mated_item_association" supertypes="Assembled_part_association">
         <explicit name="relating_view">
            <typename name="Mating_definition"/>
            <redeclaration entity-ref="Product_occurrence_definition_relationship"/>
         </explicit>
      </entity>
      <entity name="Mated_item_relationship">
         <explicit name="mated_shape" optional="YES">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Assembly_joint"/>
         </explicit>
         <explicit name="relating">
            <typename name="Mated_item_association"/>
         </explicit>
         <explicit name="related">
            <typename name="Mated_item_association"/>
         </explicit>
      </entity>
      <entity name="Mating_definition" supertypes="Assembly_definition">
         <explicit name="mating_type">
            <builtintype type="STRING"/>
         </explicit>
         <inverse attribute="relating_view" entity="Mated_item_association" name="mated_items">
            <inverse.aggregate lower="2" type="SET" upper="?"/>
         </inverse>
      </entity>
   </schema>
</express>
