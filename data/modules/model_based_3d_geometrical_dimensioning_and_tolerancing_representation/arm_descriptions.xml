  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Model_based_3d_geometrical_dimensioning_and_tolerancing_representation_arm.Gdt_representation_shape.wr:WR1">
	A <b>Gdt_representation_shape</b> shall only be used by a
	<express_ref linkend="part_view_definition:arm:Part_view_definition_arm.Part_view_definition"/>
	if the
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition.additional_contexts"/>
	contains a member of type
	<express_ref linkend="product_and_manufacturing_information_view_context:arm:Product_and_manufacturing_information_view_context_arm.Gdt_representation_view_context"/>.
</ext_description>

<!-- 
