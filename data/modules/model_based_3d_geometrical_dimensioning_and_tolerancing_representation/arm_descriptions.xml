	that represents a three-dimensional nominal integral feature.
</ext_description>

<ext_description linkend="Model_based_3d_geometrical_dimensioning_and_tolerancing_representation_arm.Nominal_3d_integral_feature.identified_item">
	an inherited attribute that shall be of type
	<express_ref linkend="Model_based_3d_geometrical_dimensioning_and_tolerancing_representation:arm:Model_based_3d_geometrical_dimensioning_and_tolerancing_representation_arm.nominal_3d_integral_feature_geometry"/>.
</ext_description>


<ext_description linkend="Model_based_3d_geometrical_dimensioning_and_tolerancing_representation_arm.gdt_representation_view_context_constraint">
	The <b>gdt_representation_view_context_constraint</b> constraints 
	<express_ref linkend="part_view_definition:arm:Part_view_definition_arm.Part_view_definition"/>
	whose
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition.additional_contexts"/>
	refers to the value
	<express_ref linkend="model_based_3d_geometrical_dimensioning_and_tolerancing_representation:arm:Model_based_3d_geometrical_dimensioning_and_tolerancing_representation_arm.mb3dgdt_additional_application_domain_enumeration.geometrical_dimensioning_and_tolerancing_representation"/>.
</ext_description>
<ext_description linkend="Model_based_3d_geometrical_dimensioning_and_tolerancing_representation_arm.gdt_representation_view_context_constraint.wr:WR1">
	a
	<express_ref linkend="part_view_definition:arm:Part_view_definition_arm.Part_view_definition"/>
	whose
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition.additional_contexts"/>
	refers to the value
	<express_ref linkend="model_based_3d_geometrical_dimensioning_and_tolerancing_representation:arm:Model_based_3d_geometrical_dimensioning_and_tolerancing_representation_arm.mb3dgdt_additional_application_domain_enumeration.geometrical_dimensioning_and_tolerancing_representation"/>
	shall have an 
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition.initial_context"/>
	with the value
	<express_ref linkend="part_view_definition:arm:Part_view_definition_arm.pvd_life_cycle_stage_enumeration.design"/>
	for the
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.View_definition_context.life_cycle_stage"/>
	and the value
	<express_ref linkend="product_and_manufacturing_information_with_nominal_3d_models:arm:Product_and_manufacturing_information_with_nominal_3d_models_arm.pamap_initial_application_domain_enumeration.model_based_product_geometry"/>
	for the
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Initial_view_definition_context.application_domain"/>.
</ext_description>


