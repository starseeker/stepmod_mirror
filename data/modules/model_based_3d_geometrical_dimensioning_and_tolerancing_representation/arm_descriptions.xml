   <express_ref linkend="model_based_3d_geometrical_dimensioning_and_tolerancing_representation:arm:Model_based_3d_geometrical_dimensioning_and_tolerancing_representation_arm.Nominal_3d_feature"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Model_based_3d_geometrical_dimensioning_and_tolerancing_representation_arm.Nominal_3d_feature">
	A <b>Nominal_3d_feature</b> is a type of
	<express_ref linkend="part_shape:arm:Part_shape_arm.Part_shape_element"/>
	that represents a three-dimensional nominal feature.
	Each <b>Nominal_3d_feature</b> is either a
	<express_ref linkend="Model_based_3d_geometrical_dimensioning_and_tolerancing_representation:arm:Model_based_3d_geometrical_dimensioning_and_tolerancing_representation_arm.Nominal_3d_integral_feature"/>
	or a
	<express_ref linkend="Model_based_3d_geometrical_dimensioning_and_tolerancing_representation:arm:Model_based_3d_geometrical_dimensioning_and_tolerancing_representation_arm.Nominal_3d_derived_feature"/>.
</ext_description>

<ext_description linkend="Model_based_3d_geometrical_dimensioning_and_tolerancing_representation_arm.Nominal_3d_feature.wr:WR1">
	the inherited attribute
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element.associated_definition"/>
	shall have an
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition.additional_contexts"/>
	with the value
	<express_ref linkend="model_based_3d_geometrical_dimensioning_and_tolerancing_representation:arm:Model_based_3d_geometrical_dimensioning_and_tolerancing_representation_arm.mb3dgdt_additional_application_domain_enumeration.geometrical_dimensioning_and_tolerancing_representation"/>.
</ext_description>

<ext_description linkend="Model_based_3d_geometrical_dimensioning_and_tolerancing_representation_arm.Nominal_3d_feature.wr:WR2">
	the inherited attribute
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element.associated_definition"/>
	shall have a value of the
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition.shape_type"/>
	<express_ref linkend="product_and_manufacturing_information_with_nominal_3d_models:arm:Product_and_manufacturing_information_with_nominal_3d_models_arm.pmiwn3m_product_shape_type_enumeration.nominal_shape"/>.
</ext_description>

