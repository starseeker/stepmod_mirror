<!--  ++  GDT_REPRESENTATION_SHAPE  ++ -->
<ae entity="Gdt_representation_shape">
	<aimelt>product_definition_shape</aimelt>
	<source>ISO 10303-41</source>
	<refpath>
		product_definition_shape &lt;=
		property_definition
		{property_definition.definition -&gt;
		 characterized_definition =
		 characterized_product_definition =
		 product_definition}
	</refpath>
</ae> 

<!--  ++  NOMINAL_3D_DERIVED_FEATURE  ++ -->
<ae entity="Nominal_3d_derived_feature">
	<aimelt>derived_shape_aspect</aimelt>
	<source>ISO 10303-47</source>
	<aa attribute="derived_from" assertion_to="Nominal_3d_feature">
		<aimelt>PATH</aimelt>
		<refpath>
derived_shape_aspect &gt;=
shape_aspect &lt;- 
shape_aspect_relationship.relating_shape_aspect
shape_aspect_relationship 
{shape_aspect_relationship =&gt; 
shape_aspect_deriving_relationship} 
shape_aspect_relationship.related_shape_aspect -&gt; 
shape_aspect
		</refpath>
	</aa>
</ae> 

<!--  ++  NOMINAL_3D_EDGE  ++ -->
<ae entity="Nominal_3d_edge">
	<aimelt>shape_aspect</aimelt>
	<source>ISO 10303-41</source>
			<refpath>
				{shape_aspect.product_definitional = .TRUE.}
				shape_aspect =
				represented_definition &lt;-
				item_identified_representation_usage.definition
				{item_identified_representation_usage =&gt;								
				 geometric_item_specific_usage} 
				item_identified_representation_usage.identified_item -&gt;
				{representation_item =&gt;
 				 topological_representation_item =&gt;
 				 edge =&gt;
 				 edge_curve}
			</refpath>	
</ae> 

<!--  ++  NOMINAL_3D_FEATURE  ++ -->
<ae entity="Nominal_3d_feature">
	<aimelt>shape_aspect</aimelt>
	<source>ISO 10303-41</source>
	<aa attribute="containing_shape" assertion_to="Gdt_representation_shape">
		<aimelt>shape_aspect.of_shape</aimelt>
	</aa>
</ae> 

<!--  ++  NOMINAL_3D_INTEGRAL_FEATURE  ++ -->
<ae entity="Nominal_3d_integral_feature">
	<aimelt>shape_aspect</aimelt>
	<source>ISO 10303-41</source>
	<refpath>
		{shape_aspect.product_definitional = .TRUE.}
	</refpath>	
</ae>
 
