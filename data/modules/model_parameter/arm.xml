<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:09" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="model_parameter_arm schema_instance"/>
   <schema name="MODEL_PARAMETER_ARM">
      <interface kind="use" schema="CHARACTERISTIC_ARM"/>
      <interface kind="use" schema="CLASSIFICATION_WITH_ATTRIBUTES_ARM"/>
      <interface kind="use" schema="DOCUMENT_ASSIGNMENT_ARM"/>
      <interface kind="use" schema="DOCUMENT_DEFINITION_ARM"/>
      <interface kind="use" schema="INDEPENDENT_PROPERTY_ARM"/>
      <interface kind="use" schema="INDEPENDENT_PROPERTY_REPRESENTATION_ARM"/>
      <interface kind="use" schema="PRODUCT_IDENTIFICATION_ARM"/>
      <interface kind="use" schema="GROUP_ARM"/>
      <interface kind="reference" schema="SPECIFICATION_DOCUMENT_ARM">
         <interfaced.item name="get_document_definition"/>
      </interface>
      <type name="mp_documented_element_select">
         <select extensible="YES" genericentity="YES" basedon="documented_element_select" selectitems="model_parameter"/>
      </type>
      <entity name="category_model_parameter" supertypes="model_parameter classification_attribute"/>
      <entity name="model_parameter" supertypes="independent_property">
         <explicit name="unit_of_measure" optional="YES">
            <typename name="unit"/>
         </explicit>
         <explicit name="valid_range" optional="YES">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="value_range"/>
         </explicit>
         <explicit name="default_value" optional="YES">
            <typename name="scalar_or_coordinated_characteristics"/>
         </explicit>
         <derived name="reference_document" expression=" get_document_definition ( self , 'reference document' , 'document_definition_arm.document_definition' ) ">
            <aggregate type="SET" lower="0" upper="?"/>
            <typename name="document_definition"/>
         </derived>
         <where label="wr1" expression=" exists ( reference_document ) and sizeof ( reference_document ) = 1 "/>
      </entity>
      <entity name="parameter_assignment" supertypes="independent_property_representation">
         <explicit name="property">
            <typename name="model_parameter"/>
            <redeclaration entity-ref="independent_property_representation"/>
         </explicit>
      </entity>
      <entity name="parameter_assignment_override">
         <explicit name="overridden_parameter_assignment">
            <typename name="parameter_assignment"/>
         </explicit>
         <explicit name="overriding_parameter_assignment">
            <typename name="parameter_assignment"/>
         </explicit>
      </entity>
      <entity name="product_specific_parameter_value_assignment">
         <explicit name="of_product">
            <typename name="product"/>
         </explicit>
         <explicit name="assigned_parameter">
            <typename name="parameter_assignment"/>
         </explicit>
         <derived name="the_model_parameter" expression=" assigned_parameter \ independent_property_representation . property ">
            <typename name="model_parameter"/>
         </derived>
         <unique label="ur1">
            <unique.attribute entity-ref="product_specific_parameter_value_assignment" attribute="the_model_parameter"/>
            <unique.attribute entity-ref="product_specific_parameter_value_assignment" attribute="of_product"/>
         </unique>
      </entity>
      <entity name="schema_based_model_parameter" supertypes="model_parameter">
         <explicit name="entity_name">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="attribute_name" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="schema_identifier">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="independent_property" old_name="property_type"/>
         </explicit>
         <unique label="ur1">
            <unique.attribute entity-ref="schema_based_model_parameter" attribute="entity_name"/>
            <unique.attribute entity-ref="schema_based_model_parameter" attribute="attribute_name"/>
            <unique.attribute entity-ref="schema_based_model_parameter" attribute="schema_identifier"/>
         </unique>
      </entity>
      <subtype.constraint name="mp_group_abstract" entity="group"/>
      <subtype.constraint name="mp_group_relationship_abstract" entity="group_relationship"/>
   </schema>
</express>
