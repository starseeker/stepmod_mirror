<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="mim_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$">
   <application name="JSDAI" owner="LKSoft" source="model_parameter_mim schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Model_parameter_mim">
      <interface kind="use" schema="Activity_method_assignment_mim"/>
      <interface kind="use" schema="Characteristic_mim"/>
      <interface kind="use" schema="Classification_with_attributes_mim"/>
      <interface kind="use" schema="Independent_property_representation_mim"/>
      <interface kind="use" schema="Group_mim"/>
      <interface kind="use" schema="product_definition_schema">
         <interfaced.item name="product_related_product_category"/>
      </interface>
      <interface kind="use" schema="product_property_definition_schema">
         <interfaced.item name="characterized_definition"/>
      </interface>
      <type name="mp_action_method_items">
         <select basedon="action_method_items" extensible="YES" genericentity="YES" selectitems="test_method_based_parameter_assignment"/>
      </type>
      <type name="mp_document_reference_item">
         <select basedon="document_reference_item" extensible="YES" genericentity="YES" selectitems="model_parameter"/>
      </type>
      <type name="mp_groupable_item">
         <select basedon="groupable_item" extensible="YES" genericentity="YES" selectitems="group_relationship"/>
      </type>
      <entity name="category_model_parameter" supertypes="model_parameter"/>
      <entity name="default_value_property_definition_representation" supertypes="property_definition_representation"/>
      <entity name="model_parameter" supertypes="general_property"/>
      <entity name="model_parameter_with_unit" supertypes="model_parameter">
         <where expression="NOT ('MODEL_PARAMETER_MIM.SCHEMA_BASED_MODEL_PARAMETER' IN TYPEOF(SELF))" label="WR1"/>
      </entity>
      <entity name="parameter_assignment" supertypes="property_definition_representation representation"/>
      <entity name="parameter_assignment_override" supertypes="representation representation_relationship">
         <explicit name="rep_1">
            <typename name="parameter_assignment"/>
            <redeclaration entity-ref="representation_relationship"/>
         </explicit>
         <explicit name="rep_2">
            <typename name="parameter_assignment"/>
            <redeclaration entity-ref="representation_relationship"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="rep_1"/>
            <unique.attribute attribute="rep_2"/>
         </unique>
         <where expression="SELF\representation_relationship.rep_1 &lt;&gt; SELF\representation_relationship.rep_2" label="WR1"/>
      </entity>
      <entity name="product_specific_parameter_value_assignment" supertypes="characterized_object product_related_product_category"/>
      <entity name="schema_based_model_parameter" supertypes="model_parameter"/>
      <entity name="test_method_based_parameter_assignment" supertypes="parameter_assignment"/>
      <entity name="valid_range_property_definition_representation" supertypes="property_definition_representation"/>
   </schema>
</express>
