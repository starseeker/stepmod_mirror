  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Modified_swept_solid_arm 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
   <ext_description linkend="Modified_swept_solid_arm"/>

  <!-- 
   ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    Schema: Modified_swept_solid_arm  Type: trim_select 
   ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
   -->
    <ext_description linkend="Modified_swept_solid_arm.trim_select">
 	 A <b>trim_select</b> is a select type used to specify the 
 	 type of end condition required when trimming an <express_ref linkend="swept_solid:arm:Swept_solid_arm.Extruded_solid"/>.
 	 </ext_description>
<!-- 
   ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    Schema: Modified_swept_solid_arm  Type: trim_intent 
   ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
   -->
    <ext_description linkend="Modified_swept_solid_arm.trim_intent">
 	 A <b>trim_intent</b> is an enumeration of the descriptive specification of the users requirements for end condtions when trimming an  ble select type used to specify the 
 	 type of end condition required when trimming an  <express_ref linkend="swept_solid:arm:Swept_solid_arm.Extruded_solid"/>.
</ext_description>
	
<ext_description linkend="Modified_swept_solid_arm.trim_intent.blind">
specifies that the extrusion terminates before it meets any other face;
</ext_description>
<ext_description linkend="Modified_swept_solid_arm.trim_intent.through_all">
specifies that the extrusion extends through the entire model;
</ext_description>
<ext_description linkend="Modified_swept_solid_arm.trim_intent.up_to_next">
specifies that the extrusion terminates when it first meets a face;
</ext_description>
<ext_description linkend="Modified_swept_solid_arm.trim_intent.unspecified">
no intent is specified.
</ext_description>
	
 	



<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Modified_swept_solid_arm  Entity: Solid_of_revolution_with_trim_conditions 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Modified_swept_solid_arm.Solid_of_revolution_with_trim_conditions">
<p>	 A <b>Solid_of_revolution_with_trim_conditions</b> is a  type of 
	 <express_ref linkend="swept_solid:arm:Swept_solid_arm.Solid_of_revolution"/>
that has been modified by trimming to change the angle of revolution.</p>
<note>This entity is fully defined in the corresponding MIM object which contains some additional constraints.
</note>
</ext_description>
<!--
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Modified_swept_solid_arm  Entity: Solid_of_revolution_with_trim_conditions Attribute: start_condition
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Modified_swept_solid_arm.Solid_of_revolution_with_trim_conditions.start_condition">
<p>the required alternative starting point for the revolution process to create the solid. 
This may be specified either as an angle or via an intersecting face.</p>
<note> a <express_ref linkend="value_with_unit:arm:Value_with_unit_arm.length_measure"/>is not a valid trim condition for this entity.
</note> 
</ext_description>
<!--
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Modified_swept_solid_arm  Entity: Solid_of_revolution_with_trim_conditions Attribute: end_condition
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Modified_swept_solid_arm.Solid_of_revolution_with_trim_conditions.end_condition">
the required alternative termination point for the revolution process to create the solid. 
This may be specified either as an angle or via an intersecting face.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Modified_swept_solid_arm  Entity: Extruded_solid_with_trim_conditions 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Modified_swept_solid_arm.Extruded_solid_with_trim_conditions">
	An <b>Extruded_solid_with_trim_conditions</b> is a  type of 
	<express_ref linkend="swept_solid:arm:Swept_solid_arm.Extruded_solid"/>
	that has been modified by trimming to change the extruded length.
	An <b>Extruded_solid_with_trim_conditions</b> may be an
	<express_ref linkend="Modified_swept_solid:arm:Modified_swept_solid_arm.Extruded_solid_with_draft_angle"/>
	or an
	<express_ref linkend="Modified_swept_solid:arm:Modified_swept_solid_arm.Extruded_solid_with_multiple_draft_angles"/>.
</ext_description>

<!--
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Modified_swept_solid_arm  Entity: Extruded_solid_with_trim_conditions Attribute: start_condition
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Modified_swept_solid_arm.Extruded_solid_with_trim_conditions.start_condition">
<p>the required alternative starting point for the linear extrusion process to create the solid. 
This may be specified either as <express_ref linkend="value_with_unit:arm:Value_with_unit_arm.length_measure"/> 
or via an intersecting face.</p>
<note number="1"> a <express_ref linkend="value_with_unit:arm:Value_with_unit_arm.plane_angle_measure"/> is not a valid trim condition for this entity.
</note>
<note number="2">This entity is fully defined in the corresponding MIM object which contains some additional constraints.

</note>
</ext_description>
<!--
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Modified_swept_solid_arm  Entity: Extruded_solid_with_trim_conditions Attribute: end_condition
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Modified_swept_solid_arm.Extruded_solid_with_trim_conditions.end_condition">
the required alternative termination point for the extrusion process to create the solid. 
This may be specified either as a length measure or via an intersecting face.
</ext_description>

<!--
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Modified_swept_solid_arm  Entity: Extruded_solid_with_trim_conditions Attribute: start_condition_intent
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Modified_swept_solid_arm.Extruded_solid_with_trim_conditions.start_condition_intent">
the intended semantics of the first trimming process.
</ext_description>
<!--
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Modified_swept_solid_arm  Entity: Extruded_solid_with_trim_conditions Attribute: end_condition_intent
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Modified_swept_solid_arm.Extruded_solid_with_trim_conditions.end_condition_intent">
the intended semantics of the final trimming process.
</ext_description>


 
 <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Modified_swept_solid_arm  Entity: Extruded_solid_with_draft_angle 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Modified_swept_solid_arm.Extruded_solid_with_draft_angle">
	 An <b>Extruded_solid_with_draft_angle</b> is a  type of <express_ref linkend="modified_swept_solid:arm:Modified_swept_solid_arm.Extruded_solid_with_trim_conditions"/>
 to which a draft angle is applied along the length of the extrusion. The cross-section of the extrusion in the plane of the defining face is unaltered by the draft angle.
 At any point on the faces generated by the extrusion process the draft angle defines the angle between the tangent plane to the surface and the direction of extrusion. 
 If the draft angle is positive the cross sectional area will increase in the positive direction of extrusion.

    </ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Modified_swept_solid_arm  Entity: Extruded_solid_with_draft_angle Attribute: draft_angle
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Modified_swept_solid_arm.Extruded_solid_with_draft_angle.draft_angle">
the angle between the tangent plane to the surfaces generated by the extrusion process and the 
<express_ref linkend="swept_solid:arm:Swept_solid_arm.Extruded_solid.extruded_direction"/>.

</ext_description>

 
 <!-- 
   ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    Schema: Modified_swept_solid_arm  Entity: Extruded_solid_with_multiple_draft_angles 
   ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
   -->
    <ext_description linkend="Modified_swept_solid_arm.Extruded_solid_with_multiple_draft_angles">
 	 An <b>Extruded_solid_with_multiple_draft_angles</b> is a  type of 
 	 <express_ref linkend="modified_swept_solid:arm:Modified_swept_solid_arm.Extruded_solid_with_trim_conditions"/>
     to which draft angles are applied along the length of the extrusion. The assigned draft angles are not the same for all edges of the swept face.
 
     </ext_description>
 
 <!-- 
    ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
     Schema: Modified_swept_solid_arm  Entity: Extruded_solid_with_multiple_draft_angles Attribute: draft_edges
    ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
    -->
     <ext_description linkend="Modified_swept_solid_arm.Extruded_solid_with_multiple_draft_angles.draft_edges">
  a list of sets of edges of the extruded face, each edge in the set having the same draft angle.
  The nth. draft angle is associated with the nth set of draft edges.    
  </ext_description>
 
 <!-- 
   ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    Schema: Modified_swept_solid_arm  Entity: Extruded_solid_with_multiple_draft_angles Attribute: draft_angles
   ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
   -->
    <ext_description linkend="Modified_swept_solid_arm.Extruded_solid_with_multiple_draft_angles.draft_angles">
 the list of draft angles to be assigned to the sets of edges in the 
 <express_ref linkend="modified_swept_solid:arm:Modified_swept_solid_arm.Extruded_solid_with_multiple_draft_angles.draft_edges"/>
  set.
 
 </ext_description>
 
 <!-- WR1 -->
 <ext_description linkend="Modified_swept_solid_arm.Extruded_solid_with_multiple_draft_angles.wr:WR1">
 	The number of
 	<express_ref linkend="modified_swept_solid:arm:Modified_swept_solid_arm.Extruded_solid_with_multiple_draft_angles.draft_angles"/>
 	shall be equal to the number of sets of <b>draft_edges</b>.
 	
</ext_description>
<!-- WR2 -->
 <ext_description linkend="Modified_swept_solid_arm.Extruded_solid_with_multiple_draft_angles.wr:WR2">
 	The 
 	<express_ref linkend="modified_swept_solid:arm:Modified_swept_solid_arm.Extruded_solid_with_multiple_draft_angles.draft_angles"/>
 	shall all be non zero.
 </ext_description>
 
 
 <!-- IP1 -->
 <ext_description linkend="Modified_swept_solid_arm.Extruded_solid_with_multiple_draft_angles.wr:IP1">
 	All the 
 	<express_ref linkend="modified_swept_solid:arm:Modified_swept_solid_arm.Extruded_solid_with_multiple_draft_angles.draft_edges"/>
 	shall be edges of the face being swept.
 	


	
</ext_description>





</ext_descriptions>


