++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Network_functional_design_view_mim.bus_element_link">
	A <b>bus_element_link</b> is a type of 
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition"/> 
	and of  
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_relationship"/> 
	that implements the ARM concept of 
	<express_ref linkend="Network_functional_design_view:arm:Network_functional_design_view_arm.Bus_element_link"/>
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
WR: bus_element_link.WR1
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Network_functional_design_view_mim.bus_element_link.wr:WR1">
The related element shall not be the relating element. 
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
UR: bus_element_link.UR1
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Network_functional_design_view_mim.bus_element_link.ur:UR1">
	The combination of related and relating attributes inherited from  
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_relationship"/> 
	shall be unique within a population of <b>bus_element_links</b>.
</ext_description>

