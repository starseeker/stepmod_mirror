<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2004-12-02T11:57:05" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="non_feature_shape_element_arm schema_instance"/>
   <schema name="Non_feature_shape_element_arm">
      <interface kind="use" schema="Shape_property_assignment_arm"/>
      <interface kind="use" schema="Basic_geometry_arm"/>
      <interface kind="use" schema="Layered_2d_shape_arm"/>
      <interface kind="reference" schema="Part_feature_location_arm"/>
      <type name="ee_product_definition_with_annotation_elements">
         <select extensible="YES" genericentity="YES"/>
      </type>
      <type name="view_shape_select">
         <select extensible="YES" genericentity="YES"/>
      </type>
      <entity name="Non_feature_shape_composition_relationship" supertypes="Geometric_model_relationship">
         <explicit name="related_feature_shape_placement" optional="YES">
            <typename name="axis_placement_or_cto2d"/>
         </explicit>
         <explicit name="resultant_feature_shape" >
            <typename name="Non_feature_shape_definition"/>
            <redeclaration entity-ref="Geometric_model_relationship" old_name="rep_2"/>            
         </explicit>
         <explicit name="related_feature_shape" >
            <typename name="Non_feature_shape_definition"/>
            <redeclaration entity-ref="Geometric_model_relationship" old_name="rep_1"/>            
         </explicit>
         <where label="WR1" expression=" EXISTS ( related_feature_shape_placement ) OR ( related_feature_shape \ representation . context_of_items :=: resultant_feature_shape \ representation . context_of_items ) "/>
      </entity>
      <entity name="Non_feature_shape_definition" supertypes="Geometric_model">
         <explicit name="model_shape" optional="YES">
            <typename name="Feature_shape_definition"/>
         </explicit>
         <explicit name="associated_element">
            <typename name="Non_feature_shape_element"/>
         </explicit>
      </entity>
      <entity name="Non_feature_shape_element" supertypes="Shape_element">
         <explicit name="scope">
            <typename name="ee_product_definition_with_annotation_elements"/>
         </explicit>
         <inverse name="associated_usage_concept" entity="Usage_concept_usage_relationship" attribute="associated_usage">
            <inverse.aggregate type="SET" lower="0"/>
         </inverse>
         <inverse name="element_shape" entity="Non_feature_shape_definition" attribute="associated_element">
            <inverse.aggregate type="SET" lower="0"/>
         </inverse>
         <unique label="UR1">
            <unique.attribute entity-ref="Non_feature_shape_element" attribute="element_name"/>
            <unique.attribute entity-ref="Non_feature_shape_element" attribute="scope"/>
         </unique>
         <where label="WR1" expression=" ( NOT ( SIZEOF ( associated_usage_concept ) &gt; 0 ) ) OR ( SIZEOF ( element_shape ) &gt; 0 ) "/>
      </entity>
      <entity name="Non_feature_shape_element_relationship" supertypes="Shape_element_relationship">
         <explicit name="relating">
            <typename name="Non_feature_shape_element"/>
            <redeclaration entity-ref="Shape_element_relationship"/>
         </explicit>
         <explicit name="related">
            <typename name="Non_feature_shape_element"/>
            <redeclaration entity-ref="Shape_element_relationship"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute entity-ref="Non_feature_shape_element_relationship" attribute="related"/>
            <unique.attribute entity-ref="Non_feature_shape_element_relationship" attribute="relating"/>
         </unique>
      </entity>
      <entity name="Reference_graphic_registration_mark" supertypes="Non_feature_shape_element"/>
      <entity name="Seating_plane" supertypes="Non_feature_shape_element"/>
      <entity name="View_level_non_feature_shape_definition" supertypes="Non_feature_shape_definition">
         <explicit name="model_shape">
            <typename name="view_shape_select"/>
            <redeclaration entity-ref="Non_feature_shape_definition"/>
         </explicit>
         <where label="WR1" expression=" SELF \ geometric_model . context_of_items = SELF \ non_feature_shape_definition . model_shape \ geometric_model . context_of_items "/>
      </entity>
   </schema>
</express>
