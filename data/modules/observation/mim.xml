<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="mim_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1258">
   <application name="JSDAI" owner="LKSoft" source="observation_mim schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Observation_mim">
      <interface kind="use" schema="action_schema">
         <interfaced.item name="action_directive"/>
         <interfaced.item name="action_relationship"/>
      </interface>
      <interface kind="use" schema="Activity_as_realized_mim"/>
      <interface kind="use" schema="Activity_mim"/>
      <interface kind="use" schema="Classification_assignment_mim"/>
      <interface kind="use" schema="Description_assignment_mim"/>
      <interface kind="use" schema="Management_resource_information_mim"/>
      <interface kind="use" schema="Name_assignment_mim"/>
      <interface kind="use" schema="Product_as_individual_mim"/>
      <interface kind="use" schema="qualified_measure_schema">
         <interfaced.item name="descriptive_representation_item"/>
      </interface>
      <interface kind="use" schema="representation_schema">
         <interfaced.item name="representation_context"/>
         <interfaced.item name="representation_item"/>
      </interface>
      <interface kind="use" schema="Work_order_mim"/>
      <interface kind="use" schema="Work_request_mim"/>
      <type name="observation_approval">
         <select basedon="mri_approval_item" selectitems="observation"/>
      </type>
      <type name="observation_classification">
         <select basedon="classification_item" selectitems="observation observation_relationship"/>
      </type>
      <type name="observation_consequence_name_item">
         <select basedon="name_item" extensible="YES" genericentity="YES" selectitems="observation_consequence"/>
      </type>
      <type name="observation_consequence_role">
         <select basedon="description_item" extensible="YES" genericentity="YES" selectitems="observation_consequence"/>
      </type>
      <type name="observation_date">
         <select selectitems="observation"/>
      </type>
      <type name="observation_date_and_time">
         <select basedon="mri_date_and_time_item" selectitems="observation"/>
      </type>
      <type name="observation_identification_item">
         <select basedon="mri_identification_item" selectitems="observation observation_consequence"/>
      </type>
      <type name="observation_organization">
         <select basedon="mri_organization_item" selectitems="observation"/>
      </type>
      <type name="observation_person_and_organization">
         <select basedon="mri_person_and_organization_item" selectitems="observation"/>
      </type>
      <type name="observation_recorder_item">
         <select basedon="action_items" extensible="YES" genericentity="YES" selectitems="product_definition_formation"/>
      </type>
      <type name="observed_context_item">
         <select basedon="action_items" extensible="YES" genericentity="YES"/>
      </type>
      <entity name="observation" supertypes="action_actual"/>
      <entity name="observation_consequence" supertypes="applied_action_assignment"/>
      <entity name="observation_relationship" supertypes="action_relationship"/>
   </schema>
</express>
