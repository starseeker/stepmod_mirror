(* UNDER DEVELOPMENT *)
SCHEMA Organization_structure_arm;
  USE FROM Certification_arm;
  USE FROM Experience_arm;
  USE FROM Information_packet_arm;
  USE FROM Location_arm;
  USE FROM Location_assignment_arm;
  USE FROM Organization_type_arm;
  USE FROM Person_organization_arm;
  USE FROM Person_organization_assignment_arm;
  USE FROM Position_in_organization_arm;
  USE FROM Qualifications_arm;
  USE FROM Type_of_person_arm;
  TYPE os_certification_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON certification_item WITH (Organization, Organization_type, Person, Person_in_organization, Position, Position_group, Position_type);
  END_TYPE;

  TYPE os_location_assignment_select = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON location_assignment_select WITH (Organization, Organization_type, Person, Person_in_organization, Position, Position_type, Type_of_person);
  END_TYPE;

  TYPE os_organization_or_person_in_organization_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON organization_or_person_in_organization_item WITH (Address_assignment, Certification, Certification_assignment, Location_assignment, Organization_or_person_in_organization_assignment, Person_in_organization, Position_assignment, Position_group_assignment, Position_position_type_assignment, Position_type, Position_type_assignment, Qualification_assignment, Qualification_type, Type_of_person_assignment);
  END_TYPE;

  TYPE os_position_group_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON position_group_item WITH (Organization_or_person_in_organization_assignment);
  END_TYPE;

  TYPE os_position_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON position_item WITH (Organization_or_person_in_organization_assignment);
  END_TYPE;

  TYPE os_position_type_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON position_type_item WITH (Organization_or_person_in_organization_assignment);
  END_TYPE;

  TYPE os_type_of_person_item_select = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON type_of_person_item_select WITH (Person, Person_in_organization, Position, Position_group, Position_type);
  END_TYPE;

  TYPE os_information_packet_select = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON information_packet_select WITH (Organization_information_packet);
  END_TYPE;

  TYPE os_information_packet_element = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON information_packet_element WITH (Address, Address_assignment, Address_based_location_representation, Certification, Certification_assignment, Experience_gained, Experience_instance, Experience_type, Experience_type_relationship, Global_location_representation, Location, Location_assignment, Location_relationship, Organization, Organization_based_location_representation, Organization_information_packet, Organization_or_person_in_organization_assignment, Organization_organization_type_relationship, Organization_relationship, Organization_type, Organizational_location_identification, Person, Person_in_organization, Person_or_organization_or_person_in_organization_in_position, Person_or_organization_or_person_in_organization_in_position_relationship, Position, Position_assignment, Position_group, Position_group_assignment, Position_group_relationship, Position_group_role, Position_position_type_assignment, Position_relationship, Position_role, Position_type, Position_type_assignment, Position_type_role, Product_based_location_identification, Qualification_assignment, Qualification_type, Qualification_type_relationship, Regional_coordinate, Regional_grid_location_representation, Type_of_person, Type_of_person_assignment, Type_of_person_definition, Type_of_person_definition_relationship, Type_of_person_definition_required_attributes_relationship);
  END_TYPE;

  TYPE os_information_packet_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON information_packet_item WITH (Organization, Person, Person_in_organization);
  END_TYPE;

  ENTITY Organization_information_packet
    SUBTYPE OF (Information_packet_definition);
  END_ENTITY;
END_SCHEMA;
