<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<?xml-stylesheet type="text/xsl"
href="../../../xsl/express.xsl" ?>
<express
   language_version="2"
   rcs.date="$Date: 2005/04/04 06:57:22 $"
   rcs.revision="$Revision: 1.7 $"
   description.file="arm_descriptions.xml">

  <schema name="Organization_structure_arm">
    <interface
      kind="use"
      schema="Experience_arm">
    </interface>
    <interface
      kind="use"
      schema="Organization_type_arm">
    </interface>
    <interface
      kind="use"
      schema="Person_organization_arm">
    </interface>
    <interface
      kind="use"
      schema="Position_in_organization_arm">
    </interface>
    <interface
      kind="use"
      schema="Qualifications_arm">
    </interface>
    <interface
      kind="use"
      schema="Type_of_person_arm">
    </interface>
    <type name="os_position_group_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="position_group_item"
      />
    </type>

    <type name="os_position_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="position_item"
      />
    </type>

    <type name="os_position_type_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="position_type_item"
      />
    </type>

    <type name="os_type_of_person_item_select">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="type_of_person_item_select"
        selectitems="Experience_gained Experience_instance Experience_type Organization Organization_type Qualification_type Position Position_group Position_type"
      />
    </type>

  </schema>
</express>
