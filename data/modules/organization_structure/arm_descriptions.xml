<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: Organization_structure_arm
   <express_ref linkend="organization_structure:arm:Organization_structure_arm"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Organization_structure_arm">
	<note>Implementation usage guide </note>
	<p>To support the implementation of this business use capability can be obtained 
	from DEXLIB see web site location
	<b>http://cvs.sourceforge.net/viewcvs.py/dexlib/dexlib/data/capabilities/</b></p>
	<p>See dexlib/data/capabilities <b>assigning </b>folders:</p>
	<p>See dexlib/data/capabilities <b> referencing</b> folders:</p>
	<ul>referencing_person_organization</ul>
	<ul>referencing_person_organization_typical</ul>
	<p>See dexlib/data/capabilities <b> representing</b> folders:</p>
	<ul>representing_location</ul>
	<ul>representing_person_organization</ul>
	<ul>representing_person_organization_typical</ul>
	<p>See dexlib/data/capabilities <b>other</b> folders:</p>
	<p>These are all implementation guides. To achive harmonization AP233 seeks to follow these guidlines exactly.</p>
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Organization_structure_arm  Type: os_certification_item 
   <express_ref linkend="organization_structure:arm:Organization_structure_arm.os_certification_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Organization_structure_arm.os_certification_item">
  Using
  <express_ref linkend="certification:arm:Certification_arm.Certification_assignment"/>,
  a
  <express_ref linkend="certification:arm:Certification_arm.Certification"/>,
  can be assigned to the entities in <b>os_certification_item</b>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Organization_structure_arm  Type: os_location_assignment_select 
   <express_ref linkend="organization_structure:arm:Organization_structure_arm.os_location_assignment_select"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Organization_structure_arm.os_location_assignment_select">
  Using 
  <express_ref linkend="location_assignment:arm:Location_assignment_arm.Location_assignment"/>,
  the entities in <b>os_location_assignment_select</b> are assigned to a specific   
  <express_ref linkend="location:arm:Location_arm.Location"/>
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Organization_structure_arm  Type: os_organization_or_person_in_organization_item 
   <express_ref linkend="organization_structure:arm:Organization_structure_arm.os_organization_or_person_in_organization_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Organization_structure_arm.os_organization_or_person_in_organization_item">
  Using 
  <express_ref
linkend="person_organization_assignment:arm:Person_organization_assignment_arm.Organization_or_person_in_organization_assignment"/>,
 an
  <express_ref
    linkend="person_organization:arm:Person_organization_arm.Organization"/>
  or
  <express_ref linkend="person_organization:arm:Person_organization_arm.Person_in_organization"/>
  can be assigned to the entity data types listed in 
  <b>os_organization_or_person_in_organization_item</b>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Organization_structure_arm  Type: os_position_group_item 
   <express_ref linkend="organization_structure:arm:Organization_structure_arm.os_position_group_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Organization_structure_arm.os_position_group_item">
  Using
  <express_ref linkend="position_in_organization:arm:Position_in_organization_arm.Position_group_assignment"/>,
  a
  <express_ref linkend="position_in_organization:arm:Position_in_organization_arm.Position_group"/> and 
  <express_ref linkend="position_in_organization:arm:Position_in_organization_arm.Position_group_role"/> 
  can be assigned to the entities in <b>os_position_group_item</b>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Organization_structure_arm  Type: os_position_item 
   <express_ref linkend="organization_structure:arm:Organization_structure_arm.os_position_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Organization_structure_arm.os_position_item">
  Using
  <express_ref linkend="position_in_organization:arm:Position_in_organization_arm.Position_assignment"/>,
  a
  <express_ref linkend="position_in_organization:arm:Position_in_organization_arm.Position"/> and 
  <express_ref linkend="position_in_organization:arm:Position_in_organization_arm.Position_role"/> 
  can be assigned to the entities in <b>os_position_item</b>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Organization_structure_arm  Type: os_position_type_item 
   <express_ref linkend="organization_structure:arm:Organization_structure_arm.os_position_type_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Organization_structure_arm.os_position_type_item">
  Using
  <express_ref linkend="position_in_organization:arm:Position_in_organization_arm.Position_type_assignment"/>,
  a
  <express_ref linkend="position_in_organization:arm:Position_in_organization_arm.Position_type"/> and 
  <express_ref linkend="position_in_organization:arm:Position_in_organization_arm.Position_type_role"/> 
  can be assigned to the entities in <b>os_position_type_item</b>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Organization_structure_arm  Type: os_type_of_person_item_select 
   <express_ref linkend="organization_structure:arm:Organization_structure_arm.os_type_of_person_item_select"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Organization_structure_arm.os_type_of_person_item_select">
  Using
  <express_ref linkend="type_of_person:arm:Type_of_person_arm.Type_of_person_assignment"/>,
  a
  <express_ref linkend="type_of_person:arm:Type_of_person_arm.Type_of_person"/> and role 
  can be assigned to the entities in <b>os_type_of_person_item_select</b>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Organization_structure_arm  Type: os_information_packet_select 
   <express_ref linkend="organization_structure:arm:Organization_structure_arm.os_information_packet_select"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Organization_structure_arm.os_information_packet_select">
  Using
    <express_ref linkend="information_packet:arm:Information_packet_arm.Information_packet_aggregate"/>,
  the information packet defined within <b>os_information_packet_select</b> can be 
  aggregated and managed as a whole.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Organization_structure_arm  Type: os_information_packet_element 
   <express_ref linkend="organization_structure:arm:Organization_structure_arm.os_information_packet_element"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Organization_structure_arm.os_information_packet_element">
  Using
    <express_ref linkend="information_packet:arm:Information_packet_arm.Information_packet_definition"/>,
  entities identified as items in <b>os_information_packet_element</b>
  define the information representation range of the information packet. 
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Organization_structure_arm  Type: os_information_packet_item 
   <express_ref linkend="organization_structure:arm:Organization_structure_arm.os_information_packet_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Organization_structure_arm.os_information_packet_item">
  Using
    <express_ref linkend="information_packet:arm:Information_packet_arm.Information_packet_definition"/>,
  entities identified as items in <b>os_information_packet_item</b>
  define the information usage domain of the information packet. 
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Organization_structure_arm  Entity: Organization_information_packet  
   <express_ref linkend="organization_structure:arm:Organization_structure_arm.Organization_information_packet"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Organization_structure_arm.Organization_information_packet">
    An <b>Organization_information_packet</b> is a collection of information that can be define over the range of 
    the <express_ref linkend="organization_structure:arm:Organization_structure_arm"/>.  It is a type of 
    <express_ref linkend="information_packet:arm:Information_packet_arm.Information_packet_definition"/>,
</ext_description>
</ext_descriptions>
