    <type name="os_organization_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="organization_item"
        selectitems="applied_person_type_assignment applied_position_in_organization_assignment applied_position_in_organization_group_assignment applied_position_in_organization_type_assignment position_in_organization_type"
      />
    </type>

    <type name="os_person_and_organization_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="person_and_organization_item"
