    <type name="os_state_observed_of_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="state_observed_of_item"
        selectitems="organization person_and_organization"
      />
    </type>

    <type name="os_attribute_language_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="attribute_language_item"
        selectitems="experience_type group applied_experience_assignment applied_person_type_assignment organization_type person_type person_type_definition person_type_definition_relationship position_in_organization position_in_organization_assignment position_in_organization_relationship position_in_organization_type qualification_type"
      />
    </type>

    <type name="os_multi_language_attribute_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="multi_language_attribute_item"
        selectitems="experience_type group applied_experience_assignment applied_person_type_assignment organization_type person_type person_type_definition person_type_definition_relationship position_in_organization position_in_organization_assignment position_in_organization_relationship position_in_organization_type qualification_type"
      />
    </type>

    <type name="os_person_type_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="person_type_item"
        selectitems="group person person_and_organization position_in_organization position_in_organization_type"
      />
    </type>

    <entity name="os_characterizable_address"
      supertypes="characterized_object address"
    >
    </entity>

    <entity name="os_characterizable_person"
      supertypes="characterized_object person"
    >
    </entity>

    <entity name="os_characterizable_person_and_organization"
      supertypes="characterized_object person_and_organization"
    >
    </entity>

  </schema>

</express>
