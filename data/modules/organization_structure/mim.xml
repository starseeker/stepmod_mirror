<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<?xml-stylesheet type="text/xsl"
href="../../../xsl/express.xsl" ?>
<express
   language_version="2"
   description.file="mim_descriptions.xml"
   rcs.date="$Date: 2007/07/11 11:37:21 $"
   rcs.revision="$Revision: 1.7 $">

  <application
    name=" Eurostep EXPRESS Parser (Eep!)"
    owner="Eurostep Limited"
    url="www.eurostep.com"
    version="1, 3, 30, 37"
    source="OsMim2.exp"/>

  <schema name="Organization_structure_mim">
    <interface
      kind="use"
      schema="Organization_type_mim">
    </interface>
    <interface
      kind="use"
      schema="Project_management_resource_information_mim">
    </interface>
    <interface
      kind="use"
      schema="product_property_definition_schema">
      <interfaced.item
        name="characterized_object"/>
    </interface>
    <type name="os_action_items">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="action_items"
        selectitems="group position_in_organization"
      />
    </type>

    <type name="os_action_method_items">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="action_method_items"
        selectitems="group organization person person_and_organization organization_type person_type person_type_definition position_in_organization position_in_organization_type"
      />
    </type>

    <type name="os_action_request_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="action_request_item"
        selectitems="organization person person_and_organization"
      />
    </type>

    <type name="os_aliasable_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="aliasable_item"
        selectitems="person person_and_organization address"
      />
    </type>

    <type name="os_approval_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="approval_item"
        selectitems="applied_organization_type_assignment experience_type group applied_experience_assignment applied_person_type_assignment applied_position_in_organization_assignment applied_position_in_organization_group_assignment applied_position_in_organization_type_assignment applied_qualification_type_assignment person_type position_in_organization_type qualification_type"
      />
    </type>

    <type name="os_classification_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="classification_item"
        selectitems="experience applied_experience_assignment applied_position_in_organization_assignment applied_position_in_organization_group_assignment applied_position_in_organization_type_assignment applied_qualification_type_assignment organization_type qualification_type_relationship"
      />
    </type>

    <type name="os_attribute_classification_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="attribute_classification_item"
        selectitems="organizational_address person_and_organization person_and_organization_address organization_relationship"
      />
    </type>

    <type name="os_date_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="date_item"
        selectitems="applied_organization_type_assignment group applied_person_type_assignment applied_position_in_organization_assignment applied_position_in_organization_group_assignment applied_position_in_organization_type_assignment"
      />
    </type>

    <type name="os_date_and_time_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="date_and_time_item"
