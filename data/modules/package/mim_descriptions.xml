++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Package_mim.package_body_edge_segment_surface">
<p>
	A <b>package_body_edge_segment_surface</b> is a type of 
	<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.shape_aspect"/> 
	and  of
	<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.shape_aspect_relationship"/> 
	that implements the ARM concept of 
	<express_ref linkend="Package:arm:Package_arm.Package_body_edge_segment_surface"/>.
</p>
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Attribute: package_body_edge_segment_surface.relating_shape_aspect
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Package_mim.package_body_edge_segment_surface.relating_shape_aspect">
	specifies a role of the
	<express_ref linkend="Extended_geometric_tolerance:mim:Extended_geometric_tolerance_mim.edge_segment_vertex"/> 
	for the <b>package_body_edge_segment_surface</b>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Attribute: package_body_edge_segment_surface.related_shape_aspect
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Package_mim.package_body_edge_segment_surface.related_shape_aspect">
	specifies a role of the
	<express_ref linkend="Extended_geometric_tolerance:mim:Extended_geometric_tolerance_mim.edge_segment_vertex"/> 
	for the <b>package_body_edge_segment_surface</b>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
WR: package_body_edge_segment_surface.WR1
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Package_mim.package_body_edge_segment_surface.wr:WR1">
	The <b>related_shape_aspect</b> shall not be equal to <b>relating_shape_aspect</b>.
</ext_description>

