<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1708">
   <application name="JSDAI" owner="LKSoft" source="packaged_connector_model_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Packaged_connector_model_arm">
      <interface kind="use" schema="Packaged_part_black_box_model_arm"/>
      <entity name="Mating_connector_relationship" supertypes="Part_definition_relationship">
         <explicit name="relating_view">
            <typename name="Packaged_connector"/>
            <redeclaration entity-ref="View_definition_relationship"/>
         </explicit>
         <explicit name="related_view">
            <typename name="Packaged_connector"/>
            <redeclaration entity-ref="View_definition_relationship"/>
         </explicit>
         <where expression="NOT EXISTS(SELF\View_definition_relationship.relation_type)" label="WR1"/>
      </entity>
      <entity name="Packaged_connector" supertypes="Packaged_part">
         <inverse attribute="connector" entity="Packaged_connector_terminal_relationship" name="characterized_connector_terminal_relationship">
            <inverse.aggregate lower="1" type="SET" upper="?"/>
         </inverse>
      </entity>
      <entity name="Packaged_connector_terminal_relationship">
         <explicit name="connector">
            <typename name="Packaged_connector"/>
         </explicit>
         <explicit name="join_terminal">
            <typename name="Packaged_part_join_terminal"/>
         </explicit>
         <explicit name="interface_terminal">
            <typename name="Packaged_part_interface_terminal"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="join_terminal"/>
         </unique>
         <unique label="UR2">
            <unique.attribute attribute="interface_terminal"/>
            <unique.attribute attribute="connector"/>
         </unique>
      </entity>
      <entity name="Packaged_part_interface_terminal" supertypes="Packaged_part_terminal">
         <derived expression="interface_terminal" name="interface_or_join_terminal">
            <typename name="interface_or_join_terminal_enumeration"/>
            <redeclaration entity-ref="Part_terminal"/>
         </derived>
         <inverse attribute="interface_terminal" entity="Packaged_connector_terminal_relationship" name="external_connector_function">
            <inverse.aggregate lower="0" type="SET" upper="?"/>
         </inverse>
      </entity>
      <subtype.constraint entity="Packaged_part_terminal" name="packaged_part_terminal_subtypes" super.expression="ONEOF (Packaged_part_interface_terminal, Packaged_part_join_terminal)"/>
   </schema>
</express>
