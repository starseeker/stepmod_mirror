<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-01-24T12:02:00" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="packaged_connector_model_arm schema_instance"/>
   <schema name="Packaged_connector_model_arm">
      <interface kind="use" schema="Packaged_part_black_box_model_arm"/>
      <entity name="Packaged_connector" supertypes="Packaged_part">
         <inverse name="characterized_connector_terminal_relationship" entity="Packaged_connector_terminal_relationship" attribute="connector">
            <inverse.aggregate type="SET" lower="1" upper="?"/>
         </inverse>
      </entity>
      <entity name="Packaged_connector_terminal_relationship">
         <explicit name="connector">
            <typename name="Packaged_connector"/>
         </explicit>
         <explicit name="join_terminal">
            <typename name="Packaged_part_join_terminal"/>
         </explicit>
         <explicit name="interface_terminal">
            <typename name="Packaged_part_interface_terminal"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute entity-ref="Packaged_connector_terminal_relationship" attribute="join_terminal"/>
         </unique>
         <unique label="UR2">
            <unique.attribute entity-ref="Packaged_connector_terminal_relationship" attribute="interface_terminal"/>
            <unique.attribute entity-ref="Packaged_connector_terminal_relationship" attribute="connector"/>
         </unique>
      </entity>
      <entity name="Packaged_part_interface_terminal" supertypes="Packaged_part_terminal">
         <inverse name="external_connector_function" entity="Packaged_connector_terminal_relationship" attribute="interface_terminal">
            <inverse.aggregate type="SET" lower="0" upper="?"/>
         </inverse>
      </entity>
      <subtype.constraint name="packaged_part_terminal_subtypes" entity="Packaged_part_terminal" super.expression="ONEOF (Packaged_part_interface_terminal, Packaged_part_join_terminal)"/>
   </schema>
</express>
