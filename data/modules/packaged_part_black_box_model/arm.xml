<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-01-06T19:54:09" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="packaged_part_black_box_model_arm schema_instance"/>
   <schema name="Packaged_part_black_box_model_arm">
      <interface kind="use" schema="Part_external_reference_arm"/>
      <interface kind="use" schema="Conductivity_material_aspects_arm"/>
      <interface kind="use" schema="Physical_unit_usage_view_arm"/>
      <interface kind="use" schema="Functional_assignment_to_part_arm"/>
      <interface kind="use" schema="Package_arm"/>
      <interface kind="use" schema="Network_functional_usage_view_arm"/>
      <type name="ppbbm_material_item_select">
         <select extensible="YES" genericentity="YES" basedon="_SELECT_material_item_select" selectitems="Packaged_part"/>
      </type>
      <entity name="Packaged_part" supertypes="Part_usage_view">
         <explicit name="used_package">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="Package"/>
         </explicit>
         <explicit name="implemented_function" optional="YES">
            <typename name="Functional_unit_usage_view"/>
         </explicit>
         <inverse name="access_mechanisms" entity="Packaged_part_terminal" attribute="associated_definition">
            <inverse.aggregate type="SET" lower="0" upper="?"/>
         </inverse>
         <where label="WR1" expression=" NOT ( EXISTS ( implemented_function ) XOR ( SIZEOF ( access_mechanisms ) &gt; 0 ) ) "/>
         <where label="WR2" expression=" single_package_version ( SELF ) "/>
         <where label="WR3" expression=" SIZEOF ( QUERY ( mi &lt;* USEDIN ( SELF , 'GENERIC_MATERIAL_ASPECTS_ARM.' + 'MATERIAL_IDENTIFICATION.' + 'ITEMS' ) | ( 'CONDUCTIVITY_MATERIAL_ASPECTS.' + 'MATERIAL_IDENTIFICATION_WITH_CONDUCTIVITY_CLASSIFICATION' IN TYPEOF ( mi ) ) ) &lt;= 1 ) "/>
      </entity>
      <entity name="Packaged_part_join_terminal" supertypes="Packaged_part_terminal"/>
      <entity name="Packaged_part_terminal" abstract.supertype="YES" supertypes="Part_terminal">
         <explicit name="terminal_of_package">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="Package_terminal"/>
         </explicit>
         <explicit name="associated_definition">
            <typename name="Packaged_part"/>
            <redeclaration entity-ref="Part_feature"/>
         </explicit>
         <where label="WR1" expression=" disjoint_package_terminals ( SELF ) "/>
         <where label="WR2" expression=" NOT EXISTS ( SELF \ part_feature . precedent_feature ) "/>
      </entity>
      <function name="disjoint_package_terminals">
         <parameter name="input">
            <typename name="Packaged_part_terminal"/>
         </parameter>
         <builtintype type="BOOLEAN"/>
         <algorithm> RETURN ( TRUE ) ; </algorithm>
      </function>
      <function name="single_package_version">
         <parameter name="input">
            <typename name="Packaged_part"/>
         </parameter>
         <builtintype type="BOOLEAN"/>
         <algorithm> RETURN ( TRUE ) ; </algorithm>
      </function>
   </schema>
</express>
