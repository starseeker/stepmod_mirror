<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1709">
   <application name="JSDAI" owner="LKSoft" source="packaged_part_white_box_model_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Packaged_part_white_box_model_arm">
      <interface kind="use" schema="Bare_die_arm"/>
      <interface kind="use" schema="Packaged_part_black_box_model_arm"/>
      <interface kind="use" schema="Physical_unit_2d_shape_arm"/>
      <interface kind="use" schema="Physical_unit_3d_shape_arm"/>
      <type name="ppwbm_analytical_model_port_assignment_select">
         <select basedon="analytical_model_port_assignment_select" extensible="YES" genericentity="YES" selectitems="Functional_unit_usage_view_terminal_definition Connection_zone Part_device_terminal Part_feature"/>
      </type>
      <entity name="Device_2d_position">
         <explicit name="placed_device">
            <typename name="Part_device"/>
         </explicit>
         <explicit name="transformation">
            <typename name="Axis_placement_2d"/>
         </explicit>
         <explicit name="part_shape">
            <typename name="Physical_unit_planar_shape_model"/>
         </explicit>
         <explicit name="device_shape">
            <typename name="Physical_unit_planar_shape_model"/>
         </explicit>
      </entity>
      <entity name="Device_3d_position">
         <explicit name="placed_device">
            <typename name="Part_device"/>
         </explicit>
         <explicit name="transformation">
            <typename name="Axis_placement_3d"/>
         </explicit>
         <explicit name="part_shape">
            <typename name="Physical_unit_3d_shape_model"/>
         </explicit>
         <explicit name="device_shape">
            <typename name="Physical_unit_3d_shape_model"/>
         </explicit>
      </entity>
      <entity name="Device_terminal_map">
         <explicit name="target_terminal">
            <typename name="Packaged_part_terminal"/>
         </explicit>
         <explicit name="mapped_device_terminal">
            <typename name="Part_device_terminal"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="mapped_device_terminal"/>
            <unique.attribute attribute="target_terminal"/>
         </unique>
      </entity>
      <entity name="Device_terminal_map_relationship">
         <explicit name="relating_device_terminal_map">
            <typename name="Device_terminal_map"/>
         </explicit>
         <explicit name="related_device_terminal_map">
            <typename name="Device_terminal_map"/>
         </explicit>
      </entity>
      <entity name="Part_device">
         <explicit name="defined_packaged_part">
            <typename name="Packaged_part"/>
         </explicit>
         <explicit name="included_device">
            <typename name="Bare_die"/>
         </explicit>
         <explicit name="item_identification">
            <builtintype type="STRING"/>
         </explicit>
         <inverse attribute="placed_device" entity="Device_3d_position" name="device_3d_position">
            <inverse.aggregate lower="0" type="SET" upper="1"/>
         </inverse>
         <inverse attribute="placed_device" entity="Device_2d_position" name="device_2d_position">
            <inverse.aggregate lower="0" type="SET" upper="1"/>
         </inverse>
         <unique label="UR1">
            <unique.attribute attribute="defined_packaged_part"/>
            <unique.attribute attribute="item_identification"/>
         </unique>
      </entity>
      <entity name="Part_device_terminal">
         <explicit name="used_bare_die_terminal">
            <typename name="Minimally_defined_bare_die_terminal"/>
         </explicit>
         <explicit name="terminated_device">
            <typename name="Part_device"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="used_bare_die_terminal"/>
            <unique.attribute attribute="terminated_device"/>
         </unique>
      </entity>
      <subtype.constraint entity="Physical_unit_keepout_shape_model" name="physical_unit_keepout_shape_model_subtypes" super.expression="ONEOF (Physical_unit_3d_keepout_shape_model, Physical_unit_planar_keepout_shape_model)"/>
      <subtype.constraint entity="Physical_unit_shape_model" name="physical_unit_shape_model_subtypes" super.expression="ONEOF (Physical_unit_3d_shape_model, Physical_unit_planar_shape_model)"/>
   </schema>
</express>
