<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<?xml-stylesheet type="text/xsl"
href="../../../xsl/express.xsl" ?>
<express
   language_version="2"
   rcs.date="$Date: 2005/11/25 16:26:54 $"
   rcs.revision="$Revision: 1.1 $">
    <schema name="Parameter_value_specification_mim">
        <interface
            kind="use"
            schema="Group_mim">
            <interfaced.item 
                name="applied_group_assignment"/>
            <interfaced.item 
                name="groupable_item"/>
        </interface>
        <interface
            kind="use"
            schema="ISO13584_generic_expressions_schema">
            <interfaced.item 
                name="generic_expression"/>
            <interfaced.item 
                name="variable_semantics"/>
        </interface>
        <interface
            kind="use"
            schema="measure_schema">
            <interfaced.item 
                name="measure_with_unit"/>
        </interface>
        <interface
            kind="use"
            schema="Name_assignment_mim">
            <interfaced.item 
                name="applied_name_assignment"/>
            <interfaced.item 
                name="name_item"/>
        </interface>
        <type name="generic_expression_groupable_item"> 
            <select
                extensible="YES"
                genericentity="YES"
                basedon="groupable_item"
                selectitems="generic_expression"/>
            <graphic.element
                page="2"
                xcoord="156"
                ycoord="57"/>
        </type>
        
        <type name="named_variable_semantics_name_item"> 
            <select
                extensible="YES"
                genericentity="YES"
                basedon="name_item"
                selectitems="named_variable_semantics"/>
            <graphic.element
                page="2"
                xcoord="433"
                ycoord="262"/>
        </type>
        
        <type name="named_variable_semantics_variable_context_groupable_item"> 
            <select
                basedon="groupable_item"
                selectitems="named_variable_semantics"/>
            <graphic.element
                page="2"
                xcoord="156"
                ycoord="199"/>
        </type>
        
        <type name="parameter_value_select"> 
            <select
                extensible="YES"
                genericentity="YES"
                selectitems="generic_expression measure_with_unit"/>
            <graphic.element
                page="2"
                xcoord="433"
                ycoord="128"/>
        </type>
        
        <entity
            name="named_variable_semantics"
            supertypes="variable_semantics"      >
            
            <graphic.element
                page="2"
                xcoord="156"
                ycoord="262"/>
        </entity>
        
        <graphic.element
            page="1"
            xcoord="213"
            ycoord="107"/>
    </schema>
    
</express>
