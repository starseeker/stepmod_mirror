(* N - ISO/TS - 10303-1022 Part and version identification - EXPRESS ARM *)
   
SCHEMA Part_and_version_identification_arm;

   
USE FROM Product_version_arm; --ISO/TS 10303-1018.

   
ENTITY Part
  SUBTYPE OF (Product);
WHERE
  WR1: SIZEOF(['part', 'raw material']*types_of_product(SELF))=1; 
END_ENTITY;

   
ENTITY Part_version
  SUBTYPE OF (Product_version);
WHERE
  WR1: 'PART_AND_VERSION_IDENTIFICATION_ARM.PART' IN TYPEOF(SELF.of_product);
END_ENTITY;


END_SCHEMA;
