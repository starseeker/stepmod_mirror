<?xml version="1.0"?>
<!-- $Id: arm.xml,v 1.11 2002/07/10 07:36:56 goset1 Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: 2002/07/10 07:36:56 $"
  rcs.revision="$Revision: 1.11 $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.16"
    source="../data/modules/part_and_version_identification/arm.exp"/>

  <schema name="Part_and_version_identification_arm">
    <interface
      kind="use"
      schema="Product_version_arm">
    </interface>

    <interface
      kind="reference"
      schema="Product_identification_arm">
      <interfaced.item
        name="types_of_product"/>
    </interface>

    <entity
      name="Part"
      supertypes="Product">
      <where
        label="WR1"
        expression="SIZEOF(['part', 'raw material']*types_of_product(SELF))=1">
      </where>
    </entity>

    <entity
      name="Part_version"
      supertypes="Product_version">
      <explicit
        name="of_product">
	<typename
	  name="Part"/>
        <redeclaration
          entity-ref="Product_version"/>
        </explicit>
      </entity>

    </schema>

  </express>
