  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Part_and_version_identification_arm 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
   <ext_description linkend="Part_and_version_identification_arm"/>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Part_and_version_identification_arm  Type: part_version_organization_or_person_in_organization_item 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Part_and_version_identification_arm.part_version_organization_or_person_in_organization_item">This select extends the assignment of an organization or a person in an organization to Part_version.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Part_and_version_identification_arm  Entity: Part 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Part_and_version_identification_arm.Part">A <b>Part</b> is a type of <b>Product</b> that is intended to be produced or employed in a production process. 
   A <b>Part</b> may represent either a single piece part, an assembly of parts, or a raw material.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Part_and_version_identification_arm  Entity: Part  Where: WR1 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Part_and_version_identification_arm.Part.wr:WR1">The part shall belong to a product category named 'part' or to a product category named 'raw material'.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Part_and_version_identification_arm  Entity: Part_version 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Part_and_version_identification_arm.Part_version">A <b>Part_version</b> is a type of <b>Product_version</b> that identifies a version of a part.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Part_and_version_identification_arm  Entity: Part_version  Where: WR1 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Part_and_version_identification_arm.Part_version.wr:WR1">The <b>Part_version</b> shall be a version of a <b>Part</b>.</ext_description>

</ext_descriptions>