<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/01/16 14:54:03 $"
  rcs.revision="$Revision: 1.3 $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.6"
    source="../data/modules/part_and_version_identification/mim.exp"/>

  <schema name="Part_and_version_identification_mim">
    <interface
      kind="use"
      schema="Product_version_mim">
    </interface>

    <interface
      kind="use"
      schema="Person_organisation_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="product_definition_schema">
      <interfaced.item
        name="product_definition_formation_with_specified_source"/>
    </interface>

    <type name="part_and_version_identification_organization_item">
      <description>An <b>part_and_version_identification_organization_item</b> extends organization assignment to <b>product_definition_formation</b> instances.</description>
      <select
        basedon="organization_item"
        selectitems="product_definition_formation">
      </select>

    </type>
    <type name="part_and_version_identification_person_and_organization_item">
      <description>A <b>part_and_version_identification_person_and_organization_item</b> extends person and organization assignment to <b>product_definition_formation</b> instances.</description>
      <select
        basedon="person_and_organization_item"
        selectitems="product_definition_formation">
      </select>

    </type>
  </schema>

</express>
