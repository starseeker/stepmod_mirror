<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date: 2011/09/14 22:59:41 $" rcs.revision="$Revision: 1.7 $" reference="ISO/TS 10303-1770">
   <application name="JSDAI" owner="LKSoft" source="part_and_zone_laminate_tables_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Part_and_zone_laminate_tables_arm">
      <interface kind="use" schema="Composite_constituent_shape_arm"/>
      <interface kind="use" schema="Ply_orientation_specification_arm"/>
      <interface kind="use" schema="Shape_property_assignment_arm"/>
      <interface kind="use" schema="Stock_material_arm"/>
      
<type name="component_class_for_assembly">
         <select selectitems="Composite_assembly Filament_laminate Ply Ply_laminate Processed_core"/>
      </type>

<type name="filiment_assembly_material">
         <select selectitems="Braided_assembly Filament_laminate Woven_assembly"/>
      </type>
      <type name="thickness_laminate_table_component">
         <select selectitems="Filament_laminate Ply Processed_core"/>
      </type>
      <entity name="Composite_assembly" supertypes="Constituent_part">
         <explicit name="layup_part">
            <typename name="Composite_assembly_table"/>
         </explicit>
         <explicit name="shape" optional="YES">
            <typename name="composite_assembly_shape"/>
         </explicit>
      </entity>
      <entity name="Composite_assembly_sequence_definition">
         <explicit name="components_in_sequence">
            <aggregate lower="1" type="BAG" upper="?"/>
            <typename name="component_class_for_assembly"/>
         </explicit>
      </entity>
      <entity name="Composite_assembly_table" supertypes="Part_laminate_table">
         <explicit name="sequence_groups">
            <aggregate lower="2" type="LIST" upper="?"/>
            <typename name="Composite_assembly_sequence_definition"/>
         </explicit>
      </entity>
      <entity name="Constituent_part" super.expression="ONEOF (Ply, Ply_piece, Ply_laminate, Filament_laminate, Processed_core, Composite_assembly)" supertypes="Part_view_definition">
         <explicit name="constituent_part_identification">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="Product_view_definition" old_name="id"/>
         </explicit>
         <explicit name="weight" optional="YES">
            <typename name="Mass_data_element"/>
         </explicit>
      </entity>
      <entity name="Edge_zone_shape" supertypes="Zone_structural_makeup_shape_representation">
         <explicit name="boundary">
            <typename name="boundary_class_zone"/>
         </explicit>
      </entity>
      <entity name="Filament_laminate" supertypes="Constituent_part">
         <explicit name="made_from">
            <typename name="Filament_assembly_material"/>
         </explicit>
         <explicit name="shape" optional="YES">
            <typename name="Filament_laminate_shape"/>
         </explicit>
      </entity>
      <entity name="Filament_laminate_shape">
         <explicit name="cross_section">
            <typename name="Shape_description_association"/>
         </explicit>
      </entity>
      <entity name="Laminate_table" super.expression="ONEOF (Part_laminate_table, Zone_structural_makeup)" supertypes="Part_view_definition">
         <explicit name="basis">
            <typename name="Reinforcement_orientation_basis"/>
         </explicit>
         <explicit name="resulting_surface" optional="YES">
            <typename name="Surface"/>
         </explicit>
         <explicit name="base_surface" optional="YES">
            <typename name="Surface_with_direction"/>
         </explicit>
      </entity>
      <entity name="Part_laminate_table" super.expression="ONEOF (Composite_assembly_table, Ply_laminate_table)" supertypes="Laminate_table"/>
      <entity name="Percentage_laminate_table" supertypes="Zone_structural_makeup">
         <explicit name="table_components">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Percentage_ply"/>
         </explicit>
         <explicit name="total_thickness">
            <typename name="Length_data_element"/>
         </explicit>
      </entity>
      <entity name="Percentage_ply">
         <explicit name="strength_orientation" optional="YES">
            <typename name="Ply_orientation_angle"/>
         </explicit>
         <explicit name="material">
            <typename name="Stock_material"/>
         </explicit>
         <explicit name="makeup_and_properties" optional="YES">
            <typename name="Zone_structural_makeup"/>
         </explicit>
         <explicit name="volume_percent">
            <typename name="Ratio_data_element"/>
         </explicit>
      </entity>
      <entity name="Ply" supertypes="Constituent_part">
         <explicit name="constituents" optional="YES">
            <aggregate lower="1" type="LIST" upper="?"/>
            <typename name="Ply_piece"/>
         </explicit>
         <explicit name="ply_thickness">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="material_orientation">
            <typename name="Direction_of_fiber_orientation_within_a_ply"/>
         </explicit>
         <explicit name="material_type">
            <typename name="ply_stock_material"/>
         </explicit>
         <explicit name="shape" optional="YES">
            <typename name="Ply_shape"/>
         </explicit>
         <derived expression="material_orientation.orientations[1]" name="material_orientation_angle">
            <typename name="Ply_orientation_angle"/>
         </derived>
      </entity>
      <entity name="Ply_laminate" supertypes="Constituent_part">
         <explicit name="shape" optional="YES">
            <typename name="ply_laminate_shape"/>
         </explicit>
         <explicit name="ply_table">
            <typename name="Ply_laminate_table"/>
         </explicit>
      </entity>
      <entity name="Ply_laminate_sequence_definition">
         <explicit name="plies_in_sequence">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Ply"/>
         </explicit>
      </entity>
      <entity name="Ply_laminate_table" supertypes="Part_laminate_table">
         <explicit name="sequence">
            <aggregate lower="2" type="LIST" upper="?"/>
            <typename name="Ply_laminate_sequence_definition"/>
         </explicit>
      </entity>
      <entity name="Ply_piece" supertypes="Constituent_part">
         <explicit name="shape" optional="YES">
            <typename name="ply_piece_shape"/>
         </explicit>
         <explicit name="warp_surface">
            <builtintype type="LOGICAL"/>
         </explicit>
      </entity>
      <entity name="Point_zone_shape" supertypes="Zone_structural_makeup_shape_representation">
         <explicit name="location">
            <typename name="Point"/>
         </explicit>
      </entity>
      <entity name="Processed_core" supertypes="Constituent_part">
         <explicit name="added_material" optional="YES">
            <aggregate lower="0" type="LIST" upper="?"/>
            <typename name="Ply"/>
         </explicit>
         <explicit name="cell_direction">
            <typename name="Direction_of_fiber_orientation_within_a_ply"/>
         </explicit>
         <explicit name="made_from_stock">
            <typename name="Stock_core"/>
         </explicit>
         <explicit name="made_from_processed_core" optional="YES">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Processed_core"/>
         </explicit>
         <explicit name="shape" optional="YES">
            <typename name="core_shape"/>
         </explicit>
         <derived expression="cell_direction.orientations[1]" name="cell_orientation_angle">
            <typename name="Ply_orientation_angle"/>
         </derived>
      </entity>
      <entity name="Smeared_material" supertypes="Zone_structural_makeup">
         <explicit name="total_thickness">
            <typename name="Length_data_element"/>
         </explicit>
      </entity>
      <entity name="Surface_with_direction">
         <explicit name="defining_surface">
            <typename name="Surface"/>
         </explicit>
         <explicit name="material_direction">
            <typename name="Direction"/>
         </explicit>
      </entity>
      <entity name="Thickness_laminate_table" supertypes="Zone_structural_makeup">
         <explicit name="table_components">
            <aggregate lower="2" type="LIST" upper="?"/>
            <typename name="thickness_laminate_table_component"/>
         </explicit>
      </entity>
      <entity name="Zone_structural_makeup" super.expression="ONEOF ((Thickness_laminate_table AND Smeared_material), (Percentage_laminate_table AND Smeared_material), Thickness_laminate_table, Percentage_laminate_table, Smeared_material)" supertypes="Laminate_table">
         <explicit name="boundary" optional="YES">
            <typename name="Zone_structural_makeup_shape_representation"/>
         </explicit>
      </entity>
      <entity name="Zone_structural_makeup_shape_representation" super.expression="ONEOF (Point_zone_shape, Edge_zone_shape)"/>
   </schema>
</express>
