		<li><express_ref linkend="Part_and_zone_laminate_tables:arm:Part_and_zone_laminate_tables_arm.Ply_piece"/>;</li>
		<li><express_ref linkend="Part_and_zone_laminate_tables:arm:Part_and_zone_laminate_tables_arm.Ply_laminate"/>;</li>
		<li><express_ref linkend="Part_and_zone_laminate_tables:arm:Part_and_zone_laminate_tables_arm.Filament_laminate"/>;</li>
		<li><express_ref linkend="Part_and_zone_laminate_tables:arm:Part_and_zone_laminate_tables_arm.Processed_core"/>;</li>
		<li><express_ref linkend="Part_and_zone_laminate_tables:arm:Part_and_zone_laminate_tables_arm.Composite_assembly"/>.</li>
	</ul>
</ext_description>

