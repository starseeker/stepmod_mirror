<!-- +++++++++++ Constituent_part +++++++++++ -->
<ext_description linkend="Part_and_zone_laminate_tables_arm.Constituent_part">
	A <b>Constituent_part</b> is a type of
	<express_ref linkend="Product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/>.
