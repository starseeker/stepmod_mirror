<!-- +++++++++++ Zone_structural_makeup +++++++++++ -->
<ext_description linkend="Part_and_zone_laminate_tables_arm.Zone_structural_makeup">
	A <b>Zone_structural_makeup</b> is a type of
	<express_ref linkend="Part_and_zone_laminate_tables:arm:Part_and_zone_laminate_tables_arm.Laminate_table"/>.
	A <b>Zone_structural_makeup</b> may be one of the following:
	<ul>
		<li><express_ref linkend="Part_and_zone_laminate_tables:arm:Part_and_zone_laminate_tables_arm.Thickness_laminate_table"/> and <express_ref linkend="Part_and_zone_laminate_tables:arm:Part_and_zone_laminate_tables_arm.Smeared_material"/>;</li>
		<li><express_ref linkend="Part_and_zone_laminate_tables:arm:Part_and_zone_laminate_tables_arm.Percentage_laminate_table"/> and <express_ref linkend="Part_and_zone_laminate_tables:arm:Part_and_zone_laminate_tables_arm.Smeared_material"/>;</li>
		<li><express_ref linkend="Part_and_zone_laminate_tables:arm:Part_and_zone_laminate_tables_arm.Thickness_laminate_table"/>;</li>
		<li><express_ref linkend="Part_and_zone_laminate_tables:arm:Part_and_zone_laminate_tables_arm.Percentage_laminate_table"/>;</li>
		<li><express_ref linkend="Part_and_zone_laminate_tables:arm:Part_and_zone_laminate_tables_arm.Smeared_material"/>.</li>
	</ul>
</ext_description>

