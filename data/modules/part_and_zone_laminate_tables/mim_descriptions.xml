<!-- +++++++++++ laminate_table +++++++++++ -->
<ext_description linkend="Part_and_zone_laminate_tables_mim.laminate_table">
	A <b>laminate_table</b> is a type of
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition"/>
	that implements the ARM concept of 
	<express_ref linkend="Part_and_zone_laminate_tables:arm:Part_and_zone_laminate_tables_arm.Laminate_table"/>.
	A <b>laminate_table</b> may be a
	<express_ref linkend="Part_and_zone_laminate_tables:mim:Part_and_zone_laminate_tables_mim.part_laminate_table"/>
	or a 
	<express_ref linkend="Part_and_zone_laminate_tables:mim:Part_and_zone_laminate_tables_mim.zone_structural_makeup"/>.
</ext_description>

