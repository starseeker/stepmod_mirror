<!-- +++++++++++ zone_structural_makeup +++++++++++ -->
<ext_description linkend="Part_and_zone_laminate_tables_mim.zone_structural_makeup">
	A <b>zone_structural_makeup</b> is a type of
	<express_ref linkend="Part_and_zone_laminate_tables:mim:Part_and_zone_laminate_tables_mim.laminate_table"/>
	that implements the ARM concept of 
	<express_ref linkend="Part_and_zone_laminate_tables:arm:Part_and_zone_laminate_tables_arm.Zone_structural_makeup"/>.
	A <b>zone_structural_makeup</b> may be one of the following:
	<ul>
		<li><express_ref linkend="Part_and_zone_laminate_tables:mim:Part_and_zone_laminate_tables_mim.thickness_laminate_table"/> and <express_ref linkend="Part_and_zone_laminate_tables:mim:Part_and_zone_laminate_tables_mim.smeared_material"/>;</li>
		<li><express_ref linkend="Part_and_zone_laminate_tables:mim:Part_and_zone_laminate_tables_mim.percentage_laminate_table"/> and <express_ref linkend="Part_and_zone_laminate_tables:mim:Part_and_zone_laminate_tables_mim.smeared_material"/>;</li>
		<li><express_ref linkend="Part_and_zone_laminate_tables:mim:Part_and_zone_laminate_tables_mim.thickness_laminate_table"/>;</li>
		<li><express_ref linkend="Part_and_zone_laminate_tables:mim:Part_and_zone_laminate_tables_mim.percentage_laminate_table"/>;</li>
		<li><express_ref linkend="Part_and_zone_laminate_tables:mim:Part_and_zone_laminate_tables_mim.smeared_material"/>.</li>
	</ul>
</ext_description>


