<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: 2005/02/03 15:10:43 $"
  rcs.revision="$Revision: 1.7 $">

  <application
    name="express2xml2.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.7"
    source="../data/modules/part_collection/arm.exp"/>

  <schema name="Part_collection_arm">

    <interface
      kind="use"
      schema="Product_occurrence_arm">
    </interface>
    <interface
      kind="use"
      schema="Part_view_definition_arm">
    </interface>
    <interface
      kind="reference"
      schema="Product_identification_arm">
      <interfaced.item
        name="types_of_product"/>
    </interface>
    <entity
      name="Collected_item_association"
      supertypes="Part_occurrence_definition_relationship">
      <explicit
        name="relating_view">
        <typename
          name="Collection_definition"/>
        <redeclaration
          entity-ref="View_definition_relationship"/>
        </explicit>
        <where
          label="WR1"
          expression="NOT ('PART_COLLECTION_ARM.ASSEMBLY_COMPONENT_RELATIONSHIP' IN TYPEOF(SELF))">
        </where>
      </entity>

      <entity
        name="Collection_definition"
        supertypes="Part_view_definition">
        <where
          label="WR1"
          expression="'collection' IN types_of_product(SELF\Product_view_definition.defined_version.of_product)">
        </where>
      </entity>

    </schema>
  </express>
