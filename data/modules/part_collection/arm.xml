<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="part_collection_arm schema_instance"/>
   <schema name="Part_collection_arm">
      <interface kind="use" schema="Product_occurrence_arm"/>
      <interface kind="use" schema="Part_view_definition_arm"/>
      <interface kind="reference" schema="Product_identification_arm">
         <interfaced.item name="types_of_product"/>
      </interface>
      <entity name="Collected_item_association" supertypes="Part_occurrence_definition_relationship">
         <explicit name="relating_view">
            <typename name="Collection_definition"/>
            <redeclaration entity-ref="Product_occurrence_definition_relationship"/>
         </explicit>
         <where label="WR1" expression="NOT ('PART_COLLECTION_ARM.ASSEMBLY_COMPONENT_RELATIONSHIP' IN TYPEOF(SELF))"/>
      </entity>
      <entity name="Collection_definition" supertypes="Part_view_definition">
         <where label="WR1" expression="'collection' IN types_of_product(SELF\Product_view_definition.defined_version.of_product)"/>
      </entity>
   </schema>
</express>
