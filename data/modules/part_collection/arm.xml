<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.22 2002/08/14 06:43:41 goset1 Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.22"
    source="../data/modules/part_collection/arm.exp"/>

  <schema name="Part_collection_arm">
    <interface
      kind="use"
      schema="Product_occurrence_arm">
    </interface>

    <interface
      kind="use"
      schema="Part_view_definition_arm">
    </interface>

    <interface
      kind="reference"
      schema="Product_identification_arm">
      <interfaced.item
        name="types_of_product"/>
    </interface>

    <entity
      name="Collected_item_association"
      supertypes="Occurrence_definition_relationship">
      <explicit
        name="relating_view">
        <redeclaration
          entity-ref="View_definition_relationship"/>
          <typename
            name="Collection_definition"/>
        </explicit>
        <explicit
          name="related_view">
          <redeclaration
            entity-ref="View_definition_relationship"/>
            <typename
              name="part_occurrence_select"/>
          </explicit>
        </entity>

        <entity
          name="Collection_definition"
          supertypes="Part_view_definition">
          <where
            label="WR1"
            expression="'collection' IN types_of(SELF\Product_view_definition.defined_version.of_product)">
          </where>
        </entity>

      </schema>

    </express>
