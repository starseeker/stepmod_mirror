<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.17 2002/07/09 14:29:45 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.17"
    source="../data/modules/part_definition_relationship/arm.exp"/>

  <schema name="Part_definition_relationship_arm">
    <interface
      kind="use"
      schema="Product_view_definition_relationship_arm">
    </interface>

    <interface
      kind="use"
      schema="Part_view_definition_arm">
    </interface>

    <interface
      kind="use"
      schema="Value_with_unit_arm">
    </interface>

    <entity
      name="Make_from_relationship"
      supertypes="View_definition_usage">
      <explicit
        name="relating_view">
        <redeclaration
          entity-ref="Product_view_definition_relationship"/>
          <typename
            name="Part_view_definition"/>
        </explicit>
        <explicit
          name="related_view">
          <redeclaration
            entity-ref="Product_view_definition_relationship"/>
            <typename
              name="Part_view_definition"/>
          </explicit>
          <explicit
            name="quantity"
            optional="YES">
            <typename
              name="Value_with_unit"/>
          </explicit>
          <explicit
            name="priority"
            optional="YES">
            <builtintype
              type="INTEGER"/>
          </explicit>
          <where
            label="WR1"
            expression="SELF\Product_view_definition_relationship.relating_view :&lt;&gt;:   SELF\Product_view_definition_relationship.related_view ">
          </where>
          <where
            label="WR2"
            expression="NOT(EXISTS(quantity)) XOR 	((NOT ('NUMBER' IN TYPEOF(quantity.value_component))) XOR (quantity.value_component &gt; 0))">
          </where>
        </entity>

      </schema>

    </express>
