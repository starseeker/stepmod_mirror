<ext_description linkend="Part_definition_relationship_mim.definitional_product_definition_usage">
	A <b>definitional_product_definition_usage</b> is a type of
	<express_ref linkend="product_structure_schema:ir_express:product_structure_schema.product_definition_usage"/>
	that implements the ARM concept of
	<express_ref linkend="part_definition_relationship:arm:Part_definition_relationship_arm.Definitional_part_view_usage"/>.
</ext_description>
<ext_description linkend="Part_definition_relationship_mim.definitional_product_definition_usage.ur:UR1">
	The combination of the inherited attributes
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_relationship.relating_product_definition"/>
	and
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_relationship.related_product_definition"/>
	shall be unique within a population of <b>definitional_product_definition_usage</b>.
</ext_description>


