<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="part_external_reference_arm schema_instance"/>
   <schema name="Part_external_reference_arm">
      <interface kind="use" schema="Part_template_arm"/>
      <entity name="Package_external_reference">
         <explicit name="design_owner">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="part_number">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="revision_code">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="product_definition_id">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="Package_terminal_external_reference">
         <explicit name="name">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="associated_definition">
            <typename name="Package_external_reference"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="associated_definition"/>
            <unique.attribute attribute="name"/>
         </unique>
      </entity>
      <entity name="Part_terminal_external_reference" supertypes="Shape_element">
         <unique label="UR1">
            <unique.attribute entity-ref="Shape_element" attribute="containing_shape"/>
            <unique.attribute entity-ref="Shape_element" attribute="element_name"/>
         </unique>
         <where label="WR1" expression="SIZEOF(['PART_VIEW_DEFINITION_ARM.PART_VIEW_DEFINITION', 'PHYSICAL_LAYOUT_TEMPLATE_ARM.TEMPLATE_DEFINITION'] *&#10;        TYPEOF(SELF\Shape_element.containing_shape\Item_shape.described_element)) &lt;&gt; 0"/>
      </entity>
   </schema>
</express>
