<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:09" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="part_external_reference_arm schema_instance"/>
   <schema name="PART_EXTERNAL_REFERENCE_ARM">
      <interface kind="use" schema="PHYSICAL_LAYOUT_TEMPLATE_ARM"/>
      <type name="physical_unit_or_part_template">
         <select selectitems="template_definition part_view_definition"/>
      </type>
      <entity name="package_external_reference">
         <explicit name="design_owner">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="part_number">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="revision_code">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="product_definition_id">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="package_terminal_external_reference">
         <explicit name="name">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="associated_definition">
            <typename name="package_external_reference"/>
         </explicit>
         <unique label="ur1">
            <unique.attribute entity-ref="package_terminal_external_reference" attribute="associated_definition"/>
            <unique.attribute entity-ref="package_terminal_external_reference" attribute="name"/>
         </unique>
      </entity>
      <entity name="part_terminal_external_reference">
         <explicit name="associated_definition">
            <typename name="physical_unit_or_part_template"/>
         </explicit>
         <explicit name="feature_name">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="feature_type">
            <builtintype type="STRING"/>
         </explicit>
         <unique label="ur1">
            <unique.attribute entity-ref="part_terminal_external_reference" attribute="associated_definition"/>
            <unique.attribute entity-ref="part_terminal_external_reference" attribute="feature_name"/>
            <unique.attribute entity-ref="part_terminal_external_reference" attribute="feature_type"/>
         </unique>
      </entity>
   </schema>
</express>
