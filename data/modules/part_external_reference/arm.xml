<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1711">
   <application name="JSDAI" owner="LKSoft" source="part_external_reference_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Part_external_reference_arm">
      <interface kind="use" schema="Part_template_arm"/>
      <entity name="Package_external_reference">
         <explicit name="design_owner">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="part_number">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="revision_code">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="product_definition_id">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="Package_terminal_external_reference">
         <explicit name="name">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="associated_definition">
            <typename name="Package_external_reference"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="associated_definition"/>
            <unique.attribute attribute="name"/>
         </unique>
      </entity>
      <entity name="Part_terminal_external_reference" supertypes="General_part_feature">
         <unique label="UR1">
            <unique.attribute attribute="SELF\Shape_element.containing_shape"/>
            <unique.attribute attribute="SELF\Shape_element.element_name"/>
         </unique>
         <where expression="SIZEOF(['PART_VIEW_DEFINITION_ARM.PART_VIEW_DEFINITION', 'PART_TEMPLATE_ARM.TEMPLATE_DEFINITION'] *&#10;        TYPEOF(SELF\Shape_element.containing_shape\Item_shape.described_element)) &lt;&gt; 0" label="WR1"/>
      </entity>
   </schema>
</express>
