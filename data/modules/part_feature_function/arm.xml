<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1712">
   <application name="JSDAI" owner="LKSoft" source="part_feature_function_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Part_feature_function_arm">
      <interface kind="use" schema="Physical_unit_usage_view_arm"/>
      <entity name="Fiducial_part_feature" supertypes="Part_tooling_feature"/>
      <entity name="Part_interface_access_feature" supertypes="Part_feature">
         <explicit name="accessed_interface">
            <typename name="Part_feature"/>
         </explicit>
         <where expression="SELF\Part_feature.material_state_change = material_removal" label="WR1"/>
         <where expression="SELF &lt;&gt; accessed_interface" label="WR2"/>
         <where expression="NOT ('FUNCTIONAL_ASSIGNMENT_TO_PART_ARM.PART_TERMINAL' IN TYPEOF(SELF))" label="WR3"/>
         <where expression="NOT ('PACKAGE_ARM.PACKAGE_BODY' IN TYPEOF(SELF))" label="WR4"/>
         <where expression="NOT ('PART_FEATURE_FUNCTION_ARM.PART_MOUNTING_FEATURE' IN TYPEOF(SELF))" label="WR5"/>
         <where expression="NOT ('INTERCONNECT_MODULE_USAGE_VIEW_ARM.INTERCONNECT_MODULE_SURFACE_FEATURE' IN TYPEOF(SELF))" label="WR6"/>
         <where expression="NOT ('PACKAGE_ARM.PACKAGE_TERMINAL' IN TYPEOF(SELF))" label="WR7"/>
         <where expression="NOT ('PACKAGE_ARM.PACKAGE_BODY_SURFACE' IN TYPEOF(SELF))" label="WR8"/>
         <where expression="NOT ('PACKAGE_ARM.POLARITY_INDICATION_FEATURE' IN TYPEOF(SELF))" label="WR9"/>
      </entity>
      <entity name="Part_mating_feature" supertypes="Part_feature">
         <explicit name="connection_area" optional="YES">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Connection_zone_in_usage_view"/>
         </explicit>
         <where expression="NOT ('PART_FEATURE_FUNCTION_ARM.PART_MOUNTING_FEATURE' IN TYPEOF(SELF))" label="WR1"/>
         <where expression="NOT ('INTERCONNECT_MODULE_USAGE_VIEW_ARM.INTERCONNECT_MODULE_SURFACE_FEATURE' IN TYPEOF(SELF))" label="WR2"/>
         <where expression="NOT ('PACKAGE_ARM.PACKAGE_TERMINAL' IN TYPEOF(SELF))" label="WR3"/>
         <where expression="NOT ('FUNCTIONAL_ASSIGNMENT_TO_PART_ARM.PART_TERMINAL' IN TYPEOF(SELF))" label="WR4"/>
      </entity>
      <entity name="Part_mounting_feature" supertypes="Part_feature">
         <explicit name="connection_area" optional="YES">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Connection_zone_in_usage_view"/>
         </explicit>
      </entity>
      <entity name="Part_tooling_feature" super.expression="ONEOF (Fiducial_part_feature, Tool_registration_mark) ANDOR Test_point_part_feature" supertypes="Part_feature"/>
      <entity name="Test_point_part_feature" supertypes="Part_tooling_feature">
         <where expression="'FUNCTIONAL_ASSIGNMENT_TO_PART_ARM.PART_TERMINAL' IN TYPEOF(SELF)" label="WR1"/>
      </entity>
      <entity name="Thermal_feature" supertypes="Part_feature"/>
      <entity name="Tool_registration_mark" supertypes="Part_tooling_feature"/>
   </schema>
</express>
