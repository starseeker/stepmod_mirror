<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2004-12-20T15:16:40" rcs.revision="1.0" description.file="mim_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="part_feature_function_arm schema_instance"/>
   <schema name="Part_feature_function_arm">
      <interface kind="use" schema="Physical_unit_usage_view_arm"/>
      <interface kind="use" schema="Geometric_tolerance_arm">
         <interfaced.item name="Single_datum"/>
      </interface>
      <entity name="Fiducial_part_feature" supertypes="Part_tooling_feature"/>
      <entity name="Part_interface_access_feature" supertypes="Part_feature">
         <explicit name="accessed_interface">
            <typename name="Part_feature"/>
         </explicit>
         <explicit name="interface_access_area" optional="YES">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="Connection_zone_in_usage_view"/>
         </explicit>
         <where label="WR1" expression=" SELF \ part_feature . material_state_change = material_removal "/>
         <where label="WR2" expression=" SELF &lt;&gt; accessed_interface "/>
         <where label="WR3" expression=" NOT ( 'AP210_ARM.PART_TERMINAL' IN TYPEOF ( SELF ) ) "/>
         <where label="WR4" expression=" NOT ( 'AP210_ARM.PACKAGE_BODY' IN TYPEOF ( SELF ) ) "/>
         <where label="WR5" expression=" NOT ( 'AP210_ARM.PART_MOUNTING_FEATURE' IN TYPEOF ( SELF ) ) "/>
         <where label="WR6" expression=" NOT ( 'AP210_ARM.INTERCONNECT_MODULE_SURFACE_FEATURE' IN TYPEOF ( SELF ) ) "/>
         <where label="WR7" expression=" NOT ( 'AP210_ARM.PACKAGE_TERMINAL' IN TYPEOF ( SELF ) ) "/>
         <where label="WR8" expression=" NOT ( 'AP210_ARM.PACKAGE_BODY_SURFACE' IN TYPEOF ( SELF ) ) "/>
         <where label="WR9" expression=" NOT ( 'AP210_ARM.POLARITY_INDICATION_FEATURE' IN TYPEOF ( SELF ) ) "/>
      </entity>
      <entity name="Part_mating_feature" supertypes="Part_feature">
         <explicit name="mating_area" optional="YES">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="Connection_zone_in_usage_view"/>
         </explicit>
         <where label="WR1" expression=" NOT ( 'AP210_ARM.PART_MOUNTING_FEATURE' IN TYPEOF ( SELF ) ) "/>
         <where label="WR2" expression=" NOT ( 'AP210_ARM.INTERCONNECT_MODULE_SURFACE_FEATURE' IN TYPEOF ( SELF ) ) "/>
         <where label="WR3" expression=" NOT ( 'AP210_ARM.PACKAGE_TERMINAL' IN TYPEOF ( SELF ) ) "/>
         <where label="WR4" expression=" NOT ( 'AP210_ARM.PART_TERMINAL' IN TYPEOF ( SELF ) ) "/>
      </entity>
      <entity name="Part_mounting_feature" supertypes="Part_feature">
         <explicit name="mounting_area" optional="YES">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="Connection_zone_in_usage_view"/>
         </explicit>
      </entity>
      <entity name="Part_tooling_feature" supertypes="Part_feature" super.expression="ONEOF (Fiducial_part_feature, Tool_registration_mark) ANDOR Test_point_part_feature"/>
      <entity name="Test_point_part_feature" supertypes="Part_tooling_feature"/>
      <entity name="Thermal_feature" supertypes="Part_feature"/>
      <entity name="Tool_registration_mark" supertypes="Part_tooling_feature"/>
   </schema>
</express>
