<?xml version="1.0" encoding="utf-8"?><?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express
  SYSTEM "../../../dtd/express.dtd">
<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="">
   <application name="JSDAI" owner="LKSoft" source="part_feature_grouping_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Part_feature_grouping_arm">
      <interface kind="use" schema="Physical_unit_usage_view_arm"/>
      <interface kind="reference" schema="Support_resource_arm">
         <interfaced.item name="bag_to_set"/>
      </interface>
      <entity abstract.supertype="YES" name="Part_array_feature" super.expression="ONEOF (Part_linear_array_feature, Part_rectangular_array_feature)" supertypes="Part_group_feature"/>
      <entity name="Part_feature_placement_link">
         <explicit name="precedent_element">
            <typename name="Part_feature"/>
         </explicit>
         <explicit name="subsequent_element">
            <typename name="Part_feature"/>
         </explicit>
         <inverse attribute="element" entity="Part_linear_array_feature" name="referenced_by"/>
         <where expression="acyclic_part_feature_placement_link(SELF, [subsequent_element], &#xA;        'PART_FEATURE_GROUPING_ARM.PART_FEATURE_PLACEMENT_LINK')" label="WR1"/>
      </entity>
      <entity name="Part_group_feature" super.expression="Part_array_feature" supertypes="Part_feature Composite_shape_element"/>
      <entity name="Part_linear_array_feature" supertypes="Part_array_feature">
         <explicit name="element">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Part_feature_placement_link"/>
         </explicit>
         <explicit name="initial_node_positive_x_displacement">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="terminus_node_positive_x_displacement">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="location_uncertainty">
            <typename name="Length_data_element"/>
         </explicit>
         <where expression="(1 + SIZEOF(element)) = pfg_get_sz_pf(element)" label="WR1"/>
      </entity>
      <entity name="Part_linear_array_feature_link">
         <explicit name="precedent_element">
            <typename name="Part_linear_array_feature"/>
         </explicit>
         <explicit name="subsequent_element">
            <typename name="Part_linear_array_feature"/>
         </explicit>
         <inverse attribute="element" entity="Part_rectangular_array_feature" name="referenced_by"/>
         <where expression="acyclic_part_linear_array_feature_link(SELF, [subsequent_element], &#xA;        'PART_FEATURE_GROUPING_ARM.PART_LINEAR_ARRAY_FEATURE_LINK')" label="WR1"/>
      </entity>
      <entity name="Part_rectangular_array_feature" supertypes="Part_array_feature">
         <explicit name="element">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Part_linear_array_feature_link"/>
         </explicit>
         <explicit name="initial_row_positive_y_displacement">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="terminus_row_positive_y_displacement">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="location_uncertainty">
            <typename name="Length_data_element"/>
         </explicit>
         <where expression="(1 + SIZEOF(element)) = pfg_get_sz_plar(element)" label="WR1"/>
      </entity>
      <function name="acyclic_part_feature_placement_link">
         <parameter name="relation">
            <typename name="Part_feature_placement_link"/>
         </parameter>
         <parameter name="relatives">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Part_feature"/>
         </parameter>
         <parameter name="specific_relation">
            <builtintype type="STRING"/>
         </parameter>
         <builtintype type="BOOLEAN"/>
         <algorithm>LOCAL
      x : SET OF Part_feature_placement_link := [];
    END_LOCAL;
    IF relation.precedent_element IN relatives THEN
      RETURN (FALSE);
    END_IF;
    x := QUERY(pd &lt;* bag_to_set(USEDIN(relation.precedent_element,
      'PART_FEATURE_GROUPING_ARM.' +
      'PART_FEATURE_PLACEMENT_LINK.' +
      'SUBSEQUENT_ELEMENT')) | specific_relation IN TYPEOF(pd));
    REPEAT i := 1 TO HIINDEX(x);
      IF NOT acyclic_part_feature_placement_link(x[i], relatives +
        relation.precedent_element, specific_relation) THEN
        RETURN (FALSE);
      END_IF;
    END_REPEAT;
    RETURN (TRUE);</algorithm>
      </function>
      <function name="acyclic_part_linear_array_feature_link">
         <parameter name="relation">
            <typename name="Part_linear_array_feature_link"/>
         </parameter>
         <parameter name="relatives">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Part_linear_array_feature"/>
         </parameter>
         <parameter name="specific_relation">
            <builtintype type="STRING"/>
         </parameter>
         <builtintype type="BOOLEAN"/>
         <algorithm>LOCAL
      x : SET OF Part_linear_array_feature_link := [];
    END_LOCAL;
    IF relation.precedent_element IN relatives THEN
      RETURN (FALSE);
    END_IF;
    x := QUERY(pd &lt;* bag_to_set(USEDIN(relation.precedent_element,
      'PART_FEATURE_GROUPING_ARM.' +
      'PART_LINEAR_ARRAY_FEATURE_LINK.' +
      'SUBSEQUENT_ELEMENT')) | specific_relation IN TYPEOF(pd));
    REPEAT i := 1 TO HIINDEX(x);
      IF NOT acyclic_part_linear_array_feature_link(x[i], relatives +
        relation.precedent_element, specific_relation) THEN
        RETURN (FALSE);
      END_IF;
    END_REPEAT;
    RETURN (TRUE);</algorithm>
      </function>
      <function name="pfg_get_sz_pf">
         <parameter name="input">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Part_feature_placement_link"/>
         </parameter>
         <builtintype type="INTEGER"/>
         <algorithm>LOCAL
      ac : SET OF Part_feature := [];
    END_LOCAL;
    REPEAT i := 1 TO HIINDEX(input);
      ac := ac + input[i]\Part_feature_placement_link.precedent_element +
        input[i]\Part_feature_placement_link.subsequent_element;
    END_REPEAT;
    RETURN (SIZEOF(ac));</algorithm>
      </function>
      <function name="pfg_get_sz_plar">
         <parameter name="input">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Part_linear_array_feature_link"/>
         </parameter>
         <builtintype type="INTEGER"/>
         <algorithm>LOCAL
      ac : SET OF Part_linear_array_feature := [];
    END_LOCAL;
    REPEAT i := 1 TO HIINDEX(input);
      ac := ac + input[i]\Part_linear_array_feature_link.precedent_element +
        input[i]\Part_linear_array_feature_link.subsequent_element;
    END_REPEAT;
    RETURN (SIZEOF(ac));</algorithm>
      </function>
   </schema>
</express>