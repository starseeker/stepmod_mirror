<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:09" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="part_feature_grouping_arm schema_instance"/>
   <schema name="PART_FEATURE_GROUPING_ARM">
      <interface kind="use" schema="PHYSICAL_UNIT_USAGE_VIEW_ARM"/>
      <interface kind="use" schema="SHAPE_COMPOSITION_ARM"/>
      <interface kind="use" schema="VALUE_WITH_UNIT_EXTENSION_ARM"/>
      <entity name="part_array_feature" abstract.supertype="YES" supertypes="part_group_feature" super.expression="ONEOF (part_linear_array_feature, part_rectangular_array_feature)"/>
      <entity name="part_feature_placement_link">
         <explicit name="precedent_element">
            <typename name="part_feature"/>
         </explicit>
         <explicit name="subsequent_element">
            <typename name="part_feature"/>
         </explicit>
         <inverse name="referenced_by" entity="part_linear_array_feature" attribute="element"/>
      </entity>
      <entity name="part_group_feature" supertypes="part_feature composite_shape_element" super.expression="part_array_feature"/>
      <entity name="part_linear_array_feature" supertypes="part_array_feature">
         <explicit name="element">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="part_feature_placement_link"/>
         </explicit>
         <explicit name="initial_node_positive_x_displacement">
            <typename name="length_data_element"/>
         </explicit>
         <explicit name="terminus_node_positive_x_displacement">
            <typename name="length_data_element"/>
         </explicit>
         <explicit name="location_uncertainty">
            <typename name="length_data_element"/>
         </explicit>
      </entity>
      <entity name="part_linear_array_feature_link">
         <explicit name="precedent_element">
            <typename name="part_linear_array_feature"/>
         </explicit>
         <explicit name="subsequent_element">
            <typename name="part_linear_array_feature"/>
         </explicit>
         <inverse name="referenced_by" entity="part_rectangular_array_feature" attribute="element"/>
      </entity>
      <entity name="part_rectangular_array_feature" supertypes="part_array_feature">
         <explicit name="element">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="part_linear_array_feature_link"/>
         </explicit>
         <explicit name="initial_row_positive_y_displacement">
            <typename name="length_data_element"/>
         </explicit>
         <explicit name="terminus_row_positive_y_displacement">
            <typename name="length_data_element"/>
         </explicit>
         <explicit name="location_uncertainty">
            <typename name="length_data_element"/>
         </explicit>
      </entity>
   </schema>
</express>
