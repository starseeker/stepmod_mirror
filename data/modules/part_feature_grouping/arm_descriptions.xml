<!-- +++++++++++ Part_feature_placement_link +++++++++++ -->
<ext_description linkend="Part_feature_grouping_arm.Part_feature_placement_link">
	A <b>Part_feature_placement_link</b> is a means to specify the order of  
	<express_ref linkend="Physical_unit_usage_view:arm:Physical_unit_usage_view_arm.Part_feature"/> 
	in a linear array.
</ext_description>

<!-- referenced_by -->
<ext_description linkend="Part_feature_grouping_arm.Part_feature_placement_link.referenced_by">
	an inverse relationship that specifies that the existence of the <b>Part_feature_placement_link</b> is dependent on 
	the existence of the  
	<express_ref linkend="Part_feature_grouping:arm:Part_feature_grouping_arm.Part_linear_array_feature"/> 
	that specifies the <b>Part_feature_placement_link</b> as its 
	<express_ref linkend="Part_feature_grouping:arm:Part_feature_grouping_arm.Part_linear_array_feature.element"/>.
</ext_description>

<!-- subsequent_element -->
<ext_description linkend="Part_feature_grouping_arm.Part_feature_placement_link.subsequent_element">
	specifies a 
	<express_ref linkend="Physical_unit_usage_view:arm:Physical_unit_usage_view_arm.Part_feature"/> 
	for the <b>Part_feature_placement_link</b>.
</ext_description>

<!-- precedent_element -->
