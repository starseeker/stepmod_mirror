<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-04-04T09:52:20" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="part_feature_grouping_mim schema_instance"/>
   <schema name="Part_feature_grouping_mim">
      <interface kind="use" schema="Physical_unit_usage_view_mim"/>
      <interface kind="use" schema="shape_aspect_definition_schema">
         <interfaced.item name="composite_shape_aspect"/>
      </interface>
      <entity name="composite_array_shape_aspect" supertypes="composite_shape_aspect" super.expression="ONEOF (linear_composite_array_shape_aspect, rectangular_composite_array_shape_aspect)"/>
      <entity name="composite_array_shape_aspect_link" supertypes="shape_aspect shape_aspect_relationship">
         <where label="WR1" expression=" SELF \ shape_aspect_relationship . related_shape_aspect :&lt;&gt;: SELF \ shape_aspect_relationship . relating_shape_aspect "/>
         <where label="WR2" expression=" 'PART_FEATURE_GROUPING_MIM.' + 'SHAPE_ASPECT' IN TYPEOF ( SELF \ shape_aspect_relationship . related_shape_aspect ) "/>
         <where label="WR3" expression=" 'PART_FEATURE_GROUPING_MIM.' + 'SHAPE_ASPECT' IN TYPEOF ( SELF \ shape_aspect_relationship . relating_shape_aspect ) "/>
         <where label="WR4" expression=" SIZEOF ( TYPEOF ( SELF ) - TYPEOF ( SELF \ shape_aspect || SELF \ shape_aspect_relationship || SELF \ composite_array_shape_aspect_link ) ) = 0 "/>
         <where label="WR5" expression=" SELF \ shape_aspect . name = '' "/>
         <where label="WR6" expression=" SELF \ shape_aspect_relationship . name = '' "/>
         <where label="WR7" expression=" SIZEOF ( USEDIN ( SELF , 'PART_FEATURE_GROUPING_MIM.' + 'SHAPE_ASPECT_RELATIONSHIP.RELATED_SHAPE_ASPECT' ) ) = 1 "/>
      </entity>
      <entity name="linear_composite_array_shape_aspect" supertypes="composite_array_shape_aspect"/>
      <entity name="linear_composite_array_shape_aspect_link" supertypes="shape_aspect shape_aspect_relationship">
         <where label="WR1" expression=" SELF \ shape_aspect_relationship . related_shape_aspect :&lt;&gt;: SELF \ shape_aspect_relationship . relating_shape_aspect "/>
         <where label="WR2" expression=" 'PART_FEATURE_GROUPING_MIM.' + 'LINEAR_COMPOSITE_ARRAY_SHAPE_ASPECT' IN TYPEOF ( SELF \ shape_aspect_relationship . related_shape_aspect ) "/>
         <where label="WR3" expression=" 'PART_FEATURE_GROUPING_MIM.' + 'LINEAR_COMPOSITE_ARRAY_SHAPE_ASPECT' IN TYPEOF ( SELF \ shape_aspect_relationship . relating_shape_aspect ) "/>
         <where label="WR4" expression=" SIZEOF ( TYPEOF ( SELF ) - TYPEOF ( SELF \ shape_aspect || SELF \ shape_aspect_relationship || SELF \ linear_composite_array_shape_aspect_link ) ) = 0 "/>
         <where label="WR5" expression=" SELF \ shape_aspect . name = '' "/>
         <where label="WR6" expression=" SELF \ shape_aspect_relationship . name = '' "/>
         <where label="WR7" expression=" SIZEOF ( USEDIN ( SELF , 'PART_FEATURE_GROUPING_MIM.' + 'SHAPE_ASPECT_RELATIONSHIP.RELATED_SHAPE_ASPECT' ) ) = 1 "/>
      </entity>
      <entity name="rectangular_composite_array_shape_aspect" supertypes="composite_array_shape_aspect"/>
   </schema>
</express>
