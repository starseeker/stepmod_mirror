<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2004-12-24T15:06:27" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="part_feature_location_arm schema_instance"/>
   <schema name="Part_feature_location_arm">
      <interface kind="use" schema="Layered_2d_shape_arm"/>
      <interface kind="reference" schema="Non_feature_shape_element_arm">
         <interfaced.item name="Non_feature_shape_definition"/>
         <interfaced.item name="View_level_non_feature_shape_definition"/>
      </interface>
      <type name="feature_or_non_feature_usage">
         <select selectitems="part_template_or_physical_unit_shape_select View_level_non_feature_shape_definition"/>
      </type>
      <type name="part_template_or_physical_unit_shape_select">
         <select extensible="YES" genericentity="YES"/>
      </type>
      <type name="usage_concept">
         <select extensible="YES" genericentity="YES"/>
      </type>
      <entity name="Usage_concept_usage_relationship">
         <explicit name="associating_usage_shape">
            <typename name="feature_or_non_feature_usage"/>
         </explicit>
         <explicit name="associated_usage">
            <typename name="usage_concept"/>
         </explicit>
         <explicit name="associated_shape_definition">
            <typename name="Geometric_model"/>
         </explicit>
         <explicit name="associated_usage_placement">
            <typename name="axis_placement_or_cto2d"/>
         </explicit>
         <where label="WR1" expression=" ( ( 'AP210_ARM.NON_FEATURE_SHAPE_DEFINITION' IN TYPEOF ( associated_shape_definition ) ) AND ( 'AP210_ARM.NON_FEATURE_SHAPE_DEFINITION' IN TYPEOF ( associating_usage_shape ) ) ) OR ( ( 'AP210_ARM.FEATURE_SHAPE_DEFINITION' IN TYPEOF ( associated_shape_definition ) ) AND ( 'AP210_ARM.FEATURE_SHAPE_DEFINITION' IN TYPEOF ( associating_usage_shape ) ) ) "/>
         <where label="WR2" expression=" NOT ( 'AP210_ARM.cartesian_transformation_2d' IN TYPEOF ( associated_usage_placement ) ) OR ( 'AP210_ARM.NON_FEATURE_SHAPE_DEFINITION' IN TYPEOF ( associating_usage_shape ) ) "/>
      </entity>
   </schema>
</express>
