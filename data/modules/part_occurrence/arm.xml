<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="part_occurrence_arm schema_instance"/>
   <schema name="Part_occurrence_arm">
      <interface kind="use" schema="Generic_product_occurrence_arm"/>
      <interface kind="use" schema="Part_view_definition_arm"/>
      <interface kind="use" schema="Assembly_structure_arm"/>
      <entity name="Definition_based_part_occurrence" supertypes="Definition_based_product_occurrence Part_occurrence">
         <explicit name="derived_from">
            <typename name="Part_view_definition"/>
            <redeclaration entity-ref="Definition_based_product_occurrence"/>
         </explicit>
      </entity>
      <entity name="Part_occurrence" abstract.supertype="YES" supertypes="Product_occurrence">
         <explicit name="defined_version">
            <typename name="Part_version"/>
            <redeclaration entity-ref="Product_view_definition"/>
         </explicit>
      </entity>
      <entity name="Part_occurrence_definition_relationship" supertypes="View_definition_relationship">
         <explicit name="relating_view">
            <typename name="Part_view_definition"/>
            <redeclaration entity-ref="View_definition_relationship"/>
         </explicit>
         <explicit name="related_view">
            <typename name="Part_occurrence"/>
            <redeclaration entity-ref="View_definition_relationship"/>
         </explicit>
         <derived name="id" expression="SELF\View_definition_relationship.related_view\Product_view_definition.id">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="View_definition_relationship"/>
         </derived>
         <where label="WR1" expression="NOT (('ASSEMBLY_STRUCTURE_ARM.ASSEMBLY_COMPONENT_RELATIONSHIP' IN&#10;TYPEOF(SELF))AND (EXISTS(SELF\Assembly_component_relationship.quantity)))"/>
      </entity>
      <entity name="Specification_based_part_occurrence" supertypes="Specification_based_product_occurrence Part_occurrence"/>
   </schema>
</express>
