<ext_description linkend="Part_shape_arm">
</ext_description>


<ext_description linkend="Part_shape_arm.ps_additional_application_domain_enumeration">
	A <b>ps_additional_application_domain_enumeration</b> provides predefined identifiers for the additional application domains of
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Additional_view_definition_context"/>s.
</ext_description>
<ext_description linkend="Part_shape_arm.ps_additional_application_domain_enumeration.electrical">
	the application domain is electrical.
</ext_description>
<ext_description linkend="Part_shape_arm.ps_additional_application_domain_enumeration.thermal">
	the application domain is thermal.
</ext_description>


<ext_description linkend="Part_shape_arm.ps_initial_application_domain_enumeration">
	The <b>ps_initial_application_domain_enumeration</b> provides predefined identifiers for the initial application domains of
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/>s.
</ext_description>
<ext_description linkend="Part_shape_arm.ps_initial_application_domain_enumeration.electrical">
	the application domain is electrical.
</ext_description>
<ext_description linkend="Part_shape_arm.ps_initial_application_domain_enumeration.thermal">
	the application domain is thermal.
</ext_description>


<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Part_shape_arm  Entity: General_part_feature  
   <express_ref linkend="part_shape:arm:Part_shape_arm.General_part_feature"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Part_shape_arm.General_part_feature">
	A <b>General_part_feature</b> is a type of
	
	<express_ref linkend="part_shape:arm:Part_shape_arm.Part_shape_element"/>
	that represents an external shape feature of a 
	<express_ref linkend="Part_view_definition:arm:Part_view_definition_arm.Part_view_definition" />.
</ext_description>
<ext_description linkend="Part_shape_arm.General_part_feature.product_definitional">
	the <b>General_part_feature</b> shall be definitional.
</ext_description>


<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Part_shape_arm  Entity: Part_shape_element  
   <express_ref linkend="part_shape:arm:Part_shape_arm.Part_shape_element"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Part_shape_arm.Part_shape_element">
	A <b>Part_shape_element</b> is a type of
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element"/>
	that represents an element of the shape of a
	<express_ref linkend="Part_view_definition:arm:Part_view_definition_arm.Part_view_definition" />.
</ext_description>

<ext_description linkend="Part_shape_arm.Part_shape_element.associated_definition">
	an inherited attribute whose value shall be of type
	<express_ref linkend="part_view_definition:arm:Part_view_definition_arm.Part_view_definition"/>.
</ext_description>

</ext_descriptions>
