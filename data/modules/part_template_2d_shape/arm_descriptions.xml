   <express_ref linkend="part_template_2d_shape:arm:Part_template_2d_shape_arm"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Part_template_2d_shape_arm">
</ext_description>

<!-- +++++++++++ TYPE padstack_arrangement +++++++++++ -->
<ext_description linkend="Part_template_2d_shape_arm.padstack_arrangement">
A <b>padstack_arrangement</b> lists the types of the padstack arrangement.
</ext_description>
<ext_description linkend="Part_template_2d_shape_arm.padstack_arrangement.top">
specifies that padstack is arranged from the top;
</ext_description>
<ext_description linkend="Part_template_2d_shape_arm.padstack_arrangement.bottom">
specifies that padstack is arranged from the bottom;
</ext_description>
<ext_description linkend="Part_template_2d_shape_arm.padstack_arrangement.symmetrical">
specifies that padstack is arranged symmetrically;
</ext_description>
<ext_description linkend="Part_template_2d_shape_arm.padstack_arrangement.swapable">
specifies that padstack is swapable;
</ext_description>

<!-- +++++++++++ TYPE template_arrangement +++++++++++ -->
<ext_description linkend="Part_template_2d_shape_arm.template_arrangement">
	A <b>template_arrangement</b> lists the possible values of the template arrangement.
</ext_description>

<!-- top -->
<ext_description linkend="Part_template_2d_shape_arm.template_arrangement.top">
	specifies an arrangement to be top;
</ext_description>

<!-- bottom -->
<ext_description linkend="Part_template_2d_shape_arm.template_arrangement.bottom">
	specifies an arrangement to be bottom;
</ext_description>

<!-- symmetrical -->
<ext_description linkend="Part_template_2d_shape_arm.template_arrangement.symmetrical">
	specifies an arrangement to be symmetrical;
</ext_description>

<!-- swapable -->
<ext_description linkend="Part_template_2d_shape_arm.template_arrangement.swapable">
	specifies an arrangement to be swapable;
</ext_description>


<!-- +++++++++++ Inter_stratum_feature_template_location +++++++++++ -->
<ext_description linkend="Part_template_2d_shape_arm.Inter_stratum_feature_template_location">
	A <b>Inter_stratum_feature_template_location</b> is a type of 
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Template_location_in_structured_template"/>.
</ext_description>

<!-- assembly -->
<ext_description linkend="Part_template_2d_shape_arm.Inter_stratum_feature_template_location.assembly">
	specifies a role of the 
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Multi_stratum_structured_template"/>
	for the <b>Inter_stratum_feature_template_location</b>.
</ext_description>

<!-- template -->
<ext_description linkend="Part_template_2d_shape_arm.Inter_stratum_feature_template_location.template">
	specifies a role of the 
	<express_ref linkend="Fabrication_technology:arm:Fabrication_technology_arm.Inter_stratum_feature_template"/>
	for the <b>Inter_stratum_feature_template_location</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Part_template_2d_shape_arm.Inter_stratum_feature_template_location.wr:WR1">
	The <express_ref linkend="Part_template_extension:arm:Part_template_extension_arm.Inter_stratum_feature_edge_segment_template"/>
	and the <express_ref linkend="Part_template_extension:arm:Part_template_extension_arm.Inter_stratum_feature_edge_template"/>
	shall not be in type of <b>template</b>.
</ext_description>

<!-- +++++++++++ Layer_based_package_keepout_shape_model +++++++++++ -->
<ext_description linkend="Part_template_2d_shape_arm.Layer_based_package_keepout_shape_model">
	A <b>Layer_based_package_keepout_shape_model</b> is a type of
	<express_ref linkend="Physical_unit_2d_shape:arm:Physical_unit_2d_shape_arm.Physical_unit_planar_keepout_shape_model"/>.
</ext_description>

<!-- kept_out_layers -->
<ext_description linkend="Part_template_2d_shape_arm.Layer_based_package_keepout_shape_model.kept_out_layers">
	specifies a role of the
	<express_ref linkend="Fabrication_technology:arm:Fabrication_technology_arm.Stratum_technology"/>
	for the <b>Layer_based_package_keepout_shape_model</b>. There shall be one or more 
	<express_ref linkend="Fabrication_technology:arm:Fabrication_technology_arm.Stratum_technology">Stratum_technologies</express_ref>
	for the <b>Layer_based_package_keepout_shape_model</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Part_template_2d_shape_arm.Layer_based_package_keepout_shape_model.wr:WR1">
	The only allowed values for <express_ref linkend="physical_unit_shape_with_parameters:arm:Physical_unit_shape_with_parameters_arm.Physical_unit_keepout_shape_model.constrained_design_object_category"/>
	are: <express_ref linkend="shape_parameters:arm:Shape_parameters_arm.keepout_product_design_object_category.interconnect_module_via"/>,
	<express_ref linkend="shape_parameters:arm:Shape_parameters_arm.keepout_product_design_object_category.interconnect_module_inter_stratum_feature"/>,
	<express_ref linkend="shape_parameters:arm:Shape_parameters_arm.keepout_product_design_object_category.interconnect_module_cutout"/>,
	<express_ref linkend="shape_parameters:arm:Shape_parameters_arm.keepout_product_design_object_category.interconnect_module_fill_area"/>
	and <express_ref linkend="shape_parameters:arm:Shape_parameters_arm.keepout_product_design_object_category.interconnect_module_stratum_feature"/>.	
</ext_description>

<!-- +++++++++++ Material_removal_structured_template +++++++++++ -->
<ext_description linkend="Part_template_2d_shape_arm.Material_removal_structured_template">
	A <b>Material_removal_structured_template</b> is a type of 
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Single_stratum_structured_template"/>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Part_template_2d_shape_arm.Material_removal_structured_template.wr:WR1">
	
</ext_description>

<!-- +++++++++++ Multi_stratum_special_symbol_template +++++++++++ -->
<ext_description linkend="Part_template_2d_shape_arm.Multi_stratum_special_symbol_template">
	A <b>Multi_stratum_special_symbol_template</b> is a type of
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Multi_stratum_structured_template"/>
	and a type of
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Special_symbol_template"/>.
</ext_description>

<!-- +++++++++++ Multi_stratum_structured_template +++++++++++ -->
<ext_description linkend="Part_template_2d_shape_arm.Multi_stratum_structured_template">
	A <b>Multi_stratum_structured_template</b> is a type of
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Structured_template"/>.
	A <b>Multi_stratum_structured_template</b> may be one of a
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Padstack_definition"/>
	or a 
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Multi_stratum_special_symbol_template"/>
	or additionally may be a
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Stratum_stack_dependent_template"/>.
</ext_description>

<!-- location -->
<ext_description linkend="Part_template_2d_shape_arm.Multi_stratum_structured_template.location">
	specifies a role of the
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.template_arrangement"/>
	for the <b>Multi_stratum_structured_template</b>.
</ext_description>


<!-- +++++++++++ Structured_template_planar_shape_model +++++++++++ -->

<ext_description linkend="Part_template_2d_shape_arm.Structured_template_planar_shape_model">
	A <b>Structured_template_planar_shape_model</b> is a type of
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Part_template_planar_shape_model"/>.
</ext_description>


<!-- shape_characterized_definition -->

<ext_description linkend="Part_template_2d_shape_arm.Structured_template_planar_shape_model.shape_characterized_definition">
	specifies a role of the
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Structured_template"/>
	for the <b>Structured_template_planar_shape_model</b>. There shall exist exactly one 
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Structured_template"/>
	for the <b>Structured_template_planar_shape_model</b>.
</ext_description>


<!-- shape_environment -->

<ext_description linkend="Part_template_2d_shape_arm.Structured_template_planar_shape_model.shape_environment">
	specifies the
	<express_ref linkend="Shape_parameters:arm:Shape_parameters_arm.application_environment"/>
	for the <b>Structured_template_planar_shape_model</b>. The value of this attribute need not be specified.
</ext_description>


<!-- shape_material_condition -->

<ext_description linkend="Part_template_2d_shape_arm.Structured_template_planar_shape_model.shape_material_condition">
	specifies the
	<express_ref linkend="Shape_parameters:arm:Shape_parameters_arm.material_condition"/>
	for the <b>Structured_template_planar_shape_model</b>.
</ext_description>


<!-- UR1 -->

<ext_description linkend="Part_template_2d_shape_arm.Structured_template_planar_shape_model.ur:UR1">
	The <b>shape_characterized_definition</b> shall be unique within a population of the <b>Structured_template_planar_shape_model</b>.
</ext_description>


<!-- +++++++++++ Padstack_definition +++++++++++ -->
<ext_description linkend="Part_template_2d_shape_arm.Padstack_definition">
	A <b>Padstack_definition</b> is a type of 
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Multi_stratum_structured_template"/>.
	A <b>Padstack_definition</b> may be a 
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Passage_padstack_definition"/> 
	
</ext_description>

<!-- template -->
<!-- 
<ext_description linkend="Part_template_2d_shape_arm.Padstack_definition.template">
	an inverse relationship that specifies that the existence of the <b>Padstack_definition</b> is dependent on the existence of the 
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Part_template_location_in_fd_or_pd"/> 
	that specifies the <b>Padstack_definition</b> as its 
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Part_template_location_in_fd_or_pd.assembly"/>.  
	There shall be one or more  
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Part_template_location_in_fd_or_pd"/> 
	for a <b>Padstack_definition</b>.
</ext_description>
-->

<!-- shape -->
<!-- 
<ext_description linkend="Part_template_2d_shape_arm.Padstack_definition.shape">
	an inverse relationship that specifies that the existence of the <b>Padstack_definition</b> is dependent on the existence of the 
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Padstack_definition_shape_model"/> 
	that specifies the <b>Padstack_definition</b> as its 
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Padstack_definition_shape_model.shape_characterized_definition"/>.
</ext_description>
-->

<!-- padstack_location -->
