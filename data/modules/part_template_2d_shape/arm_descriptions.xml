<ext_description linkend="Part_template_2d_shape_arm.Padstack_definition.padstack_location">
	specifies the 
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.padstack_arrangement"/> 
	for the <b>Padstack_definition</b>.
</ext_description>
-->

<!-- reference_implementation -->
<!-- 
<ext_description linkend="Part_template_2d_shape_arm.Padstack_definition.reference_implementation">
	specifies the 
	<express_ref linkend="Part_template_extension:arm:Part_template_extension_arm.Reference_packaged_part_interconnect_implementation"/> 
	for the <b>Padstack_definition</b>.
</ext_description>
-->

<!-- WR1 -->
<ext_description linkend="Part_template_2d_shape_arm.Padstack_definition.wr:WR1">
	The <express_ref linkend="product_identification:arm:Product_identification_arm.Product"/> 
	refered by
	<express_ref linkend="product_version:arm:Product_version_arm.Product_version.of_product"/> of
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition.defined_version"/> 
	shall be assigned by 
	<express_ref linkend="product_identification:arm:Product_identification_arm.Product_category_assignment"/>
	to at least one 
	<express_ref linkend="product_categorization:arm:Product_categorization_arm.Product_category"/> 
	with
	<express_ref linkend="product_categorization:arm:Product_categorization_arm.Product_category.name"/> 
	'template model'.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Part_template_2d_shape_arm.Padstack_definition.wr:WR2">
		
</ext_description>

<!-- WR3 -->
<ext_description linkend="Part_template_2d_shape_arm.Padstack_definition.wr:WR3">
	
</ext_description>


<!-- +++++++++++ Passage_padstack_definition +++++++++++ -->
<ext_description linkend="Part_template_2d_shape_arm.Passage_padstack_definition">
	A <b>Passage_padstack_definition</b> is a type of 
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Padstack_definition"/> 
	that references a specific 
	<express_ref linkend="Fabrication_technology:arm:Fabrication_technology_arm.Passage_technology"/>.
</ext_description>

<!-- reference_isft -->
<ext_description linkend="Part_template_2d_shape_arm.Passage_padstack_definition.reference_isft">
	specifies the 
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Inter_stratum_feature_template_location"/> 
	for the <b>Passage_padstack_definition</b>. There shall exist one or more
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Inter_stratum_feature_template_location">Inter_stratum_feature_template_locations</express_ref> 
	for the <b>Passage_padstack_definition</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Part_template_2d_shape_arm.Passage_padstack_definition.wr:WR1">
	The <b>reference_isft</b> size shall be one.
</ext_description>

<!-- +++++++++++ Shape_element_based_template_location +++++++++++ -->
<ext_description linkend="Part_template_2d_shape_arm.Shape_element_based_template_location">
	A <b>Shape_element_based_template_location</b> is a type of
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Template_location_in_structured_template"/>.
</ext_description>

<!-- reference_element -->
<ext_description linkend="Part_template_2d_shape_arm.Shape_element_based_template_location.reference_element">
	specifies a role of the
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element"/>
	for the <b>Shape_element_based_template_location</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Part_template_2d_shape_arm.Shape_element_based_template_location.wr:WR1">
	<b>reference_element</b>.<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element.containing_shape"/>
	shall be equal to 
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Template_location_in_structured_template.assembly"/>\<express_ref linkend="Fabrication_technology:arm:Fabrication_technology_arm.Dependent_template"/>.<express_ref linkend="Fabrication_technology:arm:Fabrication_technology_arm.Dependent_template.associated_item"/>.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Part_template_2d_shape_arm.Shape_element_based_template_location.wr:WR2">
	
</ext_description>

<!-- +++++++++++ Single_stratum_special_symbol_template +++++++++++ -->
<ext_description linkend="Part_template_2d_shape_arm.Single_stratum_special_symbol_template">
	A <b>Single_stratum_special_symbol_template</b> is a type of
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Single_stratum_structured_template"/>
	and a type of
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Special_symbol_template"/>.
</ext_description>

<!-- ++++++++++ Single_stratum_structured_template +++++++++++ -->
<ext_description linkend="Part_template_2d_shape_arm.Single_stratum_structured_template">
	A <b>Single_stratum_structured_template</b> is a type of
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Structured_template"/>
	and a type of
	<express_ref linkend="Fabrication_technology:arm:Fabrication_technology_arm.Single_stratum_template"/>.
	A <b>Single_stratum_structured_template</b> may be one of a
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Material_removal_structured_template"/>
	or a 
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Single_stratum_special_symbol_template"/>.
</ext_description>

<!-- of_stratum_technology -->
<ext_description linkend="Part_template_2d_shape_arm.Single_stratum_structured_template.of_stratum_technology">
	specifies a role of the
	<express_ref linkend="Fabrication_technology:arm:Fabrication_technology_arm.Stratum_technology"/>
	for the <b>Single_stratum_structured_template</b>. There shall exist one or more
	<express_ref linkend="Fabrication_technology:arm:Fabrication_technology_arm.Stratum_technology">Stratum_technologies</express_ref>
	for the <b>Single_stratum_structured_template</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Part_template_2d_shape_arm.Single_stratum_structured_template.wr:WR1">
	Only 
	<express_ref linkend="Fabrication_technology:arm:Fabrication_technology_arm.Single_stratum_template"/>
	shall help to compose a <b>Single_stratum_structured_template</b>.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Part_template_2d_shape_arm.Single_stratum_structured_template.wr:WR2">
	The technologies referenced by each template shall be the same technologies as the <b>of_stratum_technology</b>.
</ext_description>

<!-- +++++++++++ Template_location_in_structured_template_transform +++++++++++ -->
<ext_description linkend="Part_template_2d_shape_arm.Template_location_in_structured_template_transform">
	A <b>Template_location_in_structured_template_transform</b> is a type of 
	<express_ref linkend="Contextual_shape_positioning:arm:Contextual_shape_positioning_arm.Geometric_placement"/>.
	<!-- 
	A <b>Template_location_in_structured_template_transform</b> is the transform that locates and orients the 
	<express_ref linkend="part_template_2d_shape:arm:Part_template_2d_shape_arm.Part_template_planar_shape_model"/>
	in the <express_ref linkend="part_template_2d_shape:arm:Part_template_2d_shape_arm.Padstack_definition_shape_model"/>.
	-->
</ext_description>

<!-- transform -->
<ext_description linkend="Part_template_2d_shape_arm.Template_location_in_structured_template_transform.transform">
	specifies the 
	<express_ref linkend="Extended_basic_geometry:arm:Extended_basic_geometry_arm.Axis_placement_2d"/> 
	for the <b>Template_location_in_structured_template_transform</b>.
</ext_description>

<!-- reference_location -->
<ext_description linkend="Part_template_2d_shape_arm.Template_location_in_structured_template_transform.reference_location">
	specifies the 
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Template_location_in_structured_template"/> 
	for the <b>Template_location_in_structured_template_transform</b>.
</ext_description>

<!-- template_shape -->
<ext_description linkend="Part_template_2d_shape_arm.Template_location_in_structured_template_transform.template_shape">
	specifies the <express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Part_template_planar_shape_model"/> 
	for the <b>Template_location_in_structured_template_transform</b>.
</ext_description>

<!-- assembly_shape -->
<ext_description linkend="Part_template_2d_shape_arm.Template_location_in_structured_template_transform.assembly_shape">
	specifies the <express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Structured_template_planar_shape_model"/> 
	for the <b>Template_location_in_structured_template_transform</b>.
</ext_description>

<!-- UR1 -->
<ext_description linkend="Part_template_2d_shape_arm.Template_location_in_structured_template_transform.ur:UR1">
	The <b>reference_location</b> shall be unique within a population of the <b>Template_location_in_structured_template_transform</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Part_template_2d_shape_arm.Template_location_in_structured_template_transform.wr:WR1">
	The <express_ref linkend="part_template_shape_with_parameters:arm:Part_template_shape_with_parameters_arm.Part_template_shape_model.shape_characterized_part_template"/>
	of <b>template_shape</b> shall refer same entity instance as <express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Template_location_in_structured_template.template"/>
	of <b>reference_location</b>.
</ext_description>


<!-- WR2 -->
<ext_description linkend="Part_template_2d_shape_arm.Template_location_in_structured_template_transform.wr:WR2">
	The <express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Multi_layer_template_definition_shape_model.shape_characterized_definition"/>
	of <b>assembly_shape</b> shall refer same entity instance as 
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Part_template_location_in_multi_layer_template_definition.assembly"/>
	of <b>reference_location</b>.
</ext_description>

<!-- +++++++++++ Part_template_planar_shape_model +++++++++++ -->
<ext_description linkend="Part_template_2d_shape_arm.Part_template_planar_shape_model">
	A <b>Part_template_planar_shape_model</b> is a type of 
	<express_ref linkend="Layered_2d_shape:arm:Layered_2d_shape_arm.Planar_projected_shape_model"/>, 
	<express_ref linkend="Part_template_shape_with_parameters:arm:Part_template_shape_with_parameters_arm.Part_template_shape_model"/>.
	A <b>Part_template_planar_shape_model</b> is the planar shape of a 
	<express_ref linkend="Physical_layout_template:arm:Physical_layout_template_arm.Template_definition"/>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Part_template_2d_shape_arm.Part_template_planar_shape_model.wr:WR1">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.description"/>
	shall not be populated.
</ext_description>


<!-- +++++++++++ Part_feature_based_template_location +++++++++++ -->
<ext_description linkend="Part_template_2d_shape_arm.Part_feature_based_template_location">
	A <b>Part_feature_based_template_location</b> is a type of
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Template_location_in_structured_template"/>.
</ext_description>

<!-- assembly -->
<ext_description linkend="Part_template_2d_shape_arm.Part_feature_based_template_location.assembly">
	specifies a role of the
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Footprint_definition"/>
	for the <b>Part_feature_based_template_location</b>.
</ext_description>

<!-- reference_feature -->
<ext_description linkend="Part_template_2d_shape_arm.Part_feature_based_template_location.reference_feature">
	specifies a role of the
	<express_ref linkend="Physical_unit_usage_view:arm:Physical_unit_usage_view_arm.Part_feature"/>
	for the <b>Part_feature_based_template_location</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Part_template_2d_shape_arm.Part_feature_based_template_location.wr:WR1">
	The <b>Part_feature_based_template_location</b>
	shall only be instantiated as complex entity instance.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Part_template_2d_shape_arm.Part_feature_based_template_location.wr:WR2">
	The instance in role of <express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element.containing_shape"/>
	of <b>reference_feature</b> shall also be in role of <express_ref linkend="part_template_2d_shape:arm:Part_template_2d_shape_arm.Footprint_definition.reference_package"/>
	of <b>assembly</b>.
</ext_description>


<!-- +++++++++++ Template_location_in_structured_template +++++++++++ -->
<ext_description linkend="Part_template_2d_shape_arm.Template_location_in_structured_template">
<p>
	A <b>Template_location_in_structured_template</b> is the relationship that identifies the location of a  
	<express_ref linkend="Physical_layout_template:arm:Physical_layout_template_arm.Template_definition"/> in a 
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Padstack_definition"/>.  
	A <b>Template_location_in_structured_template</b> may be a 
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Inter_stratum_feature_template_location"/>
	or a <express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Stratum_specific_template_location"/>.
	The Application Object references a  
	<express_ref linkend="Physical_layout_template:arm:Physical_layout_template_arm.Template_definition"/> 
	in the formal model since there is a one to one relationship between a 
	<express_ref linkend="Physical_layout_template:arm:Physical_layout_template_arm.Template_definition"/> 
	and the associated  
	<express_ref linkend="Physical_layout_template:arm:Physical_layout_template_arm.Template_definition"/>.
	There is exactly one  
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Template_location_in_structured_template_transform"/> 
	allowed to reference a <b>Template_location_in_structured_template</b>.
	There is an optional reference to the parametric data necessary to define the shape of material to be added at the intersection of
	the <b>assembly</b> occurrence and the trace that intersects that occurrence.  A parametric model is provided and not an explicit shape model
	because the actual angle the trace makes with respect to the <b>assembly</b> occurrence is unknown, and there may be more than one intersection.
	This model only provides support for one parametric configuration for this data for the <b>Template_location_in_structured_template</b>.
	In the event that trimming of a shape between the trace and the <b>assembly</b> occurrence is to be exchanged, the use of an instance in the design of  
	<express_ref linkend="Layered_interconnect_module_with_design_intent_modifications:arm:Layered_interconnect_module_with_design_intent_modifications_arm.Material_removal_feature"/> is appropriate.
	If the <b>transform</b> attribute exists, then the parametric data shall not be provided.
	If the parametric data is provided, the <b>transform</b> shall not be provided.
</p>
	<note>
		The  <express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Padstack_definition"/> is a geometric shape concept with the result that this requires exactly one shape for the <express_ref linkend="Physical_layout_template:arm:Physical_layout_template_arm.Template_definition"/> used in a location.
	</note> 
</ext_description>

<!-- assembly -->
<ext_description linkend="Part_template_2d_shape_arm.Template_location_in_structured_template.assembly">
	specifies the 
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Structured_template"/> 
	for the <b>Template_location_in_structured_template</b>.
</ext_description>

<!-- template -->
<ext_description linkend="Part_template_2d_shape_arm.Template_location_in_structured_template.template">
	specifies the 
	<express_ref linkend="Fabrication_technology:arm:Fabrication_technology_arm.Geometric_template"/> 
	for the <b>Template_location_in_structured_template</b>.
</ext_description>

<!-- reference_designation -->
<ext_description linkend="Part_template_2d_shape_arm.Template_location_in_structured_template.reference_designation">
	specifies the text that describes the <b>Template_location_in_structured_template</b>.
</ext_description>

<!-- transform -->
<ext_description linkend="Part_template_2d_shape_arm.Template_location_in_structured_template.transform">
	an inverse relationship that specifies that the existence of the <b>Template_location_in_structured_template</b> is 
	dependent on the existence of the  
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Template_location_in_structured_template_transform"/> 
	that specifies the <b>Template_location_in_structured_template</b> as its 
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Template_location_in_structured_template_transform.reference_location"/>. 
	There shall be zero or one 
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Template_location_in_structured_template_transform"/> 
	for a <b>Template_location_in_structured_template</b>.
</ext_description>

<!-- UR1 -->
<ext_description linkend="Part_template_2d_shape_arm.Template_location_in_structured_template.ur:UR1">
	The combination of <b>assembly</b> and <b>reference_designation</b> shall be unique within a population of <b>Template_location_in_structured_template</b>s.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Part_template_2d_shape_arm.Template_location_in_structured_template.wr:WR1">
	If <b>transform</b> is provided, then 
	<express_ref linkend="part_template_shape_with_parameters:arm:Part_template_shape_with_parameters_arm.Part_template_shape_model.shape_characterized_part_template"/>
	of <express_ref linkend="part_template_2d_shape:arm:Part_template_2d_shape_arm.Template_location_in_structured_template_transform.template_shape"/>
	of <b>transform</b> shall not be 
	<express_ref linkend="part_template_extension:arm:Part_template_extension_arm.Snowball_template"/>,
	or <express_ref linkend="part_template_extension:arm:Part_template_extension_arm.Teardrop_by_angle_template"/>,
	or <express_ref linkend="part_template_extension:arm:Part_template_extension_arm.Teardrop_by_length_template"/>. If
	<b>transform</b> is not provided, then <b>template</b> shall be 
	<express_ref linkend="part_template_extension:arm:Part_template_extension_arm.Snowball_template"/>,
	or <express_ref linkend="part_template_extension:arm:Part_template_extension_arm.Teardrop_by_angle_template"/>,
	or <express_ref linkend="part_template_extension:arm:Part_template_extension_arm.Teardrop_by_length_template"/>.
</ext_description>

