<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: single_stratum_special_symbol_template
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Part_template_2d_shape_mim.single_stratum_special_symbol_template">
	A <b>single_stratum_special_symbol_template</b> is a type of 
	<express_ref linkend="Part_template_2d_shape:mim:Part_template_2d_shape_mim.single_stratum_structured_template"/> 
	and a type of 
	<express_ref linkend="Part_template_2d_shape:mim:Part_template_2d_shape_mim.special_symbol_template"/>
	that implements the ARM concept of
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Single_stratum_special_symbol_template"/>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: single_stratum_structured_template
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Part_template_2d_shape_mim.single_stratum_structured_template">
<p>
	A <b>single_stratum_structured_template</b> is a type of 
	<express_ref linkend="Part_template_2d_shape:mim:Part_template_2d_shape_mim.structured_template"/> 
	and a type of
	<express_ref linkend="Fabrication_technology:mim:Fabrication_technology_mim.single_stratum_template"/> 
	that implements the ARM concepts of
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Single_stratum_structured_template"/>.
	A <b>single_stratum_structured_template</b> may be one of a
	<express_ref linkend="Part_template_2d_shape:mim:Part_template_2d_shape_mim.material_removal_structured_template"/>
	or a
	<express_ref linkend="Part_template_2d_shape:mim:Part_template_2d_shape_mim.single_stratum_special_symbol_template"/>.
</p>
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: special_symbol_template
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Part_template_2d_shape_mim.special_symbol_template">
	A <b>special_symbol_template</b> is a type of 
	<express_ref linkend="Physical_layout_template:mim:Physical_layout_template_mim.part_template_definition"/> 
	that implements the ARM concept of
	<express_ref linkend="Part_template_2d_shape:arm:Part_template_2d_shape_arm.Special_symbol_template"/>.
	A <b>special_symbol_template</b> may be one of a
	<express_ref linkend="Part_template_2d_shape:mim:Part_template_2d_shape_mim.single_stratum_special_symbol_template"/> 
	or a 
	<express_ref linkend="Part_template_2d_shape:mim:Part_template_2d_shape_mim.multi_stratum_special_symbol_template"/>.
</ext_description>

