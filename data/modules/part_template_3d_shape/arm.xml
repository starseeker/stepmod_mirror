<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="part_template_3d_shape_arm schema_instance"/>
   <schema name="Part_template_3d_shape_arm">
      <interface kind="use" schema="Edge_shape_feature_arm"/>
      <interface kind="use" schema="Part_template_extension_arm"/>
      <interface kind="reference" schema="Requirement_decomposition_arm">
         <interfaced.item name="get_rvd"/>
      </interface>
      <type name="pt3ds_part_template_or_physical_unit_3d_shape_model_select">
         <select extensible="YES" genericentity="YES" basedon="part_template_or_physical_unit_3d_shape_model_select" selectitems="Part_template_3d_shape_model"/>
      </type>
      <type name="pt3ds_physical_unit_shape_model_select">
         <select extensible="YES" genericentity="YES" basedon="physical_unit_shape_model_select" selectitems="Part_template_3d_shape_model"/>
      </type>
      <entity name="Edge_segment_cross_section" supertypes="Shape_definition_3d_intersection">
         <explicit name="surface_intersection_result">
            <typename name="Edge_shape_model"/>
            <redeclaration entity-ref="Shape_definition_3d_intersection"/>
         </explicit>
         <explicit name="projected_volume">
            <typename name="Part_template_3d_shape_model"/>
            <redeclaration entity-ref="Shape_definition_3d_intersection"/>
         </explicit>
         <derived name="cross_section_reference" expression="SELF\Shape_definition_3d_intersection.projected_volume\Part_template_shape_model.shape_characterized_definition[1]\&#10;       Inter_stratum_feature_edge_segment_template.start_vertex">
            <typename name="Edge_segment_vertex"/>
         </derived>
         <derived name="associated_template" expression="SELF\Shape_definition_3d_intersection.projected_volume\Part_template_shape_model.shape_characterized_definition[1]">
            <typename name="Template_definition"/>
         </derived>
         <inverse name="referencing_template" entity="Inter_stratum_feature_edge_segment_template_with_cross_section" attribute="cross_section"/>
         <where label="WR1" expression="associated_template :=: referencing_template"/>
      </entity>
      <entity name="Inter_stratum_feature_edge_segment_template_with_cross_section" supertypes="Inter_stratum_feature_edge_segment_template">
         <explicit name="cross_section">
            <typename name="Edge_segment_cross_section"/>
         </explicit>
      </entity>
      <entity name="Part_template_3d_keepout_shape_model" supertypes="Non_feature_shape_model Part_template_keepout_shape_model">
         <derived name="application_technology_constraint" expression="get_rvd(SELF, 'application technology constraint')">
            <aggregate type="SET" lower="0" upper="?"/>
            <typename name="Requirement_view_definition"/>
         </derived>
         <where label="WR1" expression="(SIZEOF(application_technology_constraint) &lt;= 1)"/>
         <where label="WR2" expression="NOT EXISTS(SELF\Representation.name)"/>
         <where label="WR3" expression="NOT EXISTS(SELF\Representation.description)"/>
      </entity>
      <entity name="Part_template_3d_shape_model" supertypes="Part_template_shape_model">
         <where label="WR1" expression="NOT EXISTS(SELF\Representation.name)"/>
         <where label="WR2" expression="NOT EXISTS(SELF\Representation.description)"/>
      </entity>
   </schema>
</express>
