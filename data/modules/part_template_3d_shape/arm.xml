<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$">
   <application name="JSDAI" owner="LKSoft" source="part_template_3d_shape_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Part_template_3d_shape_arm">
      <interface kind="use" schema="Edge_shape_feature_arm"/>
      <interface kind="use" schema="Layered_interconnect_simple_template_arm"/>
      <interface kind="reference" schema="Requirement_decomposition_arm">
         <interfaced.item name="get_rvd"/>
      </interface>
      <type name="pt3ds_part_template_or_physical_unit_3d_shape_model_select">
         <select basedon="part_template_or_physical_unit_3d_shape_model_select" extensible="YES" genericentity="YES" selectitems="Part_template_3d_shape_model"/>
      </type>
      <entity name="Edge_segment_cross_section" supertypes="Shape_definition_3d_intersection">
         <explicit name="surface_intersection_result">
            <typename name="Edge_shape_model"/>
            <redeclaration entity-ref="Shape_definition_3d_intersection"/>
         </explicit>
         <explicit name="projected_volume">
            <typename name="Part_template_3d_shape_model"/>
            <redeclaration entity-ref="Shape_definition_3d_intersection"/>
         </explicit>
         <derived expression="SELF\Shape_definition_3d_intersection.projected_volume\Part_template_shape_model.shape_characterized_definition[1]\&#10;       Inter_stratum_feature_edge_segment_template.start_vertex" name="cross_section_reference">
            <typename name="Edge_segment_vertex"/>
         </derived>
         <derived expression="SELF\Shape_definition_3d_intersection.projected_volume\Part_template_shape_model.shape_characterized_definition[1]" name="associated_template">
            <typename name="Template_definition"/>
         </derived>
         <inverse attribute="cross_section" entity="Inter_stratum_feature_edge_segment_template_with_cross_section" name="referencing_template"/>
         <where expression="associated_template :=: referencing_template" label="WR1"/>
      </entity>
      <entity name="Inter_stratum_feature_edge_segment_template_with_cross_section" supertypes="Inter_stratum_feature_edge_segment_template">
         <explicit name="cross_section">
            <typename name="Edge_segment_cross_section"/>
         </explicit>
      </entity>
      <entity name="Part_template_3d_keepout_shape_model" supertypes="Non_feature_shape_model Part_template_keepout_shape_model">
         <derived expression="get_rvd(SELF, 'application technology constraint')" name="application_technology_constraint">
            <aggregate lower="0" type="SET" upper="?"/>
            <typename name="Requirement_view_definition"/>
         </derived>
         <where expression="(SIZEOF(application_technology_constraint) &lt;= 1)" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Representation.name)" label="WR2"/>
         <where expression="NOT EXISTS(SELF\Representation.description)" label="WR3"/>
      </entity>
      <entity name="Part_template_3d_shape_model" supertypes="Part_template_shape_model">
         <where expression="NOT EXISTS(SELF\Representation.name)" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Representation.description)" label="WR2"/>
      </entity>
   </schema>
</express>
