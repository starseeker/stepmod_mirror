<ext_description linkend="Part_template_3d_shape_arm">
</ext_description>

<!-- +++++++++++ Edge_segment_cross_section +++++++++++ -->
<ext_description linkend="Part_template_3d_shape_arm.Edge_segment_cross_section">
<p>
An <b>Edge_segment_cross_section</b> is a type of 
<express_ref linkend="Layered_3d_shape:arm:Layered_3d_shape_arm.Shape_definition_3d_intersection"/>.
</p>
</ext_description>

<!-- referencing_template -->
<ext_description linkend="Part_template_3d_shape_arm.Edge_segment_cross_section.referencing_template">
<p>
	specifies an inverse relationship that specifies that the existence of the <b>Edge_segment_cross_section</b> is dependent on the existence of the 
	<express_ref linkend="Part_template_3d_shape:arm:Part_template_3d_shape_arm.Inter_stratum_feature_edge_segment_template_with_cross_section"/> 
	that specifies the <b>Edge_segment_cross_section</b> as its 
	<express_ref linkend="Part_template_3d_shape:arm:Part_template_3d_shape_arm.Inter_stratum_feature_edge_segment_template_with_cross_section.cross_section"/>. 
</p></ext_description>

<!-- associated_template -->
<ext_description linkend="Part_template_3d_shape_arm.Edge_segment_cross_section.associated_template">
	specifies a role of 
	<express_ref linkend="Part_template:arm:Part_template_arm.Template_definition"/>
	for <b>Edge_segment_cross_section</b>. 
</ext_description>

<!-- cross_section_reference -->
<ext_description linkend="Part_template_3d_shape_arm.Edge_segment_cross_section.cross_section_reference">
	specifies a role of 
	<express_ref linkend="extended_geometric_tolerance:arm:Extended_geometric_tolerance_arm.Edge_segment_vertex"/>
	for <b>Edge_segment_cross_section</b>.
</ext_description>

<!-- projected_volume -->
<ext_description linkend="Part_template_3d_shape_arm.Edge_segment_cross_section.projected_volume">
<p>
	an attribute inherited from the
	<express_ref linkend="Layered_3d_shape:arm:Layered_3d_shape_arm.Shape_definition_3d_intersection"/>
	shall be redeclared as the 
	<express_ref linkend="Part_template_3d_shape:arm:Part_template_3d_shape_arm.Part_template_3d_shape_model"/>
	for the <b>Edge_segment_cross_section</b>.
</p>
</ext_description>

<!-- surface_intersection_result -->
<ext_description linkend="Part_template_3d_shape_arm.Edge_segment_cross_section.surface_intersection_result">
<p>
	an attribute inherited from the
	<express_ref linkend="Layered_3d_shape:arm:Layered_3d_shape_arm.Shape_definition_3d_intersection"/>
	shall be redeclared as the 
	<express_ref linkend="Edge_shape_feature:arm:Edge_shape_feature_arm.Edge_shape_model"/>
	for the <b>Edge_segment_cross_section</b>.
</p>
</ext_description>

<!-- WR1 -->
<ext_description linkend="Part_template_3d_shape_arm.Edge_segment_cross_section.wr:WR1">
<p>
	The <b>associated_template</b> shall also be a <b>referencing_template</b>.
</p>
</ext_description>

<!-- +++++++++++ Inter_stratum_feature_edge_segment_template_with_cross_section +++++++++++ -->
<ext_description linkend="Part_template_3d_shape_arm.Inter_stratum_feature_edge_segment_template_with_cross_section">
	A <b>Inter_stratum_feature_edge_segment_template_with_cross_section</b> is a type of
	<express_ref linkend="Layered_interconnect_simple_template:arm:Layered_interconnect_simple_template_arm.Inter_stratum_feature_edge_segment_template"/>.
</ext_description>

<!-- cross_section -->
<ext_description linkend="Part_template_3d_shape_arm.Inter_stratum_feature_edge_segment_template_with_cross_section.cross_section">
	specifies a role of the
	<express_ref linkend="Part_template_3d_shape:arm:Part_template_3d_shape_arm.Edge_segment_cross_section"/>
	for the <b>Inter_stratum_feature_edge_segment_template_with_cross_section</b>.
</ext_description>

<!-- +++++++++++ Part_template_3d_keepout_shape_model +++++++++++ -->
<ext_description linkend="Part_template_3d_shape_arm.Part_template_3d_keepout_shape_model"><p>
	A <b>Part_template_3d_keepout_shape_model</b> is a type of 
	<express_ref linkend="Non_feature_shape_element:arm:Non_feature_shape_element_arm.Non_feature_shape_model"/> 
	and a type of 
	<express_ref linkend="Part_template_shape_with_parameters:arm:Part_template_shape_with_parameters_arm.Part_template_keepout_shape_model"/>.
	A <b>Part_template_3d_keepout_shape_model</b> is the shape in three orthogonal 
	dimensions of a keepout constraint for a 
	<express_ref linkend="Part_template:arm:Part_template_arm.Template_definition"/>.  
</p>
	<note>
		Keepout regions typically include some minimal spacing around the extreme shape.
	</note>
</ext_description>

<!-- application_technology_constraint -->
<ext_description linkend="Part_template_3d_shape_arm.Part_template_3d_keepout_shape_model.application_technology_constraint">
	specifies a role of the 
	<express_ref linkend="requirement_view_definition:arm:Requirement_view_definition_arm.Requirement_view_definition"/>
	for the <b>Part_template_3d_keepout_shape_model</b>.
	There shall exist zero or more
	<express_ref linkend="requirement_view_definition:arm:Requirement_view_definition_arm.Requirement_view_definition">Requirement_view_definitions</express_ref>
	for the <b>Part_template_3d_keepout_shape_model</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Part_template_3d_shape_arm.Part_template_3d_keepout_shape_model.wr:WR1">
	If <b>application_technology_constraint</b> is provided, then it's size shall be one.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Part_template_3d_shape_arm.Part_template_3d_keepout_shape_model.wr:WR2">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.name"/>
	shall not be populated.
</ext_description>

<!-- WR -->
<ext_description linkend="Part_template_3d_shape_arm.Part_template_3d_keepout_shape_model.wr:WR3">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.description"/>
	shall not be populated.
</ext_description>

<!-- +++++++++++ Part_template_3d_shape_model +++++++++++ -->
<ext_description linkend="Part_template_3d_shape_arm.Part_template_3d_shape_model">
	A <b>Part_template_3d_shape_model</b> is a type of 
	<express_ref linkend="Part_template_shape_with_parameters:arm:Part_template_shape_with_parameters_arm.Part_template_shape_model"/>.
	A <b>Part_template_3d_shape_model</b> is the shape in three dimensions of the 
	<express_ref linkend="Part_template:arm:Part_template_arm.Template_definition"/>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Part_template_3d_shape_arm.Part_template_3d_shape_model.wr:WR1">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.name"/>
	shall not be populated.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Part_template_3d_shape_arm.Part_template_3d_shape_model.wr:WR2">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.description"/>
	shall not be populated.
</ext_description>


</ext_descriptions>

