</ext_description>
<!-- +++++++++++ Part_template_3d_keepout_shape_model +++++++++++ -->
<ext_description linkend="Part_template_3d_shape_arm.Part_template_3d_keepout_shape_model"><p>
	A <b>Part_template_3d_keepout_shape_model</b> is a type of 
	<express_ref linkend="Layered_3d_shape:arm:Layered_3d_shape_arm.three_dimensional_shape"/> 
	and a type of 
	<express_ref linkend="Non_feature_shape_element:arm:Non_feature_shape_element_arm.Usage_view_level_non_feature_shape_definition"/>.
	A <b>Part_template_3d_keepout_shape_model</b> is the shape in three orthogonal 
	dimensions of a keepout constraint for a 
	<express_ref linkend="Physical_layout_template:arm:Physical_layout_template_arm.Template_definition"/>.  
</p>
	<note>
		Keepout regions typically include some minimal spacing around the extreme shape.
	</note>
</ext_description>

<!-- application_technology_constraint -->
<ext_description linkend="Part_template_3d_shape_arm.Part_template_3d_keepout_shape_model.application_technology_constraint">
	the role of the 
	<express_ref linkend="requirement_view_definition:arm:Requirement_view_definition_arm.Requirement_view_definition"/>
	for the <b>Part_template_3d_keepout_shape_model</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Part_template_3d_shape_arm.Part_template_3d_keepout_shape_model.wr:WR1">
	If <b>application_technology_constraint</b> is provided, then it's size shall be one.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Part_template_3d_shape_arm.Part_template_3d_keepout_shape_model.wr:WR2">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.name"/>
	shall not be populated.
</ext_description>

<!-- WR -->
<ext_description linkend="Part_template_3d_shape_arm.Part_template_3d_keepout_shape_model.wr:WR3">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.description"/>
	shall not be populated.
</ext_description>

<!-- +++++++++++ Part_template_3d_shape_model +++++++++++ -->
<ext_description linkend="Part_template_3d_shape_arm.Part_template_3d_shape_model">
	A <b>Part_template_3d_shape_model</b> is a type of 
	<express_ref linkend="Part_template_shape_with_parameters:arm:Part_template_shape_with_parameters_arm.Part_template_shape_model"/>.
	A <b>Part_template_3d_shape_model</b> is the shape in three dimensions of the 
	<express_ref linkend="Physical_layout_template:arm:Physical_layout_template_arm.Template_definition"/>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Part_template_3d_shape_arm.Part_template_3d_shape_model.wr:WR1">
<p>
	<b>Part_template_3d_shape_model</b> shall not be referenced by 
	<express_ref linkend="Extended_basic_geometry:arm:Extended_basic_geometry_arm.Reference_geometric_representation_relationship"/>
	as 
	<express_ref linkend="Extended_basic_geometry:arm:Extended_basic_geometry_arm.Reference_geometric_representation_relationship.referenced_shape"/>.
</p>
</ext_description>

<!-- WR2 -->
<ext_description linkend="Part_template_3d_shape_arm.Part_template_3d_shape_model.wr:WR2">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.name"/>
	shall not be populated.
</ext_description>

<!-- WR3 -->
<ext_description linkend="Part_template_3d_shape_arm.Part_template_3d_shape_model.wr:WR3">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.description"/>
	shall not be populated.
</ext_description>


