<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="part_template_extension_arm schema_instance"/>
   <schema name="Part_template_extension_arm">
      <interface kind="use" schema="Layered_2d_shape_arm"/>
      <interface kind="use" schema="Fill_area_style_arm"/>
      <interface kind="use" schema="Fabrication_technology_arm"/>
      <interface kind="use" schema="Text_representation_arm"/>
      <type name="curve_or_axis_placement">
         <select selectitems="Curve Axis_placement"/>
      </type>
      <type name="extend_or_chord_2_extend_or_truncate_or_round">
         <enumeration items="truncate chord_2_extend round extend"/>
      </type>
      <type name="extend_or_truncate">
         <enumeration items="truncate extend"/>
      </type>
      <type name="square_or_round">
         <enumeration items="square round"/>
      </type>
      <type name="text_alignment">
         <enumeration items="top_right top_centre top_left centre_right centre_centre centre_left bottom_right bottom_centre bottom_left"/>
      </type>
      <entity name="Closed_curve_style_parameters" supertypes="Curve_style_parameters"/>
      <entity name="Curve_style_parameters" abstract.supertype="YES" super.expression="ONEOF (Closed_curve_style_parameters, Curve_style_parameters_with_ends)">
         <explicit name="name">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="corner_style">
            <typename name="extend_or_chord_2_extend_or_truncate_or_round"/>
         </explicit>
         <explicit name="curve_width">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="width_uncertainty">
            <typename name="Length_data_element"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="name"/>
         </unique>
      </entity>
      <entity name="Curve_style_parameters_with_ends" supertypes="Curve_style_parameters">
         <explicit name="end_extension">
            <typename name="extend_or_truncate"/>
         </explicit>
         <explicit name="end_style">
            <typename name="square_or_round"/>
         </explicit>
      </entity>
      <entity name="Default_trace_template" supertypes="Trace_template">
         <explicit name="of_stratum_technology">
            <typename name="Stratum_technology"/>
         </explicit>
      </entity>
      <entity name="Fill_area_template" supertypes="Parametric_template">
         <explicit name="fill_area_style" optional="YES">
            <typename name="fill_area_style_select"/>
         </explicit>
         <explicit name="outer_boundary_style">
            <typename name="Closed_curve_style_parameters"/>
         </explicit>
         <explicit name="inner_boundary_style">
            <typename name="Closed_curve_style_parameters"/>
         </explicit>
      </entity>
      <entity name="Inter_stratum_feature_edge_segment_template" supertypes="Inter_stratum_feature_template">
         <explicit name="composed_edge">
            <typename name="Inter_stratum_feature_edge_template"/>
         </explicit>
         <explicit name="start_vertex">
            <typename name="Edge_segment_vertex"/>
         </explicit>
         <explicit name="end_vertex">
            <typename name="Edge_segment_vertex"/>
         </explicit>
         <explicit name="reference_path">
            <typename name="Trimmed_curve"/>
         </explicit>
         <where label="WR1" expression="EXISTS(reference_path.start_point) AND &#10;            EXISTS(reference_path.end_point)"/>
         <where label="WR2" expression="start_vertex :&lt;&gt;: end_vertex"/>
      </entity>
      <entity name="Inter_stratum_feature_edge_template" supertypes="Inter_stratum_feature_template">
         <inverse name="edges" entity="Inter_stratum_feature_edge_segment_template" attribute="composed_edge">
            <inverse.aggregate type="SET" lower="2" upper="?"/>
         </inverse>
      </entity>
      <entity name="Parametric_template" abstract.supertype="YES" supertypes="Template_definition" super.expression="ONEOF (Fill_area_template, Teardrop_template, Trace_template)"/>
      <entity name="Part_string_template" supertypes="Template_definition">
         <explicit name="string_alignment">
            <typename name="text_alignment"/>
         </explicit>
      </entity>
      <entity name="Part_text_template" supertypes="Template_definition">
         <explicit name="of_character">
            <typename name="Character_glyph_symbol"/>
         </explicit>
      </entity>
      <entity name="Planar_closed_path_shape_model_with_parameters" supertypes="Planar_path_shape_model_with_parameters">
         <where label="WR1" expression="SIZEOF(QUERY(it &lt;* SELF\Representation.items |&#10;                        ('BASIC_CURVE_ARM.CLOSED_CURVE' in TYPEOF(it))&#10;                   )) = 1"/>
         <where label="WR2" expression="'PART_TEMPLATE_EXTENSION_ARM.CLOSED_CURVE_STYLE_PARAMETERS' IN &#10;            TYPEOF(SELF\Planar_path_shape_model_with_parameters.parameters\Trace_template.trace_style)"/>
      </entity>
      <entity name="Planar_path_shape_model_with_parameters" supertypes="Planar_projected_shape_model">
         <explicit name="parameters">
            <typename name="Trace_template"/>
         </explicit>
         <explicit name="items">
            <aggregate type="SET" lower="2" upper="2"/>
            <typename name="curve_or_axis_placement"/>
            <redeclaration entity-ref="Representation"/>
         </explicit>
         <where label="WR1" expression="SIZEOF(QUERY(it &lt;* SELF\Representation.items |&#10;                        ('BASIC_GEOMETRY_ARM.CURVE' in TYPEOF(it))&#10;                  )) = 1"/>
         <where label="WR2" expression="SIZEOF(QUERY(it &lt;* SELF\Representation.items |&#10;                        (('ELEMENTAL_GEOMETRIC_SHAPE_ARM.AXIS_PLACEMENT' in TYPEOF(it)) AND &#10;                         (it\Representation_item.name = 'origin') AND&#10;                         (it\Axis_placement.origin.coordinates[1] = 0.0) AND&#10;                         (it\Axis_placement.origin.coordinates[2] = 0.0) AND&#10;                         ((SIZEOF(it\Axis_placement.origin.coordinates) = 2) OR (it\Axis_placement.origin.coordinates[3] = 0.0))&#10;                         )&#10;                   )) = 1"/>
      </entity>
      <entity name="Snowball_template" supertypes="Teardrop_template">
         <explicit name="snowball_start_distance">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="snowball_end_distance" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="snowball_initial_spacing" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="snowball_end_spacing" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="snowball_initial_radius">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="snowball_end_radius" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="snowball_quantity_per_trace">
            <builtintype type="INTEGER"/>
         </explicit>
         <where label="WR1" expression="NOT (snowball_quantity_per_trace &gt; 1) OR (EXISTS(snowball_initial_spacing) AND EXISTS(snowball_end_spacing) AND EXISTS(snowball_end_distance) AND EXISTS(snowball_end_radius))"/>
         <where label="WR2" expression="(SIZEOF(USEDIN(SELF,'PART_TEMPLATE_SHAPE_WITH_PARAMETERS_ARM.PART_TEMPLATE_SHAPE_MODEL.' + 'SHAPE_CHARACTERIZED_DEFINITION')) = 0)"/>
         <where label="WR3" expression="NOT EXISTS(SELF\Product_view_definition.additional_characterization)"/>
      </entity>
      <entity name="Teardrop_by_angle_template" supertypes="Teardrop_template">
         <explicit name="teardrop_angle">
            <typename name="Angle_data_element"/>
         </explicit>
         <where label="WR1" expression="(SIZEOF(USEDIN(SELF,'PART_TEMPLATE_SHAPE_WITH_PARAMETERS_ARM.PART_TEMPLATE_SHAPE_MODEL.' + 'SHAPE_CHARACTERIZED_DEFINITION')) = 0)"/>
         <where label="WR2" expression="NOT EXISTS(SELF\Product_view_definition.additional_characterization)"/>
      </entity>
      <entity name="Teardrop_by_length_template" supertypes="Teardrop_template">
         <explicit name="teardrop_taper_start_distance">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="teardrop_taper_end_distance">
            <typename name="Length_data_element"/>
         </explicit>
         <where label="WR1" expression="(SIZEOF(USEDIN(SELF,'PART_TEMPLATE_SHAPE_WITH_PARAMETERS_ARM.PART_TEMPLATE_SHAPE_MODEL.' + 'SHAPE_CHARACTERIZED_DEFINITION')) = 0)"/>
         <where label="WR2" expression="NOT EXISTS(SELF\Product_view_definition.additional_characterization)"/>
      </entity>
      <entity name="Teardrop_template" abstract.supertype="YES" supertypes="Parametric_template" super.expression="ONEOF (Snowball_template, Teardrop_by_angle_template, Teardrop_by_length_template)"/>
      <entity name="Trace_template" supertypes="Parametric_template" super.expression="Default_trace_template">
         <explicit name="trace_style">
            <typename name="Curve_style_parameters"/>
         </explicit>
         <where label="WR1" expression="NOT EXISTS(SELF\Product_view_definition.name)"/>
      </entity>
      <subtype.constraint name="inter_stratum_feature_template_subtypes" entity="Inter_stratum_feature_template" super.expression="ONEOF (Component_termination_passage_template, Via_template, Unsupported_passage_template, Inter_stratum_feature_edge_segment_template, Inter_stratum_feature_edge_template)"/>
      <subtype.constraint name="pte_template_definition_subtypes" entity="Template_definition" super.expression="ONEOF (Parametric_template, Part_string_template, Part_text_template)"/>
   </schema>
</express>
