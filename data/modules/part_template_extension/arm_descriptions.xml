<!-- +++++++++++ Teardrop_by_angle_template +++++++++++ -->
<ext_description linkend="Part_template_extension_arm.Teardrop_by_angle_template"><p>
	A <b>Teardrop_by_angle_template</b> is a type of 
	<express_ref linkend="Part_template_extension:arm:Part_template_extension_arm.Teardrop_template"/> 
	that describes the shape of material to be added to a trace termination that is parameterized by the angle of the material addition geometry. 
	The <b>Teardrop_by_angle_template</b> edge is required to be tangential to the  
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Laminate_component"/> 
	that is associated by virtue of the <express_ref linkend="Fabrication_joint:arm:Fabrication_joint_arm.Fabrication_joint"/> 
	that references the occurrence of the <b>Teardrop_by_angle_template</b>.  The vertex of the angle shall be on the centreline of the 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Conductive_interconnect_element"/> 
	associated by the  <express_ref linkend="Fabrication_joint:arm:Fabrication_joint_arm.Fabrication_joint"/>.  
	The angle shall be less than 90 degrees.
</p>
	<note>
	The occurrence of a <b>Teardrop_by_angle_template</b> will be a 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Stratum_feature_template_component"/>
	that is referenced by a  
	<express_ref linkend="Fabrication_joint:arm:Fabrication_joint_arm.Fabrication_joint"/> 
	by the 
	<express_ref linkend="Fabrication_joint:arm:Fabrication_joint_arm.Fabrication_joint.auxiliary_joint_material"/> 
	attribute.
	</note>
</ext_description>

<!-- teardrop_angle -->
<ext_description linkend="Part_template_extension_arm.Teardrop_by_angle_template.teardrop_angle">
	specifies a role of the 
	<express_ref linkend="Value_with_unit_extension:arm:Value_with_unit_extension_arm.Angle_data_element"/> 
	for the <b>Teardrop_by_angle_template</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Part_template_extension_arm.Teardrop_by_angle_template.wr:WR1">
	There shall be no 
	<express_ref linkend="part_template_2d_shape:arm:Part_template_2d_shape_arm.Part_template_planar_shape_model"/> 
	refering this <b>Teardrop_by_angle_template</b> as 
	<express_ref linkend="part_template_shape_with_parameters:arm:Part_template_shape_with_parameters_arm.Part_template_shape_model.shape_characterized_definition"/>
	and no 
	<express_ref linkend="part_template_3d_shape:arm:Part_template_3d_shape_arm.Part_template_3d_shape_model"/> 
	refering this <b>Teardrop_by_angle_template</b> as 
	<express_ref linkend="part_template_shape_with_parameters:arm:Part_template_shape_with_parameters_arm.Part_template_shape_model.shape_characterized_definition"/>
	and no 
	<express_ref linkend="part_template_non_planar_shape:arm:Part_template_non_planar_shape_arm.Part_template_non_planar_2d_shape_model"/> 
	refering this <b>Teardrop_by_angle_template</b> as 
	<express_ref linkend="part_template_shape_with_parameters:arm:Part_template_shape_with_parameters_arm.Part_template_shape_model.shape_characterized_definition"/>.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Part_template_extension_arm.Teardrop_by_angle_template.wr:WR2">
	The
	<express_ref linkend="Product_view_definition:arm:Product_view_definition_arm.Product_view_definition.additional_characterization"/> 
	shall not be populated.
</ext_description>

<!-- +++++++++++ Teardrop_by_length_template +++++++++++ -->
<ext_description linkend="Part_template_extension_arm.Teardrop_by_length_template">
	A <b>Teardrop_by_length_template</b> is a type of 
	<express_ref linkend="Part_template_extension:arm:Part_template_extension_arm.Teardrop_template"/> 
	that describes the shape of material to be added to a design feature.  The <b>teardrop_taper_start_distance</b> and <b>teardrop_taper_end_distance</b> 
	shall be measured from the location of the 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Layer_connection_point"/> 
	of the 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Laminate_component"/> 
	referenced by the 
	<express_ref linkend="Fabrication_joint:arm:Fabrication_joint_arm.Fabrication_joint"/> 
	that references a 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Stratum_feature_template_component"/> 
	that is an occurrence of <b>Teardrop_by_length_template</b>.  The centreline of the  
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Stratum_feature_template_component"/> 
	that is an occurrence of the <b>Teardrop_by_length_template</b> shall be congruent with the centreline of the 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Conductive_interconnect_element"/> 
	it is associated with. 
</ext_description>

<!-- teardrop_taper_end_distance -->
<ext_description linkend="Part_template_extension_arm.Teardrop_by_length_template.teardrop_taper_end_distance">
	specifies one role of the 
	<express_ref linkend="Value_with_unit_extension:arm:Value_with_unit_extension_arm.Length_data_element"/> 
	for the <b>Teardrop_by_length_template</b>.
</ext_description>

<!-- teardrop_taper_start_distance -->
<ext_description linkend="Part_template_extension_arm.Teardrop_by_length_template.teardrop_taper_start_distance">
	specifies one role of the 
	<express_ref linkend="Value_with_unit_extension:arm:Value_with_unit_extension_arm.Length_data_element"/> 
	for the <b>Teardrop_by_length_template</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Part_template_extension_arm.Teardrop_by_length_template.wr:WR1">
	There shall be no 
	<express_ref linkend="part_template_2d_shape:arm:Part_template_2d_shape_arm.Part_template_planar_shape_model"/> 
	refering this <b>Teardrop_by_length_template</b> as 
	<express_ref linkend="part_template_shape_with_parameters:arm:Part_template_shape_with_parameters_arm.Part_template_shape_model.shape_characterized_definition"/>
	and no 
	<express_ref linkend="part_template_3d_shape:arm:Part_template_3d_shape_arm.Part_template_3d_shape_model"/> 
	refering this <b>Teardrop_by_length_template</b> as 
	<express_ref linkend="part_template_shape_with_parameters:arm:Part_template_shape_with_parameters_arm.Part_template_shape_model.shape_characterized_definition"/>
	and no 
	<express_ref linkend="part_template_non_planar_shape:arm:Part_template_non_planar_shape_arm.Part_template_non_planar_2d_shape_model"/> 
	refering this <b>Teardrop_by_length_template</b> as 
	<express_ref linkend="part_template_shape_with_parameters:arm:Part_template_shape_with_parameters_arm.Part_template_shape_model.shape_characterized_definition"/>.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Part_template_extension_arm.Teardrop_by_length_template.wr:WR2">
	The
	<express_ref linkend="Product_view_definition:arm:Product_view_definition_arm.Product_view_definition.additional_characterization"/>
	shall not be populated.
</ext_description>

<!-- +++++++++++ Teardrop_template +++++++++++ -->
<ext_description linkend="Part_template_extension_arm.Teardrop_template">
<p>
	A <b>Teardrop_template</b> is a type of 
	<express_ref linkend="Part_template_extension:arm:Part_template_extension_arm.Parametric_template"/>.
	A <b>Teardrop_template</b> is one of a
	<express_ref linkend="Part_template_extension:arm:Part_template_extension_arm.Snowball_template"/>,
	a
	<express_ref linkend="Part_template_extension:arm:Part_template_extension_arm.Teardrop_by_angle_template"/>
	or a
	<express_ref linkend="Part_template_extension:arm:Part_template_extension_arm.Teardrop_by_length_template"/>.  
</p>
</ext_description>

<!-- +++++++++++ Trace_template +++++++++++ -->
<ext_description linkend="Part_template_extension_arm.Trace_template">
<p>
	A <b>Trace_template</b> is a type of 
	<express_ref linkend="Part_template_extension:arm:Part_template_extension_arm.Parametric_template"/> 
	whose purpose is to define the width of a 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Conductive_interconnect_element"/> 
	and the style of the ends and corners of that  
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Conductive_interconnect_element"/>. 
	A <b>Trace_template</b> may be a 
	<express_ref linkend="Part_template_extension:arm:Part_template_extension_arm.Default_trace_template"/>.  
</p>
</ext_description>

<!-- trace_style -->
<ext_description linkend="Part_template_extension_arm.Trace_template.trace_style">
<p>
	specifies a role of the  
	<express_ref linkend="Part_template_extension:arm:Part_template_extension_arm.Curve_style_parameters"/> 
	for the <b>Trace_template</b>.
</p>
	<note>
		The <b>Trace_template</b> uses the parametric information in  <express_ref linkend="Part_template_extension:arm:Part_template_extension_arm.Curve_style_parameters"/>, not the graphical presentation information.
	</note>
</ext_description>

<!-- WR1 -->
<ext_description linkend="Part_template_extension_arm.Trace_template.wr:WR1">
	The 
	<express_ref linkend="Product_view_definition:arm:Product_view_definition_arm.Product_view_definition.name"/>
	shall not be populated.
</ext_description>

<!-- +++++++++++ Inter_stratum_feature_edge_segment_template +++++++++++ -->
<ext_description linkend="Part_template_extension_arm.Inter_stratum_feature_edge_segment_template">
	An <b>Inter_stratum_feature_edge_segment_template</b> is a type of 
	<express_ref linkend="Fabrication_technology:arm:Fabrication_technology_arm.Inter_stratum_feature_template"/>.
</ext_description>

<!-- reference_path -->
<ext_description linkend="Part_template_extension_arm.Inter_stratum_feature_edge_segment_template.reference_path">
	specifies the 
	<express_ref linkend="Basic_curve:arm:Basic_curve_arm.Trimmed_curve"/> 
	for the <b>Inter_stratum_feature_edge_segment_template</b>.
</ext_description>

<!-- end_vertex -->
<ext_description linkend="Part_template_extension_arm.Inter_stratum_feature_edge_segment_template.end_vertex">
	specifies one role of the 
	<express_ref linkend="Extended_geometric_tolerance:arm:Extended_geometric_tolerance_arm.Edge_segment_vertex"/> 
	for the <b>Inter_stratum_feature_edge_segment_template</b>.
</ext_description>

<!-- start_vertex -->
<ext_description linkend="Part_template_extension_arm.Inter_stratum_feature_edge_segment_template.start_vertex">
	specifies one role of the 
	<express_ref linkend="Extended_geometric_tolerance:arm:Extended_geometric_tolerance_arm.Edge_segment_vertex"/> 
	for the <b>Inter_stratum_feature_edge_segment_template</b>.
</ext_description>

<!-- composed_edge -->
<ext_description linkend="Part_template_extension_arm.Inter_stratum_feature_edge_segment_template.composed_edge">
	specifies a role of the 
	<express_ref linkend="Part_template_extension:arm:Part_template_extension_arm.Inter_stratum_feature_edge_template"/> 
	the <b>Inter_stratum_feature_edge_segment_template</b> is helping to compose.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Part_template_extension_arm.Inter_stratum_feature_edge_segment_template.wr:WR1">
	Both 
	<express_ref linkend="basic_curve:arm:Basic_curve_arm.Trimmed_curve.start_point"/> 
	and
	<express_ref linkend="basic_curve:arm:Basic_curve_arm.Trimmed_curve.end_point"/> 
	of <b>reference_path</b> shall exist.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Part_template_extension_arm.Inter_stratum_feature_edge_segment_template.wr:WR2">
	The <b>start_vertex</b> shall not be equal to the <b>end_vertex</b>.
</ext_description>

<!-- WR3 -->
<ext_description linkend="Part_template_extension_arm.Inter_stratum_feature_edge_segment_template.wr:WR3">

</ext_description>

<!-- WR4 -->
<ext_description linkend="Part_template_extension_arm.Inter_stratum_feature_edge_segment_template.wr:WR4">
	The <b>start_vertex</b> shall not be equal to the <b>end_vertex</b>.
</ext_description>



