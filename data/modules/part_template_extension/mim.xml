      <entity name="teardrop_by_length_template" supertypes="teardrop_template"/>
      <entity name="teardrop_template" abstract.supertype="YES" supertypes="parametric_template" super.expression="ONEOF (snowball_template, teardrop_by_angle_template, teardrop_by_length_template)"/>
      <entity name="trace_template" supertypes="parametric_template" super.expression="default_trace_template"/>
      <subtype.constraint name="pte_template_definition_subtypes" entity="part_template_definition" super.expression="ONEOF (parametric_template, part_string_template, part_text_template)"/>
   </schema>
