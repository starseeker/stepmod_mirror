<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="mim_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="part_template_extension_mim schema_instance"/>
   <schema name="Part_template_extension_mim">
      <interface kind="use" schema="Layered_2d_shape_mim"/>
      <interface kind="use" schema="Fill_area_style_mim"/>
      <interface kind="use" schema="Fabrication_technology_mim"/>
      <interface kind="use" schema="Text_representation_mim"/>
      <interface kind="use" schema="presentation_definition_schema">
         <interfaced.item name="text_literal"/>
      </interface>
      <entity name="fill_area_template" supertypes="part_template_definition"/>
      <entity name="inter_stratum_feature_edge_segment_template" supertypes="part_template_definition shape_aspect_relationship">
         <explicit name="relating_shape_aspect">
            <typename name="edge_segment_vertex"/>
            <redeclaration entity-ref="shape_aspect_relationship"/>
         </explicit>
         <explicit name="related_shape_aspect">
            <typename name="edge_segment_vertex"/>
            <redeclaration entity-ref="shape_aspect_relationship"/>
         </explicit>
         <where label="WR1" expression="SELF\shape_aspect_relationship.relating_shape_aspect :&lt;&gt;:&#10;       SELF\shape_aspect_relationship.related_shape_aspect"/>
      </entity>
      <entity name="inter_stratum_feature_edge_template" supertypes="part_template_definition"/>
      <entity name="part_string_template_definition" supertypes="part_template_definition"/>
      <entity name="part_text_template_definition" supertypes="part_template_definition"/>
      <entity name="snowball_template" supertypes="material_addition_feature_template"/>
      <entity name="teardrop_by_angle_template" supertypes="material_addition_feature_template"/>
      <entity name="teardrop_by_length_template" supertypes="material_addition_feature_template"/>
      <subtype.constraint name="material_addition_feature_template_subtypes" entity="material_addition_feature_template" super.expression="ONEOF (snowball_template, teardrop_by_angle_template, teardrop_by_length_template)"/>
      <subtype.constraint name="pte_part_template_definition_subtypes" entity="part_template_definition" super.expression="ONEOF (inter_stratum_feature_edge_segment_template, inter_stratum_feature_edge_template, fill_area_template, part_string_template_definition, part_text_template_definition)"/>
   </schema>
</express>
