<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: default_trace_template
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Part_template_extension_mim.default_trace_template">
	A <b>default_trace_template</b> is a type of 
	<express_ref linkend="Part_template_extension:mim:Part_template_extension_mim.trace_template"/>
	that implements the ARM concept of
	<express_ref linkend="Part_template_extension:arm:Part_template_extension_arm.Default_trace_template"/>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: fill_area_template
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
