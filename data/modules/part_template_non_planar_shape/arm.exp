SCHEMA Part_template_non_planar_shape_arm;

	USE FROM Layered_3d_shape_arm;	-- ISO/TS 10303-1694
	USE FROM Manifold_surface_arm;	-- ISO/TS 10303-1509
	USE FROM Part_template_shape_with_parameters_arm;	-- ISO/TS 10303-1720

	TYPE ptnps_part_template_or_physical_unit_3d_shape_select = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON part_template_or_physical_unit_3d_shape_select WITH 
 		(Part_template_non_planar_2d_shape);
	END_TYPE;

  ENTITY Part_template_non_planar_2d_shape
    SUBTYPE OF (Manifold_surface_shape_representation,
	 			Feature_shape_definition,
	 			Part_template_shape);
      transformation                    : Axis_placement_3d;
  END_ENTITY;

END_SCHEMA;
