<!-- +++++++++++ Part_template_non_planar_2d_shape_model +++++++++++ -->
<ext_description linkend="Part_template_non_planar_shape_arm.Part_template_non_planar_2d_shape_model">
	A <b>Part_template_non_planar_2d_shape_model</b> is a type of 
	<express_ref linkend="Manifold_surface:arm:Manifold_surface_arm.Manifold_surface_shape_representation"/>
	and a type of 
	<express_ref linkend="Part_template_shape_with_parameters:arm:Part_template_shape_with_parameters_arm.Part_template_shape_model"/>.
	A <b>Part_template_non_planar_2d_shape_model</b> is the shape of a 
	<express_ref linkend="Physical_layout_template:arm:Physical_layout_template_arm.Template_definition"/> that is represented in the form of a surface.
</ext_description>

<!-- transformation -->
<ext_description linkend="Part_template_non_planar_shape_arm.Part_template_non_planar_2d_shape_model.transformation">
	specifies a role of the 
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Axis_placement_3d"/>
	for the <b>Part_template_non_planar_2d_shape_model</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Part_template_non_planar_shape_arm.Part_template_non_planar_2d_shape_model.wr:WR1">
	The 
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.name"/>
	shall not be populated.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Part_template_non_planar_shape_arm.Part_template_non_planar_2d_shape_model.wr:WR2">
