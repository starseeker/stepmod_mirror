<?xml version="1.0" encoding="utf-8"?><?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express
  SYSTEM "../../../dtd/express.dtd">
<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="">
   <application name="JSDAI" owner="LKSoft" source="part_template_shape_with_parameters_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Part_template_shape_with_parameters_arm">
      <interface kind="use" schema="Non_feature_shape_element_arm"/>
      <interface kind="use" schema="Part_template_arm"/>
      <interface kind="reference" schema="Support_resource_arm">
         <interfaced.item name="bag_to_set"/>
      </interface>
      <type name="ptswp_external_identification_item">
         <select basedon="external_identification_item" extensible="YES" genericentity="YES" selectitems="Part_template_keepout_shape_model"/>
      </type>
      <type name="ptswp_requirement_assignment_item">
         <select basedon="requirement_assignment_item" extensible="YES" genericentity="YES" selectitems="Part_template_keepout_shape_model"/>
      </type>
      <type name="ptswp_view_shape_select">
         <select basedon="view_shape_select" extensible="YES" genericentity="YES" selectitems="Part_template_shape_model"/>
      </type>
      <entity abstract.supertype="YES" name="Geometric_template" supertypes="Template_definition">
         <inverse attribute="shape_characterized_definition" entity="Part_template_shape_model" name="shapes">
            <inverse.aggregate lower="0" type="SET" upper="?"/>
         </inverse>
         <where expression="SIZEOF(QUERY( gm &lt;* USEDIN(SELF,'')|&#xA;        (('ELEMENTAL_GEOMETRIC_SHAPE_ARM.GEOMETRIC_MODEL' IN TYPEOF(gm)) AND &#xA;        NOT('PART_TEMPLATE_SHAPE_WITH_PARAMETERS_ARM.PART_TEMPLATE_SHAPE_MODEL' IN&#xA;        TYPEOF(gm)))&#xA;        )) = 0" label="WR1"/>
      </entity>
      <entity abstract.supertype="YES" name="Part_template_keepout_shape_model" supertypes="Geometric_model">
         <explicit name="constrained_design_object_category">
            <typename name="keepout_product_design_object_category"/>
         </explicit>
         <explicit name="shape_characterized_definition">
            <typename name="Geometric_template"/>
         </explicit>
         <explicit name="shape_material_condition">
            <typename name="material_condition"/>
         </explicit>
         <explicit name="centroid_location" optional="YES">
            <typename name="Cartesian_point"/>
         </explicit>
         <explicit name="shape_environment">
            <typename name="application_environment"/>
         </explicit>
         <explicit name="shape_purpose" optional="YES">
            <typename name="predefined_keepout_purpose"/>
         </explicit>
         <derived expression="bag_to_set(QUERY( gmr &lt;* USEDIN(SELF,&#xA;        'EXTENDED_BASIC_GEOMETRY_ARM.GEOMETRIC_MODEL_RELATIONSHIP.REP_2') |&#xA;         (gmr\Representation_relationship.relation_type = 'principal part shape')))" name="principal_part_shape">
            <aggregate lower="0" type="SET" upper="1"/>
            <typename name="Geometric_model_relationship"/>
         </derived>
         <inverse attribute="item" entity="External_source_identification" name="external_shape_purpose">
            <inverse.aggregate lower="0" type="SET" upper="1"/>
         </inverse>
         <where expression="(EXISTS (shape_purpose)) XOR ((SIZEOF(external_shape_purpose)) = 1)" label="WR1"/>
         <where expression="SIZEOF(USEDIN(SELF, 'SHAPE_PROPERTY_ASSIGNMENT_ARM.SHAPE_DESCRIPTION_ASSOCIATION.REPRESENTATION'))=0" label="WR2"/>
         <where expression="SIZEOF(principal_part_shape) &lt; 2" label="WR3"/>
      </entity>
      <entity abstract.supertype="YES" name="Part_template_shape_model" supertypes="Geometric_model">
         <explicit name="shape_characterized_definition">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Geometric_template"/>
         </explicit>
         <explicit name="shape_environment" optional="YES">
            <typename name="application_environment"/>
         </explicit>
         <explicit name="shape_material_condition">
            <typename name="material_condition"/>
         </explicit>
         <derived expression="bag_to_set(QUERY( gmr &lt;* USEDIN(SELF,&#xA;        'EXTENDED_BASIC_GEOMETRY_ARM.GEOMETRIC_MODEL_RELATIONSHIP.REP_2') |&#xA;         (gmr\Representation_relationship.relation_type = 'principal part shape')))" name="principal_part_shape">
            <aggregate lower="0" type="SET" upper="1"/>
            <typename name="Geometric_model_relationship"/>
         </derived>
         <where expression="SIZEOF(principal_part_shape) &lt; 2" label="WR1"/>
         <where expression="SIZEOF(USEDIN(SELF, 'SHAPE_PROPERTY_ASSIGNMENT_ARM.SHAPE_DESCRIPTION_ASSOCIATION.REPRESENTATION'))=0" label="WR2"/>
      </entity>
      <subtype.constraint entity="Shape_element" name="shape_element_subtypes" super.expression="ONEOF (Shape_feature, Non_feature_shape_element)"/>
      <rule appliesto="Geometric_template" name="unique_part_template_shape_model_for_geometric_template">
         <algorithm>LOCAL
      ptsm : SET OF Part_template_shape_model := [];
    END_LOCAL;</algorithm>
         <where expression="SIZEOF(QUERY (gt &lt;* Geometric_template | &#xA;        (SIZEOF (QUERY (ptsm1 &lt;*&#xA;        USEDIN(gt,'PART_TEMPLATE_SHAPE_WITH_PARAMETERS_ARM.PART_TEMPLATE_SHAPE_MODEL.SHAPE_CHARACTERIZED_DEFINITION') |  &#xA;        (SIZEOF (QUERY (ptsm2 &lt;*&#xA;        USEDIN(gt,'PART_TEMPLATE_SHAPE_WITH_PARAMETERS_ARM.PART_TEMPLATE_SHAPE_MODEL.SHAPE_CHARACTERIZED_DEFINITION') |  &#xA;        ptsm1 = ptsm2)) &gt; 1)&#xA;        )) &gt; 0) )) = 0" label="WR1"/>
      </rule>
   </schema>
</express>