<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="part_template_shape_with_parameters_arm schema_instance"/>
   <schema name="Part_template_shape_with_parameters_arm">
      <interface kind="use" schema="Non_feature_shape_element_arm"/>
      <interface kind="use" schema="Physical_layout_template_arm"/>
      <interface kind="reference" schema="Characterizable_object_arm">
         <interfaced.item name="bag_to_set"/>
      </interface>
      <type name="ptswp_external_identification_item">
         <select extensible="YES" genericentity="YES" basedon="external_identification_item" selectitems="Part_template_keepout_shape_model"/>
      </type>
      <type name="ptswp_requirement_assignment_item">
         <select extensible="YES" genericentity="YES" basedon="requirement_assignment_item" selectitems="Part_template_keepout_shape_model"/>
      </type>
      <type name="ptswp_view_shape_select">
         <select extensible="YES" genericentity="YES" basedon="view_shape_select" selectitems="Part_template_shape_model"/>
      </type>
      <entity name="Geometric_template" abstract.supertype="YES" supertypes="Template_definition">
         <inverse name="shapes" entity="Part_template_shape_model" attribute="shape_characterized_definition">
            <inverse.aggregate type="SET" lower="0" upper="?"/>
         </inverse>
         <where label="WR1" expression="SIZEOF(QUERY( gm &lt;* USEDIN(SELF,'')|&#10;        (('ELEMENTAL_GEOMETRIC_SHAPE_ARM.GEOMETRIC_MODEL' IN TYPEOF(gm)) AND &#10;        NOT('PART_TEMPLATE_SHAPE_WITH_PARAMETERS_ARM.PART_TEMPLATE_SHAPE_MODEL' IN&#10;        TYPEOF(gm)))&#10;        )) = 0"/>
      </entity>
      <entity name="Part_template_keepout_shape_model" abstract.supertype="YES" supertypes="Geometric_model">
         <explicit name="constrained_design_object_category">
            <typename name="keepout_product_design_object_category"/>
         </explicit>
         <explicit name="shape_characterized_definition">
            <typename name="Geometric_template"/>
         </explicit>
         <explicit name="shape_material_condition">
            <typename name="material_condition"/>
         </explicit>
         <explicit name="centroid_location" optional="YES">
            <typename name="Cartesian_point"/>
         </explicit>
         <explicit name="shape_environment">
            <typename name="application_environment"/>
         </explicit>
         <explicit name="shape_purpose" optional="YES">
            <typename name="predefined_keepout_purpose"/>
         </explicit>
         <derived name="principal_part_shape" expression="bag_to_set(QUERY( gmr &lt;* USEDIN(SELF,&#10;        'EXTENDED_BASIC_GEOMETRY_ARM.GEOMETRIC_MODEL_RELATIONSHIP.REP_2') |&#10;         (gmr\Representation_relationship.relation_type = 'principal part shape')))">
            <aggregate type="SET" lower="0" upper="1"/>
            <typename name="Geometric_model_relationship"/>
         </derived>
         <inverse name="external_shape_purpose" entity="External_source_identification" attribute="item">
            <inverse.aggregate type="SET" lower="0" upper="1"/>
         </inverse>
         <where label="WR1" expression="(EXISTS (shape_purpose)) XOR ((SIZEOF(external_shape_purpose)) = 1)"/>
         <where label="WR2" expression="SIZEOF(USEDIN(SELF, 'SHAPE_PROPERTY_ASSIGNMENT_ARM.SHAPE_DESCRIPTION_ASSOCIATION.REPRESENTATION'))=0"/>
         <where label="WR3" expression="SIZEOF(principal_part_shape) &lt; 2"/>
      </entity>
      <entity name="Part_template_shape_model" abstract.supertype="YES" supertypes="Geometric_model">
         <explicit name="shape_characterized_definition">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="Geometric_template"/>
         </explicit>
         <explicit name="shape_environment" optional="YES">
            <typename name="application_environment"/>
         </explicit>
         <explicit name="shape_material_condition">
            <typename name="material_condition"/>
         </explicit>
         <derived name="principal_part_shape" expression="bag_to_set(QUERY( gmr &lt;* USEDIN(SELF,&#10;        'EXTENDED_BASIC_GEOMETRY_ARM.GEOMETRIC_MODEL_RELATIONSHIP.REP_2') |&#10;         (gmr\Representation_relationship.relation_type = 'principal part shape')))">
            <aggregate type="SET" lower="0" upper="1"/>
            <typename name="Geometric_model_relationship"/>
         </derived>
         <where label="WR1" expression="SIZEOF(principal_part_shape) &lt; 2"/>
      </entity>
      <entity name="Part_template_shape_representation_relationship" supertypes="Geometric_model_relationship">
         <explicit name="principal_part_shape">
            <typename name="Part_template_shape_model"/>
            <redeclaration entity-ref="Geometric_model_relationship" old_name="rep_1"/>
         </explicit>
         <explicit name="related_part_shape">
            <typename name="Part_template_shape_model"/>
            <redeclaration entity-ref="Geometric_model_relationship" old_name="rep_2"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="principal_part_shape"/>
            <unique.attribute attribute="related_part_shape"/>
         </unique>
         <where label="WR1" expression="principal_part_shape :&lt;&gt;: related_part_shape"/>
         <where label="WR2" expression="NOT EXISTS(SELF\Representation_relationship.relation_type)"/>
         <where label="WR3" expression="principal_part_shape\Geometric_model.context_of_items :=: related_part_shape\Geometric_model.context_of_items"/>
         <where label="WR4" expression="principal_part_shape\Part_template_shape_model.shape_characterized_definition :=:&#10;            related_part_shape\Part_template_shape_model.shape_characterized_definition"/>
      </entity>
      <subtype.constraint name="shape_element_subtypes" entity="Shape_element" super.expression="ONEOF (Shape_feature, Non_feature_shape_element)"/>
      <rule name="unique_part_template_shape_model_for_geometric_template" appliesto="Geometric_template">
         <algorithm>LOCAL
      ptsm : SET OF Part_template_shape_model := [];
    END_LOCAL;</algorithm>
         <where label="WR1" expression="SIZEOF(QUERY (gt &lt;* Geometric_template | &#10;        (SIZEOF (QUERY (ptsm1 &lt;*&#10;        USEDIN(gt,'PART_TEMPLATE_SHAPE_WITH_PARAMETERS_ARM.PART_TEMPLATE_SHAPE_MODEL.SHAPE_CHARACTERIZED_DEFINITION') |  &#10;        (SIZEOF (QUERY (ptsm2 &lt;*&#10;        USEDIN(gt,'PART_TEMPLATE_SHAPE_WITH_PARAMETERS_ARM.PART_TEMPLATE_SHAPE_MODEL.SHAPE_CHARACTERIZED_DEFINITION') |  &#10;        ptsm1 = ptsm2)) &gt; 1)&#10;        )) &gt; 0) )) = 0"/>
      </rule>
   </schema>
</express>
