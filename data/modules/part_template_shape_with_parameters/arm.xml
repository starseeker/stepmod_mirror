<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:10" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="part_template_shape_with_parameters_arm schema_instance"/>
   <schema name="PART_TEMPLATE_SHAPE_WITH_PARAMETERS_ARM">
      <interface kind="use" schema="NON_FEATURE_SHAPE_ELEMENT_ARM"/>
      <interface kind="use" schema="PHYSICAL_LAYOUT_TEMPLATE_ARM"/>
      <interface kind="use" schema="SHAPE_PARAMETERS_ARM"/>
      <type name="ptswp_requirement_assignment_item">
         <select extensible="YES" genericentity="YES" basedon="requirement_assignment_item" selectitems="part_template_keepout_shape"/>
      </type>
      <type name="ptswp_view_shape_select">
         <select extensible="YES" genericentity="YES" basedon="view_shape_select" selectitems="part_template_shape"/>
      </type>
      <entity name="part_template_keepout_shape" abstract.supertype="YES">
         <explicit name="constrained_design_object_category">
            <typename name="keepout_product_design_object_category"/>
         </explicit>
         <explicit name="shape_characterized_part_template">
            <typename name="template_definition"/>
         </explicit>
         <explicit name="shape_material_condition">
            <typename name="material_condition"/>
         </explicit>
         <explicit name="centroid_location" optional="YES">
            <typename name="cartesian_point"/>
         </explicit>
         <explicit name="shape_environment">
            <typename name="application_environment"/>
         </explicit>
         <explicit name="shape_purpose" optional="YES">
            <typename name="predefined_keepout_purpose"/>
         </explicit>
         <inverse name="external_shape_purpose" entity="external_source_identification" attribute="item">
            <inverse.aggregate type="SET" lower="0" upper="1"/>
         </inverse>
         <where label="wr1" expression=" ( exists ( shape_purpose ) ) xor ( ( sizeof ( external_shape_purpose ) ) = 1 ) "/>
      </entity>
      <entity name="part_template_shape" abstract.supertype="YES">
         <explicit name="shape_characterized_part_template">
            <typename name="template_definition"/>
         </explicit>
         <explicit name="shape_environment" optional="YES">
            <typename name="application_environment"/>
         </explicit>
         <explicit name="shape_material_condition">
            <typename name="material_condition"/>
         </explicit>
      </entity>
   </schema>
</express>
