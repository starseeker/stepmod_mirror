<ext_description linkend="Part_template_shape_with_parameters_arm">
</ext_description>
<!-- +++++++++++++++++++ Part_template_keepout_shape_model +++++++++++ -->
<ext_description linkend="Part_template_shape_with_parameters_arm.Part_template_keepout_shape_model">
	A <b>Part_template_keepout_shape_model</b> is a type of 
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model"/>.
	A <b>Part_template_keepout_shape_model</b> is the description of a shape that is a boundary within which other layout shapes are not permitted.
	It may also be used to indicate that physical components are also not permitted.
</ext_description>

<!-- shape_purpose -->
<ext_description linkend="Part_template_shape_with_parameters_arm.Part_template_keepout_shape_model.shape_purpose">
	specifies a role of the 
	<express_ref linkend="Shape_parameters:arm:Shape_parameters_arm.predefined_keepout_purpose"/> 
	for the <b>Part_template_keepout_shape_model</b>. The value of this attribute need not be specified.
</ext_description>

<!-- shape_environment -->
<ext_description linkend="Part_template_shape_with_parameters_arm.Part_template_keepout_shape_model.shape_environment">
	specifies a role of the 
	<express_ref linkend="Shape_parameters:arm:Shape_parameters_arm.application_environment"/>
	for the <b>Part_template_keepout_shape_model</b>.
</ext_description>

<!-- centroid_location -->
<ext_description linkend="Part_template_shape_with_parameters_arm.Part_template_keepout_shape_model.centroid_location">
	specifies a role of the 
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Cartesian_point"/>
	for the <b>Part_template_keepout_shape_model</b>. The value of this attribute need not be specified.
</ext_description>

<!-- shape_material_condition -->
<ext_description linkend="Part_template_shape_with_parameters_arm.Part_template_keepout_shape_model.shape_material_condition">
	specifies a role of the 
	<express_ref linkend="Shape_parameters:arm:Shape_parameters_arm.material_condition"/>
	for the <b>Part_template_keepout_shape_model</b>. 
</ext_description>

<!-- shape_characterized_part_template -->
<ext_description linkend="Part_template_shape_with_parameters_arm.Part_template_keepout_shape_model.shape_characterized_part_template">
	specifies a role of the 
	<express_ref linkend="Physical_layout_template:arm:Physical_layout_template_arm.Template_definition"/>
	for the <b>Part_template_keepout_shape_model</b>. 
</ext_description>

<!-- constrained_design_object_category -->
<ext_description linkend="Part_template_shape_with_parameters_arm.Part_template_keepout_shape_model.constrained_design_object_category">
	specifies a role of the 
	<express_ref linkend="Shape_parameters:arm:Shape_parameters_arm.keepout_product_design_object_category"/>
	for the <b>Part_template_keepout_shape_model</b>. 
</ext_description>

<!-- external_shape_purpose -->
<ext_description linkend="Part_template_shape_with_parameters_arm.Part_template_keepout_shape_model.external_shape_purpose">
	an inverse relationship that specifies that the existence of the <b>Part_template_keepout_shape_model</b> is dependent on the existence of the 
	<express_ref linkend="External_item_identification_assignment:arm:External_item_identification_assignment_arm.External_source_identification"/> 
	that specifies the <b>Part_template_keepout_shape_model</b> as its 
	<express_ref linkend="External_item_identification_assignment:arm:External_item_identification_assignment_arm.External_source_identification.item"/>.
</ext_description>

<!-- representation -->
<ext_description linkend="Part_template_shape_with_parameters_arm.Part_template_keepout_shape_model.representation">
	specifies a 
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.shape_model"/> 
	for the <b>Part_template_keepout_shape_model</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Part_template_shape_with_parameters_arm.Part_template_keepout_shape_model.wr:WR1">
	One and only one of <b>shape_purpose</b> and 
	<express_ref linkend="part_template_shape_with_parameters:arm:Part_template_shape_with_parameters_arm.Part_template_keepout_shape_model.external_shape_purpose"/>
	shall be provided.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Part_template_shape_with_parameters_arm.Part_template_keepout_shape_model.wr:WR2">
	
</ext_description>

<!-- +++++++++++++++++++ Part_template_shape_model +++++++++++ -->
<ext_description linkend="Part_template_shape_with_parameters_arm.Part_template_shape_model">
	A <b>Part_template_shape_model</b> is a type of 
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model"/>.
	A <b>Part_template_shape_model</b> is a shape representation of a 
	<express_ref linkend="Physical_layout_template:arm:Physical_layout_template_arm.Template_definition"/> 
	under certain specified conditions.
</ext_description>

<!-- shape_material_condition -->
<ext_description linkend="Part_template_shape_with_parameters_arm.Part_template_shape_model.shape_material_condition">
	specifies a role of the 
	<express_ref linkend="Shape_parameters:arm:Shape_parameters_arm.material_condition"/> 
	for the <b>Part_template_shape_model</b>.
</ext_description>

<!-- shape_characterized_part_template -->
<ext_description linkend="Part_template_shape_with_parameters_arm.Part_template_shape_model.shape_characterized_part_template">
	specifies a role of the 
	<express_ref linkend="Physical_layout_template:arm:Physical_layout_template_arm.Template_definition"/>
	for the <b>Part_template_shape_model</b>. There shall exist one or more
	<express_ref linkend="Physical_layout_template:arm:Physical_layout_template_arm.Template_definition">Template_definitions</express_ref>
	for the <b>Part_template_shape_model</b>.
</ext_description>

<!-- shape_environment -->
<ext_description linkend="Part_template_shape_with_parameters_arm.Part_template_shape_model.shape_environment">
	specifies a role of the 
	<express_ref linkend="Shape_parameters:arm:Shape_parameters_arm.application_environment"/>
	for the <b>Part_template_shape_model</b>. The value of this attribute need not be specified.
</ext_description>


</ext_descriptions>

