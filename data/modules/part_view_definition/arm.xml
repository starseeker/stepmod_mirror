<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/05/28 12:46:00 $"
  rcs.revision="$Revision: 1.6 $"
  description.file="arm_descriptions.xml">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.9"
    source="../data/modules/part_view_definition/arm.exp"/>

  <schema name="Part_view_definition_arm">
    <interface
      kind="use"
      schema="Part_and_version_identification_arm">
    </interface>

    <interface
      kind="use"
      schema="Product_view_definition_arm">
    </interface>

    <entity
      name="Part_view_definition"
      supertypes="Product_view_definition">
      <explicit
        name="WHERE">
        <typename
          name="SELF.definition_for.of_product.product_type.name IN [ 'part' , 'raw material' ]"/>
      </explicit>
    </entity>

  </schema>

</express>
