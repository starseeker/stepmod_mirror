<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express language_version="2" rcs.date="$Date: 2002/02/13 09:37:34 $" rcs.revision="$Revision: 1.4 $">
	<application name="express2xml.js" owner="Eurostep Limited" url="http://www.eurostep.com" version="1.6" source="../data/modules/part_view_definition/arm.exp"/>
	<schema name="Part_view_definition_arm">
		<interface kind="use" schema="Part_and_version_identification_arm">
		</interface>
		<interface kind="use" schema="Product_view_definition_arm">
		</interface>
		<interface kind="use" schema="Alias_identification_arm">
		</interface>
		<type name="part_view_definition_alias_identification_item">
			<description>This select extends the assignments of identification for aliasing to part view definitions.</description>
			<select extensible="YES" basedon="alias_identification_item" selectitems="Part_view_definition">
			</select>
		</type>
		<entity name="Part_view_definition" supertypes="Product_view_definition">
			<description>A <b>Part_view_definition</b> is a view of a part version relevant for the requirements of one or more life cycle stages and application domains. This definition is only for parts or raw materials and may have an alternate identifier.</description>
			<inverse name="idalias" entity="Alias_identification" attribute="items">
				<description>An optional alternate identifier.</description>
				<inverse.aggregate type="SET" lower="0" upper="1"/>
			</inverse>
			<where label="WR1" expression="SELF.definition_for.of_product.product_type.name IN ['part', 'raw material']">
				<description>The <b>Part_view_definition</b> shall be a view of a 'part' or 'raw material'.</description>
			</where>
		</entity>
	</schema>
</express>
