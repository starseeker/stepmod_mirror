<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: 2003/04/23 23:29:46 $"
  rcs.revision="$Revision: 1.10 $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.16"
    source="../data/modules/Part_view_definition/arm.exp"/>

  <schema name="Part_view_definition_arm">
    <interface
      kind="use"
      schema="Part_and_version_identification_arm">
    </interface>

    <interface
      kind="use"
      schema="Product_view_definition_arm">
    </interface>

    <entity
      name="Part_view_definition"
      supertypes="Product_view_definition">
      <explicit
        name="defined_version">
          <typename
            name="Part_version"/>
        <redeclaration
          entity-ref="Product_view_definition"/>
        </explicit>
      </entity>

<rule 
    name="part_view_definition_constraint" 
    appliesto="Product_view_definition">
<where label="WR1"
expression="SIZEOF ( QUERY ( pvd &lt;* Product_view_definition | 
 ( NOT 
    ( 'PART_VIEW_DEFINITION_ARM.' + 
      'PART_VIEW_DEFINITION' 
      IN TYPEOF (pvd)
    ) 
 ) 
 AND 
 ( 'PART_VIEW_DEFINITION_ARM.' +
   'PART_VERSION' 
   IN TYPEOF ( pvd.defined_version) 
 ) 
 ) ) =0
"
/>
</rule>
    </schema>

  </express>
