	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Part_view_definition_arm  Entity: Part_view_definition 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Part_view_definition_arm.Part_view_definition">
	A <b>Part_view_definition</b> is a type of 
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/>
	that defines a characterization view of a version of a 
	<express_ref linkend="part_and_version_identification:arm:Part_and_version_identification_arm.Part"/>.
</ext_description>

<!-- defined_version -->
<ext_description linkend="Part_view_definition_arm.Part_view_definition.defined_version">
	the <express_ref linkend="part_and_version_identification:arm:Part_and_version_identification_arm.Part_version"/>
	of which the <b>Part_view_definition</b> defines a view.
</ext_description>

<!-- defined_version -->
<ext_description linkend="Part_view_definition_arm.Part_view_definition.defined_version">
	an inherited attribute shall be of type
	<express_ref linkend="Part_and_version_identification:arm:Part_and_version_identification_arm.Part_version"/>
	for the <b>Part_view_definition</b>.
</ext_description>

<!-- initial_context -->
<ext_description linkend="Part_view_definition_arm.Part_view_definition.initial_context">
	an inherited attribute shall be of type
	<express_ref linkend="Part_view_definition:arm:Part_view_definition_arm.Part_view_definition_context"/>
	for the <b>Part_view_definition</b>.
</ext_description>

<!-- additional_contexts -->
<ext_description linkend="Part_view_definition_arm.Part_view_definition.additional_contexts">
	an inherited attribute shall be of type
	<express_ref linkend="Part_view_definition:arm:Part_view_definition_arm.Part_view_definition_context"/>
	for the <b>Part_view_definition</b>.
	There shall exist zero or more
	<express_ref linkend="Part_view_definition:arm:Part_view_definition_arm.Part_view_definition_context">Part_view_definition_context</express_ref>
	for the <b>Part_view_definition</b>.
</ext_description>

<!-- +++++++++++ Part_view_definition_context +++++++++++ -->
<ext_description linkend="Part_view_definition_arm.Part_view_definition_context">
	A <b>Part_view_definition_context</b> is a type of
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.View_definition_context"/>
	that is specific for
	<express_ref linkend="Part_view_definition:arm:Part_view_definition_arm.Part_view_definition"/>s.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Part_view_definition_arm.Part_view_definition_context.wr:WR1">
	A <b>Part_view_definition_context</b> shall only be referenced by
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/>s
	in the role
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition.initial_context"/>
	that are of type
	<express_ref linkend="Part_view_definition:arm:Part_view_definition_arm.Part_view_definition"/>.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Part_view_definition_arm.Part_view_definition_context.wr:WR2">
	A <b>Part_view_definition_context</b> shall only be referenced by
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/>s
	in the role
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition.additional_contexts"/>
	that are of type
	<express_ref linkend="Part_view_definition:arm:Part_view_definition_arm.Part_view_definition"/>.
</ext_description>

<!-- +++++++++++ part_view_definition_constraint +++++++++++ -->
<ext_description linkend="Part_view_definition_arm.part_view_definition_constraint">
	Every 
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/> 
	referencing a 
	<express_ref linkend="part_and_version_identification:arm:Part_and_version_identification_arm.Part_version"/>
	is a 
	<express_ref linkend="part_view_definition:arm:Part_view_definition_arm.Part_view_definition"/>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Part_view_definition_arm.part_view_definition_constraint.wr:WR1">
	Every 
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/> 
	referencing a 
	<express_ref linkend="part_and_version_identification:arm:Part_and_version_identification_arm.Part_version"/>
	is a 
	<express_ref linkend="part_view_definition:arm:Part_view_definition_arm.Part_view_definition"/>.
</ext_description>


</ext_descriptions>
