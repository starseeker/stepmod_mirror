	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Part_view_definition_arm  Entity: Part_view_definition  Where: WR1 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Part_view_definition_arm.Part_view_definition.wr:WR1">The <b>Part_view_definition</b> shall be a view of a 'part' or 'raw material'.</ext_description>
