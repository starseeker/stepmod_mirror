<ext_description linkend="Part_view_definition_arm.Part_view_definition">
	A <b>Part_view_definition</b> is a type of 
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/>
	that defines a characterization view of a version of a 
	<express_ref linkend="part_and_version_identification:arm:Part_and_version_identification_arm.Part"/>.
</ext_description>

<!-- defined_version -->
<ext_description linkend="Part_view_definition_arm.Part_view_definition.defined_version">
	the <express_ref linkend="part_and_version_identification:arm:Part_and_version_identification_arm.Part_version"/>
	of which the <b>Part_view_definition</b> defines a view.
</ext_description>

<!-- +++++++++++ Part_view_definition_context +++++++++++ -->
