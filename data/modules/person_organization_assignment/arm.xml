<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/05/28 13:16:49 $"
  rcs.revision="$Revision: 1.4 $"
  description.file="arm_descriptions.xml">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.9"
    source="../data/modules/person_organization_assignment/arm.exp"/>

  <schema name="Person_organization_assignment_arm">
    <interface
      kind="use"
      schema="Person_organization_arm">
    </interface>

    <type name="organization_or_person_in_organization_select">
      <select
        selectitems="Organization Person_in_organization">
      </select>

    </type>
    <type name="organization_or_person_in_organization_item">
      <select
        extensible="YES"
        selectitems="null">
      </select>

    </type>
    <entity
      name="Organization_or_person_in_organization_assignment">
      <explicit
        name="assigned_entity">
        <typename
          name="organization_or_person_in_organization_select"/>
      </explicit>
      <explicit
        name="role">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="organization_or_person_in_organization_item"/>
      </explicit>
    </entity>

  </schema>

</express>
