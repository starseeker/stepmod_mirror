<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/03/04 08:28:17 $"
  rcs.revision="$Revision: 1.5 $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.6"
    source="../data/modules/person_organization_assignment/mim.exp"/>

  <schema name="Person_organization_assignment_mim">
    <interface
      kind="use"
      schema="Person_organization_mim">
    </interface>

    <interface
      kind="use"
      schema="person_organization_schema">
      <interfaced.item
        name="organization_role"/>
      <interfaced.item
        name="person_and_organization_role"/>
    </interface>

    <interface
      kind="use"
      schema="management_resources_schema">
      <interfaced.item
        name="organization_assignment"/>
      <interfaced.item
        name="person_and_organization_assignment"/>
    </interface>

    <type name="organization_item">
      <description>An <b>organization_item</b> specifies those objects to which an <b>organization</b> may be assigned.</description>
      <select
        extensible="YES">
      </select>

    </type>
    <type name="person_and_organization_item">
      <description>A <b>person_and_organization_item</b> specifies those objects to which a <b>person_and_organization</b> may be assigned.</description>
      <select
        extensible="YES">
      </select>

    </type>
    <entity
      name="applied_organization_assignment"
      supertypes="organization_assignment">
      <description></description>
      <explicit
        name="items">
      <description></description>
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="organization_item"/>
      </explicit>
    </entity>

    <entity
      name="applied_person_and_organization_assignment"
      supertypes="person_and_organization_assignment">
      <description></description>
      <explicit
        name="items">
      <description></description>
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="person_and_organization_item"/>
      </explicit>
    </entity>

  </schema>

</express>
