<?xml version="1.0"?>
<!-- $Id: mim.xml,v 1.7 2002/05/28 12:46:25 buchanan Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/05/28 12:46:25 $"
  rcs.revision="$Revision: 1.7 $"
  description.file="mim_descriptions.xml">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.9"
    source="../data/modules/person_organization_assignment/mim.exp"/>

  <schema name="Person_organization_assignment_mim">
    <interface
      kind="use"
      schema="Person_organization_mim">
    </interface>

    <interface
      kind="use"
      schema="person_organization_schema">
      <interfaced.item
        name="organization_role"/>
      <interfaced.item
        name="person_and_organization_role"/>
    </interface>

    <interface
      kind="use"
      schema="management_resources_schema">
      <interfaced.item
        name="organization_assignment"/>
      <interfaced.item
        name="person_and_organization_assignment"/>
    </interface>

    <type name="organization_item">
      <select
        extensible="YES"
        selectitems="null">
      </select>

    </type>
    <type name="person_and_organization_item">
      <select
        extensible="YES"
        selectitems="null">
      </select>

    </type>
    <entity
      name="applied_organization_assignment"
      supertypes="organization_assignment">
      <explicit
        name="items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="organization_item"/>
      </explicit>
    </entity>

    <entity
      name="applied_person_and_organization_assignment"
      supertypes="person_and_organization_assignment">
      <explicit
        name="items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="person_and_organization_item"/>
      </explicit>
    </entity>

  </schema>

</express>
