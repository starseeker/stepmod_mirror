  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Person_organization_assignment_mim  Entity: applied_organization_assignment 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Person_organization_assignment_mim.applied_organization_assignment">
	 An <b>applied_organization_assignment</b> is a type of <b>organization_assignment</b> 
	 that relates an <b>organization</b> to instances of entity data types listed in <b>organization_item</b>.
	 </ext_description>

