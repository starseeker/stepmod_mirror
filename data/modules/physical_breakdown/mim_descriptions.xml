	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Physical_breakdown_mim  Entity: physical_breakdown_context  
   <express_ref linkend="physical_breakdown:mim:Physical_breakdown_mim.physical_breakdown_context"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Physical_breakdown_mim.physical_breakdown_context">
		A <b>physical_breakdown_context</b> is a type of 
		<express_ref linkend="product_breakdown:mim:Product_breakdown_mim.breakdown_context"/> 
		that implements the ARM concept of
		<express_ref linkend="Physical_breakdown:arm:Physical_breakdown_arm.Physical_breakdown_context"/>.
		A <b>physical_breakdown_context</b> identifies the relationship between a physical element and a physical breakdown of which the element is a member.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Physical_breakdown_mim  Entity: physical_element_usage  
   <express_ref linkend="physical_breakdown:mim:Physical_breakdown_mim.physical_element_usage"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Physical_breakdown_mim.physical_element_usage">
	A <b>physical_element_usage</b> is a type of 
	<express_ref linkend="product_breakdown:mim:Product_breakdown_mim.breakdown_element_usage"/>
	that implements the ARM concept of
	<express_ref linkend="Physical_breakdown:arm:Physical_breakdown_arm.Physical_element_usage"/>.
	A <b>physical_element_usage</b> identifies the relationship between a parent and child physical element.
	</ext_description>
