<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-01-03T15:24:56" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="physical_layout_template_arm schema_instance"/>
   <schema name="Physical_layout_template_arm">
      <interface kind="use" schema="Analytical_model_arm"/>
      <interface kind="use" schema="Characteristic_arm"/>
      <interface kind="use" schema="Part_view_definition_arm"/>
      <interface kind="use" schema="Part_and_version_identification_arm"/>
      <interface kind="use" schema="Shape_property_assignment_arm"/>
      <interface kind="use" schema="Feature_and_connection_zone_arm"/>
      <interface kind="use" schema="Functional_decomposition_to_design_arm"/>
      <interface kind="reference" schema="Product_identification_arm">
         <interfaced.item name="types_of_product"/>
      </interface>
      <type name="correlated_or_independent">
         <select selectitems="coordinated_characteristic characteristic"/>
      </type>
      <type name="plt_analytical_model_application_assignment_select">
         <select extensible="YES" genericentity="YES" basedon="_SELECT_analytical_model_application_assignment_select" selectitems="Layout_template_definition"/>
      </type>
      <type name="plt_physical_unit_or_part_template_or_fp_or_pd">
         <select extensible="YES" genericentity="YES" basedon="_SELECT_physical_unit_or_part_template_or_fp_or_pd" selectitems="Layout_template_definition"/>
      </type>
      <entity name="Connection_zone_in_layout_template" supertypes="Connection_zone">
         <explicit name="containing_shape">
            <typename name="Layout_template_definition"/>
            <redeclaration entity-ref="Shape_element"/>
         </explicit>
      </entity>
      <entity name="Layout_template_definition" supertypes="Template_definition Shape_element">
         <explicit name="physical_characteristic" optional="YES">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="correlated_or_independent"/>
         </explicit>
         <derived name="containing_shape" expression=" SELF ">
            <typename name="Layout_template_definition"/>
            <redeclaration entity-ref="Shape_element"/>
         </derived>
         <where label="WR1" expression=" SIZEOF ( QUERY ( prpc &lt;* USEDIN ( SELF \ product_view_definition . defined_version . of_product , 'PHYSICAL_LAYOUT_TEMPLATE_ARM.PRODUCT_CATEGORY_ASSIGNMENT.PRODUCTS' ) | ( prpc . name = 'template model' ) ) ) &gt; 0 "/>
         <where label="WR2" expression=" SELF \ shape_element . element_name = 'NULL' "/>
      </entity>
      <entity name="Reference_functional_unit_assignment_to_template">
         <explicit name="functional_design_definition_path">
            <typename name="Reference_composition_path"/>
         </explicit>
         <explicit name="implementation">
            <typename name="Layout_template_definition"/>
         </explicit>
         <explicit name="of_property" optional="YES">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="coordinated_characteristic"/>
         </explicit>
         <explicit name="path_alias">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="swap_code" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute entity-ref="Reference_functional_unit_assignment_to_template" attribute="implementation"/>
            <unique.attribute entity-ref="Reference_functional_unit_assignment_to_template" attribute="path_alias"/>
         </unique>
      </entity>
      <entity name="Reference_part_template_for_analytical_model">
         <explicit name="template">
            <typename name="Layout_template_definition"/>
         </explicit>
         <explicit name="analytical_representation">
            <typename name="Analytical_model_definition"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute entity-ref="Reference_part_template_for_analytical_model" attribute="template"/>
         </unique>
      </entity>
      <entity name="Template" supertypes="Product">
         <where label="WR1" expression=" SIZEOF ( [ 'template model' ] * types_of_product ( SELF ) ) = 1 "/>
      </entity>
      <entity name="Template_definition" supertypes="Item_shape Product_view_definition">
         <explicit name="defined_version">
            <typename name="Template_version"/>
            <redeclaration entity-ref="Product_view_definition"/>
         </explicit>
         <derived name="described_element" expression=" SELF ">
            <typename name="Template_definition"/>
            <redeclaration entity-ref="Item_shape"/>
         </derived>
      </entity>
      <entity name="Template_version" supertypes="Product_version">
         <explicit name="of_product">
            <typename name="Template"/>
            <redeclaration entity-ref="Product_version"/>
         </explicit>
      </entity>
      <subtype.constraint name="plt_connection_zone_subtypes" entity="Connection_zone" super.expression="ONEOF (Connection_zone_in_usage_view, Connection_zone_in_design_view, Connection_zone_in_layout_template)"/>
   </schema>
</express>
