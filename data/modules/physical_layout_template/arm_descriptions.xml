<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/descriptions.xsl"?>
<!DOCTYPE ext_descriptions SYSTEM "../../../dtd/description.dtd">
<ext_descriptions module_directory="physical_layout_template" 
	schema_file="arm.xml" describe.selects="YES" 
	describe.subtype_constraints="YES" rcs.date="$Date: 2004/12/31 10:48:46 $" 
	rcs.revision="$Revision: 1.7 $">
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: Physical_layout_template_arm
   <express_ref linkend="physical_layout_template:arm:Physical_layout_template_arm"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<ext_description linkend="Physical_layout_template_arm">
	</ext_description>
	<!-- +++++++++++ Reference_part_template_for_analytical_model +++++++++++ -->
	<ext_description 
		linkend="Physical_layout_template_arm.Reference_part_template_for_analytical_model">
	</ext_description>	
	<ext_description 
		linkend="Physical_layout_template_arm.Reference_part_template_for_analytical_model.template">
		the <b>template</b> specifies a <express_ref linkend="Physical_layout_template:arm:Physical_layout_template_arm.Layout_template_definition"/> for the <b>Reference_part_template_for_analytical_model</b>.
	</ext_description>
	<ext_description 
		linkend="Physical_layout_template_arm.Reference_part_template_for_analytical_model.analytical_representation">
		the <b>analytical_representation</b> specifies a <express_ref linkend="Analytical_model:arm:Analytical_model_arm.Analytical_model_definition"/> for the <b>Reference_part_template_for_analytical_model</b>.		
	</ext_description>
	<ext_description 
		linkend="Physical_layout_template_arm.Reference_part_template_for_analytical_model.ur:UR1">
		the <b>template</b> should be unique within a population of the <b>Reference_part_template_for_analytical_model</b>.
	</ext_description>
	
	<!-- +++++++++++ Reference_functional_unit_assignment_to_template +++++++++++ -->
	<ext_description 
		linkend="Physical_layout_template_arm.Reference_functional_unit_assignment_to_template">
	</ext_description>
	<ext_description 
		linkend="Physical_layout_template_arm.Reference_functional_unit_assignment_to_template.functional_design_definition_path">
		the <b>functional_design_definition_path</b> specifies a role of the <express_ref linkend="Functional_decomposition_to_design:arm:Functional_decomposition_to_design_arm.Reference_composition_path"/>
		for the <b>Reference_functional_unit_assignment_to_template</b>.
	</ext_description>
	<ext_description 
		linkend="Physical_layout_template_arm.Reference_functional_unit_assignment_to_template.implementation">
		the <b>implementation</b> specifies a role of the <express_ref linkend="Physical_layout_template:arm:Physical_layout_template_arm.Layout_template_definition"/>
		for the <b>Reference_functional_unit_assignment_to_template</b>.		
	</ext_description>
	<ext_description 
		linkend="Physical_layout_template_arm.Reference_functional_unit_assignment_to_template.of_property">
		the <b>of_property</b> specifies a role of the <express_ref linkend="Characteristic:arm:Characteristic_arm.coordinated_characteristic"/> 
		for the <b>Reference_functional_unit_assignment_to_template</b>. The <b>of_property</b> is optional and not need to be supplied.
	</ext_description>
	<ext_description 
		linkend="Physical_layout_template_arm.Reference_functional_unit_assignment_to_template.path_alias">
		the <b>path_alias</b> specifies a string for the <b>Reference_functional_unit_assignment_to_template</b>.
	</ext_description>
	<ext_description 
		linkend="Physical_layout_template_arm.Reference_functional_unit_assignment_to_template.swap_code">
		the <b>swap_code</b> specifies a string for the <b>Reference_functional_unit_assignment_to_template</b>. The <b>swap_code</b> is optional and not need to be supplied.
	</ext_description>
	<ext_description 
		linkend="Physical_layout_template_arm.Reference_functional_unit_assignment_to_template.ur:UR1">
		the <b>implementation</b> and <b>path_alias</b> shall be unique within a population of the <b>Reference_functional_unit_assignment_to_template</b>.
	</ext_description>
	<!-- +++++++++++ Connection_zone_in_layout_template +++++++++++ -->
	<ext_description 
		linkend="Physical_layout_template_arm.Connection_zone_in_layout_template">
		A <b>Connection_zone_in_layout_template</b> is a type of <express_ref linkend="Feature_and_connection_zone:arm:Feature_and_connection_zone_arm.Connection_zone"/>.
	</ext_description>
	<ext_description 
		linkend="Physical_layout_template_arm.Connection_zone_in_layout_template.containing_shape">
		the <b>containing_shape</b> specifies a <express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element"/> for the <b>Connection_zone_in_layout_template</b>.
	</ext_description>	
	<!-- +++++++++++ Layout_template_definition +++++++++++ -->
<ext_description 
		linkend="Physical_layout_template_arm.Layout_template_definition"> A 
		<b>Layout_template_definition</b> is a type of <express_ref 
		linkend="Extended_geometric_tolerance:arm:Extended_geometric_tolerance_arm.physical_feature_or_part_template"/>. 
		A <b>Layout_template_definition</b> may be one of an <express_ref 
		linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Inter_stratum_feature_template"/>, 
		a <express_ref 
		linkend="Fabrication_technology:arm:Fabrication_technology_arm.Material_removal_feature_template"/>, 
		a <express_ref 
		linkend="Printed_physical_layout_template:arm:Printed_physical_layout_template_arm.Non_conductive_cross_section_template"/>, 
		a <express_ref 
		linkend="Printed_physical_layout_template:arm:Printed_physical_layout_template_arm.Printed_part_cross_section_template"/>, 
		a <express_ref 
		linkend="Printed_physical_layout_template:arm:Printed_physical_layout_template_arm.Printed_part_template"/>, 
		a <express_ref 
		linkend="Part_template_extension:arm:Part_template_extension_arm.Special_symbol_part_template"/>, 
		a <express_ref 
		linkend="Fabrication_technology:arm:Fabrication_technology_arm.Stratum_feature_template"/>, 
		or a <express_ref 
		linkend="Part_template_extension:arm:Part_template_extension_arm.Trace_template"/>, 
		or additionally may be a <express_ref 
		linkend="Part_template_extension:arm:Part_template_extension_arm.Part_text_template"/>. 
		One reason for including information provided by members of 
		<b>Layout_template_definition</b> is that they are collectors for shape 
		data templates for layout items. </ext_description>
	<ext_description 
		linkend="Physical_layout_template_arm.Layout_template_definition.containing_shape">
		 the <b>containing_shape</b> specifies the role of the <express_ref 
		linkend="Physical_layout_template:arm:Physical_layout_template_arm.Template_definition"/> 
		for the <express_ref 
		linkend="Physical_layout_template:arm:Physical_layout_template_arm.Layout_template_definition"/>. 
		</ext_description>
	<ext_description 
		linkend="Physical_layout_template_arm.Layout_template_definition.physical_characteristic">
		 the <b>physical_characteristic</b> specifies either the <express_ref 
		linkend="Characteristic:arm:Characteristic_arm.characteristic"/> or 
		<express_ref 
		linkend="Characteristic:arm:Characteristic_arm.coordinated_characteristic"/> 
		for the <express_ref 
		linkend="Physical_layout_template:arm:Physical_layout_template_arm.Layout_template_definition"/>. 
		There may be more than one <express_ref 
		linkend="Characteristic:arm:Characteristic_arm.characteristic"/> for 
		the <express_ref 
		linkend="Physical_layout_template:arm:Physical_layout_template_arm.Layout_template_definition"/>. 
		The <b>physical_characteristic</b> need not be specified for a 
		particular <express_ref 
		linkend="Physical_layout_template:arm:Physical_layout_template_arm.Layout_template_definition"/>. 
		</ext_description>
	<ext_description linkend="Physical_layout_template_arm.Layout_template_definition.wr:WR1">
The <express_ref linkend="product_identification:arm:Product_identification_arm.Product"/> refered by
<express_ref linkend="product_version:arm:Product_version_arm.Product_version.of_product"/> of
<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition.defined_version"/> shall
be assigned by <express_ref linkend="product_identification:arm:Product_identification_arm.Product_category_assignment"/>
to at least one <express_ref linkend="product_categorization:arm:Product_categorization_arm.Product_category"/> with
<express_ref linkend="product_categorization:arm:Product_categorization_arm.Product_category.name"/> 'template model'.
	</ext_description>

	<ext_description 
		linkend="Physical_layout_template_arm.Layout_template_definition.wr:WR2">
		 The name inherited from <express_ref 
		linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element"/> 
		shall be 'NULL'. </ext_description>
	<!-- +++++++++++ Template_definition +++++++++++ -->
<ext_description linkend="Physical_layout_template_arm.Template_definition">
		 A <b>Template_definition</b> is a type of <express_ref 
		linkend="Product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/> and of
		<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Item_shape"/>
		that defines a characterization view of a version of a <express_ref 
		linkend="part_and_version_identification:arm:Part_and_version_identification_arm.Part"/>. 
		</ext_description>
	<ext_description 
		linkend="Physical_layout_template_arm.Template_definition.described_element">
		 the <b>described_element</b> is an inverse relationship that specifies 
		that the existence of the <b>Template_definition</b> is dependent on 
		the existence of a <express_ref 
		linkend="Physical_layout_template:arm:Physical_layout_template_arm.Layout_template_definition"/>to 
		reference the <b>Template_definition</b>. </ext_description>
	<ext_description 
		linkend="Physical_layout_template_arm.Template_definition.defined_version">
		<p>the <b>defined_version</b> specifies a role of the <express_ref linkend="Physical_layout_template:arm:Physical_layout_template_arm.Template_version"/> for the <b>Template_definition</b>.
		</p>
	</ext_description>
	<!-- +++++++++++ Template +++++++++++ -->
<ext_description linkend="Physical_layout_template_arm.Template"> A 
		<b>Template</b> is a type of <express_ref 
		linkend="product_identification:arm:Product_identification_arm.Product"/> 
		that can be physical realised but can typically not be produced or bought independently but only as a direct or indirect component of a
		an assembly of a <express_ref 
		linkend="part_and_version_identification:arm:Part_and_version_identification_arm.Part"/> 
		</ext_description>
	<ext_description linkend="Physical_layout_template_arm.Template.wr:wr1"> The 
		<b>Template</b> shall belong to a product category named 'template model'. 
		</ext_description>
<ext_description linkend="Physical_layout_template_arm.Template.wr:WR1">
		There shall be one and only one <express_ref linkend="Product_categorization:arm:Product_categorization_arm.Product_category"/>
		with <express_ref linkend="Product_categorization:arm:Product_categorization_arm.Product_category.name"/> 'template model'
		associated with an instance of <b>Template</b> through an instance of <express_ref linkend="Product_identification:arm:Product_identification_arm.Product_category_assignment"/>.
</ext_description>
		<!-- +++++++++++ Template_version +++++++++++ -->
<ext_description linkend="Physical_layout_template_arm.Template_version"> A 
		<b>Template_version</b> is a type of <express_ref 
		linkend="product_version:arm:Product_version_arm.Product_version"/> 
		that identifies a version of a template. A <b>Template_version</b> 
		serves as the collector of the data characterizing a template. 
		</ext_description>
	<ext_description 
		linkend="Physical_layout_template_arm.Template_version.of_product"> the 
		<express_ref linkend="physical_layout_template:arm:Physical_layout_template_arm.Template"/> of 
		which the <b>Template_version</b> specifies a version. 
		</ext_description>
</ext_descriptions>

