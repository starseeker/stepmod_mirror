<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="mim_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="physical_layout_template_mim schema_instance"/>
   <schema name="Physical_layout_template_mim">
      <interface kind="use" schema="Analytical_model_mim"/>
      <interface kind="use" schema="Feature_and_connection_zone_mim"/>
      <interface kind="use" schema="application_context_schema">
         <interfaced.item name="application_context_element"/>
      </interface>
      <type name="plt_groupable_item">
         <select basedon="groupable_item" selectitems="representation_item"/>
      </type>
      <entity name="part_template_definition" supertypes="product_definition product_definition_shape">
         <derived name="definition" expression="SELF">
            <typename name="part_template_definition"/>
            <redeclaration entity-ref="property_definition"/>
         </derived>
         <where label="WR1" expression="SIZEOF (USEDIN (SELF, 'BASIC_ATTRIBUTE_SCHEMA.' +'NAME_ATTRIBUTE.NAMED_ITEM')) = 0"/>
         <where label="WR2" expression="NOT EXISTS(SELF\property_definition.description)"/>
      </entity>
   </schema>
</express>
