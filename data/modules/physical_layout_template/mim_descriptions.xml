<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: part_template_definition
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Physical_layout_template_mim.part_template_definition"><p>
A <b>part_template_definition</b> is a type of 
<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition"/> 
and of
<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.product_definition_shape"/> 
that implements the ARM concepts of:
</p><ul> 
<li> <express_ref linkend="Fabrication_technology:arm:Fabrication_technology_arm.Blind_passage_template"/>;</li>
<li> <express_ref linkend="Fabrication_technology:arm:Fabrication_technology_arm.Component_termination_passage_template"/>;</li>
<li> <express_ref linkend="Fabrication_technology:arm:Fabrication_technology_arm.Counterbore_passage_template"/>;</li>
<li> <express_ref linkend="Fabrication_technology:arm:Fabrication_technology_arm.Countersunk_passage_template"/>;</li>
<li> <express_ref linkend="Part_template_extension:arm:Part_template_extension_arm.Default_trace_template"/>;</li>
<li> <express_ref linkend="Fabrication_technology:arm:Fabrication_technology_arm.Inter_stratum_feature_template"/>;</li>
<li> <express_ref linkend="Fabrication_technology:arm:Fabrication_technology_arm.Material_removal_feature_template"/>;</li>
<li> <express_ref linkend="Printed_physical_layout_template:arm:Printed_physical_layout_template_arm.Non_conductive_cross_section_template"/>;</li>
<li> <express_ref linkend="Physical_layout_template:arm:Physical_layout_template_arm.Template_definition"/>;</li>
<li> <express_ref linkend="Part_template_extension:arm:Part_template_extension_arm.Part_text_template"/>;</li> 
<li> <express_ref linkend="Printed_physical_layout_template:arm:Printed_physical_layout_template_arm.Printed_connector_template"/>;</li>
<li> <express_ref linkend="Printed_physical_layout_template:arm:Printed_physical_layout_template_arm.Printed_part_cross_section_template"/>;</li>
<li> <express_ref linkend="Printed_physical_layout_template:arm:Printed_physical_layout_template_arm.Printed_part_template"/>;</li>
<li> <express_ref linkend="Fabrication_technology:arm:Fabrication_technology_arm.Stratum_feature_template"/>;</li>
<li> <express_ref linkend="Part_template_extension:arm:Part_template_extension_arm.Trace_template"/>;</li>
<li> <express_ref linkend="Fabrication_technology:arm:Fabrication_technology_arm.Unsupported_passage_template"/>;</li>
<li> <express_ref linkend="Fabrication_technology:arm:Fabrication_technology_arm.Via_template"/>.</li>
</ul>
</ext_description>

<!-- definition -->
<ext_description linkend="Physical_layout_template_mim.part_template_definition.definition">
	an attribute inherited from the
	<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.property_definition"/> 
	shall be redeclared as the
	<express_ref linkend="Physical_layout_template:mim:Physical_layout_template_mim.part_template_definition"/> 
	for the <b>part_template_definition</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Physical_layout_template_mim.part_template_definition.wr:WR1">
	<p>
	No member of entity data type <b>part_template_definition</b> 
	shall be referenced by a member of entity data type 
	<express_ref linkend="basic_attribute_schema:ir_express:basic_attribute_schema.name_attribute"/> 
	as a 
	<express_ref linkend="basic_attribute_schema:ir_express:basic_attribute_schema.name_attribute.named_item"/>.
	</p>
</ext_description>

<!-- WR2 -->
<ext_description linkend="Physical_layout_template_mim.part_template_definition.wr:WR2">
	The 
	<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.property_definition.description"/>
	shall not be populated.
</ext_description>

