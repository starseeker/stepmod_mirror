      <interface kind="use" schema="Assembly_module_with_packaged_connector_component_mim"/>
      <interface kind="use" schema="Assembly_technology_mim"/>
      <interface kind="use" schema="Component_feature_mim"/>
      <interface kind="use" schema="Functional_design_elaboration_mim"/>
      <interface kind="use" schema="Interface_component_mim"/>
      <interface kind="use" schema="Packaged_connector_model_mim"/>
