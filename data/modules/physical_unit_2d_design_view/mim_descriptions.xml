<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Rule: cto2d_determinant_test
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Physical_unit_2d_design_view_mim.cto2d_determinant_test">
	The <b>cto2d_determinant_test</b> function returns true if the <b>expected_value</b> is within the bounds defined by the delta value.
</ext_description>

<!-- cto2d -->
<ext_description linkend="Physical_unit_2d_design_view_mim.cto2d_determinant_test.cto2d">
	the specified 
	<express_ref linkend="geometry_schema:ir_express:geometry_schema.cartesian_transformation_operator_2d"/>.
</ext_description>

<!-- expected_value -->
<ext_description linkend="Physical_unit_2d_design_view_mim.cto2d_determinant_test.expected_value">
	the specified real value.
</ext_description>

<!-- delta -->
<ext_description linkend="Physical_unit_2d_design_view_mim.cto2d_determinant_test.delta">
	the specified real value.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: component_2d_location
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Physical_unit_2d_design_view_mim.component_2d_location">
	A <b>component_2d_location</b> is a type of 
	<express_ref linkend="product_property_representation_schema:ir_express:product_property_representation_schema.context_dependent_shape_representation"/>,
	<express_ref linkend="product_property_representation_schema:ir_express:product_property_representation_schema.shape_representation_relationship"/>,
	<express_ref linkend="representation_schema:ir_express:representation_schema.representation_relationship_with_transformation"/>
	and
	<express_ref linkend="representation_schema:ir_express:representation_schema.definitional_representation_relationship"/>.
</ext_description>

<!-- representation_relation -->
<ext_description linkend="Physical_unit_2d_design_view_mim.component_2d_location.representation_relation">
	an attribute inherited from the
	<express_ref linkend="product_property_representation_schema:ir_express:product_property_representation_schema.context_dependent_shape_representation"/>
	shall be redeclared as the
	<express_ref linkend="Physical_unit_2d_design_view:mim:Physical_unit_2d_design_view_mim.component_2d_location"/>
	for the <b>component_2d_location</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Physical_unit_2d_design_view_mim.component_2d_location.wr:WR1">
	The 
	<express_ref linkend="representation_schema:ir_express:representation_schema.representation_relationship.name"/>
	shall be 'component 2d location'.
</ext_description>

