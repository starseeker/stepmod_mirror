<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-09-28T17:38:02" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="physical_unit_3d_design_view_arm schema_instance"/>
   <schema name="Physical_unit_3d_design_view_arm">
      <interface kind="use" schema="Layered_3d_shape_arm"/>
      <entity name="Assembly_component_3d_shape" supertypes="Feature_shape_definition">
         <explicit name="shape_characterized_component">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="Assembly_component"/>
         </explicit>
         <derived name="name" expression="''">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="Representation"/>
         </derived>
      </entity>
      <entity name="Component_3d_location" supertypes="Contextual_shape_representation Geometric_composition_with_operator_transformation">
         <explicit name="transformation">
            <typename name="Cartesian_transformation_3d"/>
            <redeclaration entity-ref="Geometric_relationship_with_operator_transformation"/>
         </explicit>
         <explicit name="placement_fixed">
            <builtintype type="BOOLEAN"/>
         </explicit>
         <explicit name="component_model">
            <typename name="Assembly_component_3d_shape"/>
            <redeclaration entity-ref="Geometric_model_relationship" old_name="rep_1"/>
         </explicit>
         <explicit name="assembly_model">
            <typename name="Physical_unit_3d_shape_model"/>
            <redeclaration entity-ref="Geometric_model_relationship" old_name="rep_2"/>
         </explicit>
         <derived name="component" expression="SELF\Contextual_shape_representation.contextual_shape.described_element">
            <typename name="Next_assembly_usage"/>
         </derived>
         <derived name="the_context" expression="assembly_model\Representation.context_of_items">
            <typename name="Geometric_coordinate_space"/>
         </derived>
         <derived name="representing_relationship" expression="SELF">
            <typename name="Geometric_model_relationship"/>
            <redeclaration entity-ref="Contextual_shape_representation"/>
         </derived>
         <derived name="relation_type" expression="'component 3d location'">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="Representation_relationship"/>
         </derived>
         <where label="WR1" expression="EXISTS (component)"/>
      </entity>
      <entity name="Component_part_3d_geometric_representation_relationship" supertypes="Geometric_model_relationship">
         <explicit name="part_shape">
            <typename name="part_template_or_physical_unit_3d_shape_model_select"/>
            <redeclaration entity-ref="Geometric_model_relationship" old_name="rep_1"/>
         </explicit>
         <explicit name="component_shape">
            <typename name="Assembly_component_3d_shape"/>
            <redeclaration entity-ref="Geometric_model_relationship" old_name="rep_2"/>
         </explicit>
         <derived name="relation_type" expression="''">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="Representation_relationship"/>
         </derived>
      </entity>
   </schema>
</express>
