   <express_ref linkend="physical_unit_3d_design_view:arm:Physical_unit_3d_design_view_arm"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Physical_unit_3d_design_view_arm">
</ext_description>

<!-- +++++++++++ Assembly_component_3d_shape_model +++++++++++ -->
<ext_description linkend="Physical_unit_3d_design_view_arm.Assembly_component_3d_shape_model">
	An <b>Assembly_component_3d_shape_model</b> is a type of 
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model"/>.
	 An <b>Assembly_component_3d_shape_model</b> is the three-dimensional shape of an 
	<express_ref linkend="Assembly_component:arm:Assembly_component_arm.Assembly_component"/>.
	The mapping of this Application Object constrains the value of the 
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.name"/> 
	attribute inherited from 
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation"/> 
	to be either '3d bound volume shape' or 'open shell based surface'.
	<note>
		When this Application Object is being used in an exchange or sharing 
		scenario, the user has no control over the value of the name attribute 
		inherited from 
		<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation"/>.
	</note>
</ext_description>

<!-- shape_characterized_component -->
<ext_description linkend="Physical_unit_3d_design_view_arm.Assembly_component_3d_shape_model.shape_characterized_component">
	specifies the role of the 
	<express_ref linkend="Assembly_component:arm:Assembly_component_arm.Assembly_component"/> 
	for the <b>Assembly_component_3d_shape_model</b>. There may be one or more  
	<express_ref linkend="Assembly_component:arm:Assembly_component_arm.Assembly_component"/> 
	for each <b>shape_characterized_component</b>.
</ext_description>

<!-- equivalent_reference_shape_model -->
<ext_description linkend="Physical_unit_3d_design_view_arm.Assembly_component_3d_shape_model.equivalent_reference_shape_model">
	specifies the reference shape model for the present AO.  
	The <b>equivalent_reference_shape_model</b> is to be provided only if the type of the shape representation of the assembly 
	is different from the type of the shape representation of the part and 	establishes an equivalency relationship between the shape representation 
	of the part and that of the assembly.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Physical_unit_3d_design_view_arm.Assembly_component_3d_shape_model.wr:WR1">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.name"/> 
	shall not be populated.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Physical_unit_3d_design_view_arm.Assembly_component_3d_shape_model.wr:WR2">
	No more than one <b>equivalent_reference_shape_model</b> shall be provided.
</ext_description>

<!-- IP1 -->
<ext_description linkend="Physical_unit_3d_design_view_arm.Assembly_component_3d_shape_model.wr:IP1">
	If the type of the shape representation of the part is the same as that of
	the assembly the <b>equivalent_reference_shape_model</b> shall not be provided.
</ext_description>

<!-- +++++++++++ Component_3d_location +++++++++++ -->
<ext_description linkend="Physical_unit_3d_design_view_arm.Component_3d_location">
	A <b>Component_3d_location</b> is a type of 
	<express_ref linkend="Contextual_shape_positioning:arm:Contextual_shape_positioning_arm.Contextual_shape_representation"/> 
	and a type of
	<express_ref linkend="Contextual_shape_positioning:arm:Contextual_shape_positioning_arm.Geometric_composition_with_operator_transformation"/>.
	A <b>Component_3d_location</b> is the translation and 
	orientation in three dimensions for the positioning of the 
	<express_ref linkend="Assembly_component:arm:Assembly_component_arm.Assembly_component"/>.  
</ext_description>

<!-- component -->
<ext_description linkend="Physical_unit_3d_design_view_arm.Component_3d_location.component">
	specifies the role of the 
	<express_ref linkend="Assembly_structure:arm:Assembly_structure_arm.Next_assembly_usage"/> 
	for the <b>Component_3d_location</b>. The  
	<express_ref linkend="Assembly_structure:arm:Assembly_structure_arm.Next_assembly_usage"/> 
	specifies the actual <b>component</b> being placed.
</ext_description>

<!-- assembly_model -->
<ext_description linkend="Physical_unit_3d_design_view_arm.Component_3d_location.assembly_model">
	specifies the role of the 
	<express_ref linkend="Physical_unit_3d_shape:arm:Physical_unit_3d_shape_arm.Physical_unit_3d_shape_model"/> 
	for the <b>Component_3d_location</b>.
</ext_description>

<!-- component_model -->
<ext_description linkend="Physical_unit_3d_design_view_arm.Component_3d_location.component_model">
	specifies the role of the 
	<express_ref linkend="Physical_unit_3d_design_view:arm:Physical_unit_3d_design_view_arm.Assembly_component_3d_shape_model"/> 
	for the <b>Component_3d_location</b>.
</ext_description>

<!-- placement_fixed -->
<ext_description linkend="Physical_unit_3d_design_view_arm.Component_3d_location.placement_fixed">
	specifies whether or not the <b>Component_3d_location</b> is 
	fixed. A value of TRUE indicates that the 
	<b>Component_3d_location</b> is fixed. A value of FALSE indicates that the <b>Component_3d_location</b> is not 
	fixed.
</ext_description>

<!-- transformation -->
<ext_description linkend="Physical_unit_3d_design_view_arm.Component_3d_location.transformation">
	specifies the role of the 
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Cartesian_transformation_3d"/> 
	for the <b>Component_3d_location</b>.
</ext_description>

<!-- the_context -->
<ext_description linkend="Physical_unit_3d_design_view_arm.Component_3d_location.the_context">
	specifies the 
	<express_ref linkend="elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_coordinate_space"/> that is
	<express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation.context_of_items"/> 
	of <b>assembly_model</b>.
</ext_description>

<!-- representing_relationship -->
<ext_description linkend="Physical_unit_3d_design_view_arm.Component_3d_location.representing_relationship">
	a redeclared attribute from supertype, that always evaluates to self.
</ext_description>

<!-- relation_type -->
<ext_description linkend="Physical_unit_3d_design_view_arm.Component_3d_location.relation_type">
	specifies the string value equal to 'component 3d location'.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Physical_unit_3d_design_view_arm.Component_3d_location.wr:WR1">
	The <b>component</b> shall exist.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Physical_unit_3d_design_view_arm.Component_3d_location.wr:WR2">
	If TYPEOF <b>component_model</b> includes 
	<express_ref linkend="Physical_unit_3d_design_view:arm:Physical_unit_3d_design_view_arm.Assembly_component_3d_shape_model"/> 
	then the component is included in 
	<b>component_model</b>.<express_ref linkend="Physical_unit_3d_design_view:arm:Physical_unit_3d_design_view_arm.Assembly_component_3d_shape_model.shape_characterized_component"/>.
</ext_description>

<!-- WR3 -->
<ext_description linkend="Physical_unit_3d_design_view_arm.Component_3d_location.wr:WR3">
	If TYPEOF <b>component_model</b>  includes 
	<express_ref linkend="Physical_unit_shape_with_parameters:arm:Physical_unit_shape_with_parameters_arm.Physical_unit_shape_model"/> 
	then the 
	<express_ref linkend="Product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/> 
	referenced by 
	<b>component_model</b>.<express_ref linkend="Physical_unit_shape_with_parameters:arm:Physical_unit_shape_with_parameters_arm.Physical_unit_shape_model.shape_characterized_definition"/> 
	is the 
	<express_ref linkend="Product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/>
	referenced by 
	<express_ref linkend="Assembly_component:arm:Assembly_component_arm.Assembly_component.derived_from"/>  
	attribute of the 
	<express_ref linkend="Assembly_component:arm:Assembly_component_arm.Assembly_component"/> 
	referenced by component.	
</ext_description>

<!-- WR4 -->
<ext_description linkend="Physical_unit_3d_design_view_arm.Component_3d_location.wr:WR4">
	If TYPEOF <b>component_model</b>  includes 
	<express_ref linkend="Part_template_shape_with_parameters:arm:Part_template_shape_with_parameters_arm.Part_template_shape_model"/>  
	then the 
	<express_ref linkend="Product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/> 
	referenced by 
	<b>component_model</b>.<express_ref linkend="Part_template_shape_with_parameters:arm:Part_template_shape_with_parameters_arm.Part_template_shape_model.shape_characterized_definition"/>  
	is the 
	<express_ref linkend="Product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/>
	referenced by 
	<express_ref linkend="Assembly_component:arm:Assembly_component_arm.Assembly_component.derived_from"/>
	attribute of the 
	<express_ref linkend="Assembly_component:arm:Assembly_component_arm.Assembly_component"/>
	referenced by component.
</ext_description>

<!-- +++++++++++ Solid_model +++++++++++ -->
<ext_description linkend="Physical_unit_3d_design_view_arm.Solid_model">
	A <b>Solid_model</b> is a type of
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Detailed_geometric_model_element"/>
	that is a complete representation of the nominal shape of a product such that all points in the interior are connected. 
	Any point can be classified as being inside, outside or on the boundary of a solid.
</ext_description>


<!-- +++++++++++ unique_assembly_component_3d_shape_model +++++++++++ -->
<ext_description linkend="Physical_unit_3d_design_view_arm.unique_assembly_component_3d_shape_model">
The <b>unique_assembly_component_3d_shape_model</b> rule constrains the population of 
<express_ref linkend="Physical_unit_3d_design_view:arm:Physical_unit_3d_design_view_arm.Assembly_component_3d_shape_model">Assembly_component_3d_shape_models</express_ref>,
so that there shall be no more than one 	<express_ref linkend="Physical_unit_3d_design_view:arm:Physical_unit_3d_design_view_arm.Assembly_component_3d_shape_model"/> 
for any <express_ref linkend="Assembly_component:arm:Assembly_component_arm.Assembly_component"/>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Physical_unit_3d_design_view_arm.unique_assembly_component_3d_shape_model.wr:WR1">
	There shall be no more than one 
	<express_ref linkend="Physical_unit_3d_design_view:arm:Physical_unit_3d_design_view_arm.Assembly_component_3d_shape_model"/> 
	for each 
	<express_ref linkend="Assembly_component:arm:Assembly_component_arm.Assembly_component"/>.
	
</ext_description>

<!-- +++++++++++ unique_physical_unit_3d_shape_model_for_part_design_and_usage_view +++++++++++ -->
<ext_description linkend="Physical_unit_3d_design_view_arm.unique_physical_unit_3d_shape_model_for_part_design_and_usage_view">
<p>
	The <b>unique_physical_unit_3d_shape_model_for_part_design_and_usage_view</b> rule constrains the shape of
	<express_ref linkend="Physical_unit_design_view:arm:Physical_unit_design_view_arm.Part_design_view"/>
	and
	<express_ref linkend="Physical_unit_usage_view:arm:Physical_unit_usage_view_arm.Part_usage_view"/>,
	so that no more than one
	<express_ref linkend="Physical_unit_3d_shape:arm:Physical_unit_3d_shape_arm.Physical_unit_3d_shape_model"/>
	with a specific set of attribute values may reference a member of 
	<express_ref linkend="Physical_unit_design_view:arm:Physical_unit_design_view_arm.Part_design_view"/>
	and
	no more than one 
	<express_ref linkend="Physical_unit_3d_shape:arm:Physical_unit_3d_shape_arm.Physical_unit_3d_shape_model"/>
	with a specific set of attribute values may reference a member of 
	<express_ref linkend="Physical_unit_usage_view:arm:Physical_unit_usage_view_arm.Part_usage_view"/>.
</p>	
	<note>
		Because the set of attributes is being compared, 
		the value equality operator is used rather than the instance equality operator.
	</note>
</ext_description>

<!-- WR1 -->
<ext_description linkend="Physical_unit_3d_design_view_arm.unique_physical_unit_3d_shape_model_for_part_design_and_usage_view.wr:WR1">
	No more than one
	<express_ref linkend="Physical_unit_3d_shape:arm:Physical_unit_3d_shape_arm.Physical_unit_3d_shape_model"/>
	with a specific set of attribute values may reference a member of 
	<express_ref linkend="Physical_unit_design_view:arm:Physical_unit_design_view_arm.Part_design_view"/>
</ext_description>

<!-- WR2 -->
<ext_description linkend="Physical_unit_3d_design_view_arm.unique_physical_unit_3d_shape_model_for_part_design_and_usage_view.wr:WR2">
	No more than one 
	<express_ref linkend="Physical_unit_3d_shape:arm:Physical_unit_3d_shape_arm.Physical_unit_3d_shape_model"/>
	with a specific set of attribute values may reference a member of 
	<express_ref linkend="Physical_unit_usage_view:arm:Physical_unit_usage_view_arm.Part_usage_view"/>.
</ext_description>

</ext_descriptions>

