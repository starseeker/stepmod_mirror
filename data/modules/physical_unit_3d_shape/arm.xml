<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$">
   <application name="JSDAI" owner="LKSoft" source="physical_unit_3d_shape_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Physical_unit_3d_shape_arm">
      <interface kind="use" schema="Non_feature_shape_element_arm"/>
      <interface kind="reference" schema="Requirement_decomposition_arm">
         <interfaced.item name="get_rvd"/>
      </interface>
      <type name="predefined_3d_purpose">
         <enumeration items="analysis_input analysis_output shock_analysis_input shock_analysis_output design vibration_analysis_input vibration_analysis_output electromagnetic_compatibility_analysis_input electromagnetic_compatibility_analysis_output thermal_analysis_input thermal_analysis_output"/>
      </type>
      <type name="pu3ds_component_application">
         <enumeration items="compliant_components_permitted no_components_permitted"/>
      </type>
      <type name="pu3ds_external_identification_item">
         <select basedon="external_identification_item" extensible="YES" genericentity="YES" selectitems="Physical_unit_3d_shape_model"/>
      </type>
      <type name="pu3ds_physical_unit_shape_model_select">
         <select basedon="physical_unit_shape_model_select" extensible="YES" genericentity="YES" selectitems="Physical_unit_3d_shape_model"/>
      </type>
      <type name="pu3ds_shape_location">
         <enumeration items="above below"/>
      </type>
      <type name="pu3ds_shape_side">
         <enumeration items="same_side opposite_side both_sides"/>
      </type>
      <type name="pu3ds_view_shape_select">
         <select basedon="view_shape_select" extensible="YES" genericentity="YES" selectitems="Physical_unit_shape_model"/>
      </type>
      <type name="shape_class">
         <enumeration items="extrusion manhattan_block other is_unknown"/>
      </type>
      <entity name="Physical_unit_3d_keepout_shape_model" supertypes="Usage_view_level_non_feature_shape_model Physical_unit_keepout_shape_model">
         <explicit name="shape_classification">
            <typename name="shape_class"/>
         </explicit>
         <explicit name="model_shape">
            <typename name="Physical_unit_3d_shape_model"/>
            <redeclaration entity-ref="Usage_view_level_non_feature_shape_model"/>
         </explicit>
         <explicit name="component_application">
            <typename name="pu3ds_component_application"/>
         </explicit>
         <explicit name="shape_distance_from_seating_plane" optional="YES">
            <typename name="Length_tolerance_characteristic"/>
         </explicit>
         <explicit name="shape_location_with_respect_to_seating_plane" optional="YES">
            <typename name="pu3ds_shape_location"/>
         </explicit>
         <explicit name="side" optional="YES">
            <typename name="pu3ds_shape_side"/>
         </explicit>
         <derived expression="get_rvd(SELF, 'application technology constraint')" name="application_technology_constraint">
            <aggregate lower="0" type="SET" upper="?"/>
            <typename name="Requirement_view_definition"/>
         </derived>
         <where expression="(SIZEOF(application_technology_constraint) &lt;= 1)" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Representation.name)" label="WR2"/>
         <where expression="NOT EXISTS(SELF\Representation.description)" label="WR3"/>
         <where expression="NOT (EXISTS(shape_distance_from_seating_plane) XOR EXISTS(shape_location_with_respect_to_seating_plane))" label="WR4"/>
         <where expression="NOT ('PACKAGE_ARM.PACKAGE' IN TYPEOF(model_shape.shape_characterized_definition)) OR&#10;            EXISTS(shape_distance_from_seating_plane)" label="WR5"/>
         <where expression="NOT EXISTS(side) OR ('PACKAGE_ARM.PACKAGE' IN TYPEOF(model_shape.shape_characterized_definition))" label="WR6"/>
      </entity>
      <entity name="Physical_unit_3d_shape_model" supertypes="Physical_unit_shape_model">
         <explicit name="shape_purpose" optional="YES">
            <typename name="predefined_3d_purpose"/>
         </explicit>
         <explicit name="shape_classification">
            <typename name="shape_class"/>
         </explicit>
         <derived expression="get_rvd(SELF, 'application technology constraint')" name="application_technology_constraint">
            <aggregate lower="0" type="SET" upper="?"/>
            <typename name="Requirement_view_definition"/>
         </derived>
         <inverse attribute="item" entity="External_source_identification" name="external_shape_purpose">
            <inverse.aggregate lower="0" type="SET" upper="1"/>
         </inverse>
         <where expression="(EXISTS (shape_purpose)) XOR ((SIZEOF(external_shape_purpose)) = 1)" label="WR1"/>
         <where expression="(SIZEOF(application_technology_constraint) &lt;= 1)" label="WR2"/>
         <where expression="NOT EXISTS(SELF\Representation.name)" label="WR3"/>
         <where expression="NOT EXISTS(SELF\Representation.description)" label="WR4"/>
      </entity>
   </schema>
</express>
