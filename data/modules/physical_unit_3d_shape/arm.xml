<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-01-05T11:42:17" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="physical_unit_3d_shape_arm schema_instance"/>
   <schema name="Physical_unit_3d_shape_arm">
      <interface kind="use" schema="Physical_unit_shape_with_parameters_arm"/>
      <interface kind="use" schema="Layered_shape_arm"/>
      <interface kind="use" schema="Non_feature_shape_element_arm"/>
      <interface kind="use" schema="Part_view_definition_arm"/>
      <type name="predefined_3d_purpose">
         <enumeration items="analysis_input analysis_output shock_analysis_input shock_analysis_output design vibration_analysis_input vibration_analysis_output electromagnetic_compatibility_analysis_input electromagnetic_compatibility_analysis_output thermal_analysis_input thermal_analysis_output"/>
      </type>
      <type name="pu3ds_physical_unit_shape_select">
         <select extensible="YES" genericentity="YES" basedon="_SELECT_physical_unit_shape_select" selectitems="Physical_unit_3d_shape"/>
      </type>
      <type name="shape_class">
         <enumeration items="extrusion manhattan_block other is_unknown"/>
      </type>
      <entity name="Physical_unit_3d_keepout_shape" supertypes="View_level_non_feature_shape_definition Physical_unit_keepout_shape">
         <explicit name="shape_classification">
            <typename name="shape_class"/>
         </explicit>
      </entity>
      <entity name="Physical_unit_3d_shape" supertypes="Feature_shape_definition Physical_unit_shape">
         <explicit name="shape_purpose" optional="YES">
            <typename name="predefined_3d_purpose"/>
         </explicit>
         <explicit name="shape_classification">
            <typename name="shape_class"/>
         </explicit>
         <inverse name="external_shape_purpose" entity="External_source_identification" attribute="item">
            <inverse.aggregate type="SET" lower="0" upper="1"/>
         </inverse>
         <where label="WR1" expression=" ( EXISTS ( shape_purpose ) ) XOR ( ( SIZEOF ( external_shape_purpose ) ) = 1 ) "/>
      </entity>
   </schema>
</express>
