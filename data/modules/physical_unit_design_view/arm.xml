<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="physical_unit_design_view_arm schema_instance"/>
   <schema name="Physical_unit_design_view_arm">
      <interface kind="use" schema="Physical_layout_template_arm"/>
      <interface kind="use" schema="Physical_unit_usage_view_arm"/>
      <type name="physical_unit_or_part_template_or_fp_or_pd">
         <select extensible="YES" genericentity="YES" selectitems="Part_view_definition Template_definition"/>
      </type>
      <type name="pudv_extended_value_with_unit">
         <select extensible="YES" genericentity="YES" basedon="extended_value_with_unit" selectitems="textual_quantity_specification"/>
      </type>
      <type name="pudv_physical_unit">
         <select extensible="YES" genericentity="YES" basedon="physical_unit" selectitems="Part_design_view"/>
      </type>
      <type name="pudv_requirement_assignment_item">
         <select extensible="YES" genericentity="YES" basedon="requirement_assignment_item" selectitems="Assembly_item_number Assembly_component_relationship"/>
      </type>
      <type name="textual_quantity_specification">
         <builtintype type="STRING"/>
      </type>
      <entity name="Assembly_alternate_product" supertypes="Alternate_product_relationship">
         <explicit name="basis_context">
            <typename name="Part_design_view"/>
         </explicit>
         <where label="WR1" expression="NOT EXISTS (SELF\Alternate_product_relationship.criteria)"/>
      </entity>
      <entity name="Assembly_component" abstract.supertype="YES" supertypes="Definition_based_product_occurrence Item_shape">
         <explicit name="derived_from">
            <typename name="physical_unit_or_part_template_or_fp_or_pd"/>
            <redeclaration entity-ref="Definition_based_product_occurrence"/>
         </explicit>
         <derived name="described_element" expression="SELF">
            <typename name="shapeable_item"/>
            <redeclaration entity-ref="Item_shape"/>
         </derived>
         <inverse name="occurrence_contexts" entity="Assembly_component_relationship" attribute="related_view">
            <inverse.aggregate type="SET" lower="1" upper="?"/>
            <redeclaration entity-ref="Product_occurrence"/>
         </inverse>
         <where label="WR1" expression="NOT (EXISTS(SELF\Definition_based_product_occurrence.derived_from)) OR ((('PHYSICAL_UNIT_DESIGN_VIEW_ARM.FOOTPRINT_OCCURRENCE' IN TYPEOF(SELF)) AND ('PHYSICAL_UNIT_DESIGN_VIEW_ARM.FOOTPRINT_DEFINITION' IN TYPEOF(derived_from))) OR (NOT ('PHYSICAL_UNIT_DESIGN_VIEW_ARM.FOOTPRINT_OCCURRENCE' IN TYPEOF(SELF)) AND NOT ('PHYSICAL_UNIT_DESIGN_VIEW_ARM.FOOTPRINT_DEFINITION' IN TYPEOF(SELF\Definition_based_product_occurrence.derived_from))) OR (('PHYSICAL_UNIT_DESIGN_VIEW_ARM.PADSTACK_OCCURRENCE' IN TYPEOF(SELF)) AND ('PHYSICAL_UNIT_DESIGN_VIEW_ARM.PADSTACK_DEFINITION' IN TYPEOF(SELF\Definition_based_product_occurrence.derived_from))) OR (NOT ('PHYSICAL_UNIT_DESIGN_VIEW_ARM.PADSTACK_OCCURRENCE' IN TYPEOF(SELF)) AND NOT ('PHYSICAL_UNIT_DESIGN_VIEW_ARM.PADSTACK_DEFINITION' IN TYPEOF(SELF\Definition_based_product_occurrence.derived_from))))"/>
         <where label="WR2" expression="SELF\Product_view_definition.defined_version :=: SELF\Definition_based_product_occurrence.derived_from.defined_version"/>
      </entity>
      <entity name="Assembly_item_number">
         <explicit name="assembly_usage">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="Assembly_component_relationship"/>
         </explicit>
         <explicit name="item_number">
            <builtintype type="STRING"/>
         </explicit>
         <derived name="assembly" expression="assembly_usage[1].related_view">
            <typename name="Product_view_definition"/>
         </derived>
         <derived name="part" expression="assembly_usage[1].relating_view\Definition_based_product_occurrence.derived_from\Product_view_definition.defined_version.of_product">
            <typename name="Product"/>
         </derived>
         <unique label="UR1">
            <unique.attribute attribute="assembly"/>
            <unique.attribute attribute="item_number"/>
         </unique>
         <where label="WR1" expression="SIZEOF(QUERY(au &lt;* assembly_usage | NOT (au.relating_view\Definition_based_product_occurrence.derived_from\Product_view_definition.defined_version.of_product = Part))) = 0"/>
         <where label="WR2" expression="SIZEOF(QUERY(au &lt;* assembly_usage | NOT (au.related_view = assembly))) = 0"/>
      </entity>
      <entity name="Component_make_from_relationship" supertypes="View_definition_relationship">
         <explicit name="reusable_component">
            <typename name="Assembly_component"/>
            <redeclaration entity-ref="View_definition_relationship" old_name="relating_view"/>
         </explicit>
         <explicit name="resultant_component">
            <typename name="Assembly_component"/>
            <redeclaration entity-ref="View_definition_relationship" old_name="related_view"/>
         </explicit>
         <where label="WR1" expression="resultant_component &lt;&gt; reusable_component"/>
      </entity>
      <entity name="Component_to_physical_usage_view_assignment" supertypes="View_definition_relationship">
         <explicit name="design_view_component">
            <typename name="Assembly_component"/>
            <redeclaration entity-ref="View_definition_relationship" old_name="relating_view"/>
         </explicit>
         <explicit name="usage_view_component">
            <typename name="Assembly_component"/>
            <redeclaration entity-ref="View_definition_relationship" old_name="related_view"/>
         </explicit>
         <where label="WR1" expression="design_view_component &lt;&gt; usage_view_component"/>
         <where label="WR2" expression="NOT EXISTS(SELF\View_definition_relationship.relation_type)"/>
      </entity>
      <entity name="Connection_zone_in_design_view" supertypes="Connection_zone">
         <derived name="associated_definition" expression="SELF\Shape_element.containing_shape.described_element">
            <typename name="Part_design_view"/>
         </derived>
         <where label="WR1" expression="NOT EXISTS(SELF\Shape_element.element_name)"/>
      </entity>
      <entity name="Next_assembly_usage_occurrence_relationship" supertypes="Next_assembly_usage">
         <explicit name="related_view">
            <typename name="Product_occurrence"/>
            <redeclaration entity-ref="View_definition_relationship"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute entity-ref="View_definition_relationship" attribute="relating_view"/>
            <unique.attribute entity-ref="Assembly_component_relationship" attribute="location_indicator"/>
         </unique>
      </entity>
      <entity name="Part_design_view" supertypes="Item_shape Part_view_definition">
         <explicit name="usage_view" optional="YES">
            <typename name="Part_usage_view"/>
         </explicit>
         <derived name="described_element" expression="SELF">
            <typename name="shapeable_item"/>
            <redeclaration entity-ref="Item_shape"/>
         </derived>
         <where label="WR1" expression="('PHYSICAL_UNIT_DESIGN_VIEW_ARM.LAYOUT_MACRO_DEFINITION' IN TYPEOF(SELF)) OR EXISTS(usage_view)"/>
      </entity>
      <entity name="Physical_component" supertypes="Assembly_component Definition_based_part_occurrence"/>
      <entity name="Thermal_component" supertypes="Assembly_component"/>
      <subtype.constraint name="pudv_connection_zone_subtypes" entity="Connection_zone" super.expression="ONEOF (Connection_zone_in_usage_view, Connection_zone_in_design_view, Connection_zone_in_layout_template)"/>
      <subtype.constraint name="pudv_part_view_definition_subtypes" entity="Part_view_definition" super.expression="ONEOF (Part_usage_view, Part_design_view)"/>
      <rule name="unique_assembly_component_relationship" appliesto="Assembly_component_relationship">
         <algorithm>LOCAL
     gee : SET OF GENERIC_ENTITY := [];
     gei : SET OF GENERIC_ENTITY := [];
   END_LOCAL;
   REPEAT i := 1 to SIZEOF(Assembly_component_relationship) by 1;
     gee  := gee + Assembly_component_relationship[i]\View_definition_relationship.related_view;
     gei  := gei + Assembly_component_relationship[i]\View_definition_relationship.relating_view;
   END_REPEAT;</algorithm>
         <where label="WR1" expression="(SIZEOF(gee) = SIZEOF(gei)) AND (SIZEOF(gee) = SIZEOF(Assembly_component_relationship))"/>
      </rule>
      <rule name="unique_assembly_item_number" appliesto="Assembly_item_number">
         <algorithm>LOCAL
     acr : BAG OF Assembly_component_relationship := [];
   END_LOCAL;
   REPEAT i := 1 to SIZEOF(Assembly_item_number) by 1;
     acr  := acr + Assembly_item_number[i].assembly_usage;
   END_REPEAT;</algorithm>
         <where label="WR1" expression="SIZEOF(acr) = SIZEOF(bag_to_set(acr))"/>
      </rule>
      <function name="bag_to_set">
         <parameter name="the_bag">
            <aggregate type="BAG" lower="0" upper="?"/>
            <builtintype type="GENERIC"/>
         </parameter>
         <aggregate type="SET" lower="0" upper="?"/>
         <builtintype type="GENERIC"/>
         <algorithm>LOCAL
      the_set : SET OF GENERIC : intype := [];
      i       : INTEGER;
    END_LOCAL;

    IF SIZEOF(the_bag) &gt; 0 THEN
      REPEAT i := 1 TO HIINDEX(the_bag);
        the_set := the_set + the_bag[i];
      END_REPEAT;
    END_IF;
    RETURN(the_set);</algorithm>
      </function>
   </schema>
</express>
