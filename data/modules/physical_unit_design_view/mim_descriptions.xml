<!-- +++++++++++ component_material_relationship +++++++++++ -->
<ext_description linkend="Physical_unit_design_view_mim.component_material_relationship">
	A <b>component_material_relationship</b> is a type of
	<express_ref linkend="group_schema:ir_express:group_schema.group"/>
	and a type of
	<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.property_definition"/>
	that implements the ARM concept of
	<express_ref linkend="Physical_unit_design_view:arm:Physical_unit_design_view_arm.Component_material_relationship"/>.
</ext_description>

<!-- definition -->
<ext_description linkend="Physical_unit_design_view_mim.component_material_relationship.definition">
	an inherited attribute shall be of type
	<express_ref linkend="Physical_unit_usage_view:mim:Physical_unit_usage_view_mim.physical_unit"/>
	for the <b>component_material_relationship</b>.
</ext_description>

<!-- +++++++++++ component_material_relationship_assignment +++++++++++ -->
<ext_description linkend="Physical_unit_design_view_mim.component_material_relationship_assignment">
	A <b>component_material_relationship_assignment</b> is a type of
	<express_ref linkend="management_resources_schema:ir_express:management_resources_schema.group_assignment"/>
	and a type of
	<express_ref linkend="basic_attribute_schema:ir_express:basic_attribute_schema.role_association"/>.
</ext_description>

<!-- assigned_group -->
<ext_description linkend="Physical_unit_design_view_mim.component_material_relationship_assignment.assigned_group">
	an inherited attribute shall be of type
	<express_ref linkend="Physical_unit_design_view:mim:Physical_unit_design_view_mim.component_material_relationship"/>
	for the <b>component_material_relationship_assignment</b>.
</ext_description>

<!-- items -->
<ext_description linkend="Physical_unit_design_view_mim.component_material_relationship_assignment.items">
	specifies a role of the
	<express_ref linkend="Physical_unit_design_view:mim:Physical_unit_design_view_mim.component_material_relationship_assignment_item"/>
	for the  <b>component_material_relationship_assignment</b>.
	There shall exist exactly one
	<express_ref linkend="Physical_unit_design_view:mim:Physical_unit_design_view_mim.component_material_relationship_assignment_item"/>
	 for the <b>component_material_relationship_assignment</b>.
</ext_description>

<!-- item_with_role -->
<ext_description linkend="Physical_unit_design_view_mim.component_material_relationship_assignment.item_with_role">
	an inherited attribute shall be of type
	<express_ref linkend="Physical_unit_design_view:mim:Physical_unit_design_view_mim.component_material_relationship_assignment"/>
	for the <b>component_material_relationship_assignment</b>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: next_assembly_usage_occurrence_relationship
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Physical_unit_design_view_mim.next_assembly_usage_occurrence_relationship">
	A <b>next_assembly_usage_occurrence_relationship</b> is a type of 
	<express_ref linkend="product_structure_schema:ir_express:product_structure_schema.next_assembly_usage_occurrence"/> 
	and a type of 
	<express_ref linkend="product_structure_schema:ir_express:product_structure_schema.product_definition_occurrence_relationship"/>
	that implements the ARM concept of
	<express_ref linkend="Physical_unit_design_view:arm:Physical_unit_design_view_arm.Next_assembly_usage_occurrence_relationship"/>.
</ext_description>
<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Attribute: occurrence_usage
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Physical_unit_design_view_mim.next_assembly_usage_occurrence_relationship.occurrence_usage">
specifies a role of the <express_ref linkend="Physical_unit_design_view:mim:Physical_unit_design_view_mim.next_assembly_usage_occurrence_relationship"/> for the <b>next_assembly_usage_occurrence_relationship</b>.
</ext_description>

<!-- +++++++++++ assembly_item_number +++++++++++ -->
<ext_description linkend="Physical_unit_design_view_mim.assembly_item_number">
	An <b>assembly_item_number</b> is a type of
	<express_ref linkend="Group:mim:Group_mim.applied_group_assignment"/>
	and a type of
	<express_ref linkend="group_schema:ir_express:group_schema.group"/>
	that implements the ARM concept of
	<express_ref linkend="Physical_unit_design_view:arm:Physical_unit_design_view_arm.Assembly_item_number"/>.
</ext_description>

<!-- items -->
<ext_description linkend="Physical_unit_design_view_mim.assembly_item_number.items">
	an inherited attribute shall be of type
	<express_ref linkend="product_structure_schema:ir_express:product_structure_schema.assembly_component_usage"/>
	for the <b>assembly_item_number</b>.
</ext_description>

<!-- assigned_group -->
<ext_description linkend="Physical_unit_design_view_mim.assembly_item_number.assigned_group">
	an inherited attribute shall be of type
	<express_ref linkend="group_schema:ir_express:group_schema.group"/>
	for the <b>assembly_item_number</b>.
</ext_description>

