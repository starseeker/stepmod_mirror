   <schema name="Physical_unit_interconnect_definition_arm">
      <interface kind="use" schema="Part_view_definition_arm"/>
      <interface kind="use" schema="Assembly_structure_arm"/>
      <entity name="Connector_based_interconnect_definition">
         <explicit name="name">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="component_2">
            <typename name="Next_assembly_usage"/>
         </explicit>
         <explicit name="component_1">
            <typename name="Next_assembly_usage"/>
         </explicit>
         <explicit name="connector_in_uv_2">
            <typename name="Next_assembly_usage"/>
         </explicit>
         <explicit name="connector_in_uv_1">
            <typename name="Next_assembly_usage"/>
         </explicit>
         <explicit name="associated_definition">
            <typename name="Physical_unit_interconnect_definition"/>
         </explicit>
         <unique label="Ur1">
            <unique.attribute entity-ref="Connector_based_interconnect_definition" attribute="name"/>
            <unique.attribute entity-ref="Connector_based_interconnect_definition" attribute="associated_definition"/>
         </unique>
         <where label="Wr19" expression=" connector_in_uv_2 &lt;&gt; component_2 "/>
         <where label="Wr18" expression=" component_1 &lt;&gt; connector_in_uv_2 "/>
         <where label="Wr17" expression=" connector_in_uv_1 &lt;&gt; component_2 "/>
         <where label="Wr16" expression=" component_1 &lt;&gt; connector_in_uv_1 "/>
         <where label="Wr15" expression=" component_1 &lt;&gt; component_2 "/>
         <where label="Wr12" expression=" NOT EXISTS ( component_2 . quantity ) "/>
         <where label="Wr11" expression=" NOT EXISTS ( component_1 . quantity ) "/>
         <where label="Wr10" expression=" NOT EXISTS ( connector_in_uv_2 . quantity ) "/>
         <where label="Wr9" expression=" NOT EXISTS ( connector_in_uv_1 . quantity ) "/>
         <where label="Wr8" expression=" EXISTS ( component_2 . location_indicator ) "/>
         <where label="Wr7" expression=" EXISTS ( component_1 . location_indicator ) "/>
         <where label="Wr6" expression=" EXISTS ( connector_in_uv_2 . location_indicator ) "/>
         <where label="Wr5" expression=" EXISTS ( connector_in_uv_1 . location_indicator ) "/>
         <where label="Wr4" expression=" 'AP210_ARM.part_usage_view' IN TYPEOF ( connector_in_uv_2 \ assembly_component_relationship . related_view ) "/>
         <where label="Wr3" expression=" 'AP210_ARM.part_usage_view' IN TYPEOF ( connector_in_uv_1 \ assembly_component_relationship . related_view ) "/>
         <where label="Wr2" expression=" 'AP210_ARM.PACKAGED_CONNECTOR_COMPONENT' IN TYPEOF ( connector_in_uv_2 \ assembly_component_relationship . relating_view ) "/>
         <where label="Wr1" expression=" 'AP210_ARM.PACKAGED_CONNECTOR_COMPONENT' IN TYPEOF ( connector_in_uv_1 \ assembly_component_relationship . relating_view ) "/>
