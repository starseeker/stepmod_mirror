<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-01-25T15:49:58" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="physical_unit_interconnect_definition_arm schema_instance"/>
   <schema name="Physical_unit_interconnect_definition_arm">
      <interface kind="use" schema="Part_view_definition_arm"/>
      <interface kind="use" schema="Assembly_structure_arm"/>
      <entity name="Connector_based_interconnect_definition">
         <explicit name="name">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="associated_definition">
            <typename name="Physical_unit_interconnect_definition"/>
         </explicit>
         <explicit name="connector_in_uv_1">
            <typename name="Next_assembly_usage"/>
         </explicit>
         <explicit name="connector_in_uv_2">
            <typename name="Next_assembly_usage"/>
         </explicit>
         <explicit name="component_1">
            <typename name="Next_assembly_usage"/>
         </explicit>
         <explicit name="component_2">
            <typename name="Next_assembly_usage"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute entity-ref="Connector_based_interconnect_definition" attribute="name"/>
            <unique.attribute entity-ref="Connector_based_interconnect_definition" attribute="associated_definition"/>
         </unique>
         <where label="WR1" expression=" 'PHYSICAL_UNIT_INTERCONNECT_DEFINITION_ARM.PACKAGED_CONNECTOR_COMPONENT' IN TYPEOF ( connector_in_uv_1 \ Assembly_component_relationship . relating_view ) "/>
         <where label="WR2" expression=" 'PHYSICAL_UNIT_INTERCONNECT_DEFINITION_ARM.PACKAGED_CONNECTOR_COMPONENT' IN TYPEOF ( connector_in_uv_2 \ Assembly_component_relationship . relating_view ) "/>
         <where label="WR3" expression=" 'PHYSICAL_UNIT_INTERCONNECT_DEFINITION_ARM.part_usage_view' IN TYPEOF ( connector_in_uv_1 \ Assembly_component_relationship . related_view ) "/>
         <where label="WR4" expression=" 'PHYSICAL_UNIT_INTERCONNECT_DEFINITION_ARM.part_usage_view' IN TYPEOF ( connector_in_uv_2 \ Assembly_component_relationship . related_view ) "/>
         <where label="WR5" expression=" EXISTS ( connector_in_uv_1 . location_indicator ) "/>
         <where label="WR6" expression=" EXISTS ( connector_in_uv_2 . location_indicator ) "/>
         <where label="WR7" expression=" EXISTS ( component_1 . location_indicator ) "/>
         <where label="WR8" expression=" EXISTS ( component_2 . location_indicator ) "/>
         <where label="WR9" expression=" NOT EXISTS ( connector_in_uv_1 . quantity ) "/>
         <where label="WR10" expression=" NOT EXISTS ( connector_in_uv_2 . quantity ) "/>
         <where label="WR11" expression=" NOT EXISTS ( component_1 . quantity ) "/>
         <where label="WR12" expression=" NOT EXISTS ( component_2 . quantity ) "/>
         <where label="WR13" expression=" component_1 \ Assembly_component_relationship . relating_view . usage_definition = connector_in_uv_1 \ Assembly_component_relationship . related_view "/>
         <where label="WR14" expression=" component_2 \ Assembly_component_relationship . relating_view . usage_definition = connector_in_uv_2 \ Assembly_component_relationship . related_view "/>
         <where label="WR15" expression=" component_1 &lt;&gt; component_2 "/>
         <where label="WR16" expression=" component_1 &lt;&gt; connector_in_uv_1 "/>
         <where label="WR17" expression=" connector_in_uv_1 &lt;&gt; component_2 "/>
         <where label="WR18" expression=" component_1 &lt;&gt; connector_in_uv_2 "/>
         <where label="WR19" expression=" connector_in_uv_2 &lt;&gt; component_2 "/>
      </entity>
      <entity name="Physical_unit_interconnect_definition" supertypes="Part_view_definition"/>
   </schema>
</express>
