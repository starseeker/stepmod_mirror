<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-09-01T14:59:02" rcs.revision="1.0" description.file="mim_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="physical_unit_interconnect_definition_arm schema_instance"/>
   <schema name="Physical_unit_interconnect_definition_arm">
      <interface kind="use" schema="Part_occurrence_arm"/>
      <entity name="Connector_based_interconnect_definition">
         <explicit name="name">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="associated_definition">
            <typename name="Physical_unit_interconnect_definition"/>
         </explicit>
         <explicit name="connector_in_uv_1">
            <typename name="Next_assembly_usage"/>
         </explicit>
         <explicit name="connector_in_uv_2">
            <typename name="Next_assembly_usage"/>
         </explicit>
         <explicit name="component_1">
            <typename name="Next_assembly_usage"/>
         </explicit>
         <explicit name="component_2">
            <typename name="Next_assembly_usage"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute entity-ref="Connector_based_interconnect_definition" attribute="name"/>
            <unique.attribute entity-ref="Connector_based_interconnect_definition" attribute="associated_definition"/>
         </unique>
         <where label="WR1" expression="'PHYSICAL_UNIT_INTERCONNECT_DEFINITION_ARM.PACKAGED_CONNECTOR_COMPONENT' IN &#10;        TYPEOF(connector_in_uv_1\View_definition_relationship.related_view)"/>
         <where label="WR2" expression="'PHYSICAL_UNIT_INTERCONNECT_DEFINITION_ARM.PACKAGED_CONNECTOR_COMPONENT' IN &#10;        TYPEOF(connector_in_uv_2\View_definition_relationship.related_view)"/>
         <where label="WR3" expression="'PHYSICAL_UNIT_INTERCONNECT_DEFINITION_ARM.PART_USAGE_VIEW' IN &#10;        TYPEOF(connector_in_uv_1\View_definition_relationship.relating_view)"/>
         <where label="WR4" expression="'PHYSICAL_UNIT_INTERCONNECT_DEFINITION_ARM.PART_USAGE_VIEW' IN &#10;        TYPEOF(connector_in_uv_2\View_definition_relationship.relating_view)"/>
         <where label="WR5" expression="EXISTS(connector_in_uv_1\Assembly_component_relationship.location_indicator)"/>
         <where label="WR6" expression="EXISTS(connector_in_uv_2\Assembly_component_relationship.location_indicator)"/>
         <where label="WR7" expression="EXISTS(component_1\Assembly_component_relationship.location_indicator)"/>
         <where label="WR8" expression="EXISTS(component_2\Assembly_component_relationship.location_indicator)"/>
         <where label="WR9" expression="NOT EXISTS(connector_in_uv_1.quantity)"/>
         <where label="WR10" expression="NOT EXISTS(connector_in_uv_2.quantity)"/>
         <where label="WR11" expression="NOT EXISTS(component_1.quantity)"/>
         <where label="WR12" expression="NOT EXISTS(component_2.quantity)"/>
         <where label="WR13" expression="component_1\View_definition_usage.related_view\Definition_based_product_occurrence.derived_from = &#10;       connector_in_uv_1\View_definition_relationship.relating_view"/>
         <where label="WR14" expression="component_2\View_definition_usage.related_view\Definition_based_product_occurrence.derived_from = &#10;       connector_in_uv_2\View_definition_relationship.relating_view"/>
         <where label="WR15" expression="component_1 &lt;&gt; component_2"/>
         <where label="WR16" expression="component_1 &lt;&gt; connector_in_uv_1"/>
         <where label="WR17" expression="connector_in_uv_1 &lt;&gt; component_2"/>
         <where label="WR18" expression="component_1 &lt;&gt; connector_in_uv_2"/>
         <where label="WR19" expression="connector_in_uv_2 &lt;&gt; component_2"/>
         <where label="WR20" expression="'PHYSICAL_UNIT_INTERCONNECT_DEFINITION_ARM.DEFINITION_BASED_PRODUCT_OCCURRENCE' IN &#10;        TYPEOF(component_1\View_definition_relationship.related_view)"/>
         <where label="WR21" expression="'PHYSICAL_UNIT_INTERCONNECT_DEFINITION_ARM.DEFINITION_BASED_PRODUCT_OCCURRENCE' IN &#10;        TYPEOF(component_2\View_definition_relationship.related_view)"/>
         <where label="WR22" expression="associated_definition = component_1\View_definition_relationship.relating_view"/>
         <where label="WR23" expression="associated_definition = component_2\View_definition_relationship.relating_view"/>
      </entity>
      <entity name="Physical_unit_interconnect_definition" supertypes="Part_view_definition"/>
   </schema>
</express>
