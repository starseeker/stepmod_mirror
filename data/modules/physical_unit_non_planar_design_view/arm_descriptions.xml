<!-- +++++++++++ Assembly_component_non_planar_shape_model +++++++++++ -->
<ext_description linkend="Physical_unit_non_planar_design_view_arm.Assembly_component_non_planar_shape_model">
	An <b>Assembly_component_non_planar_shape_model</b> is a type of 
	<express_ref linkend="Manifold_subsurface:arm:Manifold_subsurface_arm.Manifold_subsurface_model"/>.
	An <b>Assembly_component_non_planar_shape_model</b> is the shape of a 
	<express_ref linkend="Physical_unit_design_view:arm:Physical_unit_design_view_arm.Assembly_component"/> 
	that is represented in the form of a surface.
</ext_description>

<!-- shape_characterized_component -->
<ext_description linkend="Physical_unit_non_planar_design_view_arm.Assembly_component_non_planar_shape_model.shape_characterized_component">
	a set of one or more 
	<express_ref linkend="Physical_unit_design_view:arm:Physical_unit_design_view_arm.Assembly_component">Assembly_components</express_ref> 
	for the <b>Assembly_component_non_planar_shape_model</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Physical_unit_non_planar_design_view_arm.Assembly_component_non_planar_shape_model.wr:WR1">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.description"/>
	shall not be populated.
</ext_description>


<!-- +++++++++++ unique_assembly_component_non_planar_shape_model +++++++++++ -->
<ext_description linkend="Physical_unit_non_planar_design_view_arm.unique_assembly_component_non_planar_shape_model">
The <b>unique_assembly_component_non_planar_shape_model</b> rule constrains the population of
<express_ref linkend="Physical_unit_non_planar_design_view:arm:Physical_unit_non_planar_design_view_arm.Assembly_component_non_planar_shape_model">Assembly_component_non_planar_shapes</express_ref>,
so that there shall be no more than one <express_ref linkend="Physical_unit_non_planar_design_view:arm:Physical_unit_non_planar_design_view_arm.Assembly_component_non_planar_shape_model"/> 
for each <express_ref linkend="Physical_unit_design_view:arm:Physical_unit_design_view_arm.Assembly_component"/>.

</ext_description>

<!-- WR1 -->
<ext_description linkend="Physical_unit_non_planar_design_view_arm.unique_assembly_component_non_planar_shape_model.wr:WR1">
	There shall be no more than one 
	<express_ref linkend="Physical_unit_non_planar_design_view:arm:Physical_unit_non_planar_design_view_arm.Assembly_component_non_planar_shape_model"/> 
	for each 
	<express_ref linkend="Physical_unit_design_view:arm:Physical_unit_design_view_arm.Assembly_component"/>.
</ext_description>




