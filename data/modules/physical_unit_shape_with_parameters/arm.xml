<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$">
   <application name="JSDAI" owner="LKSoft" source="physical_unit_shape_with_parameters_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Physical_unit_shape_with_parameters_arm">
      <interface kind="use" schema="Physical_unit_usage_view_arm"/>
      <interface kind="use" schema="Shape_parameters_arm"/>
      <interface kind="reference" schema="Characterizable_object_arm">
         <interfaced.item name="bag_to_set"/>
      </interface>
      <type name="mass_property_quality">
         <enumeration items="high medium low is_unknown"/>
      </type>
      <type name="puswp_external_identification_item">
         <select basedon="external_identification_item" extensible="YES" genericentity="YES" selectitems="Physical_unit_keepout_shape_model"/>
      </type>
      <type name="puswp_requirement_assignment_item">
         <select basedon="requirement_assignment_item" extensible="YES" genericentity="YES" selectitems="Physical_unit_keepout_shape_model Physical_unit_shape_model"/>
      </type>
      <type name="shape_approximation_level">
         <enumeration items="coarse detailed is_unknown"/>
      </type>
      <type name="shape_extent">
         <enumeration items="envelope over_body over_lands over_breakout"/>
      </type>
      <entity abstract.supertype="YES" name="Physical_unit_keepout_shape_model" supertypes="Geometric_model">
         <explicit name="constrained_design_object_category">
            <typename name="keepout_product_design_object_category"/>
         </explicit>
         <explicit name="shape_characterized_definition">
            <typename name="Part_usage_view"/>
         </explicit>
         <explicit name="shape_material_condition">
            <typename name="material_condition"/>
         </explicit>
         <explicit name="shape_purpose" optional="YES">
            <typename name="predefined_keepout_purpose"/>
         </explicit>
         <explicit name="shape_environment">
            <typename name="application_environment"/>
         </explicit>
         <explicit name="shape_extent">
            <typename name="shape_extent"/>
         </explicit>
         <explicit name="shape_approximation_level">
            <typename name="shape_approximation_level"/>
         </explicit>
         <explicit name="centroid_location" optional="YES">
            <typename name="Cartesian_point"/>
         </explicit>
         <derived expression="bag_to_set(QUERY( gmr &lt;* USEDIN(SELF,&#10;        'EXTENDED_BASIC_GEOMETRY_ARM.GEOMETRIC_MODEL_RELATIONSHIP.REP_2') |&#10;        (gmr\Representation_relationship.relation_type = 'principal part shape')))" name="principal_part_shape">
            <aggregate lower="0" type="SET" upper="1"/>
            <typename name="Geometric_model_relationship"/>
         </derived>
         <inverse attribute="item" entity="External_source_identification" name="external_shape_purpose">
            <inverse.aggregate lower="0" type="SET" upper="1"/>
         </inverse>
         <where expression="(EXISTS (shape_purpose)) XOR ((SIZEOF(external_shape_purpose)) = 1)" label="WR1"/>
         <where expression="SIZEOF(principal_part_shape) &lt; 2" label="WR2"/>
      </entity>
      <entity abstract.supertype="YES" name="Physical_unit_shape_model" supertypes="Geometric_model">
         <explicit name="shape_characterized_definition">
            <typename name="physical_unit"/>
         </explicit>
         <explicit name="shape_material_condition">
            <typename name="material_condition"/>
         </explicit>
         <explicit name="centroid_location" optional="YES">
            <typename name="Cartesian_point"/>
         </explicit>
         <explicit name="shape_environment">
            <typename name="application_environment"/>
         </explicit>
         <explicit name="shape_extent">
            <typename name="shape_extent"/>
         </explicit>
         <explicit name="shape_approximation_level">
            <typename name="shape_approximation_level"/>
         </explicit>
         <explicit name="mass_property_quality">
            <typename name="mass_property_quality"/>
         </explicit>
         <derived expression="bag_to_set(QUERY( gmr &lt;* USEDIN(SELF,&#10;        'EXTENDED_BASIC_GEOMETRY_ARM.GEOMETRIC_MODEL_RELATIONSHIP.REP_2') |&#10;        (gmr\Representation_relationship.relation_type = 'principal part shape')))" name="principal_part_shape">
            <aggregate lower="0" type="SET" upper="1"/>
            <typename name="Geometric_model_relationship"/>
         </derived>
         <where expression="NOT ('PACKAGED_PART_BLACK_BOX_MODEL_ARM.PACKAGED_PART' IN TYPEOF(shape_characterized_definition))" label="WR1"/>
         <where expression="SIZEOF(USEDIN(SELF, 'SHAPE_PROPERTY_ASSIGNMENT_ARM.SHAPE_DESCRIPTION_ASSOCIATION.REPRESENTATION'))=0" label="WR2"/>
         <where expression="SIZEOF(principal_part_shape) &lt; 2" label="WR3"/>
      </entity>
      <subtype.constraint entity="Geometric_model" name="puswp_geometric_model_subtypes" super.expression="ONEOF (Physical_unit_keepout_shape_model, Physical_unit_shape_model)"/>
   </schema>
</express>
