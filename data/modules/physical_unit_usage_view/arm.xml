<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:10" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="physical_unit_usage_view_arm schema_instance"/>
   <schema name="PHYSICAL_UNIT_USAGE_VIEW_ARM">
      <interface kind="use" schema="ANALYTICAL_MODEL_ARM"/>
      <interface kind="use" schema="FEATURE_AND_CONNECTION_ZONE_ARM"/>
      <interface kind="use" schema="ITEM_DEFINITION_STRUCTURE_ARM"/>
      <interface kind="use" schema="PART_FEATURE_LOCATION_ARM"/>
      <interface kind="use" schema="PART_VIEW_DEFINITION_ARM"/>
      <interface kind="use" schema="REQUIREMENT_ASSIGNMENT_ARM"/>
      <interface kind="use" schema="SHAPE_COMPOSITION_ARM"/>
      <interface kind="use" schema="SHAPE_PROPERTY_ASSIGNMENT_ARM"/>
      <type name="ap210_usage_concept">
         <select extensible="YES" genericentity="YES" basedon="usage_concept" selectitems="part_feature"/>
      </type>
      <type name="material_state_change_enumeration">
         <enumeration items="material_addition material_removal"/>
      </type>
      <type name="puuv_requirement_assignment_item">
         <select extensible="YES" genericentity="YES" basedon="requirement_assignment_item" selectitems="part_feature"/>
      </type>
      <type name="puuv_specification_product_object_select">
         <select extensible="YES" genericentity="YES" basedon="specification_product_object_select" selectitems="part_feature"/>
      </type>
      <entity name="connection_zone_in_usage_view" supertypes="connection_zone">
         <derived name="associated_definition" expression=" containing_shape . described_element ">
            <typename name="part_usage_view"/>
         </derived>
      </entity>
      <entity name="connection_zone_map_identification">
         <explicit name="reference_connection_zone_shape">
            <typename name="usage_view_connection_zone_terminal_shape_relationship"/>
         </explicit>
         <explicit name="mapped_connection_zone_shape">
            <typename name="usage_view_connection_zone_terminal_shape_relationship"/>
         </explicit>
         <unique label="ur1">
            <unique.attribute entity-ref="connection_zone_map_identification" attribute="reference_connection_zone_shape"/>
            <unique.attribute entity-ref="connection_zone_map_identification" attribute="mapped_connection_zone_shape"/>
         </unique>
         <where label="wr1" expression=" reference_connection_zone_shape &lt;&gt; mapped_connection_zone_shape "/>
         <where label="wr2" expression=" reference_connection_zone_shape . associated_connection_zone_shape_definition &lt;&gt; mapped_connection_zone_shape . associated_connection_zone_shape_definition "/>
      </entity>
      <entity name="part_feature" supertypes="shape_element">
         <explicit name="associated_definition">
            <typename name="part_usage_view"/>
            <redeclaration entity-ref="shape_element" old_name="containing_shape"/>
         </explicit>
         <explicit name="material_state_change" optional="YES">
            <typename name="material_state_change_enumeration"/>
         </explicit>
         <explicit name="precedent_feature" optional="YES">
            <typename name="part_feature"/>
         </explicit>
         <inverse name="feature_shape" entity="usage_concept_usage_relationship" attribute="associated_usage">
            <inverse.aggregate type="SET" lower="0" upper="?"/>
         </inverse>
         <unique label="ur1">
            <unique.attribute entity-ref="part_feature" attribute="element_name"/>
            <unique.attribute entity-ref="part_feature" attribute="associated_definition"/>
         </unique>
      </entity>
      <entity name="part_feature_make_from_relationship" supertypes="shape_element_relationship">
         <explicit name="associated_make_from">
            <typename name="view_definition_relationship"/>
         </explicit>
         <explicit name="reusable_feature">
            <typename name="part_feature"/>
            <redeclaration entity-ref="shape_element_relationship" old_name="relating"/>
         </explicit>
         <explicit name="resultant_feature">
            <typename name="part_feature"/>
            <redeclaration entity-ref="shape_element_relationship" old_name="related"/>
         </explicit>
         <unique label="ur1">
            <unique.attribute entity-ref="part_feature_make_from_relationship" attribute="reusable_feature"/>
            <unique.attribute entity-ref="part_feature_make_from_relationship" attribute="resultant_feature"/>
         </unique>
         <where label="wr1" expression=" reusable_feature :&lt;&gt;: resultant_feature "/>
         <where label="wr2" expression=" reusable_feature . associated_definition :&lt;&gt;: resultant_feature . associated_definition "/>
      </entity>
      <entity name="part_feature_template_definition" supertypes="product_view_definition">
         <inverse name="shape" entity="part_feature_template_shape_definition" attribute="shape_characterized_template"/>
         <where label="wr1" expression=" sizeof ( query ( prpc &lt;* usedin ( self \ product_view_definition . defined_version . of_product , 'physical_unit_usage_view_arm.product_category_assignment.products' ) | ( prpc . name = 'template model' ) ) ) &gt; 0 "/>
      </entity>
      <entity name="part_feature_template_shape_definition" supertypes="feature_shape_definition">
         <explicit name="shape_characterized_template">
            <typename name="part_feature_template_definition"/>
         </explicit>
         <where label="wr1" expression=" not ( sizeof ( usedin ( self , '' ) ) = 0 ) or ( sizeof ( query ( foo &lt;* usedin ( self , '' ) | not ( 'physical_unit_usage_view_arm.usage_concept_usage_relationship' in typeof ( foo ) ) ) ) = 0 ) "/>
      </entity>
      <entity name="part_usage_view" supertypes="part_view_definition item_shape">
         <derived name="described_element" expression=" self ">
            <typename name="shapeable_item"/>
            <redeclaration entity-ref="item_shape"/>
         </derived>
         <where label="wr1" expression=" sizeof ( query ( acr &lt;* usedin ( self , 'physical_unit_usage_view_arm.' + 'part_occurrence_definition_relationship.related_view' ) | not ( 'physical_unit_usage_view_arm.packaged_connector_component' in typeof ( acr . related_view ) ) ) ) = 0 "/>
      </entity>
      <entity name="usage_view_connection_zone_terminal_shape_relationship">
         <explicit name="associating_terminal_shape">
            <typename name="part_feature_template_shape_definition"/>
         </explicit>
         <explicit name="associated_usage">
            <typename name="connection_zone_in_usage_view"/>
         </explicit>
         <explicit name="associated_connection_zone_shape_definition">
            <typename name="feature_shape_definition"/>
         </explicit>
         <explicit name="associated_usage_placement">
            <typename name="axis_placement"/>
         </explicit>
         <where label="wr1" expression=" associating_terminal_shape :&lt;&gt;: associated_connection_zone_shape_definition "/>
      </entity>
   </schema>
</express>
