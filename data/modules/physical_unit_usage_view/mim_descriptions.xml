<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: connection_zone_map_identification
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Physical_unit_usage_view_mim.connection_zone_map_identification">
<p>
	A <b>connection_zone_map_identification</b> is a type of 
	<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.shape_aspect"/> 
	and of  <express_ref linkend="representation_schema:ir_express:representation_schema.representation_relationship"/> 
	that implements the ARM concept of 
	<express_ref linkend="Physical_unit_usage_view:arm:Physical_unit_usage_view_arm.Connection_zone_map_identification"/>.
</p>
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Attribute: connection_zone_map_identification.rep_1
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Physical_unit_usage_view_mim.connection_zone_map_identification.rep_1">
	specifies a role of the
	<express_ref linkend="Physical_unit_usage_view:mim:Physical_unit_usage_view_mim.usage_view_connection_zone_terminal_shape_relationship"/>
	for the <b>connection_zone_map_identification</b>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Attribute: connection_zone_map_identification.rep_2
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Physical_unit_usage_view_mim.connection_zone_map_identification.rep_2">
	specifies a role of the
	<express_ref linkend="Physical_unit_usage_view:mim:Physical_unit_usage_view_mim.usage_view_connection_zone_terminal_shape_relationship"/>
	for the <b>connection_zone_map_identification</b>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
WR: connection_zone_map_identification.WR1
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Physical_unit_usage_view_mim.connection_zone_map_identification.wr:WR1">
