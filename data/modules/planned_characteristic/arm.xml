<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:10" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="planned_characteristic_arm schema_instance"/>
   <schema name="PLANNED_CHARACTERISTIC_ARM">
      <interface kind="use" schema="CHARACTERISTIC_ARM"/>
      <interface kind="use" schema="PRODUCT_RULE_ARM"/>
      <interface kind="use" schema="PRODUCT_VIEW_DEFINITION_ARM"/>
      <interface kind="use" schema="REQUIREMENT_ASSIGNMENT_ARM"/>
      <type name="evaluation_result_value">
         <enumeration items="pass fail undefined"/>
      </type>
      <type name="evaluation_type">
         <enumeration items="not_evaluatable invalid valid to_be_evaluated"/>
      </type>
      <entity name="evaluated_characteristic" super.expression="rule_based_evaluated_characteristic">
         <explicit name="planned_product_data">
            <typename name="planned_characteristic"/>
         </explicit>
         <explicit name="product_data_status">
            <typename name="evaluation_type"/>
         </explicit>
         <explicit name="product_characterization_data" optional="YES">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="coordinated_characteristic"/>
         </explicit>
         <explicit name="evaluation_result">
            <typename name="evaluation_result_value"/>
         </explicit>
      </entity>
      <entity name="evaluation_view_definition" supertypes="product_view_definition">
         <explicit name="evaluated_product_definition">
            <typename name="product_view_definition"/>
         </explicit>
         <explicit name="evaluation_data">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="evaluated_characteristic"/>
         </explicit>
         <where label="wr1" expression=" typeof ( self ) &lt;&gt; typeof ( evaluated_product_definition ) "/>
      </entity>
      <entity name="planned_characteristic">
         <explicit name="reference_requirement">
            <typename name="requirement_assignment"/>
         </explicit>
         <explicit name="planned_coordinated_characteristic">
            <typename name="coordinated_characteristic"/>
         </explicit>
      </entity>
      <entity name="rule_based_evaluated_characteristic" supertypes="evaluated_characteristic">
         <explicit name="evaluating_rule">
            <typename name="rule_definition"/>
         </explicit>
      </entity>
   </schema>
</express>
