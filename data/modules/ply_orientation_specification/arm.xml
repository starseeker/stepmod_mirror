<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1772">
   <application name="JSDAI" owner="LKSoft" source="ply_orientation_specification_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Ply_orientation_specification_arm">
      <interface kind="use" schema="Part_and_zone_laminate_tables_arm"/>
      <interface kind="use" schema="Value_with_unit_arm"/>
      <type name="direction_select">
         <select selectitems="Cartesian_11 Curve_11 Cylindrical_11 Direction Point_array Polar_11 User_defined_11"/>
      </type>
      <entity name="Cartesian_11" supertypes="Detailed_geometric_model_element">
         <explicit name="placement">
            <typename name="Axis_placement_3d"/>
         </explicit>
      </entity>
      <entity name="Curve_11" supertypes="Detailed_geometric_model_element">
         <explicit name="guide_curves">
            <aggregate lower="1" type="LIST" upper="?"/>
            <typename name="Curve"/>
         </explicit>
         <explicit name="fiber_11_offset">
            <typename name="Value_with_unit"/>
         </explicit>
      </entity>
      <entity name="Cylindrical_11" supertypes="Detailed_geometric_model_element">
         <explicit name="guide_curves">
            <aggregate lower="1" type="LIST" upper="?"/>
            <typename name="Curve"/>
         </explicit>
      </entity>
      <entity name="Draped_orientation_angle" supertypes="Ply_orientation_angle"/>
      <entity name="Laid_orientation_angle" supertypes="Ply_orientation_angle"/>
      <entity name="Ply_angle_representation" supertypes="Representation">
         <explicit name="items">
            <aggregate lower="1" type="SET" upper="1"/>
            <typename name="Measure_item"/>
            <redeclaration entity-ref="Representation"/>
         </explicit>
      </entity>
      <entity name="Ply_orientation_angle" super.expression="ONEOF (Draped_orientation_angle, Laid_orientation_angle)" supertypes="Representation_relationship">
         <explicit name="rep_1">
            <typename name="Ply_angle_representation"/>
            <redeclaration entity-ref="Representation_relationship"/>
         </explicit>
         <explicit name="rep_2">
            <typename name="Reinforcement_orientation_basis"/>
            <redeclaration entity-ref="Representation_relationship"/>
         </explicit>
      </entity>
      <entity name="Point_and_vector" supertypes="Detailed_geometric_model_element">
         <explicit name="location">
            <typename name="Point"/>
         </explicit>
         <explicit name="vector">
            <aggregate lower="1" type="LIST" upper="2"/>
            <typename name="Direction"/>
         </explicit>
      </entity>
      <entity name="Point_array" supertypes="Detailed_geometric_model_element">
         <explicit name="directions">
            <aggregate lower="1" type="LIST" upper="?"/>
            <typename name="Point_and_vector"/>
         </explicit>
      </entity>
      <entity name="Polar_11" supertypes="Detailed_geometric_model_element">
         <explicit name="placement">
            <typename name="Axis_placement_3d"/>
         </explicit>
      </entity>
      <entity name="Reinforcement_orientation_basis" supertypes="Representation">
         <explicit name="context_of_items">
            <typename name="Geometric_coordinate_space"/>
            <redeclaration entity-ref="Representation"/>
         </explicit>
         <explicit name="items">
            <aggregate lower="1" type="SET" upper="1"/>
            <typename name="Axis_placement_3d"/>
            <redeclaration entity-ref="Representation"/>
         </explicit>
         <explicit name="basis_11_direction">
            <typename name="direction_select"/>
         </explicit>
         <derived expression="SELF\Representation.items[1]\Axis_placement.location" name="basis_placement_location">
            <typename name="Cartesian_point"/>
         </derived>
         <inverse attribute="basis" entity="Laminate_table" name="table"/>
         <inverse attribute="rep_2" entity="Ply_orientation_angle" name="orientation"/>
         <where expression="'PLY_ORIENTATION_SPECIFICATION_ARM.AXIS_PLACEMENT_3D' IN TYPEOF(SELF\Representation.items[1])" label="WR1"/>
      </entity>
      <entity name="User_defined_11" supertypes="Representation_item">
         <explicit name="Definition">
            <typename name="String_representation_item"/>
         </explicit>
      </entity>
      <subtype.constraint entity="Detailed_geometric_model_element" name="pos_detailed_geometric_model_element_subtypes" super.expression="ONEOF (Point_and_vector, Point_array, Cartesian_11, Curve_11, Cylindrical_11, Polar_11)"/>
   </schema>
</express>
