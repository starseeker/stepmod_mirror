   <express_ref linkend="ply_orientation_specification:arm:Ply_orientation_specification_arm.Ply_orientation_angle.angle_reference"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Ply_orientation_specification_arm.Ply_orientation_angle.angle_reference">
