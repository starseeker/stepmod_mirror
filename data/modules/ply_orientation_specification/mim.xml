<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="mim_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1772">
   <application name="JSDAI" owner="LKSoft" source="ply_orientation_specification_mim schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Ply_orientation_specification_mim">
      <interface kind="use" schema="Part_and_zone_laminate_tables_mim"/>
      <interface kind="use" schema="geometry_schema">
         <interfaced.item name="point"/>
         <interfaced.item name="direction"/>
      </interface>
      <interface kind="use" schema="representation_schema">
         <interfaced.item name="compound_representation_item"/>
      </interface>
      <type name="basis_11_direction_member">
         <select selectitems="cartesian_11 curve_11 cylindrical_11 polar_11 point_array user_defined_11"/>
      </type>
      <type name="point_and_vector_member">
         <select selectitems="point direction"/>
      </type>
      <type name="point_and_vector_members">
         <aggregate lower="2" type="LIST" upper="3"/>
         <typename name="point_and_vector_member"/>
      </type>
      <type name="point_array_members">
         <aggregate lower="1" type="LIST" upper="?"/>
         <typename name="point_and_vector"/>
      </type>
      <entity name="cartesian_11" supertypes="geometric_representation_item">
         <where expression="'PLY_ORIENTATION_SPECIFICATION_MIM.AXIS_PLACEMENT_3D' IN TYPEOF(SELF)" label="WR1"/>
      </entity>
      <entity name="curve_11" supertypes="geometric_representation_item">
         <where expression="('PLY_ORIENTATION_SPECIFICATION_MIM.CURVE' IN TYPEOF(SELF)) OR&#10;         ('PLY_ORIENTATION_SPECIFICATION_MIM.MEASURE_WITH_UNIT' IN TYPEOF(SELF))" label="WR1"/>
      </entity>
      <entity name="cylindrical_11" supertypes="geometric_representation_item">
         <where expression="('PLY_ORIENTATION_SPECIFICATION_MIM.CURVE' IN TYPEOF(SELF))" label="WR1"/>
      </entity>
      <entity name="draped_orientation_angle" supertypes="ply_orientation_angle"/>
      <entity name="laid_orientation_angle" supertypes="ply_orientation_angle"/>
      <entity name="ply_angle_representation" supertypes="representation">
         <explicit name="items">
            <aggregate lower="1" type="SET" upper="1"/>
            <typename name="measure_representation_item"/>
            <redeclaration entity-ref="representation"/>
         </explicit>
      </entity>
      <entity name="ply_orientation_angle" supertypes="representation_relationship">
         <explicit name="rep_1">
            <typename name="reinforcement_orientation_basis"/>
            <redeclaration entity-ref="representation_relationship"/>
         </explicit>
         <explicit name="rep_2">
            <typename name="ply_angle_representation"/>
            <redeclaration entity-ref="representation_relationship"/>
         </explicit>
      </entity>
      <entity name="point_and_vector" supertypes="compound_representation_item geometric_representation_item">
         <explicit name="item_element">
            <typename name="point_and_vector_members"/>
            <redeclaration entity-ref="compound_representation_item"/>
         </explicit>
      </entity>
      <entity name="point_array" supertypes="compound_representation_item geometric_representation_item">
         <explicit name="item_element">
            <typename name="point_array_members"/>
            <redeclaration entity-ref="compound_representation_item"/>
         </explicit>
      </entity>
      <entity name="polar_11" supertypes="geometric_representation_item">
         <where expression="('PLY_ORIENTATION_SPECIFICATION_MIM.AXIS2_PLACEMENT_3D' IN TYPEOF(SELF))" label="WR1"/>
      </entity>
      <entity name="reinforcement_orientation_basis" supertypes="representation">
         <explicit name="items">
            <aggregate lower="1" type="SET" upper="1"/>
            <typename name="basis_11_direction_member"/>
            <redeclaration entity-ref="representation"/>
         </explicit>
      </entity>
      <entity name="user_defined_11" supertypes="representation_item"/>
      <subtype.constraint entity="representation" name="pos_representation_subtypes" super.expression="ONEOF (reinforcement_orientation_basis, ply_angle_representation)"/>
      <subtype.constraint entity="compound_representation_item" name="pos_compound_representation_item_subtypes" super.expression="ONEOF (point_and_vector, point_array)"/>
   </schema>
</express>
