<!-- +++++++++++ point_and_vector_members +++++++++++ -->
<ext_description linkend="Ply_orientation_specification_mim.point_and_vector_members">
	A <b>point_and_vector_members</b> type lists two or three 
	<express_ref linkend="Ply_orientation_specification:mim:Ply_orientation_specification_mim.point_and_vector_member">point_and_vector_members</express_ref>.
</ext_description>

<!-- +++++++++++ point_path_members +++++++++++ -->
<ext_description linkend="Ply_orientation_specification_mim.point_path_members">
	A <b>point_path_members</b> type lists one or more
	<express_ref linkend="Ply_orientation_specification:mim:Ply_orientation_specification_mim.point_and_vector">point_and_vectors</express_ref>.
</ext_description>


<!-- +++++++++++ cartesian_11 +++++++++++ -->
<ext_description linkend="Ply_orientation_specification_mim.cartesian_11">
	A <b>cartesian_11</b> is a type of fiber 11 direction specification for the ARM concept of
	<express_ref linkend="Ply_orientation_specification:ARM:Ply_orientation_specification_mim.Reinforcement_orientation_basis"/>
	that implements the ARM concept of 
	<express_ref linkend="Ply_orientation_specification:arm:Ply_orientation_specification_arm.Cartesian_11"/>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Ply_orientation_specification_mim.cartesian_11.wr:WR1">
	<p>
		If the case that <b>geomtric_representation_item</b> is a 
		<express_ref linkend="Ply_orientation_specification:mim:Ply_orientation_specification_mim.cartesian_11"/> 
		the <b>geometric_representation_item</b> shall be an 
		<express_ref linkend="geometry_schema:ir_express:geometry_schema.axis_placement_3d"/>.
	</p>
</ext_description>

<!-- +++++++++++ curve_11 +++++++++++ -->
