<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="">
   <application name="JSDAI" owner="LKSoft" source="point_direction_model_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Point_direction_model_arm">
      <interface kind="use" schema="Elemental_geometric_shape_arm"/>
      <type name="point_or_point_direction">
         <select selectitems="Point Point_direction"/>
      </type>
      <type name="point_or_point_direction_sequence">
         <select selectitems="Point_direction_sequence Point_sequence"/>
      </type>
      <entity name="Point_direction" supertypes="Detailed_geometric_model_element">
         <explicit name="origin">
            <typename name="Cartesian_point"/>
         </explicit>
         <explicit name="axis">
            <typename name="Direction"/>
         </explicit>
      </entity>
      <entity name="Point_direction_model" supertypes="Geometric_model">
         <explicit name="items">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="point_or_point_direction_sequence"/>
            <redeclaration entity-ref="Representation"/>
         </explicit>
      </entity>
      <entity name="Point_direction_sequence" supertypes="Detailed_geometric_model_element">
         <explicit name="items">
            <aggregate lower="1" type="LIST" upper="?"/>
            <typename name="point_or_point_direction"/>
         </explicit>
      </entity>
      <entity name="Point_sequence" supertypes="Detailed_geometric_model_element">
         <explicit name="items">
            <aggregate lower="1" type="LIST" upper="?"/>
            <typename name="Cartesian_point"/>
         </explicit>
      </entity>
      <subtype.constraint abstract.supertype="YES" entity="Detailed_geometric_model_element" name="pdim_exclusive_detailed_geometric_model_element" super.expression="ONEOF (Point_direction, Point_direction_sequence, Point_sequence, Point)"/>
   </schema>
</express>
