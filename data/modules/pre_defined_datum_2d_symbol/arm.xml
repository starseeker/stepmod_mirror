   <schema name="Pre_defined_datum_2d_symbol_arm">
      <interface kind="use" schema="Non_feature_shape_element_arm"/>
      <interface kind="use" schema="Layered_2d_shape_arm"/>
      <interface kind="use" schema="Extended_geometric_tolerance_arm"/>
      <entity name="Pre_defined_datum_2d_symbol" supertypes="Planar_shape Non_feature_shape_definition">
