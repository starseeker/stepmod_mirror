<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T15:27:10" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="pre_defined_datum_2d_symbol_arm schema_instance"/>
   <schema name="PRE_DEFINED_DATUM_2D_SYMBOL_ARM">
      <interface kind="use" schema="EXTENDED_GEOMETRIC_TOLERANCE_ARM"/>
      <interface kind="use" schema="LAYERED_2D_SHAPE_ARM"/>
      <interface kind="use" schema="NON_FEATURE_SHAPE_ELEMENT_ARM"/>
      <entity name="pre_defined_datum_2d_symbol" abstract.supertype="YES" supertypes="planar_shape non_feature_shape_definition" super.expression="ONEOF (pre_defined_parallel_datum_axis_2d_symbol, pre_defined_perpendicular_datum_axis_2d_symbol, pre_defined_perpendicular_datum_plane_2d_symbol)">
         <explicit name="associated_element">
            <typename name="datum_axis"/>
            <redeclaration entity-ref="non_feature_shape_definition"/>
         </explicit>
         <where label="wr1" expression=" not exists ( self \ non_feature_shape_definition . model_shape ) "/>
         <where label="wr2" expression=" self \ geometric_model . context_of_items . dimension_count = 2 "/>
      </entity>
      <entity name="pre_defined_parallel_datum_axis_2d_symbol" supertypes="pre_defined_datum_2d_symbol"/>
      <entity name="pre_defined_perpendicular_datum_axis_2d_symbol" supertypes="pre_defined_datum_2d_symbol"/>
      <entity name="pre_defined_perpendicular_datum_plane_2d_symbol" supertypes="pre_defined_datum_2d_symbol"/>
   </schema>
</express>
