<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="pre_defined_datum_3d_symbol_arm schema_instance"/>
   <schema name="Pre_defined_datum_3d_symbol_arm">
      <interface kind="use" schema="Extended_geometric_tolerance_arm"/>
      <entity name="Pre_defined_datum_axis_3d_symbol" supertypes="Geometric_model">
         <explicit name="associated_element">
            <typename name="Datum_axis"/>
         </explicit>
         <where label="WR1" expression="SELF\Geometric_model.context_of_items.dimension_count = 3"/>
         <where label="WR2" expression="NOT EXISTS(SELF\Representation.name)"/>
         <where label="WR3" expression="NOT EXISTS(SELF\Representation.description)"/>
      </entity>
      <entity name="Pre_defined_datum_plane_3d_symbol" supertypes="Geometric_model">
         <explicit name="associated_element">
            <typename name="Datum_plane"/>
         </explicit>
         <where label="WR1" expression="SELF\Geometric_model.context_of_items.dimension_count = 3"/>
         <where label="WR2" expression="NOT EXISTS(SELF\Representation.name)"/>
         <where label="WR3" expression="NOT EXISTS(SELF\Representation.description)"/>
      </entity>
      <subtype.constraint name="pdd3ds_geometric_model_subtypes" entity="Geometric_model" super.expression="ONEOF (Pre_defined_datum_axis_3d_symbol, Pre_defined_datum_plane_3d_symbol)"/>
   </schema>
</express>
