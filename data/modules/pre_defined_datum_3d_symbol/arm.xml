<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1736">
   <application name="JSDAI" owner="LKSoft" source="pre_defined_datum_3d_symbol_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Pre_defined_datum_3d_symbol_arm">
      <interface kind="use" schema="Extended_geometric_tolerance_arm"/>
      <entity name="Pre_defined_datum_axis_3d_symbol" supertypes="Geometric_model">
         <explicit name="associated_element">
            <typename name="Axis_placement_shape_element"/>
         </explicit>
         <where expression="SELF\Geometric_model.context_of_items.dimension_count = 3" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Representation.name)" label="WR2"/>
         <where expression="NOT EXISTS(SELF\Representation.description)" label="WR3"/>
      </entity>
      <entity name="Pre_defined_datum_plane_3d_symbol" supertypes="Geometric_model">
         <explicit name="associated_element">
            <typename name="Axis_placement_shape_element"/>
         </explicit>
         <where expression="SELF\Geometric_model.context_of_items.dimension_count = 3" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Representation.name)" label="WR2"/>
         <where expression="NOT EXISTS(SELF\Representation.description)" label="WR3"/>
      </entity>
      <subtype.constraint entity="Geometric_model" name="pdd3ds_geometric_model_subtypes" super.expression="ONEOF (Pre_defined_datum_axis_3d_symbol, Pre_defined_datum_plane_3d_symbol)"/>
   </schema>
</express>
