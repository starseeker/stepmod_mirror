<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-02-18T12:25:31" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="pre_defined_datum_3d_symbol_arm schema_instance"/>
   <schema name="Pre_defined_datum_3d_symbol_arm">
      <interface kind="use" schema="Extended_geometric_tolerance_arm"/>
      <interface kind="use" schema="Non_feature_shape_element_arm"/>
      <entity name="Pre_defined_datum_axis_3d_symbol" supertypes="Non_feature_shape_definition">
         <explicit name="associated_element">
            <typename name="Datum_axis"/>
            <redeclaration entity-ref="Non_feature_shape_definition"/>
         </explicit>
         <where label="WR1" expression="NOT EXISTS(SELF\Non_feature_shape_definition.model_shape)"/>
         <where label="WR2" expression="SELF\Geometric_model.context_of_items.dimension_count = 3"/>
      </entity>
      <entity name="Pre_defined_datum_plane_3d_symbol" supertypes="Non_feature_shape_definition">
         <explicit name="associated_element">
            <typename name="Datum_plane"/>
            <redeclaration entity-ref="Non_feature_shape_definition"/>
         </explicit>
         <where label="WR1" expression="NOT EXISTS(SELF\Non_feature_shape_definition.model_shape)"/>
         <where label="WR2" expression="SELF\Geometric_model.context_of_items.dimension_count = 3"/>
      </entity>
      <subtype.constraint name="pdd3ds_non_feature_shape_definition_subtypes" entity="Non_feature_shape_definition" super.expression="ONEOF (Pre_defined_datum_axis_3d_symbol, Pre_defined_datum_plane_3d_symbol)"/>
   </schema>
</express>
