<!-- +++++++++++ Pre_defined_datum_axis_3d_symbol +++++++++++ -->
<ext_description linkend="Pre_defined_datum_3d_symbol_arm.Pre_defined_datum_axis_3d_symbol">
<p>
	A <b>Pre_defined_datum_axis_3d_symbol</b> is a type of 
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model"/>.  
	A <b>Pre_defined_datum_axis_3d_symbol</b> is a standard representation for a 
	<express_ref linkend="Extended_geometric_tolerance:arm:Extended_geometric_tolerance_arm.Datum_axis"/> 
	in a 3 dimensional coordinate system.
</p>
</ext_description>

<!-- associated_element -->
<ext_description linkend="Pre_defined_datum_3d_symbol_arm.Pre_defined_datum_axis_3d_symbol.associated_element">
	specifies a role of the 
	<express_ref linkend="Extended_geometric_tolerance:arm:Extended_geometric_tolerance_arm.Datum_axis"/> 
	for the <b>Pre_defined_datum_axis_3d_symbol</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Pre_defined_datum_3d_symbol_arm.Pre_defined_datum_axis_3d_symbol.wr:WR1">
	The context of <b>Pre_defined_datum_axis_3d_symbol</b> shall be three-dimensional.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Pre_defined_datum_3d_symbol_arm.Pre_defined_datum_axis_3d_symbol.wr:WR2">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.name"/>
	shall not be populated.
</ext_description>

<!-- WR3 -->
<ext_description linkend="Pre_defined_datum_3d_symbol_arm.Pre_defined_datum_axis_3d_symbol.wr:WR3">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.description"/>
	shall not be populated.
</ext_description>

<!-- +++++++++++ Pre_defined_datum_plane_3d_symbol +++++++++++ -->
<ext_description linkend="Pre_defined_datum_3d_symbol_arm.Pre_defined_datum_plane_3d_symbol">
<p>
	A <b>Pre_defined_datum_plane_3d_symbol</b> is a type of 
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model"/>.  
	A <b>Pre_defined_datum_plane_3d_symbol</b> is a standard representation for a 
	<express_ref linkend="Extended_geometric_tolerance:arm:Extended_geometric_tolerance_arm.Datum_plane"/> 
	in a 3 dimensional coordinate system.  A <b>Pre_defined_datum_plane_3d_symbol</b> shall be parallel with the normal vector of the plane, 
	and shall intersect the plane at the origin of the symbol.
</p></ext_description>

<!-- associated_element -->
<ext_description linkend="Pre_defined_datum_3d_symbol_arm.Pre_defined_datum_plane_3d_symbol.associated_element">
	specifies a role of the 
	<express_ref linkend="Extended_geometric_tolerance:arm:Extended_geometric_tolerance_arm.Datum_plane"/> 
	for the <b>Pre_defined_datum_plane_3d_symbol</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Pre_defined_datum_3d_symbol_arm.Pre_defined_datum_plane_3d_symbol.wr:WR1">
	The context of <b>Pre_defined_datum_plane_3d_symbol</b> shall be three-dimensional.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Pre_defined_datum_3d_symbol_arm.Pre_defined_datum_plane_3d_symbol.wr:WR2">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.name"/>
	shall not be populated.
</ext_description>

<!-- WR3 -->
<ext_description linkend="Pre_defined_datum_3d_symbol_arm.Pre_defined_datum_plane_3d_symbol.wr:WR3">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.description"/>
	shall not be populated.
</ext_description>

