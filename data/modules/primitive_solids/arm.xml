<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date: 2007/03/21 11:10:23 $" rcs.revision="$Revision: 1.1 $" description.file="arm_descriptions.xml">
   <application name="Hand" owner="LMR" url="" version="1.0 beta" source="primitive_solids_arm schema_instance"/>
   <schema name="Primitive_solids_arm">
       <interface kind="use" schema="Elemental_geometric_shape_arm"/>
      <interface kind="use" schema="Value_with_unit_arm"/>
     
     
     
     
   <entity
      name="Circular_cone"
      supertypes="Detailed_geometric_model_element">
      <explicit
        name="axis_direction">
        <typename
          name="Direction"/>
      </explicit>
       <explicit
        name="axis_point">
        <typename
          name="Cartesian_point"/>
      </explicit>    
      <explicit
        name="height">
        <typename
          name="length_measure"/>
      </explicit>
      <explicit
        name="radius">
        <typename
          name="length_measure"/>
      </explicit>
      <explicit
        name="semi_angle">
        <typename
          name="plane_angle_measure"/>
      </explicit>
      <where
        label="WR1"
        expression="radius &gt;= 0.0">
      </where>
      <where
            label="WR2"
            expression="height &gt; 0.0">
      </where>
    </entity>  
     
     
     
    <entity
      name="Circular_cylinder"
      supertypes="Detailed_geometric_model_element">
       <explicit
        name="axis_direction">
        <typename
          name="Direction"/>
      </explicit>
       <explicit
        name="axis_point">
        <typename
          name="Cartesian_point"/>
      </explicit>    
     <explicit
        name="height">
        <typename
          name="length_measure"/>
      </explicit>
      <explicit
        name="radius">
        <typename
          name="length_measure"/>
      </explicit>
      <where
        label="WR1"
        expression="radius &gt;0.0">
      </where>
      <where
            label="WR2"
            expression="height &gt; 0.0">
      </where>
   </entity> 
     
    <entity
         name="Rectangular_block"
         supertypes="Detailed_geometric_model_element">
         <explicit
           name="corner_location">
           <typename
             name="Axis_placement_3d"/>
         </explicit>
          <explicit
           name="xlength">
           <typename
             name="length_measure"/>
         </explicit>    
         <explicit
           name="width">
           <typename
             name="length_measure"/>
         </explicit>
         <explicit
           name="height">
           <typename
             name="length_measure"/>
         </explicit>
         <where
           label="WR1"
           expression="xlength &gt; 0.0">
         </where>
         <where
           label="WR2"
           expression="width &gt; 0.0">
         <where
           label="WR3"
           expression="height &gt; 0.0">
         </where></where>
     
     </entity>  
   
   <entity
      name="Sphere"
      supertypes="Detailed_geometric_model_element">
      <explicit
        name="radius">
        <typename
          name="length_measure"/>
      </explicit>
      <explicit
        name="centre">
        <typename
          name="Cartesian_point"/>
      </explicit>
     <where
        label="WR1"
        expression="radius &gt; 0.0">
      </where>
     </entity>    
     
     
    
   
   
  <entity
      name="Torus"
      supertypes="Detailed_geometric_model_element">
      <explicit
        name="axis_direction">
        <typename
          name="Direction"/>
      </explicit>
       <explicit
        name="axis_point">
        <typename
          name="Cartesian_point"/>
      </explicit>    
      <explicit
        name="radius">
        <typename
          name="length_measure"/>
      </explicit>
      <explicit
        name="minor_radius">
        <typename
          name="length_measure"/>
      </explicit>
      <where
        label="WR1"
        expression="radius &gt; minor_radius">
      </where>
      <where
        label="WR2"
        expression="minor_radius &gt; 0.0">
      </where>
     </entity>  
   
  
   </schema>
</express>
