<?xml version="1.0" encoding="utf-8"?><?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express
  SYSTEM "../../../dtd/express.dtd">
<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="">
   <application name="JSDAI" owner="LKSoft" source="primitive_solids_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Primitive_solids_arm">
      <interface kind="use" schema="Basic_geometry_arm"/>
      <interface kind="use" schema="Value_with_unit_arm"/>
      <entity name="Circular_cone" supertypes="Detailed_geometric_model_element">
         <explicit name="axis_direction">
            <typename name="Direction"/>
         </explicit>
         <explicit name="axis_point">
            <typename name="Cartesian_point"/>
         </explicit>
         <explicit name="height">
            <typename name="length_measure"/>
         </explicit>
         <explicit name="radius">
            <typename name="length_measure"/>
         </explicit>
         <explicit name="semi_angle">
            <typename name="plane_angle_measure"/>
         </explicit>
         <where expression="radius &gt;= 0.0" label="WR1"/>
         <where expression="height &gt; 0.0" label="WR2"/>
      </entity>
      <entity name="Circular_cylinder" supertypes="Detailed_geometric_model_element">
         <explicit name="axis_direction">
            <typename name="Direction"/>
         </explicit>
         <explicit name="axis_point">
            <typename name="Cartesian_point"/>
         </explicit>
         <explicit name="height">
            <typename name="length_measure"/>
         </explicit>
         <explicit name="radius">
            <typename name="length_measure"/>
         </explicit>
         <where expression="radius &gt; 0.0" label="WR1"/>
         <where expression="height &gt; 0.0" label="WR2"/>
      </entity>
      <entity name="Rectangular_block" supertypes="Detailed_geometric_model_element">
         <explicit name="corner_location">
            <typename name="Axis_placement_3d"/>
         </explicit>
         <explicit name="xlength">
            <typename name="length_measure"/>
         </explicit>
         <explicit name="width">
            <typename name="length_measure"/>
         </explicit>
         <explicit name="height">
            <typename name="length_measure"/>
         </explicit>
         <where expression="xlength &gt; 0.0" label="WR1"/>
         <where expression="width &gt; 0.0" label="WR2"/>
         <where expression="height &gt; 0.0" label="WR3"/>
      </entity>
      <entity name="Sphere" supertypes="Detailed_geometric_model_element">
         <explicit name="radius">
            <typename name="length_measure"/>
         </explicit>
         <explicit name="centre">
            <typename name="Cartesian_point"/>
         </explicit>
         <where expression="radius &gt; 0.0" label="WR1"/>
      </entity>
      <entity name="Torus" supertypes="Detailed_geometric_model_element">
         <explicit name="axis_direction">
            <typename name="Direction"/>
         </explicit>
         <explicit name="axis_point">
            <typename name="Cartesian_point"/>
         </explicit>
         <explicit name="radius">
            <typename name="length_measure"/>
         </explicit>
         <explicit name="minor_radius">
            <typename name="length_measure"/>
         </explicit>
         <where expression="radius &gt; minor_radius" label="WR1"/>
         <where expression="minor_radius &gt; 0.0" label="WR2"/>
      </entity>
      <subtype.constraint entity="Detailed_geometric_model_element" name="ps_detailed_geometric_model_element_subtypes" super.expression="ONEOF (Circular_cone, Circular_cylinder, Sphere, Rectangular_block, Torus)"/>
   </schema>
</express>