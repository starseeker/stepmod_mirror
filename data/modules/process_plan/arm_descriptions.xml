  <express_ref linkend="process_plan:arm:Process_plan_arm.Process_state_relationship"/>.
</ext_description>
<ext_description linkend="Process_plan_arm.process_state_relationship_direction.bidirectional_process_state_relationship">
  the direction is bidirectional.
</ext_description>
<ext_description linkend="Process_plan_arm.process_state_relationship_direction.unidirectional_process_state_relationship">
