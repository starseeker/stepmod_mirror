<!DOCTYPE ext_descriptions SYSTEM "../../../dtd/description.dtd">
<ext_descriptions module_directory="process_plan" schema_file="mim.xml" describe.selects="YES" describe.subtype_constraints="YES" rcs.date="$Date: 2010/12/13 13:24:58 $" rcs.revision="$Revision: 1.2 $">
<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: Process_plan_mim
   <express_ref linkend="process_plan:mim:Process_plan_mim"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Process_plan_mim">
</ext_description><!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Process_plan_mim  Entity: process_operation  
   <express_ref linkend="process_plan:mim:Process_plan_mim.process_operation"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Process_plan_mim.process_operation">
	A <b>process_operation</b> is a type of
	<express_ref linkend="action_schema:ir_express:action_schema.action_method" />
	that implements the ARM concept of 
	<express_ref linkend="process_plan:arm:Process_plan_arm.Process_operation_definition"/>.
	A <b>process_operation</b> is the identification of a specific step in a process. 
	It collects all information that is common for all occurrences of this particular step in the processes which make use of this step.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Process_plan_mim  Entity: process_operation  Where: WR1 
   <express_ref linkend="process_plan:mim:Process_plan_mim.process_operation.wr:WR1"/>

  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Process_plan_mim.process_operation.wr:WR1">
	A <b>process_operation</b> shall only be refered to by 
	<express_ref linkend="action_schema:ir_express:action_schema.action_method_relationship" >action_method_relationships</express_ref> 
	as 
	<express_ref linkend="action_schema:ir_express:action_schema.action_method_relationship.relating_method" />, 
	where the 
	<express_ref linkend="action_schema:ir_express:action_schema.action_method_relationship.related_method" /> 
	refers to another instance of <b>process_operation</b>. 
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Process_plan_mim  Entity: process_operation  Where: WR2 
   <express_ref linkend="process_plan:mim:Process_plan_mim.process_operation.wr:WR2"/>

  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Process_plan_mim.process_operation.wr:WR2">
	A <b>process_operation</b> shall only be refered to by 
	<express_ref linkend="action_schema:ir_express:action_schema.action_method_relationship" >action_method_relationships</express_ref> 
	as 
	<express_ref linkend="action_schema:ir_express:action_schema.action_method_relationship.related_method" />, 
	where the 
	<express_ref linkend="action_schema:ir_express:action_schema.action_method_relationship.relating_method" /> 
	refers to another instance of <b>process_operation</b>.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Process_plan_mim  Entity: process_plan  
   <express_ref linkend="process_plan:mim:Process_plan_mim.process_plan"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Process_plan_mim.process_plan">
	A <b>process_plan</b> is a type of
	<express_ref linkend="action_schema:ir_express:action_schema.action" />
	that specifies the information necessary for manufacturing planning.
	A <b>process_plan</b> implements the ARM concept of 
	<express_ref linkend="process_plan:arm:Process_plan_arm.Process_plan"/>.   
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Process_plan_mim  Entity: process_plan  Where: WR1 
   <express_ref linkend="process_plan:mim:Process_plan_mim.process_plan.wr:WR1"/>

  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Process_plan_mim.process_plan.wr:WR1">
	The <b>process_plan</b> shall only be referenced as the 
	<express_ref linkend="action_schema:ir_express:action_schema.action_relationship.related_action" /> 
	by 
	<express_ref linkend="action_schema:ir_express:action_schema.action_relationship" >action_relationships</express_ref> 
	that reference as the 
	<express_ref linkend="action_schema:ir_express:action_schema.action_relationship.relating_action" /> 
	a 
	<b>process_plan</b> 
	or a 
	<express_ref linkend="process_plan:mim:Process_plan_mim.product_process_plan"/>. 
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Process_plan_mim  Entity: process_plan  Where: WR2 
   <express_ref linkend="process_plan:mim:Process_plan_mim.process_plan.wr:WR2"/>

  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Process_plan_mim.process_plan.wr:WR2">
	The <b>process_plan</b> shall only be referenced as the 
	<express_ref linkend="action_schema:ir_express:action_schema.action_relationship.relating_action" />  
	by 
	<express_ref linkend="action_schema:ir_express:action_schema.action_relationship" >action_relationships</express_ref> 
	that reference as the 
	<express_ref linkend="action_schema:ir_express:action_schema.action_relationship.related_action" /> 
	a 
	<b>process_plan</b>, or a 
	<express_ref linkend="process_plan:mim:Process_plan_mim.product_process_plan"/>, 
	or by 
	<express_ref linkend="action_schema:ir_express:action_schema.action_relationship" >action_relationships</express_ref> 
	that have a name of 'process operation occurrence' and that reference as the 
	<express_ref linkend="action_schema:ir_express:action_schema.action_relationship.related_action" /> 
	an 
	<express_ref linkend="action_schema:ir_express:action_schema.action" /> 
	that refers to a <b>process_plan</b> as its 
	<express_ref linkend="action_schema:ir_express:action_schema.action.chosen_method" />.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Process_plan_mim  Entity: product_definition_resource  
   <express_ref linkend="process_plan:mim:Process_plan_mim.product_definition_resource"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Process_plan_mim.product_definition_resource">
	A <b>product_definition_resource</b> is a type of
	<express_ref linkend="action_schema:ir_express:action_schema.action_resource" />
	and a type of
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition" />
	that specifies a tool that is used to perform a 
	<express_ref linkend="process_plan:mim:Process_plan_mim.process_operation"/>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Process_plan_mim  Entity: product_definition_resource  Where: WR1 
   <express_ref linkend="process_plan:mim:Process_plan_mim.product_definition_resource.wr:WR1"/>

  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Process_plan_mim.product_definition_resource.wr:WR1">
	A <b>product_definition_resource</b> is contained in the set of 
	<express_ref linkend="process_property_schema:ir_express:process_property_schema.requirement_for_action_resource.resources"/> 
	of at least one 
	<express_ref linkend="process_property_schema:ir_express:process_property_schema.requirement_for_action_resource"/>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Process_plan_mim  Entity: product_definition_resource  Where: WR2 
   <express_ref linkend="process_plan:mim:Process_plan_mim.product_definition_resource.wr:WR2"/>

  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Process_plan_mim.product_definition_resource.wr:WR2">
	A <b>product_definition_resource</b> shall reference as its 
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition.frame_of_reference" /> 
	a 
	<express_ref linkend="application_context_schema:ir_express:application_context_schema.product_definition_context" /> 
	with a 
	<express_ref linkend="application_context_schema:ir_express:application_context_schema.application_context_element.name" /> 
	of 'part definition', 'part occurrence', or 'physical occurrence'.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Process_plan_mim  Entity: product_process_plan  
   <express_ref linkend="process_plan:mim:Process_plan_mim.product_process_plan"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Process_plan_mim.product_process_plan">
	A <b>product_process_plan</b> is a type of
	<express_ref linkend="process_property_schema:ir_express:process_property_schema.product_definition_process" />
	that specifies the information necessary for manufacturing planning to realize or produce a product.
	A <b>product_process_plan</b> implements the ARM concept of 
	<express_ref linkend="process_plan:arm:Process_plan_arm.Process_plan"/>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Process_plan_mim  Entity: product_process_plan  Where: WR1 
   <express_ref linkend="process_plan:mim:Process_plan_mim.product_process_plan.wr:WR1"/>

  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Process_plan_mim.product_process_plan.wr:WR1">
	The <b>product_process_plan</b> shall only be referenced as the 
	<express_ref linkend="action_schema:ir_express:action_schema.action_relationship.related_action" /> 
	by 
	<express_ref linkend="action_schema:ir_express:action_schema.action_relationship" >action_relationships</express_ref>  
	that reference as the 
	<express_ref linkend="action_schema:ir_express:action_schema.action_relationship.relating_action" /> 
	a 
	<express_ref linkend="process_plan:mim:Process_plan_mim.process_plan"/> 
	or a <b>product_process_plan</b>. 
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Process_plan_mim  Entity: product_process_plan  Where: WR2 
   <express_ref linkend="process_plan:mim:Process_plan_mim.product_process_plan.wr:WR2"/>

  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Process_plan_mim.product_process_plan.wr:WR2">
	The <b>product_process_plan</b> shall only be referenced as the 
	<express_ref linkend="action_schema:ir_express:action_schema.action_relationship.relating_action" /> 
	by 
	<express_ref linkend="action_schema:ir_express:action_schema.action_relationship" >action_relationships</express_ref> 
	that reference as the 
	<express_ref linkend="action_schema:ir_express:action_schema.action_relationship.related_action" />  
	a 
	<express_ref linkend="process_plan:mim:Process_plan_mim.process_plan"/> 
	or a <b>product_process_plan</b>, or by 
	<express_ref linkend="action_schema:ir_express:action_schema.action_relationship" >action_relationships</express_ref> 
	that have a name of 'process operation occurrence' and that reference as the 
	<express_ref linkend="action_schema:ir_express:action_schema.action_relationship.related_action" />  
	an 
	<express_ref linkend="action_schema:ir_express:action_schema.action" /> 
	that refers to a 
	<express_ref linkend="process_plan:mim:Process_plan_mim.process_operation"/> 
	as its 
	<express_ref linkend="action_schema:ir_express:action_schema.action.chosen_method" />. 
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Process_plan_mim  Entity: product_process_plan  Where: WR3 
   <express_ref linkend="process_plan:mim:Process_plan_mim.product_process_plan.wr:WR3"/>

  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Process_plan_mim.product_process_plan.wr:WR3">
	The <b>product_process_plan</b> shall only be referenced as 
	<express_ref linkend="process_property_schema:ir_express:process_property_schema.process_product_association.process" /> 
	by 
	<express_ref linkend="process_property_schema:ir_express:process_property_schema.process_product_association" >process_product_associations</express_ref> 
	that have a 
	<express_ref linkend="process_property_schema:ir_express:process_property_schema.process_product_association.name" /> 
	of 'produced output' and that reference as 
	<express_ref linkend="process_property_schema:ir_express:process_property_schema.process_product_association.defined_product" /> 
	a 
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition" />. 
</ext_description>
</ext_descriptions>
