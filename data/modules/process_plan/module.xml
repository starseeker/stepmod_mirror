						{product_definition.frame_of_reference -&gt;
						product_definition_context &lt;=
						application_context_element
						application_context_element.name = 'part definition'}])
					</refpath>
				</alt_map>
				<alt_map id="4">
	                <description>If the resource is not used in the role of a reference tool.</description>
	                <refpath>
	                	requirement_for_action_resource &lt;=
	                	action_resource_requirement
						(action_resource_requirement.name = 'non reference tool')
					</refpath>
				</alt_map>
            </aa>
             <aa attribute="operation" assertion_to="Process_operation_occurrence">
                <aimelt>PATH</aimelt>
                <refpath>
                	requirement_for_action_resource &lt;=
					action_resource_requirement
					action_resource_requirement.operations[i] -&gt;
					characterized_action_definition
					characterized_action_definition = action
					action
					{action &lt;-
					action_relationship.related_action
					action_relationship
					action_relationship.name = 'process operation occurrence'}
				</refpath>
            </aa>
             <aa attribute="resource_definition" assertion_to="resource_definition_select">
                <aimelt>PATH</aimelt>
                <refpath>
                	requirement_for_action_resource &lt;=
					action_resource_requirement
				</refpath>
            </aa>
             <aa attribute="resource_definition" assertion_to="Product_occurrence">
                <aimelt>PATH</aimelt>
                <refpath>
					requirement_for_action_resource
					requirement_for_action_resource.resources[i] -&gt;
					action_resource =&gt;
					product_definition_resource &lt;=
					product_definition
				</refpath>
            </aa>
