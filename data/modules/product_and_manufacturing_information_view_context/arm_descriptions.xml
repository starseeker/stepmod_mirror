  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_and_manufacturing_information_view_context_arm.Gdt_representation_view_context">
	A <b>Gdt_representation_view_context</b> is a type of
	<express_ref linkend="Part_view_definition:arm:Part_view_definition_arm.Part_view_definition_context" />
	that is a semantic geometrical dimensioning and tolerancing context 
	that is to be used by a 
	<express_ref linkend="part_view_definition:arm:Part_view_definition_arm.Part_view_definition"/>
	as a member of
	<express_ref linkend="Product_view_definition:arm:Product_view_definition_arm.Product_view_definition.additional_contexts"/>.
</ext_description>

<ext_description linkend="Product_and_manufacturing_information_view_context_arm.Gdt_representation_view_context.application_domain">
	an inherited attribute shall be the
	string with value 'geometrical dimensioning and tolerancing representation'
	for the <b>Gdt_representation_view_context</b>.
</ext_description>

<ext_description linkend="Product_and_manufacturing_information_view_context_arm.Gdt_representation_view_context.life_cycle_stage">
	an inherited attribute shall be the
	string with value 'design'
	for the <b>Gdt_representation_view_context</b>.
</ext_description>

<ext_description linkend="Product_and_manufacturing_information_view_context_arm.Gdt_representation_view_context.wr:WR1">
	There shall be no users in the role 
	<express_ref linkend="Product_view_definition:arm:Product_view_definition_arm.Product_view_definition.initial_context" >Product_view_definition.initial_context</express_ref>.
</ext_description>

<ext_description linkend="Product_and_manufacturing_information_view_context_arm.Gdt_representation_view_context.wr:WR2">
	The 
	<express_ref linkend="Part_view_definition:arm:Part_view_definition_arm.Part_view_definition.initial_context" >Part_view_definition.initial_context</express_ref> 
	shall be of type 
	<express_ref linkend="product_and_manufacturing_information_view_context:arm:Product_and_manufacturing_information_view_context_arm.Model_based_view_context"/>.
</ext_description>

