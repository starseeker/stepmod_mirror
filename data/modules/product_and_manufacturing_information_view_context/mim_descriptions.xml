<ext_description linkend="Product_and_manufacturing_information_view_context_mim.valid_product_and_manufacturing_information_view_contexts">
	The <b>valid_product_and_manufacturing_information_view_contexts</b> rule constrains every instance of 
	<express_ref linkend="application_context_schema:ir_express:application_context_schema.product_definition_context"/> 
	that refers to an  
	<express_ref linkend="application_context_schema:ir_express:application_context_schema.application_context"/>
	with the
	<express_ref linkend="application_context_schema:ir_express:application_context_schema.application_context.application"/>
	value 
	'geometrical dimensioning and tolerancing representation',
	'machining feature',
	'material specification',
	'model based 3D annotation presentation',
	'model based product geometry',
	'process state' or
	'surface finish'.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Product_and_manufacturing_information_view_context_mim.valid_product_and_manufacturing_information_view_contexts.wr:WR1">
	Every
	<express_ref linkend="application_context_schema:ir_express:application_context_schema.product_definition_context"/>
	that refers to an  
	<express_ref linkend="application_context_schema:ir_express:application_context_schema.application_context"/>
	with the
	<express_ref linkend="application_context_schema:ir_express:application_context_schema.application_context.application"/>
	value 'geometrical dimensioning and tolerancing representation' shall have the
	<express_ref linkend="application_context_schema:ir_express:application_context_schema.product_definition_context.life_cycle_stage"/>
	'design'. 
</ext_description>

