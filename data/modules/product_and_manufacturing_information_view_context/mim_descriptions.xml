	value 'model based product geometry'
	shall not be referenced by a
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_context_association"/>
	in the role
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_context_association.frame_of_reference"/>.
</ext_description>

<!-- WR9 -->
<ext_description linkend="Product_and_manufacturing_information_view_context_mim.valid_product_and_manufacturing_information_view_contexts.wr:WR9">
