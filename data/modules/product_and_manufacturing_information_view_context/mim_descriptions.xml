	<express_ref linkend="application_context_schema:ir_express:application_context_schema.application_context.application"/>
	value 
	'geometrical dimensioning and tolerancing representation',
	'machining feature',
	'material specification',
	'model based 3D annotation presentation',
	'process state' or
	'surface finish'
	shall not be referenced by a
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition"/>
	in the role
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition.frame_of_reference"/>.
</ext_description>

<!-- WR10 -->
<ext_description linkend="Product_and_manufacturing_information_view_context_mim.valid_product_and_manufacturing_information_view_contexts.wr:WR10">
	Every 
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition"/>
	to which a 
	<express_ref linkend="application_context_schema:ir_express:application_context_schema.product_definition_context"/> 
	is associated via an
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_context_association"/>
	that has an  
	<express_ref linkend="application_context_schema:ir_express:application_context_schema.application_context"/>
	with the value 
	'geometrical dimensioning and tolerancing representation' or
	'model based 3D annotation presentation'
	shall refer to a
	<express_ref linkend="application_context_schema:ir_express:application_context_schema.product_definition_context"/> 
	via the 
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition.frame_of_reference"/>
	that refers to an 
	<express_ref linkend="application_context_schema:ir_express:application_context_schema.application_context"/>
	with the value 'model based product geometry'.
</ext_description>

<!-- WR11 -->
<ext_description linkend="Product_and_manufacturing_information_view_context_mim.valid_product_and_manufacturing_information_view_contexts.wr:WR11">
	Every 
	<express_ref linkend="application_context_schema:ir_express:application_context_schema.product_definition_context"/> 
	that refers to an  
	<express_ref linkend="application_context_schema:ir_express:application_context_schema.application_context"/>
	with the
	<express_ref linkend="application_context_schema:ir_express:application_context_schema.application_context.application"/>
	value 'geometrical dimensioning and tolerancing representation'
	shall be associated to a document with an associated 
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product"/>
	with the
	<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product.id"/>
	value 'ISO 1101' or 'ASME Y14.5'.
</ext_description>

<!-- WR12 -->
<ext_description linkend="Product_and_manufacturing_information_view_context_mim.valid_product_and_manufacturing_information_view_contexts.wr:WR12">
