<ext_description linkend="Product_and_manufacturing_information_with_nominal_3d_models_arm.Nominal_3d_model_shape">
	A <b>Nominal_3d_model_shape</b> is a type of
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Associated_item_shape"/>
	that identifies the nominal model of a part.
</ext_description>

<ext_description linkend="Product_and_manufacturing_information_with_nominal_3d_models_arm.pamap_initial_application_domain_enumeration">
	A <b>pamap_initial_application_domain_enumeration</b> provides pre-defined identifiers for the additional application domains of
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Additional_view_definition_context"/>s.
</ext_description>
<ext_description linkend="Product_and_manufacturing_information_with_nominal_3d_models_arm.pamap_initial_application_domain_enumeration.model_based_product_geometry">
	the application domain is model based product geometry.
</ext_description>

<ext_description linkend="Product_and_manufacturing_information_with_nominal_3d_models_arm.pmiwn3dm_documented_element_select">
</ext_description>

<ext_description linkend="Product_and_manufacturing_information_with_nominal_3d_models_arm.pmiwn3m_product_shape_type_enumeration">
	A <b>pmiwn3m_product_shape_type_enumeration</b> provides pre-defined identifiers for the additional application domains of
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Additional_view_definition_context"/>s.
</ext_description>
<ext_description linkend="Product_and_manufacturing_information_with_nominal_3d_models_arm.pmiwn3m_product_shape_type_enumeration.nominal_shape">
	the provided geometric model represents the <i>nominal model</i>.
</ext_description>

<ext_description linkend="Product_and_manufacturing_information_with_nominal_3d_models_arm.nominal_3d_model_shape_constraint">
	The <b>nominal_3d_model_shape_constraint</b> constraints 
	<express_ref linkend="part_view_definition:arm:Part_view_definition_arm.Part_view_definition"/>
	that have the value 
	<express_ref linkend="product_and_manufacturing_information_with_nominal_3d_models:arm:Product_and_manufacturing_information_with_nominal_3d_models_arm.pmiwn3m_product_shape_type_enumeration.nominal_shape"/>
	for the
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition.shape_type"/>.
</ext_description>
<ext_description linkend="Product_and_manufacturing_information_with_nominal_3d_models_arm.nominal_3d_model_shape_constraint.wr:WR1">
	a
	<express_ref linkend="part_view_definition:arm:Part_view_definition_arm.Part_view_definition"/>
	whose
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition.shape_type"/>
	is
	<express_ref linkend="product_and_manufacturing_information_with_nominal_3d_models:arm:Product_and_manufacturing_information_with_nominal_3d_models_arm.pmiwn3m_product_shape_type_enumeration.nominal_shape"/>
	shall have an 
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition.initial_context"/>
	with the value
	<express_ref linkend="part_view_definition:arm:Part_view_definition_arm.pvd_life_cycle_stage_enumeration.design"/>
	for the
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.View_definition_context.life_cycle_stage"/>
	and the value
	<express_ref linkend="product_and_manufacturing_information_with_nominal_3d_models:arm:Product_and_manufacturing_information_with_nominal_3d_models_arm.pamap_initial_application_domain_enumeration.model_based_product_geometry"/>
	for the
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Initial_view_definition_context.application_domain"/>.
</ext_description>
<ext_description linkend="Product_and_manufacturing_information_with_nominal_3d_models_arm.nominal_3d_model_shape_constraint.wr:WR2">
	a
	<express_ref linkend="part_view_definition:arm:Part_view_definition_arm.Part_view_definition"/>
	whose
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition.shape_type"/>
	is
	<express_ref linkend="product_and_manufacturing_information_with_nominal_3d_models:arm:Product_and_manufacturing_information_with_nominal_3d_models_arm.pmiwn3m_product_shape_type_enumeration.nominal_shape"/>
	shall have a
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition.primary_shape_representation"/>
	of type
	<express_ref linkend="advanced_boundary_representation:arm:Advanced_boundary_representation_arm.Advanced_brep_shape_representation"/>,
	<express_ref linkend="manifold_surface:arm:Manifold_surface_arm.Manifold_surface_shape_representation"/>,
	<express_ref linkend="manifold_subsurface:arm:Manifold_subsurface_arm.Manifold_subsurface_model"/>
	or a combination of these.
</ext_description>

</ext_descriptions>
