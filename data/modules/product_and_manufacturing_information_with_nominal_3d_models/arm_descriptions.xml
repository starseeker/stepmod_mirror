   <express_ref linkend="product_and_manufacturing_information_with_nominal_3d_models:arm:Product_and_manufacturing_information_with_nominal_3d_models_arm.Nominal_3d_model_shape"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_and_manufacturing_information_with_nominal_3d_models_arm.Nominal_3d_model_shape">
	A <b>Nominal_3d_model_shape</b> is a type of
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Associated_item_shape"/>
	that identifies the nominal model of a part.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_and_manufacturing_information_with_nominal_3d_models_arm  Entity: Nominal_3d_model_shape  Where: WR1 
   <express_ref linkend="product_and_manufacturing_information_with_nominal_3d_models:arm:Product_and_manufacturing_information_with_nominal_3d_models_arm.Nominal_3d_model_shape.wr:WR1"/>

  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_and_manufacturing_information_with_nominal_3d_models_arm.Nominal_3d_model_shape.wr:WR1">
   The 
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/>
   associated with the nominal model shall have an initial context that is a 
	<express_ref linkend="product_and_manufacturing_information_view_context:arm:Product_and_manufacturing_information_view_context_arm.Model_based_view_context"/>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_and_manufacturing_information_with_nominal_3d_models_arm  Entity: Nominal_3d_model_shape  Where: IP1 
   <express_ref linkend="product_and_manufacturing_information_with_nominal_3d_models:arm:Product_and_manufacturing_information_with_nominal_3d_models_arm.Nominal_3d_model_shape.wr:WR2"/>

  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_and_manufacturing_information_with_nominal_3d_models_arm.Nominal_3d_model_shape.wr:IP1">
	Any definitional shape associated with a
	<express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/>
	that refers to a <b>Nominal_3d_model_shape</b>
	shall be treated as nominal shape and 
	any associated geometry shall represent nominal geometry. 
</ext_description>

