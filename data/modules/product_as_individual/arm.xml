<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1164">
   <application name="JSDAI" owner="LKSoft" source="product_as_individual_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Product_as_individual_arm">
      <interface kind="use" schema="Configuration_item_arm"/>
      <interface kind="use" schema="Identification_assignment_arm"/>
      <interface kind="use" schema="Product_identification_arm"/>
      <interface kind="use" schema="Product_version_arm"/>
      <interface kind="use" schema="Product_view_definition_arm"/>
      <type name="product_as_individual_identification_item">
         <select basedon="identification_item" selectitems="Product_as_individual Product_as_individual_version"/>
      </type>
      <type name="product_design_view_or_configuration">
         <select selectitems="Product_view_definition Product_configuration"/>
      </type>
      <type name="product_version_or_configuration">
         <select selectitems="Product_version Product_configuration"/>
      </type>
      <entity name="Product_as_individual" supertypes="Product"/>
      <entity abstract.supertype="YES" name="Product_as_individual_version" super.expression="ONEOF (Product_as_planned, Product_as_realized)" supertypes="Product_version">
         <explicit name="of_product">
            <typename name="Product_as_individual"/>
            <redeclaration entity-ref="Product_version"/>
         </explicit>
      </entity>
      <entity name="Product_as_individual_view" supertypes="Product_view_definition">
         <explicit name="defined_version">
            <typename name="Product_as_individual_version"/>
            <redeclaration entity-ref="Product_view_definition"/>
         </explicit>
      </entity>
      <entity name="Product_as_planned" supertypes="Product_as_individual_version"/>
      <entity name="Product_as_realized" supertypes="Product_as_individual_version"/>
      <entity name="Product_design_to_individual">
         <explicit name="product_design">
            <typename name="Product"/>
         </explicit>
         <explicit name="individual_product">
            <typename name="Product_as_individual"/>
         </explicit>
      </entity>
      <entity name="Product_design_version_to_individual">
         <explicit name="product_design_version">
            <typename name="product_version_or_configuration"/>
         </explicit>
         <explicit name="individual_product">
            <typename name="Product_as_individual_version"/>
         </explicit>
      </entity>
      <entity name="Product_design_view_to_individual">
         <explicit name="product_as_individual_view">
            <typename name="Product_as_individual_view"/>
         </explicit>
         <explicit name="product_view_definition">
            <typename name="product_design_view_or_configuration"/>
         </explicit>
      </entity>
      <entity name="Product_planned_to_realized">
         <explicit name="planned_product">
            <typename name="Product_as_planned"/>
         </explicit>
         <explicit name="realized_product">
            <typename name="Product_as_realized"/>
         </explicit>
      </entity>
   </schema>
</express>
