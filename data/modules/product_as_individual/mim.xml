<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="mim_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$">
   <application name="JSDAI" owner="LKSoft" source="product_as_individual_mim schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Product_as_individual_mim">
      <interface kind="use" schema="Configuration_item_mim"/>
      <interface kind="use" schema="Identification_assignment_mim"/>
      <interface kind="use" schema="product_definition_schema">
         <interfaced.item name="product"/>
         <interfaced.item name="product_definition"/>
         <interfaced.item name="product_definition_formation"/>
         <interfaced.item name="product_definition_formation_relationship"/>
         <interfaced.item name="product_definition_relationship"/>
         <interfaced.item name="product_related_product_category"/>
         <interfaced.item name="product_relationship"/>
      </interface>
      <interface kind="use" schema="Product_identification_mim"/>
      <interface kind="use" schema="Product_version_mim"/>
      <interface kind="use" schema="Product_view_definition_mim"/>
      <type name="product_as_individual_identification_item">
         <select basedon="identification_item" selectitems="product product_definition_formation"/>
      </type>
      <entity name="product_as_planned" supertypes="product_definition_formation"/>
      <entity name="product_design_to_individual" supertypes="product_relationship">
         <explicit name="individual_product">
            <typename name="product"/>
            <redeclaration entity-ref="product_relationship" old_name="related_product"/>
         </explicit>
         <explicit name="product_design">
            <typename name="product"/>
            <redeclaration entity-ref="product_relationship" old_name="relating_product"/>
         </explicit>
      </entity>
      <entity name="product_design_version_to_individual" supertypes="product_definition_formation_relationship">
         <explicit name="individual_product">
            <typename name="product_definition_formation"/>
            <redeclaration entity-ref="product_definition_formation_relationship" old_name="related_product_definition_formation"/>
         </explicit>
         <explicit name="product_design_version">
            <typename name="product_definition_formation"/>
            <redeclaration entity-ref="product_definition_formation_relationship" old_name="relating_product_definition_formation"/>
         </explicit>
      </entity>
      <entity name="product_planned_to_realized" supertypes="product_definition_formation_relationship">
         <explicit name="planned_product">
            <typename name="product_definition_formation"/>
            <redeclaration entity-ref="product_definition_formation_relationship" old_name="relating_product_definition_formation"/>
         </explicit>
         <explicit name="realized_product">
            <typename name="product_definition_formation"/>
            <redeclaration entity-ref="product_definition_formation_relationship" old_name="related_product_definition_formation"/>
         </explicit>
      </entity>
   </schema>
</express>
