<schema name="Product_as_individual_mim">
<interface kind="use" schema="classification_schema">
<interfaced.item name="classification" />
<interfaced.item name="classified_select" />
</interface>
<interface kind="use" schema="Identification_assignment_mim">
</interface>
<interface kind="use" schema="product_definition_schema">
<interfaced.item name="product_definition_formation" />
<interfaced.item name="product_definition_formation_relationship" />
</interface>
<type name="product_as_individual_classified_item">
<select basedon="classified_select" selectitems="product_as_individual product_design_to_individual product_planned_to_realized" />
<graphic.element page="3" xcoord="488" ycoord="453" />
