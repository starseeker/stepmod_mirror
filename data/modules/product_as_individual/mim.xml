<type name="product_as_individual_identification_item">
<select basedon="identification_item" selectitems="product_as_individual" />
<graphic.element page="3" xcoord="269" ycoord="453" />
</type>
<entity name="product_as_individual" abstract.supertype="YES" supertypes="product_definition_formation" super.expression="ONEOF(product_as_planned, product_as_realized)">
<graphic.element page="2" xcoord="141" ycoord="361" />
</entity>
<entity name="product_as_planned" supertypes="product_as_individual">
<graphic.element page="2" xcoord="85" ycoord="162" />
</entity>
<entity name="product_as_realized" supertypes="product_as_individual">
<graphic.element page="2" xcoord="765" ycoord="162" />
</entity>
<entity name="product_design_to_individual" supertypes="product_definition_formation_relationship">
<explicit name="individual_product">
<typename name="product_as_individual" />
<redeclaration entity-ref="product_definition_formation_relationship" old_name="related_product_definition_formation" />
</explicit>
<explicit name="product_version">
<typename name="product_definition_formation" />
<redeclaration entity-ref="product_definition_formation_relationship" old_name="relating_product_definition_formation" />
</explicit>
<graphic.element page="2" xcoord="538" ycoord="396" />
</entity>
<entity name="product_planned_to_realized" supertypes="product_definition_formation_relationship">
<explicit name="planned_product">
<typename name="product_as_planned" />
<redeclaration entity-ref="product_definition_formation_relationship" old_name="relating_product_definition_formation" />
</explicit>
<explicit name="realized_product">
<typename name="product_as_realized" />
<redeclaration entity-ref="product_definition_formation_relationship" old_name="related_product_definition_formation" />
</explicit>
<graphic.element page="2" xcoord="418" ycoord="162" />
</entity>
<graphic.element page="1" xcoord="277" ycoord="630" />
