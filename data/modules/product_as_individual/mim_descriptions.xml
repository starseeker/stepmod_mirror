    Schema: Product_as_individual_mim  Entity: product_planned_to_realized  Attribute: realized_product 
    <express_ref linkend="product_as_individual:mim:Product_as_individual_mim.product_realized_to_decommissioned.realized_product"/>
    ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
  <ext_description linkend="Product_as_individual_mim.product_realized_to_decommissioned.realized_product">
    the realized product represented by a
    <express_ref 
