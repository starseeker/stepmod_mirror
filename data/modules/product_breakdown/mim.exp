(* UNDER DEVELOPMENT *)
SCHEMA Product_breakdown_mim;

USE FROM management_resources_schema
(group_assignment);

USE FROM product_definition_schema
(product_definition,
 product_definition_relationship);

USE FROM Product_identification_mim;

USE FROM Product_version_mim;

USE FROM Product_view_definition_mim;

TYPE group_item = EXTENSIBLE SELECT 
(product_definition,
 product_definition_relationship);
END_TYPE;

ENTITY applied_group_assignment
SUBTYPE OF (group_assignment);
items : SET [1 : ?] OF group_item;
END_ENTITY;

ENTITY breakdown_context
SUBTYPE OF (product_definition_relationship);
END_ENTITY;

ENTITY breakdown_element_usage
SUBTYPE OF (product_definition_relationship);
END_ENTITY;

ENTITY breakdown_of
SUBTYPE OF (product_definition_relationship);
END_ENTITY;

ENTITY product_definition_element_relationship
SUBTYPE OF (product_definition_relationship);
END_ENTITY;

END_SCHEMA;
