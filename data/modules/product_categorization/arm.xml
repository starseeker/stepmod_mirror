<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.15 2002/06/18 11:51:06 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.15"
    source="../data/modules/product_categorization/arm.exp"/>

  <schema name="Product_categorization_arm">
    <entity
      name="Product_category">
      <explicit
        name="id"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="Product_category_hierarchy">
      <explicit
        name="super_category">
        <typename
          name="Product_category"/>
      </explicit>
      <explicit
        name="sub_category">
        <typename
          name="Product_category"/>
      </explicit>
    </entity>

  </schema>

</express>
