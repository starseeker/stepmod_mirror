<?xml version="1.0" encoding="utf-8"?>
  <?xml-stylesheet 
  type="text/xsl" 
  href="../../../xsl/descriptions.xsl"?>
<?DOCTYPE 
  express SYSTEM "../../../dtd/description.dtd"?>
<ext_descriptions module_directory="product_concept_identification" schema_file="arm.xml" rcs.date="$Date: $" rcs.revision="$Revision: $">


  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_concept_identification_arm 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
   <ext_description linkend="Product_concept_identification_arm"/>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_concept_identification_arm  Type: product_concept_organization_or_person_in_organization_item 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_concept_identification_arm.product_concept_organization_or_person_in_organization_item">
				This select extends the assignments of an organization or a person in an organization to Product_concept.
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_concept_identification_arm  Type: product_concept_project_item 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_concept_identification_arm.product_concept_project_item">
				This select extends the assignments of projects to Product_concept.
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_concept_identification_arm  Entity: Market 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_concept_identification_arm.Market">
				A <b>Market</b> is an object which identifies all or a specific subset of users/ consumers of proposed or realized product.
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_concept_identification_arm  Entity: Market  Attribute: name 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_concept_identification_arm.Market.name">
					words by which the market is known.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_concept_identification_arm  Entity: Market  Attribute: description 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_concept_identification_arm.Market.description">
					text which describes the market.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_concept_identification_arm  Entity: Market  Attribute: type 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_concept_identification_arm.Market.type">
					an identification of the kind of market.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_concept_identification_arm  Entity: Product_concept 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_concept_identification_arm.Product_concept">
				A <b>Product_concept</b> is a conceptual model of something an organization has identified as a general class of end item product for a market. A Product_concept is uniquely identified by an identifier and an organizational scope of that identifier.
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_concept_identification_arm  Entity: Product_concept  Attribute: id 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_concept_identification_arm.Product_concept.id">
					the identification of the Product_concept assigned by the organization.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_concept_identification_arm  Entity: Product_concept  Attribute: name 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_concept_identification_arm.Product_concept.name">
					words by which the product is known.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_concept_identification_arm  Entity: Product_concept  Attribute: description 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_concept_identification_arm.Product_concept.description">
					text which describes the product.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_concept_identification_arm  Entity: Product_concept  Attribute: target_market 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_concept_identification_arm.Product_concept.target_market">
					the Market for which the Product_concept is intended.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_concept_identification_arm  Entity: Product_concept  Attribute: identifier 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_concept_identification_arm.Product_concept.identifier">
					the organization (or person in an organization) that assigned and owns the Product_concept.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_concept_identification_arm  Entity: Product_concept  Attribute: projects 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_concept_identification_arm.Product_concept.projects">
					the optional set of projects related to the Product_concept.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_concept_identification_arm  Entity: Product_concept  Where: WR1 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_concept_identification_arm.Product_concept.wr:WR1">
					The <b>identifier</b> role shall be 'concept owner'.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_concept_identification_arm  Entity: Product_concept  Unique: UR1 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_concept_identification_arm.Product_concept.ur:UR1">
					The combination of the id, and identifier shall be unique.
				</ext_description>
</ext_descriptions>