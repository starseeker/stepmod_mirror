<?xml version="1.0"?><!-- $Id: mim.xml,v 1.3 2002/01/16 15:01:00 robbod Exp $ --><?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?><!DOCTYPE express SYSTEM "../../../dtd/express.dtd"><express language_version="2" rcs.date="$Date: 2002/01/16 15:01:00 $" rcs.revision="$Revision: 1.3 $">	<application name="express2xml.js" owner="Eurostep Limited" url="http://www.eurostep.com" version="1.6" source="../data/modules/product_concept_identification/mim.exp"/>	<schema name="Product_concept_identification_mim">		<interface kind="use" schema="Person_organisation_assignment_mim"/>		<interface kind="use" schema="Project_mim"/>		<interface kind="use" schema="product_concept_schema">			<interfaced.item name="product_concept"/>		</interface>		<interface kind="use" schema="application_context_schema">			<interfaced.item name="product_concept_context"/>		</interface>		<type name="product_concept_organization_item">			<description>				A <b>product_concept_organization_item</b> extends organization assignment to <b>product_concept</b> instances.			</description>			<select basedon="organization_item" selectitems="product_concept"/>		</type>		<type name="product_concept_person_and_organization_item">			<description>				A <b>product_concept_person_and_organization_item</b> extends person and organization assignment to <b>product_concept</b> instances.			</description>			<select basedon="person_and_organization_item" selectitems="product_concept"/>		</type>		<type name="product_concept_organizational_project_item">			<description>				A <b>product_concept_organizational_project_item</b> extends organizational project assignment to <b>product_concept</b> instances.			</description>			<select basedon="organizational_project_item" selectitems="product_concept"/>		</type>	</schema></express>