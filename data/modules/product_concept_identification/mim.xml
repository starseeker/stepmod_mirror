<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.2 2001/11/21 15:38:19 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.2"
    source="../data/modules/product_concept_identification/mim.exp"/>

  <schema name="Product_concept_identification_mim">
    <interface
      kind="use"
      schema="Person_organisation_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="Project_mim">
    </interface>

    <interface
      kind="use"
      schema="product_concept_schema">
      <interfaced.item
        name="product_concept"/>
    </interface>

    <interface
      kind="use"
      schema="application_context_schema">
      <interfaced.item
        name="product_concept_context"/>
    </interface>

    <type name="product_concept_organization_item">
      <select
        basedon="organization_item"
        selectitems="product_concept">
      </select>

    </type>
    <type name="product_concept_person_and_organization_item">
      <select
        basedon="person_and_organization_item"
        selectitems="product_concept">
      </select>

    </type>
    <type name="product_concept_organizational_project_item">
      <select
        basedon="organizational_project_item"
        selectitems="product_concept">
      </select>

    </type>
  </schema>

</express>
