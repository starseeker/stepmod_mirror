  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_definition_mim  Entity: environment_definition  
   <express_ref linkend="product_environment_definition:mim:Product_environment_definition_mim.environment_definition"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_definition_mim.environment_definition">
        A <b>environment_definition</b> is a type of
  <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product"/> 
        
  that is used to identify the definition of typical environment in which 
  a product is to be, has been or is planned to be deployed in, operated in, or supported in.
</ext_description>
  
  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_definition_mim  Entity: environment_definition_version  
   <express_ref linkend="product_environment_definition:mim:Product_environment_definition_mim.environment_definition_version"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_definition_mim.environment_definition_version">
        A <b>environment_definition_version</b> is a type of 
  <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_formation"/>
 
  that identifies a particular version of an 
  <express_ref linkend="product_environment_definition:mim:Product_environment_definition_mim.environment_definition"/>.
</ext_description>
  
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_definition_mim  Entity: environment_definition_version  Attribute: of_environment 
   <express_ref linkend="product_environment_definition:mim:Product_environment_definition_mim.environment_definition_version.of_environment"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_definition_mim.environment_definition_version.of_environment">
  the 
  <express_ref
    linkend="product_environment_definition:mim:Product_environment_definition_mim.environment_definition"/>
  for which the <b>environment_definition_version</b> identifies a version. 
</ext_description>
  
  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_definition_mim  Entity: environment_definition_view  
   <express_ref linkend="product_environment_definition:mim:Product_environment_definition_mim.environment_definition_view"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_definition_mim.environment_definition_view">
        A <b>environment_definition_view</b> is a type of
  <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition"/> 
  and a type of 
  <express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.characterized_object"/>
  
  
  that provides a view of a version of an environment definition relevant
  for one or more application domains. This view collects the
  characteristics that define an environment.
</ext_description>
  
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_definition_mim  Entity: environment_definition_view  Attribute: defined_environment_version 
   <express_ref linkend="product_environment_definition:mim:Product_environment_definition_mim.environment_definition_view.defined_environment_version"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_definition_mim.environment_definition_view.defined_environment_version">
  the
  <express_ref
    linkend="product_environment_definition:mim:Product_environment_definition_mim.environment_definition_version"/>
  that the <b>environment_definition_view</b> defines. 
  
</ext_description>
  
  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_definition_mim  Entity: environment_assignment  
   <express_ref linkend="product_environment_definition:mim:Product_environment_definition_mim.environment_assignment"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_definition_mim.environment_assignment">
        A <b>environment_assignment</b> is a type of 
  <express_ref linkend="group_schema:ir_express:group_schema.group"/>
         
  that assigns a product to the 
  environment in which it operates or it is supported.
</ext_description>
  
  
  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_definition_mim  Entity: environment_view_definition_relationship  
   <express_ref linkend="product_environment_definition:mim:Product_environment_definition_mim.environment_view_definition_relationship"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_definition_mim.environment_view_definition_relationship">
        A <b>environment_view_definition_relationship</b> is a type of 
  <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_relationship"/> 
        
  that represents an association between two
  <express_ref
    linkend="product_environment_definition:mim:Product_environment_definition_mim.environment_definition_view"/>
  objects.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_definition_mim  Entity: environment_view_definition_relationship  Attribute: related_environment 
   <express_ref linkend="product_environment_definition:mim:Product_environment_definition_mim.environment_view_definition_relationship.related_environment"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_definition_mim.environment_view_definition_relationship.related_environment">
  the other instance of  
  <express_ref linkend="product_environment_definition:mim:Product_environment_definition_mim.environment_definition_view"/>
  that is part of the relationship. If one element of the relationship is
  dependent upon the other, this attribute shall be the dependent one.  
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_definition_mim  Entity: environment_view_definition_relationship  Attribute: relating_environment 
   <express_ref linkend="product_environment_definition:mim:Product_environment_definition_mim.environment_view_definition_relationship.relating_environment"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_definition_mim.environment_view_definition_relationship.relating_environment">
  one of the instances of 
  <express_ref linkend="product_environment_definition:mim:Product_environment_definition_mim.environment_definition_view"/>
  that is a part of the relationship. 
</ext_description>
</ext_descriptions>
