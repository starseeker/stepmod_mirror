</ext_description><!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_arm  Entity: Observed_environment_to_definition  
   <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_to_definition"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_arm.Observed_environment_to_definition">
  An <b>Observed_environment_to_definition</b> is a relationship between a record of a set of observations about an
  environment, represented by an 
  <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment"/>,
  and a typical environment, represented by an
  <express_ref
    linkend="product_environment_definition:arm:Product_environment_definition_arm.Environment_definition"/>.
  The typical environment is the expected environment about which
  observations have been made.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_arm  Entity: Observed_environment_to_definition  Attribute: defined_environment 
   <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_to_definition.defined_environment"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_arm.Observed_environment_to_definition.defined_environment">
  the typical environment about which the recorded observations have been made.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_arm  Entity: Observed_environment_to_definition  Attribute: observed_environment 
   <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_to_definition.observed_environment"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_arm.Observed_environment_to_definition.observed_environment">
  the record of observations about an environment.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_arm  Entity: Observed_environment_to_definition_version  
   <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_to_definition_version"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_arm.Observed_environment_to_definition_version">
  An <b>Observed_environment_to_definition_version</b> is a relationship between a version
  of the record of a set of observations about an
  environment, represented by an 
  <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_version"/>,
  and a version of the typical environment, represented by 
  <express_ref
    linkend="product_environment_definition:arm:Product_environment_definition_arm.Environment_definition_version"/>.
  The typical environment is the expected environment about which
  observations have been made.
      </ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_arm  Entity: Observed_environment_to_definition_version  Attribute: defined_environment_version 
   <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_to_definition_version.defined_environment_version"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_arm.Observed_environment_to_definition_version.defined_environment_version">
  the version of the typical environment about which the recorded observations have been made. 
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_arm  Entity: Observed_environment_to_definition_version  Attribute: observed_environment_version 
   <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_to_definition_version.observed_environment_version"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_arm.Observed_environment_to_definition_version.observed_environment_version">
  the version of the record of observations about an environment.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_arm  Entity: Observed_environment_to_definition_view  
   <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_to_definition_view"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_arm.Observed_environment_to_definition_view">
  An <b>Observed_environment_to_definition_view</b> is a relationship between a view
  of the record of a set of observations about an
  environment, represented by an 
  <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_view"/>,
  and a view of the typical environment, represented by 
  <express_ref
    linkend="product_environment_definition:arm:Product_environment_definition_arm.Environment_definition_view"/>.
  The typical environment is the expected environment about which
  observations have been made.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_arm  Entity: Observed_environment_to_definition_view  Attribute: defined_environment_view 
   <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_to_definition_view.defined_environment_view"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_arm.Observed_environment_to_definition_view.defined_environment_view">
  the view of the typical environment about which the recorded observations have been made. 
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_arm  Entity: Observed_environment_to_definition_view  Attribute: observed_environment_view 
   <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_to_definition_view.observed_environment_view"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_arm.Observed_environment_to_definition_view.observed_environment_view">
  the view of the record of observations about an environment.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_arm  Entity: Observed_environment  
   <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_arm.Observed_environment">
  An <b>Observed_environment</b> is a type of
  <express_ref linkend="product_identification:arm:Product_identification_arm.Product"/>
  that represents a record of observations about the environment in which a
  product is or has been operating.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_arm  Entity: Observed_environment_assignment  
   <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_assignment"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_arm.Observed_environment_assignment">
  An <b>Observed_environment_assignment</b> is an assignment
  of a product to the environment in which is or has operated. 
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_arm  Entity: Observed_environment_assignment  Attribute: environment 
   <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_assignment.environment"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_arm.Observed_environment_assignment.environment">
a record of the environment, 
<express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_view"/>,
in which the product is supported or operates. 
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_arm  Entity: Observed_environment_assignment  Attribute: item 
   <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_assignment.item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_arm.Observed_environment_assignment.item">
  the product that is or was operating in the environment.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_arm  Entity: Observed_environment_version  
   <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_version"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_arm.Observed_environment_version">
  An <b>Observed_environment_version</b> is a type of
  <express_ref linkend="product_version:arm:Product_version_arm.Product_version"/>
  that identifies a particular version of an 
  <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment"/>.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_arm  Entity: Observed_environment_version  Attribute: of_environment 
   <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_version.of_environment"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_arm.Observed_environment_version.of_environment">
  the 
  <express_ref
    linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment"/>
  for which the <b>Observed_environment_version</b> identifies a version.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_arm  Entity: Observed_environment_view  
   <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_view"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_arm.Observed_environment_view">
  An <b>Observed_environment_view</b> is a type of
  <express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/>
    that provides a view of a version of an observed environment relevant
    for one or more application domains. This view collects the
    characteristics of the observations on the environment.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_arm  Entity: Observed_environment_view  Attribute: observed_environment_version 
   <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_view.observed_environment_version"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_arm.Observed_environment_view.observed_environment_version">
  the 
  <express_ref
    linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_version"/>
  of the <b>Observed_environment_view</b>. 
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_arm  Entity: Observed_environment_view_definition_relationship  
   <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_view_definition_relationship"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_arm.Observed_environment_view_definition_relationship">
  An <b>Observed_environment_view_definition_relationship</b> is a type of
       <express_ref
    linkend="product_view_definition_relationship:arm:Product_view_definition_relationship_arm.View_definition_relationship"/>
  that represents an association between two
  <express_ref
    linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_view"/> 
  objects.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_arm  Entity: Observed_environment_view_definition_relationship  Attribute: relating_environment 
   <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_view_definition_relationship.relating_environment"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_arm.Observed_environment_view_definition_relationship.relating_environment">
  one of the instances of 
  <express_ref
    linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_view"/> 
  that is a part of the relationship. 
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_arm  Entity: Observed_environment_view_definition_relationship  Attribute: related_environment 
   <express_ref linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_view_definition_relationship.related_environment"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_arm.Observed_environment_view_definition_relationship.related_environment">
  the other instance of  
  <express_ref
    linkend="product_environment_observed:arm:Product_environment_observed_arm.Observed_environment_view"/> 
  that is part of the relationship. If one element of the relationship is
  dependent upon the other, this attribute shall be the dependent one.  
</ext_description>
</ext_descriptions>
