</ext_description><!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_mim  Entity: observed_environment  
   <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_mim.observed_environment">
        A <b>observed_environment</b> is a type of
  <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product" /> that represents a record of observations about the environment in which a
  product is or has been operating.
</ext_description><!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_mim  Entity: observed_environment_assignment  
   <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_assignment"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_mim.observed_environment_assignment">
        A <b>observed_environment_assignment</b> is a type of
  <express_ref linkend="group_schema:ir_express:group_schema.group"/> that assigns
  a product to the environment in which is or has operated. 
</ext_description><!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_mim  Entity: observed_environment_to_definition  
   <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_to_definition"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_mim.observed_environment_to_definition">
        A <b>observed_environment_to_definition</b> is a type of
  <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_relationship" /> 
  that relates an 
  <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment"/> to an 
  <express_ref linkend="product_environment_definition:mim:Product_environment_definition_mim.environment_definition"/>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_mim  Entity: observed_environment_to_definition  Attribute: defined_environment 
   <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_to_definition.defined_environment"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_mim.observed_environment_to_definition.defined_environment">
  the <express_ref linkend="product_environment_definition:mim:Product_environment_definition_mim.environment_definition"/> 
  about which the recorded observations have been made.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_mim  Entity: observed_environment_to_definition  Attribute: observed_environment 
   <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_to_definition.observed_environment"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_mim.observed_environment_to_definition.observed_environment">
  the <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment"/> that comprises the set of recorded observations.
</ext_description>
  
  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_mim  Entity: observed_environment_to_definition_version  
   <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_to_definition_version"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_mim.observed_environment_to_definition_version">
        A <b>observed_environment_to_definition_version</b> is a type of
  <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_formation_relationship" /> 
  that relates an <express_ref linkend="product_environment_definition:mim:Product_environment_definition_mim.environment_definition_version"/> 
  to an <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_version"/>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_mim  Entity: observed_environment_to_definition_version  Attribute: defined_environment_version 
   <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_to_definition_version.defined_environment_version"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_mim.observed_environment_to_definition_version.defined_environment_version">
  the <express_ref linkend="product_environment_definition:mim:Product_environment_definition_mim.environment_definition_version"/> about which the recorded observations have been made. 
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_mim  Entity: observed_environment_to_definition_version  Attribute: observed_environment_version 
   <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_to_definition_version.observed_environment_version"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_mim.observed_environment_to_definition_version.observed_environment_version">
  the <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_version"/> 
  that comprises the set of recorded observations.
</ext_description>
  
  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_mim  Entity: observed_environment_to_definition_view  
   <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_to_definition_view"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_mim.observed_environment_to_definition_view">
        A <b>observed_environment_to_definition_view</b> is a type of
  <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_relationship" /> that 
  relates an <express_ref linkend="product_environment_definition:mim:Product_environment_definition_mim.environment_definition_view"/> 
  to an <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_view"/>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_mim  Entity: observed_environment_to_definition_view  Attribute: defined_environment_view 
   <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_to_definition_view.defined_environment_view"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_mim.observed_environment_to_definition_view.defined_environment_view">
  the <express_ref linkend="product_environment_definition:mim:Product_environment_definition_mim.environment_definition_view"/> 
    about which the recorded observations have been made. 
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_mim  Entity: observed_environment_to_definition_view  Attribute: observed_environment_view 
   <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_to_definition_view.observed_environment_view"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_mim.observed_environment_to_definition_view.observed_environment_view">
  the <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_view"/>
  that comprises the set of recorded observations.
</ext_description>
  
  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_mim  Entity: observed_environment_version  
   <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_version"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_mim.observed_environment_version">
        A <b>observed_environment_version</b> is a type of
  <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_formation" /> 
  that represents a version of the record of observations about an environment.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_mim  Entity: observed_environment_version  Attribute: of_environment 
   <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_version.of_environment"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_mim.observed_environment_version.of_environment">
  the <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment"/> 
  for which a version is identified.
</ext_description>
  
  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_mim  Entity: observed_environment_view  
   <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_view"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_mim.observed_environment_view">
        A <b>observed_environment_view</b> is a type of
  <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition" /> 
  and a type of 
  <express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.characterized_object"/> 
  that represents a view of the record of observations about an environment.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_mim  Entity: observed_environment_view  Attribute: observed_environment_version 
   <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_view.observed_environment_version"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_mim.observed_environment_view.observed_environment_version">
  the <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_version"/> 
  that represents a version of the record of observations about an environment.
</ext_description>
  
  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_mim  Entity: observed_environment_view_definition_relationship  
   <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_view_definition_relationship"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_mim.observed_environment_view_definition_relationship">
        A <b>observed_environment_view_definition_relationship</b> is a type of
  <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_relationship" /> 
  that represents an association between two 
  <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_view"/> objects.
  
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_mim  Entity: observed_environment_view_definition_relationship  Attribute: relating_environment 
   <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_view_definition_relationship.relating_environment"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_mim.observed_environment_view_definition_relationship.relating_environment">
  one of the instances of 
  <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_view"/> 
  that is a part of the relationship. 
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_environment_observed_mim  Entity: observed_environment_view_definition_relationship  Attribute: related_environment 
   <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_view_definition_relationship.related_environment"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_environment_observed_mim.observed_environment_view_definition_relationship.related_environment">
  the other instance of  
  <express_ref linkend="product_environment_observed:mim:Product_environment_observed_mim.observed_environment_view"/> 
  that is part of the relationship. If one element of the relationship is
  dependent upon the other, this attribute shall be the dependent one.  
</ext_description>
</ext_descriptions>
