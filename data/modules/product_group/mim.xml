<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="mim_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1278">
   <application name="JSDAI" owner="LKSoft" source="product_group_mim schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Product_group_mim">
      <interface kind="use" schema="basic_attribute_schema">
         <interfaced.item name="id_attribute"/>
      </interface>
      <interface kind="use" schema="Classification_assignment_mim"/>
      <interface kind="use" schema="Effectivity_application_mim"/>
      <interface kind="use" schema="group_schema">
         <interfaced.item name="group"/>
         <interfaced.item name="group_relationship"/>
      </interface>
      <interface kind="use" schema="management_resources_schema">
         <interfaced.item name="group_assignment"/>
      </interface>
      <interface kind="use" schema="Product_as_individual_mim"/>
      <interface kind="use" schema="Product_concept_identification_mim"/>
      <interface kind="use" schema="product_concept_schema">
         <interfaced.item name="product_concept"/>
      </interface>
      <interface kind="use" schema="product_definition_schema">
         <interfaced.item name="product"/>
         <interfaced.item name="product_definition_formation"/>
      </interface>
      <interface kind="use" schema="Product_identification_mim"/>
      <interface kind="use" schema="product_property_definition_schema">
         <interfaced.item name="characterized_object"/>
      </interface>
      <interface kind="use" schema="Product_version_mim"/>
      <interface kind="use" schema="Property_assignment_mim"/>
      <interface kind="use" schema="qualified_measure_schema">
         <interfaced.item name="descriptive_representation_item"/>
      </interface>
      <type name="attribute_select">
         <select selectitems="product_group_context product_group_purpose"/>
      </type>
      <type name="product_group_classification_item">
         <select basedon="classification_item" selectitems="product_group product_group_relationship"/>
      </type>
      <type name="product_group_effectivity_item">
         <select basedon="effectivity_item" selectitems="product_group product_group_membership product_group_relationship"/>
      </type>
      <type name="product_select">
         <select extensible="YES" genericentity="YES" selectitems="product product_concept product_definition_formation product_group"/>
      </type>
      <entity name="product_group" supertypes="group"/>
      <entity name="product_group_attribute_assignment" supertypes="group_assignment">
         <explicit name="assigned_group">
            <typename name="product_group_attributes"/>
            <redeclaration entity-ref="group_assignment"/>
         </explicit>
         <explicit name="attributes">
            <aggregate lower="1" type="SET" upper="1"/>
            <typename name="attribute_select"/>
         </explicit>
      </entity>
      <entity name="product_group_attribute_set" supertypes="group_assignment">
         <explicit name="assigned_group">
            <typename name="product_group"/>
            <redeclaration entity-ref="group_assignment"/>
         </explicit>
         <explicit name="product_group_attributes">
            <aggregate lower="1" type="SET" upper="1"/>
            <typename name="product_group_attributes"/>
         </explicit>
      </entity>
      <entity name="product_group_attributes" supertypes="group"/>
      <entity name="product_group_context" supertypes="descriptive_representation_item"/>
      <entity name="product_group_membership" supertypes="characterized_object group_assignment">
         <explicit name="member">
            <typename name="product_select"/>
         </explicit>
      </entity>
      <entity name="product_group_membership_rules" supertypes="group_assignment">
         <explicit name="assigned_group">
            <typename name="product_group"/>
            <redeclaration entity-ref="group_assignment"/>
         </explicit>
         <explicit name="product_group_rules">
            <aggregate lower="1" type="SET" upper="1"/>
            <typename name="product_group_rules"/>
         </explicit>
      </entity>
      <entity name="product_group_purpose" supertypes="descriptive_representation_item"/>
      <entity name="product_group_relationship" supertypes="group_relationship"/>
      <entity name="product_group_rule" supertypes="descriptive_representation_item"/>
      <entity name="product_group_rule_assignment" supertypes="group_assignment">
         <explicit name="assigned_group">
            <typename name="product_group_rules"/>
            <redeclaration entity-ref="group_assignment"/>
         </explicit>
         <explicit name="product_group_rules">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="product_group_rule"/>
         </explicit>
      </entity>
      <entity name="product_group_rules" supertypes="group"/>
   </schema>
</express>
