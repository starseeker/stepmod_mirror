<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_group_mim  Entity: product_group  
   <express_ref linkend="product_group:mim:Product_group_mim.product_group"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_group_mim.product_group">
  A <b>product_group</b> identifies a set of
<express_ref
  linkend="product_definition_schema:ir_express:product_definition_schema.product"/>s,
 <express_ref
   linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_formation"/>s,
<express_ref
linkend="product_concept_schema:ir_express:product_concept_schema.product_concept"/>s,
<express_ref
  linkend="product_as_individual:mim:Product_as_individual_mim.product_as_individual"/>s
  that have been grouped together.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_group_mim  Entity: product_group  Attribute: membership_rule 
   <express_ref linkend="product_group:mim:Product_group_mim.product_group.membership_rule"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_group_mim.product_group.membership_rule">
  the list of criteria for a product to be a member of the group.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_group_mim  Entity: product_group  Attribute: product_group_context 
   <express_ref linkend="product_group:mim:Product_group_mim.product_group.product_group_context"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_group_mim.product_group.product_group_context">
  the text describing the context in which the product group is formed.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_group_mim  Entity: product_group  Attribute: purpose 
   <express_ref linkend="product_group:mim:Product_group_mim.product_group.purpose"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_group_mim.product_group.purpose">
  the purpose of this product group.
  <example>all individual products which are subject of a planned maintenance.</example>
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_group_mim  Entity: product_group_membership  
   <express_ref linkend="product_group:mim:Product_group_mim.product_group_membership"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_group_mim.product_group_membership">
   The <b>product_group_membership</b> identifies that an instance of an entity defined in
  the type
  <express_ref linkend="product_group:mim:Product_group_mim.product_select"/>
   belongs to a 
  <express_ref linkend="product_group:mim:Product_group_mim.product_group"/>. 
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_group_mim  Entity: product_group_membership  Attribute: member 
   <express_ref linkend="product_group:mim:Product_group_mim.product_group_membership.member"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_group_mim.product_group_membership.member">
  The instance that is a member of the 
  <express_ref linkend="product_group:mim:Product_group_mim.product_group"/>.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_group_mim  Entity: product_group_relationship  
   <express_ref linkend="product_group:mim:Product_group_mim.product_group_relationship"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_group_mim.product_group_relationship">
  <b>product_group_relationship</b> relates two 
  <express_ref linkend="product_group:mim:Product_group_mim.product_group"/>s.
  <express_ref linkend="classification_assignment:mim:Classification_assignment_mim.applied_classification_assignment"/>
  is used to specify the meaning or type of the relationship.
  <note>
    A subset is a common type of relationship used to indicate that one
    <express_ref linkend="product_group:mim:Product_group_mim.product_group"/>
    is a subset of another.
  </note>
</ext_description>

