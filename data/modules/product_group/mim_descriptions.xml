<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_group_mim  Entity: product_group  
   <express_ref linkend="product_group:mim:Product_group_mim.product_group"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Product_group_mim.product_group">
  A <b>product_group</b> identifies a set of
<express_ref
  linkend="product_definition_schema:ir_express:product_definition_schema.product"/>s,
 <express_ref
   linkend="product_definition_schema:ir_express:product_definition_schema.product_definition_formation"/>s,
<express_ref
linkend="product_concept_schema:ir_express:product_concept_schema.product_concept"/>s,
<express_ref
  linkend="product_as_individual:mim:Product_as_individual_mim.product_as_individual"/>s
  that have been grouped together.
</ext_description>


