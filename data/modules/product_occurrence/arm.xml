<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1063">
   <application name="JSDAI" owner="LKSoft" source="product_occurrence_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Product_occurrence_arm">
      <interface kind="use" schema="Assembly_structure_arm"/>
      <interface kind="use" schema="Product_view_definition_relationship_arm"/>
      <interface kind="use" schema="Extended_measure_representation_arm"/>
      <interface kind="use" schema="Specified_product_arm"/>
      <interface kind="use" schema="Part_view_definition_arm"/>
      <type name="extended_value_with_unit">
         <select extensible="YES" selectitems="Value_with_unit Value_limit Value_range"/>
      </type>
      <entity name="Definition_based_product_occurrence" supertypes="Product_occurrence">
         <explicit name="derived_from">
            <typename name="Part_view_definition"/>
         </explicit>
         <where expression="SELF\Product_view_definition.defined_version = derived_from.defined_version" label="WR1"/>
      </entity>
      <entity abstract.supertype="YES" name="Product_occurrence" super.expression="ONEOF (Definition_based_product_occurrence, Specification_based_product_occurrence, Specified_occurrence) AND ONEOF (Product_occurrence_with_quantity, Single_instance)" supertypes="Product_view_definition">
         <explicit name="defined_version">
            <typename name="Part_version"/>
            <redeclaration entity-ref="Product_view_definition"/>
         </explicit>
         <derived expression="View_definition_context('','') || Initial_view_definition_context(?)" name="initial_context">
            <typename name="Initial_view_definition_context"/>
            <redeclaration entity-ref="Product_view_definition"/>
         </derived>
         <inverse attribute="related_view" entity="Product_occurrence_definition_relationship" name="occurrence_contexts">
            <inverse.aggregate lower="0" type="SET" upper="?"/>
         </inverse>
         <inverse attribute="related_view" entity="View_definition_relationship" name="view_definition_contexts">
            <inverse.aggregate lower="0" type="SET" upper="?"/>
         </inverse>
         <where expression="SIZEOF(occurrence_contexts) + SIZEOF(view_definition_contexts) &gt;= 1" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Product_view_definition.additional_characterization)" label="WR2"/>
         <where expression="SIZEOF(QUERY( oc &lt;* occurrence_contexts |&#10;            NOT( NOT('ASSEMBLY_STRUCTURE_ARM.ASSEMBLY_COMPONENT_RELATIONSHIP' IN TYPEOF(oc)) OR&#10;                 NOT EXISTS(oc\Assembly_component_relationship.quantity))&#10;               )) = 0" label="WR3"/>
      </entity>
      <entity abstract.supertype="YES" name="Product_occurrence_with_quantity" super.expression="ONEOF (Quantified_instance, Selected_instance)" supertypes="Product_occurrence">
         <explicit name="occurrence_quantity" optional="YES">
            <typename name="extended_value_with_unit"/>
         </explicit>
      </entity>
      <entity name="Quantified_instance" supertypes="Product_occurrence_with_quantity">
         <explicit name="occurrence_quantity">
            <typename name="Value_with_unit"/>
            <redeclaration entity-ref="Product_occurrence_with_quantity"/>
         </explicit>
      </entity>
      <entity name="Selected_instance" supertypes="Product_occurrence_with_quantity">
         <explicit name="quantity_criterion" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="Single_instance" supertypes="Product_occurrence"/>
      <entity name="Specification_based_part_version" supertypes="Specification_based_product_version Part_version"/>
      <entity name="Specification_based_product_occurrence" supertypes="Product_occurrence">
         <explicit name="defined_version">
            <typename name="Specification_based_part_version"/>
            <redeclaration entity-ref="Product_view_definition"/>
         </explicit>
      </entity>
      <entity name="Specified_occurrence" supertypes="Product_occurrence">
         <explicit name="definition">
            <typename name="Product_occurrence"/>
         </explicit>
         <explicit name="upper_usage">
            <typename name="Product_occurrence"/>
         </explicit>
      </entity>
   </schema>
</express>
