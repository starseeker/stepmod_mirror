<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: 2002/07/01 15:27:30 $"
  rcs.revision="$Revision: 1.6 $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.15"
    source="../data/modules/product_relationship/arm.exp"/>

  <schema name="Product_relationship_arm">
    <interface
      kind="use"
      schema="Product_identification_arm">
    </interface>

    <entity
      name="Product_relationship">
      <explicit
        name="relation_type">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="relating_product">
        <typename
          name="Product"/>
      </explicit>
      <explicit
        name="related_product">
        <typename
          name="Product"/>
      </explicit>
    </entity>

  </schema>

</express>
