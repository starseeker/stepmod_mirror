<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/05/27 20:34:52 $"
  rcs.revision="$Revision: 1.4 $"
  description.file="arm_descriptions.xml">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.9"
    source="arm.exp"/>

  <schema name="Product_relationship_arm">
    <interface
      kind="use"
      schema="Product_identification_arm">
    </interface>

    <entity
      name="Product_relationship">
      <explicit
        name="relating_product">
        <typename
          name="Product"/>
      </explicit>
      <explicit
        name="related_product">
        <typename
          name="Product"/>
      </explicit>
      <explicit
        name="relation_type">
        <typename
          name="string"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

  </schema>

</express>
