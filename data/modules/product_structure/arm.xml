<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.26"
    source="../data/modules/product_structure/arm.exp"/>

  <schema name="Product_structure_arm">
    <interface
      kind="use"
      schema="Assembly_structure_arm">
    </interface>

    <interface
      kind="use"
      schema="Contextual_shape_positioning_arm">
    </interface>

    <interface
      kind="use"
      schema="Part_definition_relationship_arm">
    </interface>

    <interface
      kind="use"
      schema="Single_part_representation_arm">
    </interface>

    <interface
      kind="use"
      schema="Product_replacement_arm">
    </interface>

    <interface
      kind="use"
      schema="Product_version_relationship_arm">
    </interface>

    <type name="ps_product_property_assignment">
      <select
        basedon="property_assignment_select"
        selectitems="View_definition_relationship">
      </select>

    </type>
    <entity
      name="Alternate_part_relationship"
      supertypes="Alternate_product_relationship">
      <explicit
        name="alternate_product">
        <redeclaration
          entity-ref="Alternate_product_relationship"/>
          <typename
            name="Part"/>
        </explicit>
        <explicit
          name="base_product">
          <redeclaration
            entity-ref="Alternate_product_relationship"/>
            <typename
              name="Part"/>
          </explicit>
        </entity>

      </schema>

    </express>
