<ext_descriptions module_directory="product_version" schema_file="arm.xml" rcs.date="$Date: $" rcs.revision="$Revision: $">


  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_version_arm 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
   <ext_description linkend="Product_version_arm"/>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_version_arm  Type: product_version_security_classification_item 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_version_arm.product_version_security_classification_item">This select is extends a security_classification_assignment to apply to <b>Product_version</b> instances.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_version_arm  Type: product_version_organization_or_person_in_organization_item 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_version_arm.product_version_organization_or_person_in_organization_item">This select extends the assignment of an organization or a person in an organization to Product_version.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_version_arm  Type: product_version_approval_item 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_version_arm.product_version_approval_item">This select extends an approval_assignment to apply to <b>Product_version</b> instances.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_version_arm  Type: product_version_contract_item 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_version_arm.product_version_contract_item">This select extends an contract_assignment to apply to Product_version instances.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_version_arm  Entity: Product_version 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_version_arm.Product_version">A <b>Product_version</b> is the identification of a variant incarnation of a <b>Product</b> which is form, fit and functionally equivalent to it predecessors. The set of all versions of a product represents the history of the product.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_version_arm  Entity: Product_version  Attribute: id 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_version_arm.Product_version.id">the identification of the version</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_version_arm  Entity: Product_version  Attribute: description 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_version_arm.Product_version.description">the words used to describe the version</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_version_arm  Entity: Product_version  Attribute: of_product 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_version_arm.Product_version.of_product"/>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_version_arm  Entity: Product_version  Attribute: security 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_version_arm.Product_version.security">the <b>Security_classification</b> of the version</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_version_arm  Entity: Product_version  Attribute: creator 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_version_arm.Product_version.creator">the organization (and person) that initially defined the version</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_version_arm  Entity: Product_version  Attribute: custodian 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_version_arm.Product_version.custodian">the organization (and person) controls the master documentation of the version</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_version_arm  Entity: Product_version  Attribute: approval 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_version_arm.Product_version.approval">the approval(s) of the version</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_version_arm  Entity: Product_version  Attribute: contracts 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_version_arm.Product_version.contracts">the contract(s) sponsoring the version</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_version_arm  Entity: Product_version  Where: WR1 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_version_arm.Product_version.wr:WR1">the creator role shall be 'creator'</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_version_arm  Entity: Product_version  Where: WR2 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_version_arm.Product_version.wr:WR2">if the custodian exists, the custodian role shall be 'design supplier'</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_version_arm  Entity: Product_version  Unique: UR1 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_version_arm.Product_version.ur:UR1">the combination of the <b>id</b> and <b>product</b> reference shall be unique</ext_description>
</ext_descriptions>