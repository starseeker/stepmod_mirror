  <!--
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_version_arm  Entity: Product_version
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  -->
   <ext_description linkend="Product_version_arm.Product_version">A <b>Product_version</b> is the identification of a variant incarnation of a <b>Product</b> which is form, fit and functionally equivalent to it predecessors. The set of all versions of a product represents the history of the product.</ext_description>

  <!--
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_version_arm  Entity: Product_version  Attribute: id
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  -->
   <ext_description linkend="Product_version_arm.Product_version.id">the identification of the version</ext_description>

  <!--
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_version_arm  Entity: Product_version  Attribute: description
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  -->
   <ext_description linkend="Product_version_arm.Product_version.description">the words used to describe the version</ext_description>

  <!--
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_version_arm  Entity: Product_version  Attribute: of_product
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  -->
   <ext_description linkend="Product_version_arm.Product_version.of_product"/>

  <!--
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_version_arm  Entity: Product_version  Unique: UR1
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
