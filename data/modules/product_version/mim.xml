<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.2 2001/11/21 15:38:19 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.2"
    source="../data/modules/product_version/mim.exp"/>

  <schema name="Product_version_mim">
    <interface
      kind="use"
      schema="Person_organisation_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="Approval_mim">
    </interface>

    <interface
      kind="use"
      schema="Contract_mim">
    </interface>

    <interface
      kind="use"
      schema="Security_classification_mim">
    </interface>

    <interface
      kind="use"
      schema="Product_identification_mim">
    </interface>

    <interface
      kind="use"
      schema="product_definition_schema">
      <interfaced.item
        name="product_definition_formation"/>
    </interface>

    <type name="product_version_organization_item">
      <select
        basedon="organization_item"
        selectitems="product_definition_formation">
      </select>

    </type>
    <type name="product_version_person_and_organization_item">
      <select
        basedon="person_and_organization_item"
        selectitems="product_definition_formation">
      </select>

    </type>
    <type name="product_version_security_classification_item">
      <select
        basedon="security_classification_item"
        selectitems="product_definition_formation">
      </select>

    </type>
    <type name="product_version_approval_item">
      <select
        basedon="approval_item"
        selectitems="product_definition_formation">
      </select>

    </type>
    <type name="product_version_contract_item">
      <select
        basedon="contract_item"
        selectitems="product_definition_formation">
      </select>

    </type>
  </schema>

</express>
