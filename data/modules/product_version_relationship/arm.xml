<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.15"
    source="../data/modules/product_version_relationship/arm.exp"/>

  <schema name="Product_version_relationship_arm">
    <interface
      kind="use"
      schema="Product_version_arm">
    </interface>

    <entity
      name="Product_version_relationship">
      <explicit
        name="relating_version">
        <typename
          name="Product_version"/>
      </explicit>
      <explicit
        name="related_version">
        <typename
          name="Product_version"/>
      </explicit>
      <explicit
        name="relation_type">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <where
        label="WR1"
        expression="relating_version :&lt;&gt;: related_version">
      </where>
    </entity>

    <entity
      name="Supplied_part_relationship"
      supertypes="Version_relationship">
      <where
        label="WR1"
        expression="SELF\Version_relationship.relation_type IN ['supplied item', 'supplied document']">
      </where>
      <where
        label="IP1"
        >
      </where>
    </entity>

  </schema>

</express>
