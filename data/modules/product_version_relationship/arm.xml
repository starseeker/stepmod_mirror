<?xml version="1.0"?>
<!-- $Id: arm.xml,v 1.2 2002/05/28 12:48:06 buchanan Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/05/28 12:48:06 $"
  rcs.revision="$Revision: 1.2 $"
  description.file="arm_descriptions.xml">


  <schema name="Product_version_relationship_arm">
    <interface
      kind="use"
      schema="Product_version_arm">
    </interface>

    <entity
      name="Version_relationship">
      <explicit
        name="relating_version">
        <typename
          name="Product_version"/>
      </explicit>
      <explicit
        name="related_version">
        <typename
          name="Product_version"/>
      </explicit>
      <explicit
        name="relation_type">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="ENTITY">
        <typename
          name="ENTITY Version_sequence_relationship SUBTYPE OF ( Version_relationship )"/>
      </explicit>
      <where
        label="WR1"
        expression="relation_type='sequence'">
      </where>
    </entity>

    <entity
      name="Supplied_part_relationship"
      supertypes="Version_relationship">
      <where
        label="WR1"
        expression="relation_type='supplied part'">
      </where>
    </entity>

  </schema>

</express>
