<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date: 2006/02/24 15:21:55 $" rcs.revision="$Revision: 1.4 $" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="product_view_definition_arm schema_instance"/>
   <schema name="Product_view_definition_arm">
      <interface kind="use" schema="Product_version_arm"/>
      <entity name="Product_view_definition" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="id" optional="YES">
            <builtintype type="STRING" fixed="NO"/>
         </explicit>
         <explicit name="name" optional="YES">
            <builtintype type="STRING" fixed="NO"/>
         </explicit>
         <explicit name="additional_characterization" optional="YES">
            <builtintype type="STRING" fixed="NO"/>
         </explicit>
         <explicit name="initial_context" optional="NO">
            <typename name="View_definition_context"/>
         </explicit>
         <explicit name="additional_contexts" optional="NO">
            <aggregate type="SET" lower="0" upper="?" unique="NO" optional="NO"/>
            <typename name="View_definition_context"/>
         </explicit>
         <explicit name="defined_version" optional="NO">
            <typename name="Product_version"/>
         </explicit>
         <where label="WR1" expression="NOT (initial_context IN additional_contexts)"/>
         <where label="WR2" expression="EXISTS(id) OR (TYPEOF(SELF\Product_view_definition) &lt;&gt; TYPEOF(SELF))"/>
      </entity>
      <entity name="View_definition_context" abstract.supertype="NO" abstract.entity="NO">
         <explicit name="application_domain" optional="NO">
            <builtintype type="STRING" fixed="NO"/>
         </explicit>
         <explicit name="life_cycle_stage" optional="NO">
            <builtintype type="STRING" fixed="NO"/>
         </explicit>
         <explicit name="description" optional="YES">
            <builtintype type="STRING" fixed="NO"/>
         </explicit>
         <where label="WR1" expression="(SIZEOF (USEDIN(SELF, 'PRODUCT_VIEW_DEFINITION_ARM.' +&#10;    'PRODUCT_VIEW_DEFINITION.INITIAL_CONTEXT')) &gt; 0) OR&#10;    (SIZEOF (USEDIN(SELF, 'PRODUCT_VIEW_DEFINITION_ARM.' +&#10;    'PRODUCT_VIEW_DEFINITION.ADDITIONAL_CONTEXTS')) &gt; 0)"/>
      <where label="IP1"/>
      </entity>
   </schema>
</express>
