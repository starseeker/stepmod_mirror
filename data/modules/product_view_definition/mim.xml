<!DOCTYPE express
  SYSTEM "../express_model.dtd">
<express rcs.date="$Date:$" rcs.revision="$Revision:$">
   <application name="pdm_module2mim.xml" owner="PDTec GmbH" url="http://www.pdtec.de" version="0.8" source="../data/modules/approval/mim.exp"/>
   <schema name="product_view_definition_mim">
      <interface kind="use" schema="person_organisation_assignment_mim"/>
      <interface kind="use" schema="approval_mim"/>
      <interface kind="use" schema="date_time_assignment_mim"/>
      <interface kind="use" schema="product_version_mim"/>
      <interface kind="use" schema="product_definition_schema">
         <interfaced.item name="product_definition_context_association"/>
         <interfaced.item name="product_definition"/>
      </interface>
      <interface kind="use" schema="application_context_schema">
         <interfaced.item name="product_definition_context"/>
      </interface>
      <type name="product_definition_approval_item">
         <select basedon="approval_item" selectitems="product_definition"/>
      </type>
      <type name="product_definition_date_item">
         <select basedon="date_item" selectitems="product_definition"/>
      </type>
      <type name="product_definition_date_and_time_item">
         <select basedon="date_and_time_item" selectitems="product_definition"/>
      </type>
      <type name="product_definition_organization_item">
         <select basedon="organization_item" selectitems="product_definition"/>
      </type>
      <type name="product_definition_person_and_organization_item">
         <select basedon="person_and_organization_item" selectitems="product_definition"/>
      </type>
   </schema>
</express>