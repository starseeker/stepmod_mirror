<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.6 2002/01/16 12:11:43 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.6"
    source="../data/modules/product_view_definition/mim.exp"/>

  <schema name="Product_view_definition_mim">
    <interface
      kind="use"
      schema="Person_organisation_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="Approval_mim">
    </interface>

    <interface
      kind="use"
      schema="Date_time_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="Product_version_mim">
    </interface>

    <interface
      kind="use"
      schema="product_definition_schema">
      <interfaced.item
        name="product_definition"/>
      <interfaced.item
        name="product_definition_context_association"/>
    </interface>

    <interface
      kind="use"
      schema="application_context_schema">
      <interfaced.item
        name="product_definition_context"/>
    </interface>

    <type name="product_definition_approval_item">
      <select
        basedon="approval_item"
        selectitems="product_definition">
      </select>

    </type>
    <type name="product_definition_date_item">
      <select
        basedon="date_item"
        selectitems="product_definition">
      </select>

    </type>
    <type name="product_definition_date_and_time_item">
      <select
        basedon="date_and_time_item"
        selectitems="product_definition">
      </select>

    </type>
    <type name="product_definition_organization_item">
      <select
        basedon="organization_item"
        selectitems="product_definition">
      </select>

    </type>
    <type name="product_definition_person_and_organization_item">
      <select
        basedon="person_and_organization_item"
        selectitems="product_definition">
      </select>

    </type>
  </schema>

</express>
