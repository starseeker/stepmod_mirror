<ext_descriptions module_directory="product_view_definition_relationship" schema_file="arm.xml" rcs.date="$Date: 2002/03/19 13:34:13 $" rcs.revision="$Revision: 1.1 $">


  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_view_definition_relationship_arm 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
   <ext_description linkend="Product_view_definition_relationship_arm"/>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_view_definition_relationship_arm  Type: view_definition_usage_security_classification_item 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_view_definition_relationship_arm.view_definition_usage_security_classification_item">This select is extends a security_classification_assignment to apply to View_definition_usage instances.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_view_definition_relationship_arm  Entity: View_definition_relationship 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_view_definition_relationship_arm.View_definition_relationship">A <b>View_definition_relationship</b> relates two <b>Product_view_definitions</b>.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_view_definition_relationship_arm  Entity: View_definition_relationship  Attribute: view1 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_view_definition_relationship_arm.View_definition_relationship.view1">a <b>Product_view_definition</b> to be related to <b>view2</b>.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_view_definition_relationship_arm  Entity: View_definition_relationship  Attribute: view2 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_view_definition_relationship_arm.View_definition_relationship.view2">a <b>Product_view_definition</b> to be related to <b>view1</b>.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_view_definition_relationship_arm  Entity: View_definition_usage 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_view_definition_relationship_arm.View_definition_usage">
      <p>A <b>View_definition_usage</b> specifies that one <b>Product_view_definition</b> has another <b>Product_view_definition</b> as a constituent in its composition.</p>
      
      <p>
         <b>view1</b> is a <b>Product_view_definition</b> which is composed of <b>view2</b>.</p>
      
      <p>
         <b>view2</b> is a <b>Product_view_definition</b> which is part of the composition of <b>view1</b>.</p>
      
   </ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_view_definition_relationship_arm  Entity: View_definition_usage  Attribute: security 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_view_definition_relationship_arm.View_definition_usage.security">the <b>Security_classification</b> of the <b>View_definition_usage</b>
   </ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_view_definition_relationship_arm  Entity: Assembly_component 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_view_definition_relationship_arm.Assembly_component">An <b>Assembly_component</b> specifies that one <b>Product_view_definition</b> uses or may use another <b>Product_view_definition</b> as a component in its assembly.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_view_definition_relationship_arm  Entity: Assembly_component  Attribute: quantity 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_view_definition_relationship_arm.Assembly_component.quantity">the amount of <b>view2</b> which is needed to make <b>view1</b>.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_view_definition_relationship_arm  Entity: Assembly_component  Attribute: location_indicator 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_view_definition_relationship_arm.Assembly_component.location_indicator">a label for a unique component location in <b>view1</b>.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_view_definition_relationship_arm  Entity: Make_from 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_view_definition_relationship_arm.Make_from">A <b>Make_from</b> specifies that one <b>Product_view_definition</b> is or is planned to be made from another <b>Product_view_definition</b>.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_view_definition_relationship_arm  Entity: Make_from  Attribute: quantity 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_view_definition_relationship_arm.Make_from.quantity">the amount of <b>view1</b> which can be made from a single <b>view2</b>.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Product_view_definition_relationship_arm  Entity: Make_from  Attribute: priority 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Product_view_definition_relationship_arm.Make_from.priority">an indication, where <b>view1</b> can be made from a number of possible different items, of the relative choice priority of <b>view2</b>.</ext_description>
</ext_descriptions>