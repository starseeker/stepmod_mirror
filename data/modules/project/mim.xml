<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express rcs.date="$Date: 2002/04/03 21:34:16 $" rcs.revision="$Revision: 1.7 $">
   <application name="pdm_module2mim.xml" owner="PDTec GmbH" url="http://www.pdtec.de" version="0.8" source="../data/modules/approval/mim.exp"/>
   <schema name="project_mim">
      <interface kind="use" schema="event_mim"/>
      <interface kind="use" schema="person_organization_mim"/>
      <interface kind="use" schema="person_organisation_assignment_mim"/>
      <interface kind="use" schema="person_organization_schema">
         <interfaced.item name="organizational_project_relationship"/>
         <interfaced.item name="organizational_project"/>
      </interface>
      <interface kind="use" schema="management_resources_schema">
         <interfaced.item name="organizational_project_role"/>
         <interfaced.item name="organizational_project_assignment"/>
      </interface>
      <type name="organizational_project_item">
         <select extensible="YES"/>
      </type>
      <type name="organizational_project_organization_item">
         <select basedon="organization_item" selectitems="organizational_project"/>
      </type>
      <type name="organizational_project_person_and_organization_item">
         <select basedon="person_and_organization_item" selectitems="organizational_project"/>
      </type>
      <entity name="applied_organizational_project_assignment" supertypes="organizational_project_assignment">
         <explicit name="items">
            <aggregate type="SET" lower="1" upper="?"/>
            <typename name="organizational_project_item"/>
         </explicit>
      </entity>
   </schema>
</express>