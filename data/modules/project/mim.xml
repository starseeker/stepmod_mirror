<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.15 2002/06/18 11:51:06 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="mim_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.15"
    source="../data/modules/project/mim.exp"/>

  <schema name="Project_mim">
    <interface
      kind="use"
      schema="Date_time_mim">
    </interface>

    <interface
      kind="use"
      schema="Event_mim">
    </interface>

    <interface
      kind="use"
      schema="Person_organization_mim">
    </interface>

    <interface
      kind="use"
      schema="Date_time_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="person_organization_schema">
      <interfaced.item
        name="organizational_project"/>
      <interfaced.item
        name="organizational_project_relationship"/>
    </interface>

    <interface
      kind="use"
      schema="management_resources_schema">
      <interfaced.item
        name="organizational_project_assignment"/>
      <interfaced.item
        name="organizational_project_role"/>
    </interface>

    <type name="project_as_date_item">
      <select
        basedon="date_item"
        selectitems="organizational_project">
      </select>

    </type>
    <type name="project_as_date_and_time_item">
      <select
        basedon="date_and_time_item"
        selectitems="organizational_project">
      </select>

    </type>
    <type name="project_item">
      <select
        extensible="YES">
      </select>

    </type>
    <entity
      name="applied_organizational_project_assignment"
      supertypes="organizational_project_assignment">
      <explicit
        name="items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="project_item"/>
      </explicit>
    </entity>

  </schema>

</express>
