<?xml version="1.0"?>
<!-- $Id: mim.xml,v 1.4 2002/02/13 09:45:51 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/02/13 09:45:51 $"
  rcs.revision="$Revision: 1.4 $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.6"
    source="../data/modules/project/mim.exp"/>

  <schema name="Project_mim">
    <interface
      kind="use"
      schema="Event_mim">
    </interface>

    <interface
      kind="use"
      schema="Person_organisation_mim">
    </interface>

    <interface
      kind="use"
      schema="Person_organisation_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="person_organization_schema">
      <interfaced.item
        name="organizational_project"/>
      <interfaced.item
        name="organizational_project_relationship"/>
    </interface>

    <interface
      kind="use"
      schema="management_resources_schema">
      <interfaced.item
        name="organizational_project_assignment"/>
      <interfaced.item
        name="organizational_project_role"/>
    </interface>

    <type name="organizational_project_item">
      <description>An <b>organizational_project_item</b> specifies those objects to which an <b>organizational_project</b> may be assigned.</description>
      <select
        extensible="YES">
      </select>

    </type>
    <type name="organizational_project_organization_item">
      <description>An <b>organizational_project_organization_item</b> extends organization assignment to <b>organizational_project</b> instances.</description>
      <select
        basedon="organization_item"
        selectitems="organizational_project">
      </select>

    </type>
    <type name="organizational_project_person_and_organization_item">
      <description>An <b>organizational_project_person_and_organization_item</b> extends person and organization assignment to <b>organizational_project</b> instances</description>
      <select
        basedon="person_and_organization_item"
        selectitems="organizational_project">
      </select>

    </type>
    <entity
      name="applied_organizational_project_assignment"
      supertypes="organizational_project_assignment">
      <description></description>
      <explicit
        name="items">
      <description></description>
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="organizational_project_item"/>
      </explicit>
    </entity>

  </schema>

</express>
