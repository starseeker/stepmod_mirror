<?xml version="1.0"?>
<!-- $Id: mim.xml,v 1.10 2002/05/28 12:48:27 buchanan Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/05/28 12:48:27 $"
  rcs.revision="$Revision: 1.10 $"
  description.file="arm_descriptions.xml">

  <schema name="Project_mim">
    <interface
      kind="use"
      schema="Event_mim">
    </interface>

    <interface
      kind="use"
      schema="Person_organization_mim">
    </interface>

    <interface
      kind="use"
      schema="Person_organisation_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="person_organization_schema">
      <interfaced.item
        name="organizational_project"/>
      <interfaced.item
        name="organizational_project_relationship"/>
    </interface>

    <interface
      kind="use"
      schema="management_resources_schema">
      <interfaced.item
        name="organizational_project_assignment"/>
      <interfaced.item
        name="organizational_project_role"/>
    </interface>

    <type name="organizational_project_item">
      <select
        extensible="YES"
        selectitems="null">
      </select>

    </type>
    <type name="organizational_project_organization_item">
      <select
        basedon="organization_item"
        selectitems="organizational_project">
      </select>

    </type>
    <type name="organizational_project_person_and_organization_item">
      <select
        basedon="person_and_organization_item"
        selectitems="organizational_project">
      </select>

    </type>
    <entity
      name="applied_organizational_project_assignment"
      supertypes="organizational_project_assignment">
      <explicit
        name="items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="organizational_project_item"/>
      </explicit>
    </entity>

  </schema>

</express>
