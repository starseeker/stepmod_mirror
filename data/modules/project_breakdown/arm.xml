<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<?xml-stylesheet type="text/xsl"
href="../../../xsl/express.xsl" ?>
<express
   language_version="2"
   rcs.date="$Date: 2005/03/02 15:01:10 $"
   rcs.revision="$Revision: 1.3 $"
   description.file="arm_descriptions.xml">

     <schema name="Project_breakdown_arm">
    <interface
      kind="use"
      schema="Configuration_item_arm">
    </interface>
    <interface
      kind="use"
      schema="Independent_property_arm">
    </interface>
    <interface
      kind="use"
      schema="Product_as_individual_arm">
    </interface>
    <interface
      kind="use"
      schema="Product_breakdown_arm">
    </interface>
    <interface
      kind="use"
      schema="Product_categorization_arm">
    </interface>
    <interface
      kind="use"
      schema="Product_concept_identification_arm">
    </interface>
    <interface
      kind="use"
      schema="Product_group_arm">
    </interface>
    <interface
      kind="use"
      schema="Product_identification_arm">
    </interface>
    <interface
      kind="use"
      schema="Product_relationship_arm">
    </interface>
    <interface
      kind="use"
      schema="Product_version_arm">
    </interface>
    <interface
      kind="use"
      schema="Product_version_relationship_arm">
    </interface>
    <interface
      kind="use"
      schema="Product_view_definition_arm">
    </interface>
    <interface
      kind="use"
      schema="Product_view_definition_relationship_arm">
    </interface>
    <interface
      kind="use"
      schema="Project_arm">
    </interface>
    <interface
      kind="use"
      schema="Selected_item_arm">
    </interface>
    <type name="pb_product_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="product_item"
        selectitems="Product_group Product_view_definition View_definition_usage"
      />
    </type>

    <type name="pb_project_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="project_item"
        selectitems="Breakdown Independent_property Product Product_concept Product_configuration Product_group Product_group_membership Product_group_relationship Product_version"
      />
    </type>

    <type name="pb_selected_item_context_items">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="selected_item_context_items"
        selectitems="Breakdown Product_as_individual Product_as_individual_version Product_concept Product_configuration Project"
      />
    </type>

    <type name="pb_selected_item_select">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="selected_item_select"
        selectitems="Product Product_version"
      />
    </type>

  </schema>
</express>
