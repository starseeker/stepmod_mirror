<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Project_breakdown_arm  Type: pb_product_item 
   <express_ref linkend="project_breakdown:arm:Project_breakdown_arm.pb_product_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Project_breakdown_arm.pb_product_item">
  Using
  <express_ref linkend="product_breakdown:arm:Product_breakdown_arm.Product_definition_element_relationship"/>
   the entities defined in <b>pb_product_item</b> define the product that is related to the 
   <express_ref linkend="product_breakdown:arm:Product_breakdown_arm.Breakdown_element_definition"/> 
   or a 
   <express_ref linkend="product_breakdown:arm:Product_breakdown_arm.Breakdown_element_usage"/>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Project_breakdown_arm  Type: pb_project_item 
   <express_ref linkend="project_breakdown:arm:Project_breakdown_arm.pb_project_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Project_breakdown_arm.pb_project_item">
  Using
  <express_ref linkend="project:arm:Project_arm.Project_assignment"/>
  a
  <express_ref linkend="project:arm:Project_arm.Project"/>
  can be assigned to the entities in <b>pb_project_item</b>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Project_breakdown_arm  Type: pb_selected_item_context_items 
   <express_ref linkend="project_breakdown:arm:Project_breakdown_arm.pb_selected_item_context_items"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Project_breakdown_arm.pb_selected_item_context_items">
   A <express_ref linkend="selected_item:arm:Selected_item_arm.Selected_item"/>
   is a type of 
   <express_ref linkend="class:arm:Class_arm.Class"/>   
   used to classify identified product or activity data as being significant for 
   configuration management purposes. Using
  <express_ref linkend="selected_item:arm:Selected_item_arm.Selected_item_assignment"/>,
  the entities in  <b>pb_selected_item_context_items</b> provide the context in 
  which the product or activity data is being identified as being a
  <express_ref linkend="selected_item:arm:Selected_item_arm.Selected_item"/>.
</ext_description>
<!-- 
