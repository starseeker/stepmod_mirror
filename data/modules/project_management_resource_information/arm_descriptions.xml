<ext_description linkend="Project_management_resource_information_arm">
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Project_management_resource_information_arm  Type: pmri_certification_item 
   <express_ref linkend="project_management_resource_information:arm:Project_management_resource_information_arm.pmri_certification_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Project_management_resource_information_arm.pmri_certification_item">
  Using
  <express_ref linkend="certification:arm:Certification_arm.Certification_assignment"/>,
  a
  <express_ref linkend="certification:arm:Certification_arm.Certification"/>,
  can be assigned to the entities in <b>pmri_certification_item</b>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Project_management_resource_information_arm  Type: pmri_classification_item 
   <express_ref linkend="project_management_resource_information:arm:Project_management_resource_information_arm.pmri_classification_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Project_management_resource_information_arm.pmri_classification_item">
  Using
  <express_ref linkend="classification_assignment:arm:Classification_assignment_arm.Classification_assignment"/>,
  a
  <express_ref linkend="class:arm:Class_arm.Class"/>
  can be assigned to the entities in <b>pmri_classification_item</b>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Project_management_resource_information_arm  Type: pmri_classified_attribute_select 
   <express_ref linkend="project_management_resource_information:arm:Project_management_resource_information_arm.pmri_classified_attribute_select"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Project_management_resource_information_arm.pmri_classified_attribute_select">
  Using 
  <express_ref linkend="attribute_classification:arm:Attribute_classification_arm.Attribute_classification"/>,
  a <express_ref linkend="class:arm:Class_arm.Class"/>
  can be assigned to attributes on instances of the entities in
  <b>pmri_classified_attribute_select</b> 
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Project_management_resource_information_arm  Type: pmri_condition_item 
   <express_ref linkend="project_management_resource_information:arm:Project_management_resource_information_arm.pmri_condition_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Project_management_resource_information_arm.pmri_condition_item">
  Using 
  <express_ref linkend="condition:arm:Condition_arm.Condition_assignment"/>,
  a <express_ref linkend="condition:arm:Condition_arm.Condition"/>
  can be assigned to the entities in <b>pmri_condition_item</b> 
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Project_management_resource_information_arm  Type: pmri_condition_parameter_item 
   <express_ref linkend="project_management_resource_information:arm:Project_management_resource_information_arm.pmri_condition_parameter_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Project_management_resource_information_arm.pmri_condition_parameter_item">
  Using
  <express_ref linkend="condition:arm:Condition_arm.Condition_parameter"/>
  the product or activity data identified in 
  <b>pmri_condition_parameter_item</b>
  can be identified as a parameter against which a
  <express_ref linkend="condition:arm:Condition_arm.Condition"/>
  is evaluated. 
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Project_management_resource_information_arm  Type: pmri_contract_item 
   <express_ref linkend="project_management_resource_information:arm:Project_management_resource_information_arm.pmri_contract_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Project_management_resource_information_arm.pmri_contract_item">
  Using
  <express_ref linkend="contract:arm:Contract_arm.Contract_assignment"/>,
  a
  <express_ref linkend="contract:arm:Contract_arm.Contract"/>
  can be assigned to the entities in <b>pmri_contract_item</b>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Project_management_resource_information_arm  Type: pmri_documented_element_select 
   <express_ref linkend="project_management_resource_information:arm:Project_management_resource_information_arm.pmri_documented_element_select"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Project_management_resource_information_arm.pmri_documented_element_select">
  Using
  <express_ref linkend="document_assignment:arm:Document_assignment_arm.Document_assignment"/>,
  a
<express_ref linkend="document_and_version_identification:arm:Document_and_version_identification_arm.Document"/>, 
<express_ref linkend="document_and_version_identification:arm:Document_and_version_identification_arm.Document_version"/>, 
<express_ref linkend="document_definition:arm:Document_definition_arm.Digital_document_definition"/>
 or 
<express_ref linkend="file_identification:arm:File_identification_arm.File"/>
  can be assigned to the entities in <b>pmri_documented_element_select</b>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Project_management_resource_information_arm  Type: pmri_effectivity_item 
   <express_ref linkend="project_management_resource_information:arm:Project_management_resource_information_arm.pmri_effectivity_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Project_management_resource_information_arm.pmri_effectivity_item">
  Using 
  <express_ref linkend="effectivity_application:arm:Effectivity_application_arm.Effectivity_assignment"/>,
  an <express_ref linkend="effectivity:arm:Effectivity_arm.Effectivity"/>
  can be assigned to the entities in <b>pmri_effectivity_item</b> 
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Project_management_resource_information_arm  Type: pmri_justification_item 
