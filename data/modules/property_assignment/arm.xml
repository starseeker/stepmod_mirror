<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.15 2002/06/18 11:51:06 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.15"
    source="../data/modules/property_assignment/arm.exp"/>

  <schema name="Property_assignment_arm">
    <interface
      kind="use"
      schema="Independent_property_arm">
    </interface>

    <type name="property_assignment_select">
      <select
        extensible="YES">
      </select>

    </type>
    <entity
      name="Applied_independent_element_property"
      supertypes="Element_property">
      <explicit
        name="base_element_property">
        <typename
          name="Independent_property"/>
      </explicit>
      <derived
        name="SELF\Element_property.name"
        expression="Independent_property.property_type">
        <builtintype
          type="STRING"/>
      </derived>
    </entity>

    <entity
      name="Element_property">
      <explicit
        name="described_element">
        <typename
          name="property_assignment_select"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="name"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

  </schema>

</express>
