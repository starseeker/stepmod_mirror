  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Property_assignment_arm  Entity: Element_property 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Property_assignment_arm.Element_property">
        An Element_property specifies a property that is
        assigned to an element.
      </ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Property_assignment_arm  Entity: Element_property  Attribute: name 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Property_assignment_arm.Element_property.name">
          the name by which the property is known.
        </ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Property_assignment_arm  Entity: Element_property  Attribute: description 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Property_assignment_arm.Element_property.description">
          text that describes the property.
        </ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Property_assignment_arm  Entity: Element_property  Attribute: described_element 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Property_assignment_arm.Element_property.described_element">
          the element to which the property is assigned.
        </ext_description>
  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Property_assignment_arm  Entity: Applied_independent_element_property  
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Property_assignment_arm.Applied_independent_element_property">
          This element specifies an Element_property that is derived from an eIndependent_property.
        </ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Property_assignment_arm  Entity: Applied_independent_element_property Attribute: base_element_property  
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Property_assignment_arm.Applied_independent_element_property.base_element_property">the Independent_property from which it is derived.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Property_assignment_arm  Entity: Applied_independent_element_property Attribute: name
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Property_assignment_arm.Applied_independent_element_property.name">the name of the property, which must be the same as that of the base property.</ext_description>

