<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express language_version="2" rcs.date="$Date: $" rcs.revision="$Revision: $" description.file="arm_descriptions.xml">
	<application name="GraphicalEXPRESS" owner="Eurostep" url="www.eurostep.com" version="1.3.14" source="qualifications_arm_v004.vsd"/>
	<schema name="Qualifications_arm">
		<interface kind="use" schema="Certification_arm"/>
		<interface kind="use" schema="Classification_assignment_arm"/>
		<interface kind="use" schema="Date_time_assignment_arm"/>
		<interface kind="use" schema="Person_organization_arm"/>
		<interface kind="use" schema="Person_organization_assignment_arm"/>
		<type name="qualification_organization_or_person_in_organization_item">
			<select extensible="YES" genericentity="YES" basedon="organization_or_person_in_organization_item" selectitems="Qualification_assignment"/>
			<graphic.element page="2" xcoord="241" ycoord="666"/>
		</type>
		<type name="qualifications_certification_item">
			<select extensible="YES" genericentity="YES" basedon="certification_item" selectitems="Qualification_assignment"/>
			<graphic.element page="2" xcoord="184" ycoord="525"/>
		</type>
		<type name="qualifications_classified_select">
			<select extensible="YES" genericentity="YES" basedon="classification_item" selectitems="Qualification_type"/>
			<graphic.element page="2" xcoord="425" ycoord="440"/>
		</type>
		<type name="qualifications_date_or_date_time_item">
			<select extensible="YES" genericentity="YES" basedon="date_or_date_time_item" selectitems="Qualification_assignment"/>
			<graphic.element page="2" xcoord="184" ycoord="383"/>
		</type>
		<type name="qualifications_select">
			<select extensible="YES" genericentity="YES" selectitems="Organization Person Person_in_organization"/>
			<graphic.element page="2" xcoord="156" ycoord="255"/>
		</type>
		<entity name="Qualification_assignment">
			<explicit name="assigned_qualification_type">
				<typename name="Qualification_type"/>
			</explicit>
			<explicit name="received_by">
				<typename name="qualifications_select"/>
			</explicit>
			<graphic.element page="2" xcoord="156" ycoord="319"/>
		</entity>
		<entity name="Qualification_type">
			<explicit name="name">
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="description" optional="YES">
				<builtintype type="STRING"/>
			</explicit>
			<graphic.element page="2" xcoord="461" ycoord="319"/>
		</entity>
		<entity name="Qualification_type_relationship">
			<explicit name="name">
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="description" optional="YES">
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="related">
				<typename name="Qualification_type"/>
			</explicit>
			<explicit name="relating">
				<typename name="Qualification_type"/>
			</explicit>
			<graphic.element page="2" xcoord="461" ycoord="248"/>
		</entity>
		<graphic.element page="1" xcoord="234" ycoord="262"/>
	</schema>
</express>
