<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml2.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.38"
    source="../data/modules/qualified_measure/arm.exp"/>

  <schema name="Qualified_measure_arm">

    <interface
      kind="use"
      schema="External_item_identification_assignment_arm">
    </interface>
    <interface
      kind="use"
      schema="External_library_arm">
    </interface>
    <interface
      kind="use"
      schema="Measure_representation_arm">
    </interface>
    <entity
      name="Qualfied_numerical_item_with_unit"
      supertypes="Numerical_item_with_unit">
      <explicit
        name="qualifier">
        <aggregate
          type="SET"/>
        <typename
          name="Type_qualifier"/>
      </explicit>
    </entity>

    <entity
      name="Type_qualifier">
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="Externally_defined_type_qualifier"
      supertypes="Type_qualifier">
      <explicit
        name="id">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="external_source">
        <typename
          name="External_library"/>
      </explicit>
    </entity>

  </schema>
</express>
