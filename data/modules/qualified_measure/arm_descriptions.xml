<ext_descriptions module_directory="qualified_measure" schema_file="arm.xml" describe.selects="YES" describe.subtype_constraints="YES" rcs.date="$Date: 2004/11/16 07:30:26 $" rcs.revision="$Revision: 1.13 $">
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: Qualified_measure_arm
   <express_ref linkend="qualified_measure:arm:Qualified_measure_arm"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<ext_description linkend="Qualified_measure_arm">
</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Qualified_measure_arm  Entity: Qualfied_numerical_item_with_unit  
   <express_ref linkend="qualified_measure:arm:Qualified_measure_arm.Qualfied_numerical_item_with_unit"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Qualified_measure_arm.Qualfied_numerical_item_with_unit">
        A <b>Qualfied_numerical_item_with_unit</b> is a type of <express_ref linkend="measure_representation:arm:Measure_representation_arm.Numerical_item_with_unit"/> that specifies the type of a datum.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Qualified_measure_arm  Entity: Qualfied_numerical_item_with_unit  Attribute: qualifier 
   <express_ref linkend="qualified_measure:arm:Qualified_measure_arm.Qualfied_numerical_item_with_unit.qualifier"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Qualified_measure_arm.Qualfied_numerical_item_with_unit.qualifier">
	the <express_ref linkend="qualified_measure:arm:Qualified_measure_arm.Type_qualifier"/> that specifies the type of datum of the <b>Qualfied_numerical_item_with_unit</b>.
</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Qualified_measure_arm  Entity: Type_qualifier  
   <express_ref linkend="qualified_measure:arm:Qualified_measure_arm.Type_qualifier"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Qualified_measure_arm.Type_qualifier">
        A <b>Type_qualifier</b> is the specification of the type of a datum.
      </ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Qualified_measure_arm  Entity: Type_qualifier  Attribute: name 
   <express_ref linkend="qualified_measure:arm:Qualified_measure_arm.Type_qualifier.name"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Qualified_measure_arm.Type_qualifier.name">
		<p>the words by which the <b>Type_qualifier</b> is known. 
        Where applicable the following values shall be used:</p>
		<ul>
			<li>'nominal': the value is a nominal value;</li>
			<li>'basic': the value is a basic value;</li>
			<li>'minimum': the value is the minimum value;</li>
			<li>'maximum': the value is the maximum value;</li>
			<li>'lower deviation': the value is maximum deviation in the negative direction from the nominal or basic value;</li>
			<li>'upper deviation': the value is maximum deviation in the positive direction from the nominal or basic value.</li>
		</ul>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Qualified_measure_arm  Entity: Externally_defined_type_qualifier  
   <express_ref linkend="qualified_measure:arm:Qualified_measure_arm.Externally_defined_type_qualifier"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Qualified_measure_arm.Externally_defined_type_qualifier">
        A <b>Externally_defined_type_qualifier</b> is a type of <express_ref linkend="qualified_measure:arm:Qualified_measure_arm.Type_qualifier"/> whose meaning is defined in an external library.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Qualified_measure_arm  Entity: Externally_defined_type_qualifier  Attribute: id 
   <express_ref linkend="qualified_measure:arm:Qualified_measure_arm.Externally_defined_type_qualifier.id"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Qualified_measure_arm.Externally_defined_type_qualifier.id">
       the identifier for the <b>Externally_defined_type_qualifier</b>.        
      </ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Qualified_measure_arm  Entity: Externally_defined_type_qualifier  Attribute: external_source 
   <express_ref linkend="qualified_measure:arm:Qualified_measure_arm.Externally_defined_type_qualifier.external_source"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Qualified_measure_arm.Externally_defined_type_qualifier.external_source">
	the identification of the external library that provides the meaning of the <b>Externally_defined_type_qualifier</b>.
</ext_description>
</ext_descriptions>
