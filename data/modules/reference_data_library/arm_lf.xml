    <type name="involved_class_select">
      <typename name="activity_item"/>
      <where
        label="wr1"
        expression="NOT (&apos;REFERENCE_DATA_LIBRARY_ARM_LF.ORGANIZATION&apos; IN TYPEOF(SELF))">
      </where>
      <where
        label="wr2"
        expression="NOT (&apos;REFERENCE_DATA_LIBRARY_ARM_LF.PERSON&apos; IN TYPEOF(SELF))">
      </where>
      <where
        label="wr3"
        expression="NOT (&apos;REFERENCE_DATA_LIBRARY_ARM_LF.PERSON_IN_ORGANIZATION&apos; IN TYPEOF(SELF))">
      </where>
