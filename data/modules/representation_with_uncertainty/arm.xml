<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.26 2002/12/23 10:00:14 goset1 Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.26"
    source="../data/modules/representation_with_uncertainty/arm.exp"/>

  <schema name="Representation_with_uncertainty_arm">
    <interface
      kind="use"
      schema="Foundation_representation_arm">
    </interface>

    <entity
      name="Representation_with_uncertainty"
      supertypes="Representation">
      <explicit
        name="uncertainty">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="Uncertainty_with_unit"/>
      </explicit>
    </entity>

    <entity
      name="Representation_item_with_uncertainty"
      supertypes="Representation_item">
      <explicit
        name="uncertainty">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="Uncertainty_value"/>
      </explicit>
    </entity>

    <entity
      name="Uncertainty_value">
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="uncertainty">
        <builtintype
          type="REAL"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

  </schema>

</express>
