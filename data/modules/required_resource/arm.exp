SCHEMA Required_resource_arm;

USE FROM Classification_arm;

USE FROM Resource_item_arm;

USE FROM Value_with_unit_arm;

TYPE required_resource_action = EXTENSIBLE SELECT ;
END_TYPE;

TYPE required_resource_classified_select = SELECT BASED_ON classified_select WITH
