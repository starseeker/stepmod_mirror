<?xml version="1.0"?>

<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <description>
  
  </description>

  <application
    name="GraphicalEXPRESS"
    owner="Eurostep" url="www.eurostep.com"
    version="1.3"
    source="required_resource_arm.vsd"/>

  <schema name="Required_resource_arm">
    <interface
      kind="use"
      schema="Classification_arm">
    </interface>
    <interface
      kind="use"
      schema="Expressions_for_logic_arm">
    </interface>
    <interface
      kind="use"
      schema="Resource_item_arm">
    </interface>
    <interface
      kind="use"
      schema="Value_with_unit_arm">
    </interface>
    <type name="required_resource_classified_select"> 
      <select
         basedon="classified_select"
        selectitems="Required_resource Required_resource_relationship"/>
      <graphic.element
        page="2"
        xcoord="173"
        ycoord="416"/>
    </type>

    <type name="resource_required_select"> 
      <select
      extensible="YES"
        />
      <graphic.element
        page="2"
        xcoord="357"
        ycoord="479"/>
    </type>

    <type name="specification_item_select"> 
      <select
      extensible="YES"
        />
      <graphic.element
        page="2"
        xcoord="162"
        ycoord="99"/>
    </type>

    <entity
      name="Required_resource"
      abstract.supertype="YES"
      super.expression="ONEOF(Required_resource_item, Required_resource_specification)">
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="resource_required_by">
        <typename name="resource_required_select"/>
      </explicit>
      <explicit
        name="required_quantity"
        optional="YES">
        <typename name="Value_with_unit"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="357"
        ycoord="338"/>
    </entity>

    <entity
      name="Required_resource_item"
      supertypes="Required_resource"
      >
      <explicit
        name="realized_by">
        <typename name="Resource_item"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="435"
        ycoord="231"/>
    </entity>

    <entity
      name="Required_resource_relationship"
      >
      <explicit
        name="relating">
        <typename name="Required_resource"/>
      </explicit>
      <explicit
        name="related">
        <typename name="Required_resource"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="172"
        ycoord="338"/>
    </entity>

    <entity
      name="Required_resource_specification"
      supertypes="Required_resource"
      >
      <graphic.element
        page="2"
        xcoord="279"
        ycoord="231"/>
    </entity>

    <entity
      name="Required_resource_specification_relationship"
      >
      <explicit
        name="relating">
        <typename name="Required_resource_specification"/>
      </explicit>
      <explicit
        name="related">
        <typename name="specification_item_select"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="162"
        ycoord="177"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="251"
      ycoord="211"/>
</schema>

  <schema name="Value_with_unit_arm">
    <entity
      name="Value_with_unit"
      >
      <graphic.element
        page="4"
        xcoord="311"
        ycoord="602"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="43"
      ycoord="211"/>
</schema>

  <schema name="Classification_arm">
    <type name="classified_select"> 
      <select
      extensible="YES"
      genericentity="YES"
        />
      <graphic.element
        page="5"
        xcoord="255"
        ycoord="637"/>
    </type>

    <graphic.element
      page="1"
      xcoord="471"
      ycoord="211"/>
</schema>

  <schema name="Expressions_for_logic_arm">
    <entity
      name="Logical_expression"
      >
      <graphic.element
        page="6"
        xcoord="325"
        ycoord="659"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="46"
      ycoord="98"/>
</schema>

  <schema name="Resource_item_arm">
    <entity
      name="Resource_item"
      >
      <graphic.element
        page="3"
        xcoord="311"
        ycoord="602"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="471"
      ycoord="98"/>
</schema>

</express>
