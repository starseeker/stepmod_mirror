<?xml version="1.0"?>

<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <description>
  
  </description>

  <application
    name="GraphicalEXPRESS"
    owner="Eurostep" url="www.eurostep.com"
    version="1.3"
    source="required_resource_arm 2.0.vsd"/>

  <schema name="Required_resource_arm">
    <interface
      kind="use"
      schema="Classification_arm">
    </interface>
    <interface
      kind="use"
      schema="Resource_item_arm">
    </interface>
    <interface
      kind="use"
      schema="Value_with_unit_arm">
    </interface>
    <type name="required_resource_action"> 
      <select
      extensible="YES"
        />
      <graphic.element
        page="2"
        xcoord="325"
        ycoord="411"/>
    </type>

    <type name="required_resource_classified_select"> 
      <select
         basedon="classified_select"
        selectitems="Required_resource Required_resource_action_relationship Required_resource_relationship"/>
      <graphic.element
        page="2"
        xcoord="111"
        ycoord="333"/>
    </type>

    <type name="specification_item_select"> 
      <select
      extensible="YES"
        />
      <graphic.element
        page="2"
        xcoord="130"
        ycoord="42"/>
    </type>

    <entity
      name="Required_resource"
      abstract.supertype="YES"
      super.expression="ONEOF(Required_resource_by_resource_item, Required_resource_by_specification)">
      <explicit
        name="name"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="required_quantity"
        optional="YES">
        <typename name="Value_with_unit"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="325"
        ycoord="242"/>
    </entity>

    <entity
      name="Required_resource_action_relationship"
      >
      <explicit
        name="relating">
        <typename name="Required_resource"/>
      </explicit>
      <explicit
        name="related">
        <typename name="required_resource_action"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="325"
        ycoord="333"/>
    </entity>

    <entity
      name="Required_resource_by_resource_item"
      supertypes="Required_resource"
      >
      <explicit
        name="resource_item">
        <typename name="Resource_item"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="403"
        ycoord="148"/>
    </entity>

    <entity
      name="Required_resource_by_specification"
      supertypes="Required_resource"
      >
      <graphic.element
        page="2"
        xcoord="248"
        ycoord="148"/>
    </entity>

    <entity
      name="Required_resource_relationship"
      >
      <explicit
        name="relating">
        <typename name="Required_resource"/>
      </explicit>
      <explicit
        name="related">
        <typename name="Required_resource"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="140"
        ycoord="242"/>
    </entity>

    <entity
      name="Specification_item_relationship"
      >
      <explicit
        name="relating">
        <typename name="Required_resource_by_specification"/>
      </explicit>
      <explicit
        name="related">
        <typename name="specification_item_select"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="130"
        ycoord="106"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="225"
      ycoord="135"/>
</schema>

  <schema name="Value_with_unit_arm">
    <entity
      name="Value_with_unit"
      >
      <graphic.element
        page="4"
        xcoord="311"
        ycoord="602"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="85"
      ycoord="135"/>
</schema>

  <schema name="Classification_arm">
    <type name="classified_select"> 
      <select
      extensible="YES"
      genericentity="YES"
        />
      <graphic.element
        page="5"
        xcoord="255"
        ycoord="637"/>
    </type>

    <graphic.element
      page="1"
      xcoord="368"
      ycoord="135"/>
</schema>

  <schema name="Resource_item_arm">
    <entity
      name="Resource_item"
      >
      <graphic.element
        page="3"
        xcoord="311"
        ycoord="602"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="225"
      ycoord="57"/>
</schema>

</express>
