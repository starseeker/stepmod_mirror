<?xml version="1.0"?>

<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <description>
  
  </description>

  <application
    name="GraphicalEXPRESS"
    owner="Eurostep" url="www.eurostep.com"
    version="1.3"
    source="required_resource_arm.vsd"/>

  <schema name="Required_resource_arm">
    <interface
      kind="use"
      schema="Classification_assignment_arm">
    </interface>
    <interface
      kind="use"
      schema="Resource_item_arm">
    </interface>
    <interface
      kind="use"
      schema="Value_with_unit_arm">
    </interface>
    <type name="required_resource_action_item"> 
      <select
      extensible="YES"
        />
      <graphic.element
        page="2"
        xcoord="321"
        ycoord="377"/>
    </type>

    <type name="required_resource_classification_item"> 
      <select
      extensible="YES"
      genericentity="YES"
         basedon="classification_item"
        selectitems="Required_resource Required_resource_action_assignment Required_resource_relationship"/>
      <graphic.element
        page="2"
        xcoord="165"
        ycoord="267"/>
    </type>

    <entity
      name="Required_resource"
      abstract.supertype="YES"
      super.expression="ONEOF(Required_resource_by_resource_item, Required_resource_by_specification)">
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="required_quantity"
        optional="YES">
        <typename name="Value_with_unit"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="321"
        ycoord="178"/>
    </entity>

    <entity
      name="Required_resource_action_assignment"
      >
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="assigned_entity">
        <typename name="Required_resource"/>
      </explicit>
      <explicit
        name="item">
        <typename name="required_resource_action_item"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="321"
        ycoord="299"/>
    </entity>

    <entity
      name="Required_resource_by_resource_item"
      supertypes="Required_resource"
      >
      <explicit
        name="resource_item">
        <typename name="Resource_item"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="399"
        ycoord="70"/>
    </entity>

    <entity
      name="Required_resource_by_specification"
      supertypes="Required_resource"
      >
      <graphic.element
        page="2"
        xcoord="243"
        ycoord="70"/>
    </entity>

    <entity
      name="Required_resource_relationship"
      >
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="relating">
        <typename name="Required_resource"/>
      </explicit>
      <explicit
        name="related">
        <typename name="Required_resource"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="80"
        ycoord="178"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="225"
      ycoord="135"/>
</schema>

  <schema name="Value_with_unit_arm">
    <entity
      name="Value_with_unit"
      >
      <graphic.element
        page="4"
        xcoord="311"
        ycoord="602"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="85"
      ycoord="135"/>
</schema>

  <schema name="Resource_item_arm">
    <entity
      name="Resource_item"
      >
      <graphic.element
        page="3"
        xcoord="311"
        ycoord="602"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="225"
      ycoord="57"/>
</schema>

  <schema name="Classification_assignment_arm">
    <type name="classification_item"> 
      <select
      extensible="YES"
      genericentity="YES"
        />
      <graphic.element
        page="5"
        xcoord="229"
        ycoord="113"/>
    </type>

    <graphic.element
      page="1"
      xcoord="372"
      ycoord="134"/>
</schema>

</express>
