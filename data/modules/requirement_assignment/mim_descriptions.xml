	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Requirement_assignment_mim  Entity: assigned_requirement  
   <express_ref linkend="requirement_assignment:mim:Requirement_assignment_mim.assigned_requirement"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Requirement_assignment_mim.assigned_requirement">
		A <b>assigned_requirement</b> 
		is a type of 
		<express_ref linkend="management_resources_schema:ir_express:management_resources_schema.group_assignment"/> 
		that identifies a relationship between a requirement assigment and a product definition.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Requirement_assignment_mim  Entity: assigned_requirement  Attribute: assigned_group 
   <express_ref linkend="requirement_assignment:mim:Requirement_assignment_mim.assigned_requirement.assigned_group"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Requirement_assignment_mim.assigned_requirement.assigned_group">
		the 
	<express_ref linkend="requirement_assignment:mim:Requirement_assignment_mim.requirement_assignment"/>
		that is assigned by the 
		<b>
			assigned_requirement</b>.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Requirement_assignment_mim  Entity: assigned_requirement  Attribute: items 
   <express_ref linkend="requirement_assignment:mim:Requirement_assignment_mim.assigned_requirement.items"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Requirement_assignment_mim.assigned_requirement.items">
		the 
		<express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition"/> 
		that is assigned to by the 
		<b>
			assigned_requirement</b>.
	</ext_description>
