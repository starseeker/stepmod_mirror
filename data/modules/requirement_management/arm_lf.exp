  wr3: NOT ('REQUIREMENT_MANAGEMENT_ARM_LF.ASSIGNED_PROPERTY' IN TYPEOF(SELF));
  wr4: NOT ('REQUIREMENT_MANAGEMENT_ARM_LF.CERTIFICATION' IN TYPEOF(SELF));
  wr5: NOT ('REQUIREMENT_MANAGEMENT_ARM_LF.DIGITAL_DOCUMENT_DEFINITION' IN TYPEOF(SELF));
  wr6: NOT ('REQUIREMENT_MANAGEMENT_ARM_LF.DIGITAL_FILE' IN TYPEOF(SELF));
  wr7: NOT ('REQUIREMENT_MANAGEMENT_ARM_LF.DOCUMENT' IN TYPEOF(SELF));
  wr8: NOT ('REQUIREMENT_MANAGEMENT_ARM_LF.DOCUMENT_ASSIGNMENT' IN TYPEOF(SELF));
  wr9: NOT ('REQUIREMENT_MANAGEMENT_ARM_LF.DOCUMENT_VERSION' IN TYPEOF(SELF));
  wr10: NOT ('REQUIREMENT_MANAGEMENT_ARM_LF.EFFECTIVITY' IN TYPEOF(SELF));
  wr11: NOT ('REQUIREMENT_MANAGEMENT_ARM_LF.EVENT' IN TYPEOF(SELF));
  wr12: NOT ('REQUIREMENT_MANAGEMENT_ARM_LF.HARDCOPY' IN TYPEOF(SELF));
  wr13: NOT ('REQUIREMENT_MANAGEMENT_ARM_LF.INDEPENDENT_PROPERTY' IN TYPEOF(SELF));
  wr14: NOT ('REQUIREMENT_MANAGEMENT_ARM_LF.INDEPENDENT_PROPERTY_RELATIONSHIP' IN TYPEOF(SELF));
  wr15: NOT ('REQUIREMENT_MANAGEMENT_ARM_LF.INFORMATION_RIGHT' IN TYPEOF(SELF));
  wr16: NOT ('REQUIREMENT_MANAGEMENT_ARM_LF.INFORMATION_USAGE_RIGHT' IN TYPEOF(SELF));
  wr17: NOT ('REQUIREMENT_MANAGEMENT_ARM_LF.ORGANIZATION_OR_PERSON_IN_ORGANIZATION_ASSIGNMENT' IN TYPEOF(SELF));
  wr18: NOT ('REQUIREMENT_MANAGEMENT_ARM_LF.PERSON' IN TYPEOF(SELF));
  wr19: NOT ('REQUIREMENT_MANAGEMENT_ARM_LF.PERSON_IN_ORGANIZATION' IN TYPEOF(SELF));
  wr20: NOT ('REQUIREMENT_MANAGEMENT_ARM_LF.PHYSICAL_DOCUMENT_DEFINITION' IN TYPEOF(SELF));
  wr21: NOT ('REQUIREMENT_MANAGEMENT_ARM_LF.PROJECT' IN TYPEOF(SELF));
