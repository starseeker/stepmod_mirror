<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1142">
   <application name="JSDAI" owner="LKSoft" source="requirement_view_definition_relationship_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Requirement_view_definition_relationship_arm">
      <interface kind="use" schema="Product_view_definition_relationship_arm"/>
      <interface kind="use" schema="Requirement_view_definition_arm"/>
      <entity name="Requirement_collection_relationship" supertypes="Requirement_view_definition_relationship">
         <explicit name="collection">
            <typename name="Requirement_view_definition"/>
            <redeclaration entity-ref="Requirement_view_definition_relationship" old_name="primary"/>
         </explicit>
         <explicit name="member">
            <typename name="Requirement_view_definition"/>
            <redeclaration entity-ref="Requirement_view_definition_relationship" old_name="secondary"/>
         </explicit>
      </entity>
      <entity name="Requirement_precedence_relationship" supertypes="Requirement_view_definition_relationship"/>
      <entity name="Requirement_view_definition_relationship" super.expression="ONEOF (Requirement_collection_relationship, Requirement_precedence_relationship, Tracing_relationship)" supertypes="View_definition_relationship">
         <explicit name="primary">
            <typename name="Requirement_view_definition"/>
            <redeclaration entity-ref="View_definition_relationship" old_name="relating_view"/>
         </explicit>
         <explicit name="secondary">
            <typename name="Requirement_view_definition"/>
            <redeclaration entity-ref="View_definition_relationship" old_name="related_view"/>
         </explicit>
         <where expression="primary :&lt;&gt;: secondary" label="WR1"/>
      </entity>
      <entity name="Tracing_relationship" supertypes="Requirement_view_definition_relationship">
         <explicit name="traces_from">
            <typename name="Requirement_view_definition"/>
            <redeclaration entity-ref="Requirement_view_definition_relationship" old_name="primary"/>
         </explicit>
         <explicit name="traces_to">
            <typename name="Requirement_view_definition"/>
            <redeclaration entity-ref="Requirement_view_definition_relationship" old_name="secondary"/>
         </explicit>
      </entity>
   </schema>
</express>
