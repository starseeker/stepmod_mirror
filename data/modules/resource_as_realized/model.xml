<?xml version="1.0"?>

<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <description>
  
  </description>

  <application
    name="GraphicalEXPRESS"
    owner="Eurostep" url="www.eurostep.com"
    version="1.3"
    source="resource_as_realized_arm.vsd"/>

  <schema name="Resource_as_realized_arm">
    <interface
      kind="use"
      schema="Activity_arm">
    </interface>
    <interface
      kind="use"
      schema="Classification_arm">
    </interface>
    <interface
      kind="use"
      schema="Required_resource_arm">
    </interface>
    <interface
      kind="use"
      schema="Resource_item_arm">
    </interface>
    <interface
      kind="use"
      schema="Value_with_unit_arm">
    </interface>
    <type name="resource_as_realized_activity_item"> 
      <select
         basedon="activity_item"
        selectitems="Resource_as_realized"/>
      <graphic.element
        page="2"
        xcoord="425"
        ycoord="269"/>
    </type>

    <type name="resource_as_realized_classified_select"> 
      <select
         basedon="classified_select"
        selectitems="Resource_as_realized Resource_as_realized_relationship"/>
      <graphic.element
        page="2"
        xcoord="262"
        ycoord="269"/>
    </type>

    <entity
      name="Resource_as_realized"
      >
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="quantity"
        optional="YES">
        <typename name="Value_with_unit"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="425"
        ycoord="187"/>
    </entity>

    <entity
      name="Resource_as_realized_relationship"
      >
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="relating">
        <typename name="Resource_as_realized"/>
      </explicit>
      <explicit
        name="related">
        <typename name="Required_resource"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="262"
        ycoord="187"/>
    </entity>

    <entity
      name="Resource_as_realized_resource_item"
      supertypes="Resource_as_realized"
      >
      <explicit
        name="resource_item">
        <typename name="Resource_item"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="425"
        ycoord="104"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="262"
      ycoord="150"/>
</schema>

  <schema name="Resource_item_arm">
    <entity
      name="Resource_item"
      >
      <graphic.element
        page="3"
        xcoord="311"
        ycoord="659"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="430"
      ycoord="150"/>
</schema>

  <schema name="Required_resource_arm">
    <entity
      name="Required_resource"
      >
      <graphic.element
        page="5"
        xcoord="311"
        ycoord="616"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="87"
      ycoord="150"/>
</schema>

  <schema name="Classification_arm">
    <type name="classified_select"> 
      <select
      extensible="YES"
      genericentity="YES"
        />
      <graphic.element
        page="6"
        xcoord="311"
        ycoord="637"/>
    </type>

    <graphic.element
      page="1"
      xcoord="262"
      ycoord="248"/>
</schema>

  <schema name="Activity_arm">
    <type name="activity_item"> 
      <select
      extensible="YES"
      genericentity="YES"
        />
      <graphic.element
        page="7"
        xcoord="240"
        ycoord="212"/>
    </type>

    <graphic.element
      page="1"
      xcoord="184"
      ycoord="49"/>
</schema>

  <schema name="Value_with_unit_arm">
    <entity
      name="Value_with_unit"
      >
      <graphic.element
        page="4"
        xcoord="297"
        ycoord="659"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="340"
      ycoord="49"/>
</schema>

</express>
