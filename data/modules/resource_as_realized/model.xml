  <schema name="Resource_as_realized_arm">
    <interface
      kind="use"
      schema="Activity_arm">
    </interface>
    <interface
      kind="use"
      schema="Classification_assignment_arm">
    </interface>
    <interface
      kind="use"
      schema="Required_resource_arm">
    </interface>
    <interface
      kind="use"
      schema="Resource_item_arm">
    </interface>
    <interface
      kind="use"
      schema="Resource_management_arm">
    </interface>
    <interface
      kind="use"
      schema="Value_with_unit_arm">
    </interface>
    <type name="resource_as_realized_activity_item"> 
      <select
      extensible="YES"
      genericentity="YES"
         basedon="activity_item"
        selectitems="Resource_as_realized"/>
      <graphic.element
        page="2"
        xcoord="425"
        ycoord="266"/>
    </type>

    <type name="resource_as_realized_classification_item"> 
      <select
      extensible="YES"
      genericentity="YES"
         basedon="classification_item"
        selectitems="Resource_as_realized Resource_as_realized_relationship"/>
      <graphic.element
        page="2"
        xcoord="262"
        ycoord="266"/>
    </type>

    <type name="resource_as_realized_relationship_select"> 
      <select
        selectitems="Required_resource Resource_event"/>
      <graphic.element
        page="2"
        xcoord="262"
        ycoord="119"/>
    </type>

    <entity
      name="Resource_as_realized"
      >
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="quantity"
        optional="YES">
        <typename name="Value_with_unit"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="425"
        ycoord="187"/>
    </entity>

    <entity
      name="Resource_as_realized_relationship"
      >
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="relating">
        <typename name="Resource_as_realized"/>
      </explicit>
      <explicit
        name="related">
        <typename name="resource_as_realized_relationship_select"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="262"
        ycoord="187"/>
    </entity>

    <entity
      name="Resource_as_realized_resource_item"
      supertypes="Resource_as_realized"
      >
      <explicit
        name="resource_item">
        <typename name="Resource_item"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="425"
        ycoord="104"/>
