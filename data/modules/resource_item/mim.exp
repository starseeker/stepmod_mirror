SCHEMA Resource_item_mim;

USE FROM action_schema
(action_method,
 action_resource,
 action_resource_relationship,
 action_resource_type,
 supported_item);

USE FROM Classification_assignment_mim;

USE FROM measure_schema
(measure_value,
 measure_with_unit);

USE FROM process_property_representation_schema
(resource_property_representation);

USE FROM process_property_schema
(characterized_resource_definition,
 resource_property);

USE FROM representation_schema
(representation,
 representation_context,
 representation_item,
 value_representation_item);

USE FROM Value_with_unit_mim;

TYPE resource_item_classification_item = EXTENSIBLE SELECT BASED_ON classification_item WITH
  (action_resource,
   action_resource_relationship);
END_TYPE;

END_SCHEMA;

