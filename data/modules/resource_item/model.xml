<?xml version="1.0"?>

<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <description>
  
  </description>

  <application
    name="GraphicalEXPRESS"
    owner="Eurostep" url="www.eurostep.com"
    version="1.3"
    source="resource item arm.vsd"/>

  <schema name="Resource_item_arm">
    <interface
      kind="use"
      schema="Classification_assignment_arm">
    </interface>
    <interface
      kind="use"
      schema="Value_with_unit_arm">
    </interface>
    <type name="resource_assignment_item"> 
      <select
      extensible="YES"
        />
      <graphic.element
        page="2"
        xcoord="462"
        ycoord="328"/>
    </type>

    <type name="resource_item_classification_item"> 
      <select
      extensible="YES"
      genericentity="YES"
         basedon="classification_item"
        selectitems="Resource_item Resource_item_assignment Resource_item_relationship"/>
      <graphic.element
        page="2"
        xcoord="277"
        ycoord="257"/>
    </type>

    <type name="resource_item_select"> 
      <select
      extensible="YES"
        />
      <graphic.element
        page="2"
        xcoord="503"
        ycoord="113"/>
    </type>

    <entity
      name="Resource_group_relationship"
      supertypes="Resource_item_relationship"
      >
      <explicit
        name="quantity"
        optional="YES">
        <typename name="Value_with_unit"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="184"
        ycoord="106"/>
    </entity>

    <entity
      name="Resource_item"
      >
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="resource_item"
        optional="YES">
        <typename name="resource_item_select"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="380"
        ycoord="177"/>
    </entity>

    <entity
      name="Resource_item_assignment"
      >
      <explicit
        name="assigned_entity">
        <typename name="Resource_item"/>
      </explicit>
      <explicit
        name="item">
        <typename name="resource_assignment_item"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="462"
        ycoord="262"/>
    </entity>

    <entity
      name="Resource_item_relationship"
      >
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="relating">
        <typename name="Resource_item"/>
      </explicit>
      <explicit
        name="related">
        <typename name="Resource_item"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="184"
        ycoord="177"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="255"
      ycoord="92"/>
</schema>

  <schema name="Value_with_unit_arm">
    <entity
      name="Value_with_unit"
      >
      <graphic.element
        page="3"
        xcoord="297"
        ycoord="659"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="382"
      ycoord="92"/>
</schema>

  <schema name="Classification_assignment_arm">
    <type name="classification_item"> 
      <select
      extensible="YES"
      genericentity="YES"
        />
      <graphic.element
        page="4"
        xcoord="255"
        ycoord="106"/>
    </type>

    <graphic.element
      page="1"
      xcoord="123"
      ycoord="92"/>
</schema>

</express>
