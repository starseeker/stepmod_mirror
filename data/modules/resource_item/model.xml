<?xml version="1.0"?>

<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <description>
  
  </description>

  <application
    name="GraphicalEXPRESS"
    owner="Eurostep" url="www.eurostep.com"
    version="1.3"
    source="resource item arm.vsd"/>

  <schema name="Resource_item_arm">
    <interface
      kind="use"
      schema="Classification_arm">
    </interface>
    <interface
      kind="use"
      schema="Value_with_unit_arm">
    </interface>
    <type name="resource_item_classified_select"> 
      <select
         basedon="classified_select"
        selectitems="Resource_item Resource_item_relationship"/>
      <graphic.element
        page="2"
        xcoord="260"
        ycoord="255"/>
    </type>

    <type name="resource_item_select"> 
      <select
      extensible="YES"
        />
      <graphic.element
        page="2"
        xcoord="518"
        ycoord="113"/>
    </type>

    <entity
      name="Resource_group_relationship"
      supertypes="Resource_item_relationship"
      >
      <explicit
        name="quantity"
        optional="YES">
        <typename name="Value_with_unit"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="165"
        ycoord="106"/>
    </entity>

    <entity
      name="Resource_item"
      >
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="resource_item"
        optional="YES">
        <typename name="resource_item_select"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="380"
        ycoord="177"/>
    </entity>

    <entity
      name="Resource_item_relationship"
      >
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="relating">
        <typename name="Resource_item"/>
      </explicit>
      <explicit
        name="related">
        <typename name="Resource_item"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="165"
        ycoord="177"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="255"
      ycoord="92"/>
</schema>

  <schema name="Value_with_unit_arm">
    <entity
      name="Value_with_unit"
      >
      <graphic.element
        page="3"
        xcoord="297"
        ycoord="659"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="382"
      ycoord="92"/>
</schema>

  <schema name="Classification_arm">
    <type name="classified_select"> 
      <select
      extensible="YES"
      genericentity="YES"
        />
      <graphic.element
        page="4"
        xcoord="311"
        ycoord="609"/>
    </type>

    <graphic.element
      page="1"
      xcoord="127"
      ycoord="92"/>
</schema>

</express>
