   <express_ref linkend="resource_management:arm:Resource_management_arm.Resource_location.description"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Resource_management_arm.Resource_location.description">
<p>A description of the resource location.
</p>
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Resource_management_arm  Entity: Resource_location  Attribute: location 
   <express_ref linkend="resource_management:arm:Resource_management_arm.Resource_location.location"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
