  <schema name="Resource_management_arm">
    <interface
      kind="use"
      schema="Activity_arm">
    </interface>
    <interface
      kind="use"
      schema="Classification_arm">
    </interface>
    <interface
      kind="use"
      schema="Required_resource_arm">
    </interface>
    <interface
      kind="use"
      schema="Resource_item_arm">
    </interface>
    <interface
      kind="use"
      schema="State_observed_arm">
    </interface>
    <interface
      kind="use"
      schema="Time_interval_arm">
    </interface>
    <interface
      kind="use"
      schema="Value_with_unit_arm">
    </interface>
    <type name="allocated_item"> 
      <select
        selectitems="Managed_resource Resource_item"/>
      <graphic.element
        page="3"
        xcoord="325"
        ycoord="184"/>
    </type>

    <type name="allocated_resource_activity_item"> 
      <select
         basedon="activity_item"
        selectitems="Allocated_resource"/>
      <graphic.element
        page="3"
        xcoord="325"
        ycoord="340"/>
    </type>

    <type name="allocated_resource_classified_select"> 
      <select
         basedon="classified_select"
        selectitems="Allocated_required_resource_relationship Allocated_resource"/>
      <graphic.element
        page="3"
        xcoord="198"
        ycoord="340"/>
    </type>

    <type name="allocated_resource_time_interval_item"> 
      <select
         basedon="time_interval_item"
        selectitems="Allocated_resource_by_time_interval"/>
      <graphic.element
        page="3"
        xcoord="566"
        ycoord="170"/>
    </type>

    <type name="managed_resource_activity_item"> 
      <select
         basedon="activity_item"
        selectitems="Managed_resource"/>
      <graphic.element
        page="2"
        xcoord="453"
        ycoord="416"/>
    </type>

    <type name="managed_resource_classified_select"> 
      <select
         basedon="classified_select"
        selectitems="Managed_resource Managed_resource_relationship Resource_location"/>
      <graphic.element
        page="2"
        xcoord="178"
        ycoord="209"/>
    </type>

    <type name="managed_resource_state_of_select"> 
      <select
         basedon="state_of_select"
        selectitems="Managed_resource"/>
      <graphic.element
        page="2"
        xcoord="226"
        ycoord="416"/>
    </type>

    <type name="resource_location_item"> 
      <select
      extensible="YES"
        />
      <graphic.element
        page="2"
        xcoord="340"
        ycoord="70"/>
    </type>

    <entity
      name="Allocated_required_resource_relationship"
      >
      <explicit
        name="relating">
        <typename name="Allocated_resource"/>
      </explicit>
      <explicit
        name="related">
        <typename name="Required_resource"/>
      </explicit>
      <graphic.element
        page="3"
        xcoord="141"
        ycoord="262"/>
    </entity>

    <entity
      name="Allocated_resource"
      >
      <explicit
        name="item">
        <typename name="allocated_item"/>
      </explicit>
      <graphic.element
        page="3"
        xcoord="325"
        ycoord="262"/>
    </entity>

    <entity
      name="Allocated_resource_by_quantity"
      supertypes="Allocated_resource"
      >
      <explicit
        name="quantity">
        <typename name="Value_with_unit"/>
      </explicit>
      <graphic.element
        page="3"
        xcoord="453"
        ycoord="304"/>
    </entity>

    <entity
      name="Allocated_resource_by_time_interval"
      supertypes="Allocated_resource"
      >
      <graphic.element
        page="3"
        xcoord="453"
        ycoord="219"/>
    </entity>

    <entity
      name="Managed_resource"
      >
      <explicit
        name="item">
        <typename name="Resource_item"/>
      </explicit>
      <explicit
        name="quantity"
        optional="YES">
        <typename name="Value_with_unit"/>
      </explicit>
      <explicit
        name="location"
        optional="YES">
        <typename name="Resource_location"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="340"
        ycoord="347"/>
    </entity>

    <entity
      name="Managed_resource_relationship"
      >
      <explicit
        name="relating">
        <typename name="Managed_resource"/>
      </explicit>
      <explicit
        name="related">
        <typename name="Managed_resource"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="178"
        ycoord="347"/>
    </entity>

    <entity
      name="Resource_location"
      >
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="location"
        optional="YES">
        <typename name="resource_location_item"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="340"
        ycoord="134"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="235"
      ycoord="111"/>
</schema>

