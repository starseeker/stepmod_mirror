<?xml version="1.0"?>

<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <description>
  
  </description>

  <application
    name="GraphicalEXPRESS"
    owner="Eurostep" url="www.eurostep.com"
    version="1.3"
    source="resource_management.vsd"/>

  <schema name="Resource_management_arm">
    <interface
      kind="use"
      schema="Classification_arm">
    </interface>
    <interface
      kind="use"
      schema="Required_resource_arm">
    </interface>
    <interface
      kind="use"
      schema="Resource_item_arm">
    </interface>
    <interface
      kind="use"
      schema="Value_with_unit_arm">
    </interface>
    <type name="allocated_resource_classified_select"> 
      <select
         basedon="classified_select"
        selectitems="Resource_allocation"/>
      <graphic.element
        page="3"
        xcoord="187"
        ycoord="453"/>
    </type>

    <type name="allocation_of_select"> 
      <select
        selectitems="Provision_location_relationship Provision_of_resource Resource_location_relationship"/>
      <graphic.element
        page="3"
        xcoord="362"
        ycoord="255"/>
    </type>

    <type name="managed_resource_classified_select"> 
      <select
         basedon="classified_select"
        selectitems="Provision_location_relationship Resource_location"/>
      <graphic.element
        page="2"
        xcoord="199"
        ycoord="212"/>
    </type>

    <type name="provided_by_select"> 
      <select
      extensible="YES"
        />
      <graphic.element
        page="2"
        xcoord="613"
        ycoord="311"/>
    </type>

    <type name="resource_allocation_select"> 
      <select
      extensible="YES"
        />
      <graphic.element
        page="3"
        xcoord="362"
        ycoord="453"/>
    </type>

    <type name="resource_location_select"> 
      <select
      extensible="YES"
        />
      <graphic.element
        page="2"
        xcoord="168"
        ycoord="439"/>
    </type>

    <entity
      name="Provision_location_relationship"
      >
      <explicit
        name="relating">
        <typename name="Provision_of_resource"/>
      </explicit>
      <explicit
        name="related">
        <typename name="Resource_location"/>
      </explicit>
      <explicit
        name="quantity"
        optional="YES">
        <typename name="Value_with_unit"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="381"
        ycoord="262"/>
    </entity>

    <entity
      name="Provision_of_resource"
      >
      <explicit
        name="provision_of">
        <typename name="Resource_item"/>
      </explicit>
      <explicit
        name="provided_by">
        <typename name="provided_by_select"/>
      </explicit>
      <explicit
        name="quantity"
        optional="YES">
        <typename name="Value_with_unit"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="483"
        ycoord="375"/>
    </entity>

    <entity
      name="Resource_allocation"
      >
      <explicit
        name="allocation_of">
        <typename name="allocation_of_select"/>
      </explicit>
      <explicit
        name="allocation_to">
        <typename name="resource_allocation_select"/>
      </explicit>
      <explicit
        name="quantity"
        optional="YES">
        <typename name="Value_with_unit"/>
      </explicit>
      <graphic.element
        page="3"
        xcoord="362"
        ycoord="347"/>
    </entity>

    <entity
      name="Resource_allocation_required_resource_relationship"
      >
      <explicit
        name="relating">
        <typename name="Resource_allocation"/>
      </explicit>
      <explicit
        name="related">
        <typename name="Required_resource"/>
      </explicit>
      <graphic.element
        page="3"
        xcoord="188"
        ycoord="347"/>
    </entity>

    <entity
      name="Resource_location"
      >
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="location"
        optional="YES">
        <typename name="resource_location_select"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="279"
        ycoord="375"/>
    </entity>

    <entity
      name="Resource_location_relationship"
      >
      <explicit
        name="relating">
        <typename name="Resource_item"/>
      </explicit>
      <explicit
        name="related">
        <typename name="Resource_location"/>
      </explicit>
      <explicit
        name="quantity">
        <typename name="Value_with_unit"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="279"
        ycoord="503"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="243"
      ycoord="218"/>
</schema>

  <schema name="Classification_arm">
    <type name="classified_select"> 
      <select
      extensible="YES"
      genericentity="YES"
        />
      <graphic.element
        page="4"
        xcoord="297"
        ycoord="609"/>
    </type>

    <graphic.element
      page="1"
      xcoord="70"
      ycoord="218"/>
</schema>

  <schema name="Required_resource_arm">
    <entity
      name="Required_resource"
      >
      <graphic.element
        page="5"
        xcoord="283"
        ycoord="602"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="72"
      ycoord="98"/>
</schema>

  <schema name="Resource_item_arm">
    <entity
      name="Resource_item"
      >
      <graphic.element
        page="6"
        xcoord="297"
        ycoord="595"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="411"
      ycoord="98"/>
</schema>

  <schema name="Value_with_unit_arm">
    <entity
      name="Value_with_unit"
      >
      <graphic.element
        page="7"
        xcoord="311"
        ycoord="609"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="411"
      ycoord="218"/>
</schema>

</express>
