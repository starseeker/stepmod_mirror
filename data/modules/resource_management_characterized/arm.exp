SCHEMA Resource_management_characterized_arm;

USE FROM Condition_assignment_arm;

USE FROM Contract_arm;

USE FROM Date_time_assignment_arm;

USE FROM Document_assignment_arm;

USE FROM Effectivity_application_arm;

USE FROM Identification_assignment_arm;

USE FROM Justification_arm;

USE FROM Person_organization_assignment_arm;

USE FROM Property_assignment_arm;

USE FROM Required_resource_characterized_arm;

USE FROM Resource_management_arm;

USE FROM State_definition_arm;

USE FROM State_observed_arm;

TYPE resource_management_contract_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON contract_item WITH
(Managed_resource,
 Resource_event,
 Resource_location);
END_TYPE;

TYPE resource_management_date_or_date_time_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON date_or_date_time_item WITH
(Managed_resource,
 Resource_event,
 Resource_location);
END_TYPE;

TYPE resource_management_document_element_select = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON document_element_select WITH
(Resource_event,
 Resource_location);
END_TYPE;

TYPE resource_management_effectivity_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON effectivity_item WITH
(Managed_resource,
 Managed_resource_relationship,
 Resource_location);
END_TYPE;

TYPE resource_management_identification_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON identification_item WITH
(Managed_resource,
 Resource_event,
 Resource_location);
END_TYPE;

TYPE resource_management_item_in_scope = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON item_in_scope WITH
(Managed_resource,
 Managed_resource_relationship,
 Resource_event,
 Resource_event_relationship,
 Resource_location,
 Resource_location_relationship);
END_TYPE;

TYPE resource_management_justification_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON justification_item WITH
(Resource_event);
END_TYPE;

TYPE resource_management_person_or_person_in_organization_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON person_or_person_in_organization_item WITH
(Managed_resource,
 Resource_event,
 Resource_location);
END_TYPE;

TYPE resource_management_property_assignment_select = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON property_assignment_select WITH
(Managed_resource,
 Resource_event,
 Resource_location);
END_TYPE;

TYPE resource_management_state_definition_of_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON state_definition_of_item WITH
(Managed_resource,
 Resource_event,
 Resource_location);
END_TYPE;

TYPE resource_management_state_of_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON state_of_item WITH
(Managed_resource,
 Resource_event,
 Resource_location);
END_TYPE;

END_SCHEMA;