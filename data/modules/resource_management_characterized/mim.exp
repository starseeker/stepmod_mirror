SCHEMA Resource_management_characterized_mim;

USE FROM action_schema
(action,
 action_relationship,
 action_resource,
 action_resource_relationship);

USE FROM Condition_assignment_mim;

USE FROM Contract_mim;

USE FROM Date_time_assignment_mim;

USE FROM Document_assignment_mim;

USE FROM Effectivity_application_mim;

USE FROM Identification_assignment_mim;

USE FROM Justification_mim;

USE FROM Person_organization_assignment_mim;

USE FROM process_property_schema
(process_property_association,
 property_or_shape_select,
 property_process);

USE FROM Property_assignment_mim;

USE FROM Required_resource_characterized_mim;

USE FROM Resource_management_mim;

USE FROM State_definition_mim;

USE FROM State_observed_mim;

TYPE resource_management_contract_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON contract_item WITH
(action,
 action_resource);
END_TYPE;

TYPE resource_management_date_and_time_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON date_and_time_item WITH
(action,
 action_resource);
END_TYPE;

TYPE resource_management_date_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON date_item WITH
(action,
 action_resource);
END_TYPE;

TYPE resource_management_document_reference_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON document_reference_item WITH
(action,
 action_resource);
END_TYPE;

TYPE resource_management_effectivity_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON effectivity_item WITH
(action_resource,
 action_resource_relationship);
END_TYPE;

TYPE resource_management_identification_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON identification_item WITH
(action,
 action_resource);
END_TYPE;

TYPE resource_management_item_in_scope = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON item_in_scope WITH
(action,
 action_relationship,
 action_resource,
 action_resource_relationship);
END_TYPE;

TYPE resource_management_justification_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON justification_item WITH
(action);
END_TYPE;

TYPE resource_management_organization_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON organization_item WITH
(action,
 action_resource);
END_TYPE;

TYPE resource_management_person_and_organization_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON person_and_organization_item WITH
(action,
 action_resource);
END_TYPE;

TYPE resource_management_state_of_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON state_of_item WITH
(action,
 action_resource);
END_TYPE;

TYPE resource_management_state_type_of_item = EXTENSIBLE GENERIC_ENTITY SELECT BASED_ON state_type_of_item WITH
(action,
 action_resource);
END_TYPE;

END_SCHEMA;