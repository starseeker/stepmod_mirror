  <schema name="Risk_arm">
    <interface
      kind="use"
      schema="Activity_arm">
    </interface>
    <interface
      kind="use"
      schema="Document_assignment_arm">
    </interface>
    <interface
      kind="use"
      schema="Event_assignment_arm">
    </interface>
    <interface
      kind="use"
      schema="Measure_representation_arm">
    </interface>
    <interface
      kind="use"
      schema="Probability_arm">
    </interface>
    <interface
      kind="use"
      schema="Product_relationship_arm">
    </interface>
    <interface
      kind="use"
      schema="Product_view_definition_relationship_arm">
    </interface>
    <interface
      kind="use"
      schema="Property_assignment_arm">
    </interface>
    <type name="lessons_learned_select">
      <select
        extensible="YES"
        basedon="documented_element_select"
        selectitems="Risk Risk_impact_assignment Risk_perception_source_assignment Risk_treatment"
      />
    </type>

    <type name="risk_activity">
      <select
        selectitems="Risk_acceptance Risk_analysis Risk_assessment Risk_communication Risk_control Risk_estimation Risk_evaluation Risk_identification Risk_treatment"
      />
    </type>

    <type name="risk_activity_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="activity_item"
        selectitems="Lessons_learned Risk Risk_assessment Risk_consequence Risk_control risk_criteria_select Risk_estimation Risk_evaluation Risk_identification Risk_impact Risk_impact_assignment Risk_perception_context Risk_perception_relationship Risk_perception_source_assignment risk_perception_source_item Risk_relationship Risk_treatment Assigned_property Event_assignment"
      />
    </type>

    <type name="risk_communication_select">
      <select
        selectitems="Lessons_learned Risk Risk_assessment Risk_consequence Risk_control Risk_estimation Risk_evaluation Risk_identification Risk_impact_assignment Risk_perception Risk_perception_context Risk_perception_relationship Risk_perception_source_assignment Risk_treatment Assigned_property Event_assignment"
      />
    </type>

    <type name="risk_criteria_select">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="activity_item"
        selectitems="Risk_perception"
      />
    </type>

    <type name="risk_estimation_select">
      <select
        selectitems="Risk_consequence Risk_impact_assignment Assigned_property Event_assignment"
      />
    </type>

    <type name="risk_event_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="event_item"
        selectitems="Risk_perception"
      />
    </type>

    <type name="risk_impact_item">
      <select
        extensible="YES"
        genericentity="YES"
      />
    </type>

    <type name="risk_perception_source_item">
      <select
        extensible="YES"
        genericentity="YES"
        selectitems="Activity Activity_method Product"
      />
    </type>

    <type name="risk_property_assignment_select">
      <select
        extensible="YES"
        basedon="property_assignment_select"
        selectitems="Risk_perception Event"
      />
    </type>

    <type name="risk_treatment_select">
      <select
        selectitems="Risk_consequence Risk_impact Risk_perception Risk_perception_relationship Risk_relationship Assigned_property"
      />
    </type>

    <entity name="Contained_acceptance"
      supertypes="Risk_activity_structure"
    >
      <explicit
        name="parent">
        <typename name="Risk_control"/>
        <redeclaration
