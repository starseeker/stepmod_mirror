      </explicit>
      <derived
        name="items"
        expression="[associated_risk]">
        <aggregate type="SET"
          lower="1"
          upper="?"
        />
        <typename name="Risk_perception"/>
        <redeclaration
          entity-ref="Event_assignment"/>
      </derived>
      <derived
        name="role"
        expression="&apos;risk of event&apos;">
        <builtintype type="STRING"
        />
        <redeclaration
          entity-ref="Event_assignment"/>
      </derived>
