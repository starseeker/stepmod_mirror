    >
      <explicit
        name="property">
        <typename name="Risk_level"/>
        <redeclaration
          entity-ref="Property_representation"/>
      </explicit>
      <explicit
        name="criticality_factor">
        <typename name="Property_value_representation"/>
        <redeclaration
          entity-ref="Property_definition_representation"
          old_name="rep"/>
      </explicit>
    </entity>

    <entity name="Risk_communicated_items"
      supertypes="Applied_activity_assignment"
    >
      <explicit
        name="assigned_activity">
        <typename name="Risk_communication"/>
        <redeclaration
          entity-ref="Applied_activity_assignment"/>
      </explicit>
      <explicit
        name="items">
        <aggregate type="SET"
          lower="1"
          upper="?"
        />
        <typename name="risk_communication_select"/>
        <redeclaration
          entity-ref="Applied_activity_assignment"/>
      </explicit>
      <derived
        name="role"
        expression="&apos;risk_communicated_items&apos;">
        <builtintype type="STRING"
        />
        <redeclaration
          entity-ref="Applied_activity_assignment"/>
      </derived>
    </entity>

    <entity name="Risk_communication"
