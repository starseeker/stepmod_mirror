ENTITY Contained_communication
  SUBTYPE OF (Risk_activity_structure);
  SELF\Activity_relationship.relating_activity : Risk_control;
  SELF\Activity_relationship.related_activity : Risk_communication;
END_ENTITY;

ENTITY Contained_estimation
  SUBTYPE OF (Risk_activity_structure);
  SELF\Activity_relationship.relating_activity : Risk_analysis;
  SELF\Activity_relationship.related_activity : Risk_estimation;
END_ENTITY;

ENTITY Contained_evaluation
  SUBTYPE OF (Risk_activity_structure);
  SELF\Activity_relationship.relating_activity : Risk_assessment;
  SELF\Activity_relationship.related_activity : Risk_evaluation;
END_ENTITY;

ENTITY Contained_identification
  SUBTYPE OF (Risk_activity_structure);
  SELF\Activity_relationship.relating_activity : Risk_analysis;
  SELF\Activity_relationship.related_activity : Risk_identification;
