<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Schedule_arm  Type: ss_activity_item 
   <express_ref linkend="schedule:arm:Schedule_arm.ss_activity_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Schedule_arm.ss_activity_item">
  Using
  <express_ref linkend="activity:arm:Activity_arm.Applied_activity_assignment"/>,
  entities identified as items in <b>ss_activity_item</b>
  can be associated with an 
  <express_ref linkend="activity:arm:Activity_arm.Activity"/>.  
  along with their associated roll of input, ouput or constraint.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Schedule_arm  Type: ss_activity_method_item 
   <express_ref linkend="schedule:arm:Schedule_arm.ss_activity_method_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Schedule_arm.ss_activity_method_item">
  Using
  <express_ref linkend="activity_method_assignment:arm:Activity_method_assignment_arm.Applied_activity_method_assignment"/>,
  entities identified as items in <b>ss_activity_method_item</b>
  can be associated with an 
  <express_ref linkend="activity_method:arm:Activity_method_arm.Activity_method"/>.  
  along with a textural description of the association's role.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Schedule_arm  Type: ss_activity_realization_select 
   <express_ref linkend="schedule:arm:Schedule_arm.ss_activity_realization_select"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Schedule_arm.ss_activity_realization_select">
  Using
  <express_ref linkend="activity_method_implementation:arm:Activity_method_implementation_arm.Activity_method_realization"/>,
  entities identified as items in <b>ss_activity_realization_select</b> can be associated with an 
  <express_ref linkend="activity_method:arm:Activity_method_arm.Activity_method"/>.  
  These provide further specification of how the associated activity method is to be performed with the 
  <express_ref linkend="activity_method:arm:Activity_method_arm.Activity_method"/>.  
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Schedule_arm  Type: ss_characterized_activity_definition 
   <express_ref linkend="schedule:arm:Schedule_arm.ss_characterized_activity_definition"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Schedule_arm.ss_characterized_activity_definition">
  Using
  <express_ref linkend="process_property_assignment:arm:Process_property_assignment_arm.Activity_property"/>,
  entities identified as items in <b>ss_characterized_activity_definition</b> 
  provide the object of which the  
  <express_ref linkend="process_property_assignment:arm:Process_property_assignment_arm.Activity_property"/> 
  identifies a characteristic
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Schedule_arm  Type: ss_characterized_resource_select 
   <express_ref linkend="schedule:arm:Schedule_arm.ss_characterized_resource_select"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Schedule_arm.ss_characterized_resource_select">
  Using
  <express_ref linkend="resource_property_assignment:arm:Resource_property_assignment_arm.Resource_property"/>,
   entities identified as items in <b>ss_characterized_resource_select</b> 
   provide the object of which the 
   <express_ref linkend="resource_property_assignment:arm:Resource_property_assignment_arm.Resource_property"/>
   identifies a characteristic. 
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Schedule_arm  Type: ss_condition_evaluation_item 
   <express_ref linkend="schedule:arm:Schedule_arm.ss_condition_evaluation_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Schedule_arm.ss_condition_evaluation_item">
  Using
  <express_ref linkend="condition_evaluation:arm:Condition_evaluation_arm.Condition_evaluation_assignment"/>
  entities identified as items in <b>ss_condition_evaluation_item</b> 
  identify the product or activity data to which the 
  <express_ref linkend="condition_evaluation:arm:Condition_evaluation_arm.Condition_evaluation"/>
  has been assigned.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Schedule_arm  Type: ss_condition_evaluation_parameter_item 
   <express_ref linkend="schedule:arm:Schedule_arm.ss_condition_evaluation_parameter_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Schedule_arm.ss_condition_evaluation_parameter_item">
  Using
  <express_ref linkend="condition_evaluation:arm:Condition_evaluation_arm.Condition_evaluation_parameter"/>
  entities identified as items in <b>ss_condition_evaluation_parameter_item</b>
  are product or activity data which acted as a parameter to the 
  <express_ref linkend="condition_evaluation:arm:Condition_evaluation_arm.Condition_evaluation"/>
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Schedule_arm  Type: ss_event_item 
   <express_ref linkend="schedule:arm:Schedule_arm.ss_event_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Schedule_arm.ss_event_item">
  Using
  <express_ref linkend="event_assignment:arm:Event_assignment_arm.Event_assignment"/>,
  an
  <express_ref linkend="event:arm:Event_arm.Event"/> 
  can be assigned to the entities in <b>ss_event_item</b>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Schedule_arm  Type: ss_property_assignment_select 
   <express_ref linkend="schedule:arm:Schedule_arm.ss_property_assignment_select"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Schedule_arm.ss_property_assignment_select">
  Using
  <express_ref linkend="property_assignment:arm:Property_assignment_arm.Assigned_property"/>
  a property  
  can be assigned to the entities in <b>ss_property_assignment_select</b>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Schedule_arm  Type: ss_required_resource_item 
   <express_ref linkend="schedule:arm:Schedule_arm.ss_required_resource_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Schedule_arm.ss_required_resource_item">
  Using
  <express_ref linkend="required_resource:arm:Required_resource_arm.Required_resource_assignment"/>,
  entities identified as items in <b>ss_required_resource_item</b>
  define the items to which there is an association with the  
  <express_ref linkend="required_resource:arm:Required_resource_arm.Required_resource"/>,
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Schedule_arm  Type: ss_resource_as_realized_item 
   <express_ref linkend="schedule:arm:Schedule_arm.ss_resource_as_realized_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Schedule_arm.ss_resource_as_realized_item">
  Using
  <express_ref linkend="resource_as_realized:arm:Resource_as_realized_arm.Resource_as_realized_assignment"/>,
  entities identified as items in <b>ss_resource_as_realized_item</b>
  define the items that are used or consumed by the  
  <express_ref linkend="resource_as_realized:arm:Resource_as_realized_arm.Resource_as_realized"/>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Schedule_arm  Type: ss_resource_assignment_item 
   <express_ref linkend="schedule:arm:Schedule_arm.ss_resource_assignment_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Schedule_arm.ss_resource_assignment_item">
  Using
  <express_ref linkend="resource_item:arm:Resource_item_arm.Resource_item_assignment"/>,
  <b>ss_resource_assignment_item</b> identifies the resource assignment item to which the  
    <express_ref linkend="resource_item:arm:Resource_item_arm.Resource_item"/>, is assigned
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Schedule_arm  Type: ss_resource_item_select 
   <express_ref linkend="schedule:arm:Schedule_arm.ss_resource_item_select"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Schedule_arm.ss_resource_item_select">
  Using
  <express_ref linkend="resource_item:arm:Resource_item_arm.Resource_item"/>,
  <b>ss_resource_item_select</b> identifies the item that is designated as a resource within the 
  application context.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Schedule_arm  Type: ss_state_definition_of_item 
   <express_ref linkend="schedule:arm:Schedule_arm.ss_state_definition_of_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Schedule_arm.ss_state_definition_of_item">
  Using
  <express_ref linkend="state_definition:arm:State_definition_arm.Applied_state_definition_assignment"/>,
  a
  <express_ref linkend="state_definition:arm:State_definition_arm.State_definition"/>
  can be assigned to the entities in <b>ss_state_definition_of_item</b>.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Schedule_arm  Type: ss_state_of_item 
   <express_ref linkend="schedule:arm:Schedule_arm.ss_state_of_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Schedule_arm.ss_state_of_item">
  Using
  <express_ref linkend="state_observed:arm:State_observed_arm.Applied_state_assignment"/>,
  a
  <express_ref linkend="state_observed:arm:State_observed_arm.State"/>
  can be assigned to the entities in <b>ss_state_of_item</b>.
</ext_description>
</ext_descriptions>
