<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.9 2002/02/25 12:10:19 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.9"
    source="../data/modules/security_classification/mim.exp"/>

  <schema name="Security_classification_mim">
    <interface
      kind="use"
      schema="Approval_mim">
    </interface>

    <interface
      kind="use"
      schema="Date_time_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="Person_organization_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="Alias_identification_mim">
    </interface>

    <interface
      kind="use"
      schema="security_classification_schema">
      <interfaced.item
        name="security_classification"/>
      <interfaced.item
        name="security_classification_level"/>
    </interface>

    <interface
      kind="use"
      schema="management_resources_schema">
      <interfaced.item
        name="security_classification_assignment"/>
    </interface>

    <type name="security_classification_item">
      <select
        extensible="YES"
        selectitems="null">
      </select>

    </type>
    <type name="security_classification_approval_item">
      <select
        basedon="approval_item"
        selectitems="security_classification">
      </select>

    </type>
    <type name="security_classification_date_item">
      <select
        basedon="date_item"
        selectitems="security_classification">
      </select>

    </type>
    <type name="security_classification_date_and_time_item">
      <select
        basedon="date_and_time_item"
        selectitems="security_classification">
      </select>

    </type>
    <type name="security_classification_organization_item">
      <select
        basedon="organization_item"
        selectitems="security_classification">
      </select>

    </type>
    <type name="security_classification_person_and_organization_item">
      <select
        basedon="person_and_organization_item"
        selectitems="security_classification">
      </select>

    </type>
    <type name="security_classification_alias_identification_item">
      <select
        basedon="alias_identification_item"
        selectitems="security_classification">
      </select>

    </type>
    <entity
      name="applied_security_classification_assignment"
      supertypes="security_classification_assignment">
      <explicit
        name="items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="security_classification_item"/>
      </explicit>
    </entity>

  </schema>

</express>
