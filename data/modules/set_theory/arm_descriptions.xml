   <express_ref linkend="set_theory:arm:Set_theory_arm.Complement.set_1"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Set_theory_arm.Complement.set_1">
  the 
  <express_ref linkend="class:arm:Class_arm.Class"/>
