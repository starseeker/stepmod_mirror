<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-04-19T18:23:22" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="shape_composition_arm schema_instance"/>
   <schema name="Shape_composition_arm">
      <interface kind="use" schema="Shape_property_assignment_arm"/>
      <entity name="Composite_group_shape_element" supertypes="Composite_shape_element"/>
      <entity name="Composite_shape_element" abstract.supertype="YES" supertypes="Shape_element" super.expression="ONEOF (Composite_group_shape_element, Composite_unit_shape_element)">
         <inverse name="composing_relationships" entity="Shape_element_composing_relationship" attribute="relating">
            <inverse.aggregate type="SET" lower="2" upper="?"/>
         </inverse>
      </entity>
      <entity name="Composite_unit_shape_element" supertypes="Composite_shape_element"/>
      <entity name="Shape_element_boundary_relationship" supertypes="Shape_element_relationship"/>
      <entity name="Shape_element_composing_relationship" supertypes="Shape_element_constituent_relationship">
         <explicit name="relating">
            <typename name="Composite_shape_element"/>
            <redeclaration entity-ref="Shape_element_relationship"/>
         </explicit>
      </entity>
      <entity name="Shape_element_constituent_relationship" supertypes="Shape_element_relationship" super.expression="Shape_element_composing_relationship">
         <unique label="UR1">
            <unique.attribute entity-ref="Shape_element_constituent_relationship" attribute="related"/>
            <unique.attribute entity-ref="Shape_element_constituent_relationship" attribute="relating"/>
         </unique>
         <where label="WR1" expression=" SELF \ Shape_element_relationship . related &lt;&gt; SELF \ Shape_element_relationship . relating "/>
      </entity>
      <entity name="Shape_element_locating_relationship" supertypes="Shape_element_constituent_relationship">
         <explicit name="locating_shape_element">
            <typename name="Shape_element"/>
            <redeclaration entity-ref="Shape_element_relationship" old_name="relating"/>
         </explicit>
         <explicit name="located_shape_element">
            <typename name="Shape_element"/>
            <redeclaration entity-ref="Shape_element_relationship" old_name="related"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute entity-ref="Shape_element_locating_relationship" attribute="located_shape_element"/>
            <unique.attribute entity-ref="Shape_element_locating_relationship" attribute="locating_shape_element"/>
         </unique>
      </entity>
   </schema>
</express>
