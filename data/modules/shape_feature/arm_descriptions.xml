<!DOCTYPE ext_descriptions SYSTEM "../../../dtd/description.dtd">
<ext_descriptions module_directory="shape_feature" schema_file="arm.xml" describe.selects="YES" describe.subtype_constraints="YES" rcs.date="$Date: 2006/03/14 11:24:23 $" rcs.revision="$Revision: 1.10 $">
<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: Shape_feature_arm
   <express_ref linkend="shape_feature:arm:Shape_feature_arm"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Shape_feature_arm">
</ext_description>



<!-- ++++++++++ Shape_feature_occurrence +++++++++++ -->
<ext_description linkend="Shape_feature_arm.Shape_feature_occurrence">
	A <b>Shape_feature_occurrence</b> is a type of 
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element"/>.
	A <b>Shape_feature_occurrence</b> is one of an 
	<express_ref linkend="Shape_feature:arm:Shape_feature_arm.Instanced_feature"/>
	or a
	<express_ref linkend="Shape_feature:arm:Shape_feature_arm.Placed_feature"/>.
</ext_description>

<!-- ++++++++++ Geometric_model_constituent_relationship ++++++++++++ -->
<ext_description linkend="Shape_feature_arm.Geometric_model_constituent_relationship">
	A <b>Geometric_model_constituent_relationship</b> is a type of 
	<express_ref linkend="Contextual_shape_positioning:arm:Contextual_shape_positioning_arm.Geometric_placement_operation"/>.
</ext_description>

<!-- constituent_element -->
<ext_description linkend="Shape_feature_arm.Geometric_model_constituent_relationship.constituent_element">
	specifies a role of the 
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element"/> 
	for the <b>Geometric_model_constituent_relationship</b>.
</ext_description>

<!-- constituent_model -->
<ext_description linkend="Shape_feature_arm.Geometric_model_constituent_relationship.constituent_model">
	specifies a role of the 
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model"/> 
	for the <b>Geometric_model_constituent_relationship</b>.
</ext_description>

<!-- composed_model -->
<ext_description linkend="Shape_feature_arm.Geometric_model_constituent_relationship.composed_model">
	specifies an inverse relationship that specifies that the existence of the <b>Geometric_model_constituent_relationship</b> is dependent on the existence of the 
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model"/> 
	that specifies the <b>Geometric_model_constituent_relationship</b> as its 
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model.items"/>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Shape_feature_arm.Geometric_model_constituent_relationship.wr:WR1">
<p>
	<b>Geometric_model_constituent_relationship</b> shall be complex with 
	<express_ref linkend="contextual_shape_positioning:arm:Contextual_shape_positioning_arm.Geometric_placement"/>
	or with 
	<express_ref linkend="Contextual_shape_positioning:arm:Contextual_shape_positioning_arm.Geometric_operator_transformation"/>.
</p>
</ext_description>

<!-- Wr2 -->
<ext_description linkend="Shape_feature_arm.Geometric_model_constituent_relationship.wr:WR2">
	The <b>composed_model</b> shall not be a 
	<express_ref linkend="Contextual_shape_positioning:arm:Contextual_shape_positioning_arm.Geometric_placement_model"/>.	
</ext_description>

<!-- +++++++++++ Instanced_feature +++++++++++ -->
<ext_description linkend="Shape_feature_arm.Instanced_feature">
	An <b>Instanced_feature</b> is a type of 
	<express_ref linkend="Shape_feature:arm:Shape_feature_arm.Shape_feature_occurrence"/> 
	and a type of 
	<express_ref linkend="Shape_feature:arm:Shape_feature_arm.Shape_feature_definition"/>.
	An <b>Instanced_feature</b> is the identification of a feature as an element of the
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Item_shape"/>  
	of a product. All geometric data defining the feature are defined in
	the coordinate space of the product.
</ext_description>

<!-- ++++++++++ Shape_feature_definition +++++++++++ -->
<ext_description linkend="Shape_feature_arm.Shape_feature_definition">
	A <b>Shape_feature_definition</b> is a type of 
	<express_ref linkend="Characterizable_object:arm:Characterizable_object_arm.Characterizable_object"/>.
	A <b>Shape_feature_definition</b> is a set of parameters and type information
	representing a feature that is either defined as a component of an item
	(<express_ref linkend="Shape_feature:arm:Shape_feature_arm.Instanced_feature"/>) 
	or independently in its own coordinate space for multiple use
	(<express_ref linkend="Shape_feature:arm:Shape_feature_arm.Placed_feature"/>).
</ext_description>

<!-- feature_model -->
<ext_description linkend="Shape_feature_arm.Shape_feature_definition.feature_model">
	specifies a role of the
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model"/>
	for the <b>Shape_feature_definition</b>.
</ext_description>

<!-- of_shape -->
<ext_description linkend="Shape_feature_arm.Shape_feature_definition.of_shape">
	specifies an inverse relationship that specifies that the existence of the <b>Shape_feature_definition</b> is dependent on the existence of the 
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Item_shape"/> 
	that specifies the <b>Shape_feature_definition</b> as its 
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Item_shape.described_element"/>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Shape_feature_arm.Shape_feature_definition.wr:WR1">
	The <b>feature_model</b> shall exist.
</ext_description>

<!-- +++++++++++ Placed_feature +++++++++++ -->
<ext_description linkend="Shape_feature_arm.Placed_feature">
	A <b>Placed_feature</b> is a type of 
	<express_ref linkend="Shape_feature:arm:Shape_feature_arm.Shape_feature_occurrence"/>.
	A <b>Placed_feature</b> is the occurrence of a feature on an 
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Item_shape"/> 
	of a product where the definition of the feature is given in its own coordinate space.
</ext_description>

<!-- definition -->
<ext_description linkend="Shape_feature_arm.Placed_feature.definition">
specifies a role of the <express_ref linkend="Shape_feature:arm:Shape_feature_arm.Shape_feature_definition"/> for the <b>Placed_feature</b>.
</ext_description>
<!-- feature_shape -->

<ext_description linkend="Shape_feature_arm.Placed_feature.feature_shape">
specifies an inverse relationship that specifies that the existence of the <b>Placed_feature</b> is dependent on the existence of the <express_ref linkend="Shape_feature:arm:Shape_feature_arm.Geometric_model_constituent_relationship"/> that specifies the <b>Placed_feature</b> as its <express_ref linkend="Shape_feature:arm:Shape_feature_arm.Geometric_model_constituent_relationship.constituent_element"/>.
</ext_description>


<!-- +++++++++++ get_geometric_model +++++++++++ -->
<ext_description linkend="Shape_feature_arm.get_geometric_model">
The <b>get_geometric_model</b> function returns the set of <express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model">Geometric_models</express_ref>
that are associated to <b>input</b> by means of <express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Shape_description_association">Shape_description_associations</express_ref>.
</ext_description>

<!-- input -->
<ext_description linkend="Shape_feature_arm.get_geometric_model.input">
	the specified set of 
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Item_shape"/>.
</ext_description>


</ext_descriptions>
