<!-- ++++++++++ Feature_definition +++++++++++ -->
<ext_description linkend="Shape_feature_arm.Feature_definition">
	A <b>Feature_definition</b> is a type of 
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Characterizable_object"/>.
</ext_description>

<!-- feature_model -->
<ext_description linkend="Shape_feature_arm.Feature_definition.feature_model">
	specifies a role of the
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model"/>
	for the <b>Feature_definition</b>.
</ext_description>

<!-- ++++++++++ Feature_occurrence +++++++++++ -->
<ext_description linkend="Shape_feature_arm.Feature_occurrence">
A <b>Feature_occurrence</b> is a type of <express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element"/>.
</ext_description>

