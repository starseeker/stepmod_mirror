<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: shape_feature_mim

++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="shape_feature_mim">
</ext_description>

<!-- +++++++++++ shape_feature_definition +++++++++++ -->
<ext_description linkend="Shape_feature_mim.shape_feature_definition">
	A <b>shape_feature_definition</b> is a type of 
	<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.characterized_object"/>
	that implements the ARM concept of
	<express_ref linkend="Shape_feature:arm:Shape_feature_arm.Shape_feature_definition"/>.
	A <b>shape_feature_definition</b> is a preconceived form pattern.
</ext_description>

<!-- +++++++++++ instanced_feature +++++++++++ -->
<ext_description linkend="Shape_feature_mim.instanced_feature">
	An <b>instanced_feature</b> is a type of 
	<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.shape_aspect"/> 
	and a type of 
	<express_ref linkend="Shape_feature:mim:Shape_feature_mim.shape_feature_definition"/>
	that implements the ARM concept of
	<express_ref linkend="Shape_feature:arm:Shape_feature_arm.Instanced_feature"/>.
	An <b>instanced_feature</b> is the identification of a preconceived form pattern on a part. 
</ext_description>

<!-- WR1 -->
<ext_description linkend="Shape_feature_mim.instanced_feature.wr:WR1">
The <express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.property_definition.definition"/>
of <express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.shape_aspect.of_shape"/> 
shall be of type <express_ref linkend="product_definition_schema:ir_express:product_definition_schema.product_definition"/>.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Shape_feature_mim.instanced_feature.wr:WR2">
The <express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.shape_aspect.product_definitional"/>
inherited from <express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.shape_aspect"/> 
shall be set to TRUE.
</ext_description>


<!-- +++++++++++ placed_feature +++++++++++ -->
<ext_description linkend="Shape_feature_mim.placed_feature">
	A <b>placed_feature</b> is a type of 
	<express_ref linkend="product_property_definition_schema:ir_express:product_property_definition_schema.shape_aspect"/>
	that implements the ARM concept of
	<express_ref linkend="Shape_feature:arm:Shape_feature_arm.Placed_feature"/>.
	A <b>placed_feature</b> is the identification of a preconceived form pattern on a part as a result of the placement of a
	<express_ref linkend="Shape_feature:mim:Shape_feature_mim.shape_feature_definition"/> 
	which is defined in its own context.
</ext_description>

</ext_descriptions>
