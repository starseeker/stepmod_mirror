{ iso standard 10303 part (11) version (4) }

SCHEMA Shape_property_assignment_arm;

USE FROM Property_assignment_arm;

TYPE shape_property_assignment = EXTENSIBLE SELECT BASED_ON property_assignment_select WITH
  (Item_shape,
   Shape_element,
   Shape_element_relationship);
END_TYPE;

ENTITY Item_shape;
END_ENTITY;

ENTITY Property_of_shape
SUBTYPE OF (Element_property);
WHERE
  WR1: 'SHAPE_PROPERTY_ASSIGNMENT_ARM.SHAPE_PROPERTY_ASSIGNMENT' IN TYPEOF(SELF\Property_of_shape.described_element);
END_ENTITY;

ENTITY Shape_element;
  composition : Item_shape;
END_ENTITY;

ENTITY Shape_element_relationship;
  description : OPTIONAL STRING;
  element_name : OPTIONAL STRING;
  related : Shape_element;
  relating : Shape_element;
END_ENTITY;

END_SCHEMA;
