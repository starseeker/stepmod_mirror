<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.0 2001-11-15 11:24:44+00 rob Exp rob $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.0"
    source="../data/modules/shape_property_assignment/arm.exp"/>

  <schema name="Shape_property_assignment_arm">
    <interface
      kind="use"
      schema="Property_assignment_arm">
    </interface>

    <type name="item_shape_or_shape_element_select">
      <select
        extensible="YES"
        basedon="property_assignment_select"
        selectitems="Item_shape Shape_element">
      </select>

    </type>
    <type name="shape_property_assignment">
      <select
        basedon="property_assignment_select"
        selectitems="shape_select">
      </select>

    </type>
    <type name="shape_select">
      <select
        selectitems="Item_shape Shape_element Shape_element_relationship">
      </select>

    </type>
    <entity
      name="Assembly_component_shapeSUBTYPE"
      supertypes="Element_property">
      <where
        label="WR1"
        expression="'PRODUCT_VIEW_DEFINITION_STRUCTURE_PROPERTIES_ARM.PRODUCT_VIEW_DEFINITION_STRUCTURE_PROPERTY_ASSIGNMENT'       IN TYPEOF(SELF\Element_property.described_element)">
      </where>
    </entity>

    <entity
      name="Item_shape"
      supertypes="Element_property">
    </entity>

    <entity
      name="Property_of_shape"
      supertypes="Element_property">
      <where
        label="WR1"
        expression="'SHAPE_PROPERTY_ASSIGNMENT_ARM.SHAPE_PROPERTY_ASSIGNMENT'       IN TYPEOF(SELF\Property_of_shape.described_element)">
      </where>
    </entity>

    <entity
      name="Shape_element">
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="composition">
        <typename
          name="Item_shape"/>
      </explicit>
      <explicit
        name="element_name"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="Shape_element_relationship">
      <explicit
        name="relating">
        <typename
          name="Shape_element"/>
      </explicit>
      <explicit
        name="related">
        <typename
          name="Shape_element"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="relation_type"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

  </schema>

</express>
