<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1032">
   <application name="JSDAI" owner="LKSoft" source="shape_property_assignment_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Shape_property_assignment_arm">
      <interface kind="use" schema="Property_assignment_arm"/>
      <interface kind="use" schema="Assembly_structure_arm"/>
      <interface kind="use" schema="External_model_arm"/>
      <interface kind="use" schema="Geometric_model_relationship_arm"/>
      <interface kind="use" schema="Product_view_definition_relationship_arm"/>
      <type name="assigned_shape_property_select">
         <select extensible="YES" genericentity="YES" selectitems="shapeable_item Shape_element Shape_element_relationship"/>
      </type>
      <type name="chained_geometric_model_link">
         <select selectitems="Geometric_placement_operation Geometric_coordinate_space Geometric_model_relationship"/>
      </type>
      <type name="described_element_select">
         <select selectitems="View_definition_relationship Product_occurrence_definition_relationship"/>
      </type>
      <type name="geometric_item_specific_usage_select">
         <select selectitems="Shape_element Shape_element_relationship"/>
      </type>
      <type name="shape_element_or_relationship_select">
         <select selectitems="Shape_element Shape_element_relationship"/>
      </type>
      <type name="shape_model">
         <select extensible="YES" genericentity="YES" selectitems="External_geometric_model Geometric_model"/>
      </type>
      <type name="shapeable_item">
         <select extensible="YES" genericentity="YES" selectitems="Product_occurrence_definition_relationship Product_view_definition View_definition_relationship"/>
      </type>
      <type name="spa_property_assignment_select">
         <select basedon="property_assignment_select" selectitems="shapeable_item Shape_element Shape_element_relationship"/>
      </type>
      <type name="spa_represented_definition">
         <select basedon="represented_definition" extensible="YES" genericentity="YES" selectitems="shapeable_item Shape_element Shape_element_relationship"/>
      </type>
      <entity name="Assigned_shape_property" supertypes="Assigned_property">
         <explicit name="described_element">
            <typename name="assigned_shape_property_select"/>
            <redeclaration entity-ref="Assigned_property"/>
         </explicit>
      </entity>
      <entity name="Chain_based_geometric_item_specific_usage" supertypes="Geometric_item_specific_usage">
         <explicit name="nodes">
            <aggregate lower="2" type="LIST" unique="YES" upper="?"/>
            <typename name="Geometric_model"/>
         </explicit>
         <explicit name="undirected_link">
            <aggregate lower="1" type="LIST" unique="YES" upper="?"/>
            <typename name="chained_geometric_model_link"/>
         </explicit>
      </entity>
      <entity name="Geometric_item_specific_usage">
         <explicit name="name">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="description" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="definition">
            <typename name="geometric_item_specific_usage_select"/>
         </explicit>
         <explicit name="identified_item">
            <typename name="geometric_model_item"/>
         </explicit>
         <explicit name="used_model">
            <typename name="shape_model"/>
         </explicit>
      </entity>
      <entity name="Shape_dependent_property_representation" supertypes="Property_definition_representation">
         <explicit name="definition">
            <typename name="Assigned_shape_property"/>
            <redeclaration entity-ref="Property_definition_representation"/>
         </explicit>
      </entity>
      <entity name="Shape_description_association" supertypes="Property_definition_representation">
         <explicit name="definition">
            <typename name="shapeable_item"/>
            <redeclaration entity-ref="Property_definition_representation"/>
         </explicit>
         <explicit name="used_representation">
            <typename name="shape_model"/>
            <redeclaration entity-ref="Property_definition_representation"/>
         </explicit>
      </entity>
      <entity name="Shape_element">
         <explicit name="id" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="element_name">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="description" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="associated_definition">
            <typename name="shapeable_item"/>
         </explicit>
         <explicit name="product_definitional" optional="YES">
            <builtintype type="BOOLEAN"/>
         </explicit>
         <explicit name="identified_item" optional="YES">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="geometric_model_item"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="id"/>
            <unique.attribute attribute="associated_definition"/>
         </unique>
         <where expression="EXISTS(element_name) OR (TYPEOF(SELF\Shape_element) &lt;&gt; TYPEOF(SELF))" label="WR1"/>
      </entity>
      <entity name="Shape_element_description_association" supertypes="Property_definition_representation">
         <explicit name="definition">
            <typename name="shape_element_or_relationship_select"/>
            <redeclaration entity-ref="Property_definition_representation"/>
         </explicit>
         <explicit name="used_representation">
            <typename name="shape_model"/>
            <redeclaration entity-ref="Property_definition_representation"/>
         </explicit>
      </entity>
      <entity name="Shape_element_relationship">
         <explicit name="relation_type" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="description" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="relating">
            <typename name="Shape_element"/>
         </explicit>
         <explicit name="related">
            <typename name="Shape_element"/>
         </explicit>
         <where expression="EXISTS(relation_type) OR (TYPEOF(SELF\Shape_element_relationship) &lt;&gt; TYPEOF(SELF))" label="WR1"/>
      </entity>
      <subtype.constraint entity="Property_definition_representation" name="property_definition_representation_subtype" super.expression="ONEOF (Shape_description_association, Shape_dependent_property_representation, Shape_element_description_association)"/>
   </schema>
</express>
