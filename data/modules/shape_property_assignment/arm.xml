<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.15 2002/06/18 11:51:06 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.15"
    source="../data/modules/shape_property_assignment/arm.exp"/>

  <schema name="Shape_property_assignment_arm">
    <interface
      kind="use"
      schema="Elemental_geometric_shape_arm">
    </interface>

    <interface
      kind="use"
      schema="Foundation_representation_arm">
    </interface>

    <interface
      kind="use"
      schema="Product_view_definition_arm">
    </interface>

    <interface
      kind="use"
      schema="Product_view_definition_relationship_arm">
    </interface>

    <type name="shapeable_item">
      <select
        extensible="YES"
        selectitems="Characterizable_object Product_view_definition View_definition_relationship">
      </select>

    </type>
    <type name="shape_dependent_select">
      <select
        selectitems="Item_shape Shape_element">
      </select>

    </type>
    <type name="shape_select">
      <select
        selectitems="Item_shape Shape_element Shape_element_relationship">
      </select>

    </type>
    <entity
      name="Item_shape">
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="id"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="Characterizable_object">
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="Shape_element">
      <explicit
        name="containing_shape">
        <typename
          name="Item_shape"/>
      </explicit>
      <explicit
        name="element_name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="id"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="Shape_element_relationship">
      <explicit
        name="related">
        <typename
          name="Shape_element"/>
      </explicit>
      <explicit
        name="relating">
        <typename
          name="Shape_element"/>
      </explicit>
      <explicit
        name="relation_type">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="Shape_dependent_property_representation">
      <explicit
        name="characterized_item">
        <typename
          name="shape_dependent_select"/>
      </explicit>
      <explicit
        name="shape_characteristic">
        <typename
          name="Representation"/>
      </explicit>
      <explicit
        name="characteristic_type">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="Shape_description_association">
      <explicit
        name="represented_characteristic">
        <typename
          name="shape_select"/>
      </explicit>
      <explicit
        name="representation">
        <typename
          name="Geometric_model"/>
      </explicit>
      <explicit
        name="role"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

  </schema>

</express>
