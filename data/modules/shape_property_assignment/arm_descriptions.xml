<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: Shape_property_assignment_arm
   <express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->


<ext_description linkend="Shape_property_assignment_arm">
</ext_description>

<!-- +++++++++++ Assigned_shape_property +++++++++++ -->
<ext_description linkend="Shape_property_assignment_arm.Assigned_shape_property">
	An <b>Assigned_shape_property</b> is a type of
	<express_ref linkend="property_assignment_arm:arm:Property_assignment_arm.Assigned_property"/>
	for the purpose of identifying a unique shape property for an object.
</ext_description>
<ext_description linkend="Shape_property_assignment_arm.Assigned_shape_property.described_element">
	an inherited attribute that is constrained to be of type
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.assigned_shape_property_select"/>.
</ext_description>
<ext_description linkend="Shape_property_assignment_arm.Assigned_shape_property.ur:UR1">
	There shall be a most one <b>Assigned_shape_property</b> for the
	<express_ref linkend="property_assignment_arm:arm:Property_assignment_arm.Assigned_property.described_element"/>.
</ext_description>

<!-- +++++++++++ Associated_item_shape +++++++++++ -->
<ext_description linkend="Shape_property_assignment_arm.Associated_item_shape">
	An <b>Associated_item_shape</b> is a type of
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Item_shape"/>
	that is associated to either a
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model"/>
	or an
	<express_ref linkend="External_model:arm:External_model_arm.External_geometric_model"/>.
</ext_description>

<!-- associated_model -->
<ext_description linkend="Shape_property_assignment_arm.Associated_item_shape.associated_model">
	specifies the role of the
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.shape_model"/>
	for the <b>Associated_item_shape</b>.
</ext_description>

<!-- association -->
<ext_description linkend="Shape_property_assignment_arm.Associated_item_shape.association">
	specifies an inverse relationship that specifies that the existence of the <b>Associated_item_shape</b> is dependent on the existence of the 
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Shape_description_association"/> 
	that specifies the <b>Associated_item_shape</b> as its 
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Shape_description_association.definition"/>.
</ext_description>

<!-- +++++++++++ Associated_shape_element +++++++++++ -->
<ext_description linkend="Shape_property_assignment_arm.Associated_shape_element">
	An <b>Associated_shape_element</b> is a type of
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element"/>
	that represents the
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Detailed_geometric_model_element"/>
	to which it is associated.
</ext_description>

<!-- containing_shape -->
<ext_description linkend="Shape_property_assignment_arm.Associated_shape_element.associated_definition">
	an inherited atribute  shall be of type
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Associated_item_shape"/>
	for the <b>Associated_shape_element</b>.
</ext_description>

<!-- associated_item -->
<ext_description linkend="Shape_property_assignment_arm.Associated_shape_element.associated_item">
	specifies the role of the
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Detailed_geometric_model_element"/>
	for the <b>Associated_shape_element</b>.
</ext_description>

<!-- association -->
<ext_description linkend="Shape_property_assignment_arm.Associated_shape_element.association">
	specifies an inverse relationship that specifies that the existence of the <b>Associated_shape_element</b> is dependent on the existence of the 
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Geometric_item_specific_usage"/> 
	that specifies the <b>Associated_shape_element</b> as its 
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Geometric_item_specific_usage.definition"/>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Shape_property_assignment_arm.Associated_shape_element.wr:WR1">
	The <b>associated_item</b> shall be populated.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Shape_property_assignment_arm.Associated_shape_element.wr:WR2">
	The associated geometric element shall be defined in the same instance of
	<express_ref linkend="representation_schema:ir_express:representation_schema.representation_context"/>
	that is also the one used for the 
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Associated_item_shape"/>
	of this <b>Associated_shape_element</b>.
</ext_description>

<!-- +++++++++++ Chain_based_geometric_item_specific_usage +++++++++++ -->		
<ext_description linkend="Shape_property_assignment_arm.Chain_based_geometric_item_specific_usage">
	A <b>Chain_based_geometric_item_specific_usage</b> is a type of 
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Geometric_item_specific_usage"/>
	that represents a chain of 
	<express_ref linkend="elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model"/>s 
	in a graph of 
	<express_ref linkend="elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model"/>s, 
	where the undirected links in the graph can be 
	<express_ref linkend="elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_coordinate_space"/>s, 
	<express_ref linkend="geometric_model_relationship:arm:Geometric_model_relationship_arm.Geometric_model_relationship"/>s, 
	or 
	<express_ref linkend="elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_placement_operation"/>s. 
</ext_description>

<!-- nodes -->
 <ext_description linkend="Shape_property_assignment_arm.Chain_based_geometric_item_specific_usage.nodes">
	the list of  
	<express_ref linkend="elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model">Geometric_models</express_ref>
	in the chain.
	There shall exist two or more 
	<express_ref linkend="elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model">Geometric_models</express_ref>
	for the <b>Chain_based_geometric_item_specific_usage</b>.
</ext_description>
<!-- undirected_link -->
 <ext_description linkend="Shape_property_assignment_arm.Chain_based_geometric_item_specific_usage.undirected_link">
	the list of items that relate nodes in the chain.
	There shall exist one or more
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.chained_geometric_model_link">chained_geometric_model_links</express_ref>
	for the <b>Chain_based_geometric_item_specific_usage</b>.
</ext_description>

<!-- +++++++++++ Contextual_item_shape +++++++++++ -->
		
 <ext_description linkend="Shape_property_assignment_arm.Contextual_item_shape">
	A <b>Contextual_item_shape</b> is a type of <express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Item_shape"/>
	
	 that identifies the shape of a product version in the context of its use in another product version.
	<p>The product version whose contextual shape is identified, is the product version associated with the <express_ref linkend="product_view_definition_relationship:arm:Product_view_definition_relationship_arm.View_definition_relationship.related_view"/>
	
	 of the <express_ref linkend="product_view_definition_relationship:arm:Product_view_definition_relationship_arm.View_definition_usage"/>.
	</p>
	<example>
	Flexible part may have several shapes, each associated with a particular occurrence of the part in assemblies.
	</example>
</ext_description>

<ext_description linkend="Shape_property_assignment_arm.Contextual_item_shape.described_element">
	the <express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.described_element_select"/>
	that identifies the using product version and the used product version.
</ext_description>
<ext_description linkend="Shape_property_assignment_arm.Contextual_item_shape.shaped_product">
	the <express_ref linkend="product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/>
	that identifies the definition of the product version whose shape is considered.
</ext_description>

<!-- +++++++++++ Geometric_item_specific_usage +++++++++++ -->
<ext_description linkend="Shape_property_assignment_arm.Geometric_item_specific_usage">
<p>
	A <b>Geometric_item_specific_usage</b> is an identification of a 
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Detailed_geometric_model_element"/> 
	within a 
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model"/>
	or an 
	<express_ref linkend="External_model:arm:External_model_arm.External_geometric_model"/> 
	as being the element that describes a
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element"/> 
	or 
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element_relationship"/> 
	of a 
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.shapeable_item"/> 
	that is described by the
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model"/> 
	or an 
	<express_ref linkend="External_model:arm:External_model_arm.External_geometric_model"/>.
</p>
	<example>
		In an application protocol, an instance of 
		<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model"/>
		describes a 
		<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.shapeable_item"/>. 
		One element of the
		<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model"/> 
		- a curve
		- represents the boundary of a hole that is a 
		<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element"/> 
		in the product. 
		This entity data type may be used to state that the curve describes the hole, in the context of the entire
		<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model"/> 
		describing the shape of the product.
	</example>
</ext_description>

<!-- name -->
<ext_description linkend="Shape_property_assignment_arm.Geometric_item_specific_usage.name">
	specifies a STRING value.
</ext_description>

<!-- description -->
<ext_description linkend="Shape_property_assignment_arm.Geometric_item_specific_usage.description">
	specifies additional information about the <b>Geometric_item_specific_usage</b>.
	The value of this attribute need not be specified.
</ext_description>

<!-- definition -->
<ext_description linkend="Shape_property_assignment_arm.Geometric_item_specific_usage.definition">
	specifies the role of the
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element"/>
	that is being represented by the 
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Geometric_item_specific_usage.identified_item"/>
	for the <b>Geometric_item_specific_usage</b>.
</ext_description>

<!-- identified_item -->
<ext_description linkend="Shape_property_assignment_arm.Geometric_item_specific_usage.identified_item">
	specifies the role of the
	<express_ref linkend="Elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Detailed_geometric_model_element"/>
	for the <b>Geometric_item_specific_usage</b>.
</ext_description>

<!-- used_model -->
<ext_description linkend="Shape_property_assignment_arm.Geometric_item_specific_usage.used_model">
	specifies the role of the
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.shape_model"/>
	that includes the
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Geometric_item_specific_usage.identified_item"/>
	in it's set of items. The <b>used_model</b> describes the
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.shapeable_item"/> 
	for the <b>Geometric_item_specific_usage</b>.
</ext_description>


<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Shape_property_assignment_arm  Entity: Item_shape  
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
  <ext_description linkend="Shape_property_assignment_arm.Item_shape">
	An <b>Item_shape</b> is the shape of a 
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.shapeable_item"/>.
</ext_description>

   <ext_description linkend="Shape_property_assignment_arm.Item_shape.described_element">
specifies the role of the <express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.shapeable_item"/>
whose shape is the <b>Item_shape</b>.
</ext_description>

  <ext_description linkend="Shape_property_assignment_arm.Item_shape.description">
specifies the text that provides further information about the <b>Item_shape</b>. The value of this attribute need not be specified.
</ext_description>
				
   <ext_description linkend="Shape_property_assignment_arm.Item_shape.id">
specifies the text that provides an identifier for the <b>Item_shape</b>. The value of this attribute need not be specified.
</ext_description>

<ext_description linkend="Shape_property_assignment_arm.Item_shape.ur:UR1">
	There shall be a most one <b>Item_shape</b> for the
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Item_shape.described_element"/>.
</ext_description>

 
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Shape_property_assignment_arm  Entity: Shape_element  
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Shape_property_assignment_arm.Shape_element">
A <b>Shape_element</b> is a portion of a shape.
<example>
The threaded portion of the shape of a bolt may be represented as a <b>Shape_element</b>, so that other properties, such as surface finish, may be associated with it.
</example>
</ext_description>

<!-- containing_shape -->
<ext_description linkend="Shape_property_assignment_arm.Shape_element.associated_definition">
	the 
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.shapeable_item"/> 
	that the <b>Shape_element</b> is a part of.
</ext_description>

<!-- description -->
<ext_description linkend="Shape_property_assignment_arm.Shape_element.description">
the text that provides further information about the <b>Shape_element</b>. The value of this attribute need not be specified.
</ext_description>

<!-- element_name -->
<ext_description linkend="Shape_property_assignment_arm.Shape_element.element_name">
the name of the <b>Shape_element</b>. 
</ext_description>

<!-- id -->
<ext_description linkend="Shape_property_assignment_arm.Shape_element.id">
the identifier that distiguishes the <b>Shape_element</b>. The value of this attribute need not be specified.
</ext_description>

<ext_description linkend="Shape_property_assignment_arm.Shape_element.product_definitional">
	an indication that the <b>Shape_element</b> is on the physical boundary of the <b>associated_definition</b>.
	If the value of this attribute is TRUE, the <b>Shape_element</b> being identified is on such a boundary. 
	If the value is FALSE, the <b>Shape_element</b> being identified is not on such a boundary. 
	If the value is not provided, it is not known whether or not the <b>Shape_element</b> being identified is on such a boundary.
</ext_description>

<ext_description linkend="Shape_property_assignment_arm.Shape_element.ur:UR1">
	In the case that an <b>id</b> is provided, it shall be unique within the scope of the referenced
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.shapeable_item"/>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Shape_property_assignment_arm.Shape_element.wr:WR1">
If SELF is exact instance of <b>Shape_element</b>, then <b>element_name</b>
shall be provided.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Shape_property_assignment_arm  Entity: Shape_element_relationship  
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
  <ext_description linkend="Shape_property_assignment_arm.Shape_element_relationship">
A <b>Shape_element_relationship</b> is a relationship between two instances of <express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element"/>.
</ext_description>

<!-- description -->
<ext_description linkend="Shape_property_assignment_arm.Shape_element_relationship.description">
the text that provides further information about the <b>Shape_element_relationship</b>. The value of this attribute need not be specified.
</ext_description>

<!-- related -->
<ext_description linkend="Shape_property_assignment_arm.Shape_element_relationship.related">
the other  instance of <express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element">Shape_element</express_ref> that takes part in the relationship.
</ext_description>

<!-- relating -->
<ext_description linkend="Shape_property_assignment_arm.Shape_element_relationship.relating">
one of the  instances of <express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element"/>  that takes part in the relationship.
.
</ext_description>

<!-- relation_type -->
<ext_description linkend="Shape_property_assignment_arm.Shape_element_relationship.relation_type">
the string that specifies the type of the relationship. The value of this attribute need not be specified.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Shape_property_assignment_arm.Shape_element_relationship.wr:WR1">
If SELF is exact instance of <b>Shape_element_relationship</b>, then <b>relation_type</b>
shall be provided.
</ext_description>


<!--
 ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Shape_property_assignment_arm  Entity: Shape_dependent_property_representation  
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Shape_property_assignment_arm.Shape_dependent_property_representation">
	A <b>Shape_dependent_property_representation</b> is a type of
	<express_ref linkend="property_assignment_arm:arm:Property_assignment_arm.Property_definition_representation"/>.
    that is the representation of a characteristic derived from or imposed to an 
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.shapeable_item"/>
	or a <express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element"/>.
	<example number="1">
		The centre of mass is an example of shape dependent property, of a shape, that can be represented by a cartesian point.
	</example>
	<example number="2">
		The area and the volumes are examples of characteristic that can be represented by a measure value with unit.
	</example>
	<note number="1">
		A <b>Shape_dependent_property_representation</b> is independent of the representations of the shape.
	</note> 
	<note number="2">The present version of this application module does not enable to represent whether the characteristics has been imposed or was derived.
		It is expected that a future version will add this capability.
	</note>
</ext_description>

<ext_description linkend="Shape_property_assignment_arm.Shape_dependent_property_representation.definition">
	the 
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.shapeable_item"/>
	or the <express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element"/>
	from which the characteristic has been derived or imposed.  
</ext_description>
<ext_description linkend="Shape_property_assignment_arm.Shape_dependent_property_representation.property_representation">
    <p>
	the <express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation"/>
 	that contains the items that express the characteristic.
	</p>
	<note>Depending on the  <b>characteristic_type</b>, the <b>described_element</b>  may need to be a <express_ref linkend="elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model"/>.</note>
</ext_description>
  
	<ext_description linkend="Shape_property_assignment_arm.Shape_dependent_property_representation.characteristic_type">
          <p>
the string that specifies the type of the characteristic that is
represented.
          </p>
<p>Where applicable, the following values shall be used:</p>
<ul>
      <li>'centre of mass': the characteristic is the centre of the mass of the <b>described_element</b>, evaluated or required independently of any representation of the <b>described_element</b>;</li>
<li>'centroid': the characteristic is the centre of mass of a body as
evaluated from a geometric representation of the <b>described_element</b>;
<note>
An instance of <express_ref linkend="foundation_representation:arm:Foundation_representation_arm.Representation_relationship"/>

 may be used to relate the <b>characteristic_type</b> with the <express_ref linkend="elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model"/>

 from which the centroid was computed.
</note>
</li> 

<li>'moment of inertia': the characteristic is the 3*3 matrix of inertia of the  <b>described_element</b>;</li>
      <li>'surface area': the characteristic is the extent of the surface of the <b>described_element</b>;</li>
      <li>'volume': the characteristic is the volume of the <b>described_element</b>.</li> 
</ul>
</ext_description>
   <ext_description linkend="Shape_property_assignment_arm.Shape_dependent_property_representation.description">
the text that provides further information about the <b>Shape_dependent_property_representation</b>.   The value of this attribute need not be specified.
</ext_description>


<!--
 ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Shape_property_assignment_arm  Entity: Shape_description_association  
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Shape_property_assignment_arm.Shape_description_association">
	A <b>Shape_description_association</b> is a type of
	<express_ref linkend="property_assignment_arm:arm:Property_assignment_arm.Property_definition_representation"/>
	that is an association of a geometric representation with an
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.shapeable_item"/>.
</ext_description>
<ext_description linkend="Shape_property_assignment_arm.Shape_description_association.representation">
	the <express_ref linkend="elemental_geometric_shape:arm:Elemental_geometric_shape_arm.Geometric_model"/>
	that provides the representation.
</ext_description>
<ext_description linkend="Shape_property_assignment_arm.Shape_description_association.definition">
	an inherited attribute that is constrained to be of type
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.shapeable_item"/>.
</ext_description>
<ext_description linkend="Shape_property_assignment_arm.Shape_description_association.used_representation">
	an inherited attribute that is constrained to be of type
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.shape_model"/>.
</ext_description>

<!--
 ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Shape_property_assignment_arm  Entity: Shape_element_description_association  
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Shape_property_assignment_arm.Shape_element_description_association">
	A <b>Shape_element_description_association</b> is a type of
	<express_ref linkend="property_assignment_arm:arm:Property_assignment_arm.Property_definition_representation"/>
	that is an association of a geometric representation of a
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element"/>
	or a
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element_relationship"/>.
  <p>
  	In case the <b>Shape_element_description_association</b> is associated to a 
  	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element_relationship"/>, 
  	the associated geometric model represents characteristic of the related 
  	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element"/>
	in the context of its relating 
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element"/>.
  </p>
</ext_description>
<ext_description linkend="Shape_property_assignment_arm.Shape_element_description_association.definition">
	an inherited attribute that is constrained to be of type
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.shape_element_or_relationship_select"/>.
</ext_description>
<ext_description linkend="Shape_property_assignment_arm.Shape_element_description_association.used_representation">
	an inherited attribute that is constrained to be of type
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.shape_model"/>.
</ext_description>


<!-- +++++++++++ spa_get_definition +++++++++++ -->
<ext_description linkend="Shape_property_assignment_arm.spa_get_definition">
	The <b>spa_get_definition</b> function returns the 
	<express_ref linkend="Product_view_definition:arm:Product_view_definition_arm.Product_view_definition"/> 
	that is the 
	related view 
	for either the 
	<express_ref linkend="Product_view_definition_relationship:arm:Product_view_definition_relationship_arm.View_definition_usage"/> 
	or the 
	<express_ref linkend="Assembly_structure:arm:Assembly_structure_arm.Product_occurrence_definition_relationship"/> 
	that is in the role of
	<express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Contextual_item_shape.described_element"/> 
	for the member of 
