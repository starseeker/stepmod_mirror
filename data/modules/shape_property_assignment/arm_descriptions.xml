<ext_descriptions module_directory="shape_property_assignment" schema_file="arm.xml" rcs.date="$Date: 2002/05/10 16:14:42 $" rcs.revision="$Revision: 1.2 $" describe.selects="YES">


  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Shape_property_assignment_arm 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
   <ext_description linkend="Shape_property_assignment_arm"/>



  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Shape_property_assignment_arm  Type: shape_property_assignment 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Shape_property_assignment_arm.shape_property_assignment">
				This extends the assignment of properties to <express_ref linkend="Shape_property_assignment:arm:Item_shape">Item_shape</express_ref>, <express_ref linkend="Shape_property_assignment:arm:Shape_element">Shape_element</express_ref>, and <express_ref linkend="Shape_property_assignment:arm:Shape_element_relationship">Shape_element_relationship</express_ref>.
			</ext_description>



  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Shape_property_assignment_arm  Entity: Item_shape 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Shape_property_assignment_arm.Item_shape">
				An Item_shape specifies the shape of an item.
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Shape_property_assignment_arm  Entity: Property_of_shape 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Shape_property_assignment_arm.Property_of_shape">
				A Property_of_shape specifies a property associated to a shape.
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Shape_property_assignment_arm  Entity: Property_of_shape  Where: WR1 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Shape_property_assignment_arm.Property_of_shape.wr:WR1">The described_element is a shape_property_assignment.</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Shape_property_assignment_arm  Entity: Shape_element 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Shape_property_assignment_arm.Shape_element">
				A Shape_element specifies a portion of shape that has to be identified explicitly to be associated with other information.
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Shape_property_assignment_arm  Entity: Shape_element  Attribute: description 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Shape_property_assignment_arm.Shape_element.description">
					specifies additional information about the Shape_element.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Shape_property_assignment_arm  Entity: Shape_element  Attribute: composition 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Shape_property_assignment_arm.Shape_element.composition">
					specifies the <express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Item_shape">Item_shape</express_ref> that the <express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element">Shape_element</express_ref> is part of.
				</ext_description>



  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Shape_property_assignment_arm  Entity: Shape_element_relationship 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Shape_property_assignment_arm.Shape_element_relationship">
				A Shape_element_relationship specifies a relationship between two <express_ref linkend="Shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element">Shape_element</express_ref> objects.
			</ext_description>
  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Shape_property_assignment_arm  Entity: Shape_element_relationship  Attribute: element_name 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Shape_property_assignment_arm.Shape_element_relationship.element_name">
					specifies the word or group of words by which the <express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element_relationship">
Shape_element_relationship</express_ref> is referred to.
				</ext_description>
  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Shape_property_assignment_arm  Entity: Shape_element_relationship  Attribute: relating 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Shape_property_assignment_arm.Shape_element_relationship.relating">
					specifies the first of the two <express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element">Shape_element</express_ref> objects related by a <express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element_relationship">Shape_element_relationship</express_ref>.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Shape_property_assignment_arm  Entity: Shape_element_relationship  Attribute: related 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Shape_property_assignment_arm.Shape_element_relationship.related">
					specifies the second of the two <express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element">Shape_element</express_ref> objects related by a <express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element_relationship">Shape_element_relationship</express_ref>.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Shape_property_assignment_arm  Entity: Shape_element_relationship  Attribute: description 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Shape_property_assignment_arm.Shape_element_relationship.description">
					specifies additional information about the Shape_element_relationship.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Shape_property_assignment_arm  Entity: Shape_element_relationship  Attribute: relation_type 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Shape_property_assignment_arm.Shape_element_relationship.relation_type">
					specifies the meaning of the relationship.
				</ext_description>
</ext_descriptions>



