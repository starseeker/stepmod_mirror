SCHEMA Shield_arm;

USE FROM Component_feature_arm;

USE FROM Functional_design_elaboration_arm;

	TYPE shieldable_item = SELECT
    (Assembly_component,
   	Component_feature,
   	Physical_connectivity_definition);
	END_TYPE;

  ENTITY Shield
    SUBTYPE OF (Assembly_component);
      shielded_item : SET[1:?] OF shieldable_item;
  END_ENTITY;

END_SCHEMA;
