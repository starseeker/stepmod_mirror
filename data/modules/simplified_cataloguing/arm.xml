<?xml version='1.0' encoding='UTF-8'?>
<!-- $Id: express2xml.js,v 1.38 2004/08/24 16:58:55 thendrix Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml2.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.38"
    source="../data/modules/simplified_cataloguing/arm.exp"/>

  <schema name="Simplified_cataloguing_arm">

    <interface
      kind="use"
      schema="Characterizable_object_arm">
    </interface>
    <interface
      kind="use"
      schema="Classification_with_attributes_arm">
    </interface>
    <interface
      kind="use"
      schema="Extended_measure_representation_arm">
    </interface>
    <interface
      kind="use"
      schema="External_class_arm">
    </interface>
    <interface
      kind="use"
      schema="External_properties_arm">
    </interface>
    <interface
      kind="use"
      schema="Foundation_representation_arm">
    </interface>
    <interface
      kind="use"
      schema="Measure_representation_arm">
    </interface>
    <interface
      kind="use"
      schema="Name_assignment_arm">
    </interface>
    <interface
      kind="use"
      schema="Part_view_definition_arm">
    </interface>
    <interface
      kind="use"
      schema="Plib_class_reference_arm">
    </interface>
    <interface
      kind="use"
      schema="Property_assignment_arm">
    </interface>
    <interface
      kind="use"
      schema="Value_with_unit_arm">
    </interface>
    <type name="external_library_assigned_name_select">
      <select
        basedon="assigned_name_select"
        selectitems="External_library">
      </select>
    </type>

    <entity
      name="Currency">
      <explicit
        name="name"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="Currency_representation_item"
      supertypes="Representation_item">
      <explicit
        name="money_system">
        <typename
          name="currency"/>
      </explicit>
    </entity>

    <entity
      name="External_currency"
      supertypes="Currency">
      <explicit
        name="id">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="external_source">
        <typename
          name="external_library_assigned_name_select"/>
      </explicit>
    </entity>

    <entity
      name="External_feature"
      supertypes="Feature">
      <explicit
        name="id">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="external_source">
        <typename
          name="external_library_assigned_name_select"/>
      </explicit>
    </entity>

    <entity
      name="External_library">
      <explicit
        name="id">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="External_representation_item"
      supertypes="Representation_item">
      <explicit
        name="id">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="external_source">
        <typename
          name="external_library_assigned_name_select"/>
      </explicit>
    </entity>

    <entity
      name="External_string_representation_item"
      supertypes="Representation_item">
    </entity>

    <entity
      name="External_unit"
      supertypes="Unit">
      <explicit
        name="id">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="external_source">
        <typename
          name="external_library_assigned_name_select"/>
      </explicit>
    </entity>

    <entity
      name="Feature"
      supertypes="Characterizable_object">
    </entity>

    <entity
      name="Property_composition_relationship"
      supertypes="Representation_relationship">
    </entity>

  </schema>
</express>
