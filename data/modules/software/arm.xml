<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2004-12-24T15:06:28" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="software_arm schema_instance"/>
   <schema name="Software_arm">
      <interface kind="use" schema="Document_definition_arm"/>
      <interface kind="use" schema="Document_and_version_identification_arm"/>
      <interface kind="use" schema="Product_view_definition_arm"/>
      <interface kind="reference" schema="Product_identification_arm">
         <interfaced.item name="types_of_product"/>
      </interface>
      <entity name="Software" supertypes="Document">
         <where label="WR1" expression=" SIZEOF ( [ 'software' , 'analytical model' , 'rule' ] * types_of_product ( SELF ) ) = 1 "/>
      </entity>
      <entity name="Software_definition" supertypes="Digital_document_definition">
         <explicit name="defined_version">
            <typename name="Software_version"/>
            <redeclaration entity-ref="Product_view_definition"/>
         </explicit>
      </entity>
      <entity name="Software_version" supertypes="Document_version">
         <explicit name="of_product">
            <typename name="Software"/>
            <redeclaration entity-ref="Product_version"/>
         </explicit>
      </entity>
   </schema>
</express>
