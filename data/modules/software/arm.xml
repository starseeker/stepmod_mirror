<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-11-11T16:27:23" rcs.revision="1.0" description.file="arm_descriptions.xml">
<application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="software_arm schema_instance"/>
<schema name="Software_arm">
<interface kind="use" schema="Document_definition_arm"/>
<interface kind="use" schema="Information_product_arm"/>
<interface kind="reference" schema="Product_identification_arm">
<interfaced.item name="types_of_product"/>
</interface>
<entity name="Software" supertypes="Information_product">
<where label="WR1" expression="SIZEOF(['software', 'information'] IN types_of_product(SELF)) = 2"/>
</entity>
<entity name="Software_definition" supertypes="Information_definition">
<explicit name="defined_version">
<typename name="Software_version"/>
<redeclaration entity-ref="Product_view_definition"/>
</explicit>
</entity>
<entity name="Software_version" supertypes="Information_version">
<explicit name="of_product">
<typename name="Software"/>
<redeclaration entity-ref="Product_version"/>
</explicit>
</entity>
<rule name="software_definition_constraint" appliesto="Product_view_definition">
<where label="WR1" expression="SIZEOF ( QUERY ( pvd &lt;* Product_view_definition | ( NOT ( 'SOFTWARE_ARM.' + 'SOFTWARE_DEFINITION' IN TYPEOF (pvd) ) ) AND ( 'SOFTWARE_ARM.' + 'SOFTWARE_VERSION' IN TYPEOF ( pvd.defined_version) ) ) ) =0"/>
</rule>
<rule name="software_version_constraint" appliesto="Product_version">
<where label="WR1" expression="SIZEOF ( QUERY ( pv &lt;* Product_version | ( NOT ( 'SOFTWARE_ARM.' + 'SOFTWARE_VERSION' IN TYPEOF ( pv) ) ) AND ( 'SOFTWARE_ARM.' + 'SOFTWARE' IN TYPEOF ( pv.of_product) ) ) ) =0"/>
</rule>
</schema>
</express>
