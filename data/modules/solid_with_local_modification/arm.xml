  			  	            type="LIST"
  			  	             lower="1"
  			  	              upper="?"/>
  			  	          <aggregate    
  			  	              type="SET"
  			  	              lower="1"
  			  	              upper="?" />
  			  	              <typename name="Advanced_face"/>
  	          </explicit>
  	           <explicit name="thickness_list">       
  	                  <aggregate    
  	       	 	              type="LIST"
  	       	 	              lower="1"
  	       	 	              upper="?" />
  	       	              <typename name="length_measure"/>
  	              </explicit>
       
                        <where
  	             	  label="WR1"
  	             	  expression="SIZEOF(thickened_face_list)=SIZEOF(thickness_list)">
  	               </where>
  	  </entity>
  
 
