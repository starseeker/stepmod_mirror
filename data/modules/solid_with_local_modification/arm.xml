           <explicit name="deleted_faces">       
  	                   <aggregate    
  	        	 	              type="SET"
  	        	 	              lower="1"
  	        	 	              upper="?" />
  	        	              <typename name="Advanced_face"/>
  	   </explicit>
             <explicit name="thickness">
             <typename name="length_measure"/>
          </explicit>
        </entity>
      
       
       <entity
         name="Deleted_face_solid_with_offsets"
         supertypes="Deleted_face_solid">
            
             <explicit name="thickness2">
               <typename name="length_measure"/>
             </explicit>
                    <where
  	         	          label="WR1"
  	         	          expression="thickness2 > 0.0">
  	             </where>
         
               <where
        	          label="WR2"
        	          expression="SELF\Deleted_face_solid.thickness &lt;&gt; thickness2">
             </where>
        </entity>
      
      
   
      
      
       <entity name="Complex_deleted_face_solid"
         supertypes="Deleted_face_solid">
          
          <explicit name="thickened_face_list">
