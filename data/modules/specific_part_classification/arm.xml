<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="">
   <application name="JSDAI" owner="LKSoft" source="specific_part_classification_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Specific_part_classification_arm">
      <interface kind="use" schema="Part_and_version_identification_arm"/>
      <entity name="Assembly_part" supertypes="Discrete_part"/>
      <entity name="Boxed_part" supertypes="Assembly_part"/>
      <entity name="Collection_part" supertypes="Part"/>
      <entity name="Continious_part" supertypes="Part"/>
      <entity name="Detailed_part" supertypes="Discrete_part"/>
      <entity name="Discrete_part" super.expression="ONEOF (Assembly_part, Raw_material_part, Tool_part, Detailed_part)" supertypes="Part"/>
      <entity name="In_process_part" supertypes="Part"/>
      <entity name="Inseparable_assembly_part" supertypes="Assembly_part"/>
      <entity name="Raw_area_material" supertypes="Raw_material_part"/>
      <entity name="Raw_length_material" supertypes="Raw_material_part"/>
      <entity name="Raw_material_part" super.expression="ONEOF (Raw_length_material, Raw_area_material, Raw_volume_material)" supertypes="Discrete_part"/>
      <entity name="Raw_volume_material" supertypes="Raw_material_part"/>
      <entity name="Tool_part" supertypes="Discrete_part"/>
      <subtype.constraint entity="Part" name="spc_part_subtypes" super.expression="ONEOF (Continious_part, Collection_part, Discrete_part)"/>
   </schema>
</express>
