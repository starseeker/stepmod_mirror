<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.16 2002/07/05 11:10:27 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.16"
    source="../data/modules/specified_product/arm.exp"/>

  <schema name="Specified_product_arm">
    <interface
      kind="use"
      schema="Product_class_arm">
    </interface>

    <entity
      name="Product_specification">
      <explicit
        name="id">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="version_id"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="name"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="containing_class">
        <typename
          name="Product_class"/>
      </explicit>
      <explicit
        name="defining_specifications">
        <aggregate
          type="SET"
          lower="0"
          upper="?"/>
        <typename
          name="Specification"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

  </schema>

</express>
