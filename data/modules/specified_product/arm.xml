<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express language_version="2" description.file="arm_descriptions.xml" rcs.date="$Date: 2002/09/16 15:31:37 $" rcs.revision="$Revision: 1.3 $">
	<application name="express2xml.js" owner="Eurostep Limited" url="http://www.eurostep.com" version="1.22" source="../data/modules/specified_product/arm.exp"/>
	<schema name="Specified_product_arm">
		<interface kind="use" schema="Configuration_item_arm"/>
		<interface kind="use" schema="Product_class_arm"/>
		<interface kind="use" schema="Product_version_arm"/>
		<entity name="Product_specification" supertypes="Configuration_watched_item">
			<explicit name="version_id" optional="YES">
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="item_context">
				<typename name="Product_class"/>
				<redeclaration entity-ref="Configuration_watched_item"/>
			</explicit>
			<explicit name="defining_specifications">
				<aggregate type="SET" lower="0" upper="?"/>
				<typename name="Specification"/>
			</explicit>
		</entity>
		<entity name="Specification_based_product_version" supertypes="Product_version">
			<inverse name="specification_source" entity="Item_design_association" attribute="proposed_design"/>
			<where label="WR1" expression="'SPECIFIED_PRODUCT_ARM.PRODUCT_SPECIFICATION' IN TYPEOF(specification_source.watched_item)"/>
		</entity>
	</schema>
</express>
