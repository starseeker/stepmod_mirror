<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="mim_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.16"
    source="../data/modules/specified_product/mim.exp"/>

  <schema name="Specified_product_mim">
    <interface
      kind="use"
      schema="Product_class_mim">
    </interface>

    <interface
      kind="use"
      schema="configuration_management_schema">
      <interfaced.item
        name="configurable_item"/>
      <interfaced.item
        name="configuration_item"/>
    </interface>

    <interface
      kind="use"
      schema="product_property_definition_schema">
      <interfaced.item
        name="characterized_object"/>
    </interface>

    <entity
      name="Product_specification"
      supertypes="configuration_item characterized_object configurable_item">
    </entity>

  </schema>

</express>
