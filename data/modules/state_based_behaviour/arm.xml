<schema name="State_based_behaviour_arm">
    <interface
      kind="use"
      schema="Behaviour_description_assignment_arm">
    </interface>
    <interface
      kind="use"
      schema="Classification_assignment_arm">
    </interface>
    <interface
      kind="use"
      schema="Condition_arm">
    </interface>
		   <interface
      kind="use"
      schema="Expression_arm">
    </interface>
    <interface
      kind="use"
      schema="External_class_arm">
    </interface>
    <interface
      kind="use"
      schema="General_model_parameter_arm">
    </interface>
    <interface
      kind="use"
      schema="Identification_assignment_arm">
    </interface>
    <interface
      kind="use"
      schema="Name_assignment_arm">
    </interface>
    <interface
      kind="use"
      schema="State_based_behaviour_representation_arm">
    </interface>
    <type name="sbb_assigned_name_select"> 
      <select
         basedon="assigned_name_select"
        selectitems="behaviour_model Condition General_model_parameter Product Product_view_definition State_definition State_transition_definition"/>
      <graphic.element
        page="4"
        xcoord="291"
        ycoord="680"/>
    </type>

    <type name="sbb_behaviour_model"> 
      <select
         basedon="behaviour_model"
        selectitems="State_based_behaviour_model"/>
      <graphic.element
        page="6"
        xcoord="155"
        ycoord="623"/>
    </type>

    <type name="sbb_classification_item"> 
      <select
         basedon="classification_item"
        selectitems="Product Product_version Product_view_definition State_definition State_transition_definition"/>
      <graphic.element
        page="5"
        xcoord="134"
        ycoord="680"/>
    </type>

    <type name="sbb_condition_item"> 
      <select
      extensible="YES"
      genericentity="YES"
         basedon="condition_item"
        selectitems="State_transition_definition"/>
      <graphic.element
        page="6"
        xcoord="113"
        ycoord="368"/>
    </type>

    <type name="sbb_condition_parameter_item"> 
      <select
      extensible="YES"
      genericentity="YES"
         basedon="condition_parameter_item"
        selectitems="parameter_value_select"/>
      <graphic.element
        page="6"
        xcoord="450"
        ycoord="368"/>
    </type>

    <type name="sbb_identification_item"> 
      <select
         basedon="identification_item"
        selectitems="Product Product_version Product_view_definition"/>
      <graphic.element
        page="5"
        xcoord="141"
        ycoord="439"/>
    </type>

    <type name="sbb_parameter_value_select"> 
      <select
         basedon="parameter_value_select"
        selectitems="behaviour_model Product_view_definition"/>
      <graphic.element
        page="2"
        xcoord="279"
        ycoord="680"/>
    </type>

    <entity
      name="Condition_expression_assignment"
      >

      <explicit
        name="assigned_expression">
        <typename name="Boolean_expression"/>
      </explicit>
      <explicit
        name="specified_condition">
        <typename name="Condition"/>
      </explicit>
      <graphic.element
        page="3"
        xcoord="394"
        ycoord="666"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="303"
      ycoord="637"/>
  </schema>

  
