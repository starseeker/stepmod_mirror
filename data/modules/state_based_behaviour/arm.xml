<?xml version="1.0"?>
<express
  language_version="2"
  rcs.date="$Date: 2005/08/23 16:30:48 $"
  rcs.revision="$Revision: 1.0 $">

  <application
    name="GraphicalEXPRESS"
    owner="Eurostep" url="www.eurostep.com"
    version="1.3.19"
    source="State_based_behaviour_arm-schema-diagram.vsd"/>

<schema name="State_based_behaviour_arm">
    <interface
      kind="use"
      schema="Behaviour_description_assignment_arm">
    </interface>
    <interface
      kind="use"
      schema="Classification_assignment_arm">
    </interface>
    <interface
      kind="use"
      schema="Condition_arm">
    </interface>
    <interface
      kind="use"
      schema="External_class_arm">
    </interface>
    <interface
      kind="use"
      schema="General_model_parameter_arm">
    </interface>
    <interface
      kind="use"
      schema="Identification_assignment_arm">
    </interface>
    <interface
      kind="use"
      schema="Name_assignment_arm">
    </interface>
    <interface
      kind="use"
      schema="State_based_behaviour_representation_arm">
    </interface>
    <type name="sbb_behaviour_model"> 
      <select
         basedon="behaviour_model"
        selectitems="State_based_behaviour_model"/>
      
    </type>

    <type name="sbb_condition_item"> 
      <select
      extensible="YES"
      genericentity="YES"
         basedon="condition_item"
        selectitems="State_transition_definition"/>
      
    </type>

    <type name="sbb_condition_parameter_item"> 
      <select
      extensible="YES"
      genericentity="YES"
         basedon="condition_parameter_item"
        selectitems="parameter_value_select"/>
      
    </type>

    <type name="sbb_parameter_value_instance_select"> 
      <select
      extensible="YES"
      genericentity="YES"
         basedon="parameter_value_instance_select"
        selectitems="State_based_behaviour_model"/>
      
    </type>

    
  </schema>

  
</express>
