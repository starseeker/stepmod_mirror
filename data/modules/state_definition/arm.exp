SCHEMA State_definition_arm;

USE FROM Classification_assignment_arm;

USE FROM Condition_assignment_arm;

USE FROM Identification_assignment_arm;

USE FROM Property_assignment_arm;

TYPE state_definition_classified_select = SELECT BASED_ON classification_item WITH
(Applied_state_definition_assignment,
 State_definition,
 State_definition_relationship);
END_TYPE;

TYPE state_definition_condition = SELECT BASED_ON item_in_scope WITH
(State_definition);
END_TYPE;

TYPE state_definition_identification_item = SELECT BASED_ON identification_item WITH
(State_definition,
 State_definition_relationship);
END_TYPE;

TYPE state_definition_of_item = EXTENSIBLE SELECT ;
END_TYPE;

TYPE state_definition_property_assignment = SELECT BASED_ON property_assignment_select WITH
(State_definition,
 State_definition_relationship);
END_TYPE;

ENTITY And_state_cause_effect_definition
SUBTYPE OF (State_cause_effect_definition);
END_ENTITY;

ENTITY Applied_state_definition_assignment;
assigned_to : state_definition_of_item;
described_state_definition : State_definition;
END_ENTITY;

ENTITY Composition_of_state_definition
SUBTYPE OF (State_definition_relationship);
SELF\State_definition_relationship.related RENAMED part : State_definition;
SELF\State_definition_relationship.relating RENAMED whole : State_definition;
END_ENTITY;

ENTITY Or_state_cause_effect_definition
SUBTYPE OF (State_cause_effect_definition);
END_ENTITY;

ENTITY Proper_subset
SUBTYPE OF (State_definition_relationship);
SELF\State_definition_relationship.related RENAMED proper_subset : State_definition;
SELF\State_definition_relationship.relating RENAMED proper_superset : State_definition;
END_ENTITY;

ENTITY Sequence_of_state_definition
SUBTYPE OF (State_definition_relationship);
SELF\State_definition_relationship.related RENAMED predecessor : State_definition;
SELF\State_definition_relationship.relating RENAMED successor : State_definition;
END_ENTITY;

ENTITY State_cause_effect_definition
SUBTYPE OF (State_definition_relationship);
SELF\State_definition_relationship.related RENAMED cause : SET [1 : ?] OF State_definition;
SELF\State_definition_relationship.relating RENAMED effect : SET [1 : ?] OF State_definition;
END_ENTITY;

ENTITY State_complement
SUBTYPE OF (State_definition_relationship);
SELF\State_definition_relationship.relating RENAMED universe : State_definition;
SELF\State_definition_relationship.related RENAMED set_1 : State_definition;
SELF\State_definition_relationship.related RENAMED set_2 : State_definition;
END_ENTITY;

ENTITY State_definition;
name : STRING;
description : OPTIONAL STRING;
END_ENTITY;

ENTITY State_definition_relationship;
name : STRING;
description : STRING;
relating : State_definition;
related : State_definition;
END_ENTITY;

ENTITY State_transition_definition
SUBTYPE OF (State_definition_relationship);
SELF\State_definition_relationship.relating RENAMED end_state : State_definition;
SELF\State_definition_relationship.related RENAMED start_state : State_definition;
END_ENTITY;

ENTITY Subset
SUBTYPE OF (State_definition_relationship);
SELF\State_definition_relationship.related RENAMED subset : State_definition;
SELF\State_definition_relationship.relating RENAMED superset : State_definition;
END_ENTITY;

ENTITY Symptom_definition
SUBTYPE OF (State_definition_relationship);
SELF\State_definition_relationship.related RENAMED symptom_cause : State_definition;
SELF\State_definition_relationship.relating RENAMED symptom_effect : State_definition;
END_ENTITY;

ENTITY Xor_state_cause_effect_definition
SUBTYPE OF (State_cause_effect_definition);
END_ENTITY;

END_SCHEMA;