<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.26 2002/12/23 10:00:14 goset1 Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.26"
    source="../data/modules/state_definition/arm.exp"/>

  <schema name="State_definition_arm">
    <interface
      kind="use"
      schema="Classification_assignment_arm">
    </interface>

    <interface
      kind="use"
      schema="Condition_assignment_arm">
    </interface>

    <interface
      kind="use"
      schema="Identification_assignment_arm">
    </interface>

    <interface
      kind="use"
      schema="Property_assignment_arm">
    </interface>

    <type name="state_definition_classified_select">
      <select
        basedon="classification_item"
        selectitems="Applied_state_definition_assignment State_definition State_definition_relationship">
      </select>

    </type>
    <type name="state_definition_condition">
      <select
        basedon="host_item"
        selectitems="State_definition">
      </select>

    </type>
    <type name="state_definition_identification_item">
      <select
        basedon="identification_item"
        selectitems="State_definition State_definition_relationship">
      </select>

    </type>
    <type name="state_definition_of_item">
      <select
        extensible="YES"
        genericentity="YES">
      </select>

    </type>
    <type name="state_definition_property_assignment">
      <select
        basedon="property_assignment_select"
        selectitems="State_definition State_definition_relationship">
      </select>

    </type>
    <entity
      name="And_state_cause_effect_definition"
      supertypes="State_cause_effect_definition">
    </entity>

    <entity
      name="Applied_state_definition_assignment">
      <explicit
        name="assigned_to">
        <typename
          name="state_definition_of_item"/>
      </explicit>
      <explicit
        name="described_state_definition">
        <typename
          name="State_definition"/>
      </explicit>
    </entity>

    <entity
      name="Composition_of_state_definition"
      supertypes="State_definition_relationship">
      <explicit
        name="related RENAMED part">
        <redeclaration
          entity-ref="State_definition_relationship"/>
          <aggregate
            type="SET"
            lower="2"
            upper="?"/>
          <typename
            name="State_definition"/>
        </explicit>
        <explicit
          name="relating RENAMED whole">
          <redeclaration
            entity-ref="State_definition_relationship"/>
            <typename
              name="State_definition"/>
          </explicit>
        </entity>

        <entity
          name="Or_state_cause_effect_definition"
          supertypes="State_cause_effect_definition">
        </entity>

        <entity
          name="Sequence_of_state_definition"
          supertypes="State_definition_relationship">
          <explicit
            name="relating RENAMED successor">
            <redeclaration
              entity-ref="State_definition_relationship"/>
              <typename
                name="State_definition"/>
            </explicit>
            <explicit
              name="related RENAMED predecessor">
              <redeclaration
                entity-ref="State_definition_relationship"/>
                <typename
                  name="State_definition"/>
              </explicit>
            </entity>

            <entity
              name="State_cause_effect_definition"
              supertypes="State_definition_relationship">
              <explicit
                name="relating RENAMED effect">
                <redeclaration
                  entity-ref="State_definition_relationship"/>
                  <aggregate
                    type="SET"
                    lower="1"
                    upper="?"/>
                  <typename
                    name="State_definition"/>
                </explicit>
                <explicit
                  name="related RENAMED cause">
                  <redeclaration
                    entity-ref="State_definition_relationship"/>
                    <aggregate
                      type="SET"
                      lower="1"
                      upper="?"/>
                    <typename
                      name="State_definition"/>
                  </explicit>
                </entity>

                <entity
                  name="State_complement_definition"
                  supertypes="State_definition_relationship">
                  <explicit
                    name="relating RENAMED universe">
                    <redeclaration
                      entity-ref="State_definition_relationship"/>
                      <typename
                        name="State_definition"/>
                    </explicit>
                    <explicit
                      name="related RENAMED set_1">
                      <redeclaration
                        entity-ref="State_definition_relationship"/>
                        <typename
                          name="State_definition"/>
                      </explicit>
                      <explicit
                        name="related RENAMED set_2">
                        <redeclaration
                          entity-ref="State_definition_relationship"/>
                          <typename
                            name="State_definition"/>
                        </explicit>
                      </entity>

                      <entity
                        name="State_definition">
                        <explicit
                          name="name">
                          <builtintype
                            type="STRING"/>
                        </explicit>
                        <explicit
                          name="description"
                          optional="YES">
                          <builtintype
                            type="STRING"/>
                        </explicit>
                      </entity>

                      <entity
                        name="State_definition_relationship">
                        <explicit
                          name="name">
                          <builtintype
                            type="STRING"/>
                        </explicit>
                        <explicit
                          name="description">
                          <builtintype
                            type="STRING"/>
                        </explicit>
                        <explicit
                          name="relating">
                          <aggregate
                            type="SET"
                            lower="1"
                            upper="?"/>
                          <typename
                            name="State_definition"/>
                        </explicit>
                        <explicit
                          name="related">
                          <aggregate
                            type="SET"
                            lower="1"
                            upper="?"/>
                          <typename
                            name="State_definition"/>
                        </explicit>
                      </entity>

                      <entity
                        name="State_proper_subset_definition"
                        supertypes="State_definition_relationship">
                        <explicit
                          name="relating RENAMED proper_superset">
                          <redeclaration
                            entity-ref="State_definition_relationship"/>
                            <typename
                              name="State_definition"/>
                          </explicit>
                          <explicit
                            name="related RENAMED proper_subset">
                            <redeclaration
                              entity-ref="State_definition_relationship"/>
                              <typename
                                name="State_definition"/>
                            </explicit>
                          </entity>

                          <entity
                            name="State_subset_definition"
                            supertypes="State_definition_relationship">
                            <explicit
                              name="relating RENAMED superset">
                              <redeclaration
                                entity-ref="State_definition_relationship"/>
                                <typename
                                  name="State_definition"/>
                              </explicit>
                              <explicit
                                name="related RENAMED subset">
                                <redeclaration
                                  entity-ref="State_definition_relationship"/>
                                  <typename
                                    name="State_definition"/>
                                </explicit>
                              </entity>

                              <entity
                                name="State_symptom_definition"
                                supertypes="State_definition_relationship">
                                <explicit
                                  name="relating RENAMED symptom_effect">
                                  <redeclaration
                                    entity-ref="State_definition_relationship"/>
                                    <aggregate
                                      type="SET"
                                      lower="1"
                                      upper="?"/>
                                    <typename
                                      name="State_definition"/>
                                  </explicit>
                                  <explicit
                                    name="related RENAMED symptom_cause">
                                    <redeclaration
                                      entity-ref="State_definition_relationship"/>
                                      <aggregate
                                        type="SET"
                                        lower="1"
                                        upper="?"/>
                                      <typename
                                        name="State_definition"/>
                                    </explicit>
                                  </entity>

                                  <entity
                                    name="State_transition_definition"
                                    supertypes="State_definition_relationship">
                                    <explicit
                                      name="relating RENAMED end_state">
                                      <redeclaration
                                        entity-ref="State_definition_relationship"/>
                                        <typename
                                          name="State_definition"/>
                                      </explicit>
                                      <explicit
                                        name="related RENAMED start_state">
                                        <redeclaration
                                          entity-ref="State_definition_relationship"/>
                                          <typename
                                            name="State_definition"/>
                                        </explicit>
                                      </entity>

                                      <entity
                                        name="Xor_state_cause_effect_definition"
                                        supertypes="State_cause_effect_definition">
                                      </entity>

                                    </schema>

                                  </express>
