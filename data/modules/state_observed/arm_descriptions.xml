  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: State_observed_arm  Entity: State  Attribute: name 
   <express_ref linkend="state_observed:arm:State_observed_arm.State.name"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="State_observed_arm.State.name">
