<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: State_observed_mim  Entity: condition_assertion_assessment  
   <express_ref linkend="state_observed:mim:State_observed_mim.condition_assertion_assessment"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
