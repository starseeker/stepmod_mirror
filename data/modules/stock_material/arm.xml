<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="stock_material_arm schema_instance"/>
   <schema name="Stock_material_arm">
      <interface kind="use" schema="Approval_arm"/>
      <interface kind="use" schema="Document_definition_arm"/>
      <interface kind="use" schema="Elemental_geometric_shape_arm"/>
      <interface kind="use" schema="Generic_material_aspects_arm"/>
      <interface kind="use" schema="Part_and_version_identification_arm"/>
      <type name="ply_stock_material">
         <select selectitems="Discontinuous_fiber_assembly Filament_assembly Isotropic_material"/>
      </type>
      <type name="sma_approval_item">
         <select extensible="YES" genericentity="YES" basedon="approval_item" selectitems="Stock_material"/>
      </type>
      <entity name="Anisotropic_material" supertypes="Stock_material"/>
      <entity name="Braided_assembly" supertypes="Stock_material"/>
      <entity name="Discontinuous_fiber_assembly" supertypes="Stock_material"/>
      <entity name="Filament_assembly" supertypes="Stock_material"/>
      <entity name="Isotropic_material" supertypes="Stock_material"/>
      <entity name="Material_direction">
         <explicit name="material_orientation">
            <typename name="Direction"/>
         </explicit>
      </entity>
      <entity name="Material_specification" supertypes="Document_definition"/>
      <entity name="Stock_core" supertypes="Stock_material"/>
      <entity name="Stock_material" supertypes="Product_view_definition" super.expression="ONEOF (Anisotropic_material, Braided_assembly, Discontinuous_fiber_assembly, Filament_assembly, Isotropic_material, Stock_core, Woven_assembly)">
         <explicit name="property">
            <typename name="Material_property_value_representation"/>
         </explicit>
         <explicit name="reference_direction" optional="YES">
            <typename name="Material_direction"/>
         </explicit>
         <explicit name="defined_version">
            <typename name="Part_version"/>
            <redeclaration entity-ref="Product_view_definition"/>
         </explicit>
         <explicit name="specified_material">
            <typename name="Material_specification"/>
         </explicit>
      </entity>
      <entity name="Woven_assembly" supertypes="Stock_material"/>
   </schema>
</express>
