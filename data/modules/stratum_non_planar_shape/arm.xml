<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1748">
   <application name="JSDAI" owner="LKSoft" source="stratum_non_planar_shape_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Stratum_non_planar_shape_arm">
      <interface kind="use" schema="Layered_interconnect_module_design_arm"/>
      <interface kind="use" schema="Manifold_surface_arm"/>
      <type name="snps_sf_shape_select">
         <select basedon="limd_sf_shape_select" extensible="YES" genericentity="YES" selectitems="Shell_based_surface_model"/>
      </type>
      <type name="snps_stratum_shape_model_select">
         <select basedon="limd_stratum_shape_model_select" extensible="YES" genericentity="YES" selectitems="Stratum_average_surface_shape_model Stratum_surface_shape_model"/>
      </type>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Stratum_average_surface_shape_model" supertypes="Topologically_constrained_manifold_surface_shape_representation">
         <explicit name="of_surface" optional="NO">
            <typename name="Stratum_average_surface"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="of_surface"/>
         </unique>
         <where expression="NOT EXISTS(SELF\Representation.name)" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Representation.description)" label="WR2"/>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Stratum_surface_shape_model" supertypes="Topologically_constrained_manifold_surface_shape_representation">
         <explicit name="of_surface" optional="NO">
            <typename name="Stratum_surface"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="of_surface"/>
         </unique>
         <where expression="NOT EXISTS(SELF\Representation.name)" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Representation.description)" label="WR2"/>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="Topologically_constrained_manifold_surface_shape_representation" supertypes="Manifold_surface_shape_representation">
         <where label="IP1"/>
         <where label="IP2"/>
      </entity>
   </schema>
