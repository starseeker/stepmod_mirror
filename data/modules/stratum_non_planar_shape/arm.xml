<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$">
   <application name="JSDAI" owner="LKSoft" source="stratum_non_planar_shape_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Stratum_non_planar_shape_arm">
      <interface kind="use" schema="Layered_interconnect_module_design_arm"/>
      <interface kind="use" schema="Manifold_surface_arm"/>
      <type name="stratum_surface_shape_model_select">
         <select selectitems="Stratum_surface_shape_model Stratum_average_surface_shape_model"/>
      </type>
      <entity name="Stratum_average_surface_shape_model" supertypes="Manifold_surface_shape_representation">
         <explicit name="of_surface">
            <typename name="Stratum_average_surface"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="of_surface"/>
         </unique>
         <where expression="NOT EXISTS(SELF\Representation.name)" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Representation.description)" label="WR2"/>
      </entity>
      <entity name="Stratum_feature_non_planar_2d_shape_model" supertypes="Manifold_surface_shape_representation">
         <explicit name="shape_characterized_component">
            <typename name="Stratum_feature"/>
         </explicit>
         <where expression="NOT EXISTS(SELF\Representation.name)" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Representation.description)" label="WR2"/>
      </entity>
      <entity name="Stratum_feature_shape_stratum_average_surface_shape_relationship">
         <explicit name="of_stratum_feature_shape">
            <typename name="Stratum_feature_non_planar_2d_shape_model"/>
         </explicit>
         <explicit name="of_stratum_average_surface_shape">
            <typename name="Stratum_average_surface_shape_model"/>
         </explicit>
      </entity>
      <entity name="Stratum_feature_shape_stratum_surface_shape_relationship">
         <explicit name="of_stratum_feature_shape">
            <typename name="Stratum_feature_non_planar_2d_shape_model"/>
         </explicit>
         <explicit name="of_stratum_surface_shape">
            <typename name="Stratum_surface_shape_model"/>
         </explicit>
      </entity>
      <entity name="Stratum_surface_shape_model" supertypes="Manifold_surface_shape_representation">
         <explicit name="of_surface">
            <typename name="Stratum_surface"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="of_surface"/>
         </unique>
         <where expression="NOT EXISTS(SELF\Representation.name)" label="WR1"/>
         <where expression="NOT EXISTS(SELF\Representation.description)" label="WR2"/>
      </entity>
   </schema>
</express>
