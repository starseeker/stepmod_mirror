</ext_description>

<!-- +++++++++++ Stratum_feature_non_planar_2d_shape_model +++++++++++ -->
<ext_description linkend="Stratum_non_planar_shape_arm.Stratum_feature_non_planar_2d_shape_model">
	A <b>Stratum_feature_non_planar_2d_shape_model</b> is a type of 
	<express_ref linkend="Manifold_surface:arm:Manifold_surface_arm.Manifold_surface_shape_representation"/>.
	A <b>Stratum_feature_non_planar_2d_shape_model</b> is the representation of a 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Stratum_feature"/> 
	using an open manifold surface. 
</ext_description>

<!-- shape_characterized_component -->
<ext_description linkend="Stratum_non_planar_shape_arm.Stratum_feature_non_planar_2d_shape_model.shape_characterized_component">
	specifies a role of a 
	<express_ref linkend="Layered_interconnect_module_design:arm:Layered_interconnect_module_design_arm.Stratum_feature"/> 
	for the <b>Stratum_feature_non_planar_2d_shape_model</b>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Stratum_non_planar_shape_arm.Stratum_feature_non_planar_2d_shape_model.wr:WR1">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.name"/> 
	shall not be populated.
</ext_description>

<!-- WR2 -->
<ext_description linkend="Stratum_non_planar_shape_arm.Stratum_feature_non_planar_2d_shape_model.wr:WR2">
	The
	<express_ref linkend="Foundation_representation:arm:Foundation_representation_arm.Representation.description"/> 
	shall not be populated.
</ext_description>

<!-- +++++++++++ Stratum_feature_shape_stratum_average_surface_shape_relationship +++++++++++ -->
<ext_description linkend="Stratum_non_planar_shape_arm.Stratum_feature_shape_stratum_average_surface_shape_relationship">
A <b>Stratum_feature_shape_stratum_average_surface_shape_relationship</b> is the association between a  <express_ref linkend="Stratum_non_planar_shape:arm:Stratum_non_planar_shape_arm.Stratum_feature_non_planar_2d_shape_model"/> and a  <express_ref linkend="Stratum_non_planar_shape:arm:Stratum_non_planar_shape_arm.Stratum_average_surface_shape_model"/> for the purpose of specifying that the points on the <express_ref linkend="Stratum_non_planar_shape:arm:Stratum_non_planar_shape_arm.Stratum_feature_non_planar_2d_shape_model"/> shall be in the domain of the points on the  <express_ref linkend="Stratum_non_planar_shape:arm:Stratum_non_planar_shape_arm.Stratum_average_surface_shape_model"/>.
</ext_description>
<ext_description linkend="Stratum_non_planar_shape_arm.Stratum_feature_shape_stratum_average_surface_shape_relationship.of_stratum_average_surface_shape">
specifies the independent <express_ref linkend="Stratum_non_planar_shape:arm:Stratum_non_planar_shape_arm.Stratum_average_surface_shape_model"/> for the <b>Stratum_feature_shape_stratum_average_surface_shape_relationship</b>.
</ext_description>
<ext_description linkend="Stratum_non_planar_shape_arm.Stratum_feature_shape_stratum_average_surface_shape_relationship.of_stratum_feature_shape">
specifies the dependent <express_ref linkend="Stratum_non_planar_shape:arm:Stratum_non_planar_shape_arm.Stratum_feature_non_planar_2d_shape_model"/> for the <b>Stratum_feature_shape_stratum_average_surface_shape_relationship</b>.
</ext_description>

<!-- +++++++++++ Stratum_feature_shape_stratum_surface_shape_relationship +++++++++++ -->
<ext_description linkend="Stratum_non_planar_shape_arm.Stratum_feature_shape_stratum_surface_shape_relationship">
A <b>Stratum_feature_shape_stratum_surface_shape_relationship</b> is the association between a <express_ref linkend="Stratum_non_planar_shape:arm:Stratum_non_planar_shape_arm.Stratum_feature_non_planar_2d_shape_model"/> and a <express_ref linkend="Stratum_non_planar_shape:arm:Stratum_non_planar_shape_arm.Stratum_surface_shape_model"/> for the purpose of specifying that the points on the <express_ref linkend="Stratum_non_planar_shape:arm:Stratum_non_planar_shape_arm.Stratum_feature_non_planar_2d_shape_model"/> shall be in the domain of the points on the  <express_ref linkend="Stratum_non_planar_shape:arm:Stratum_non_planar_shape_arm.Stratum_surface_shape_model"/>.
</ext_description>
<ext_description linkend="Stratum_non_planar_shape_arm.Stratum_feature_shape_stratum_surface_shape_relationship.of_stratum_surface_shape">
specifies the independent <express_ref linkend="Stratum_non_planar_shape:arm:Stratum_non_planar_shape_arm.Stratum_surface_shape_model"/> for the <b>Stratum_feature_shape_stratum_surface_shape_relationship</b>.
</ext_description>
<ext_description linkend="Stratum_non_planar_shape_arm.Stratum_feature_shape_stratum_surface_shape_relationship.of_stratum_feature_shape">
specifies the dependent <express_ref linkend="Stratum_non_planar_shape:arm:Stratum_non_planar_shape_arm.Stratum_feature_non_planar_2d_shape_model"/> for the <b>Stratum_feature_shape_stratum_surface_shape_relationship</b>.
</ext_description>

