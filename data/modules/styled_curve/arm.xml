<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-02-11T13:31:56" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="styled_curve_arm schema_instance"/>
   <schema name="Styled_curve_arm">
      <interface kind="use" schema="Basic_curve_arm"/>
      <interface kind="use" schema="Value_with_unit_extension_arm"/>
      <type name="curve_font_or_scaled_curve_font_select">
         <select selectitems="Curve_style_font_and_scaling curve_style_font_select"/>
      </type>
      <type name="curve_style_font_select">
         <select selectitems="Solid_curve Externally_defined_curve_font Curve_style_font"/>
      </type>
      <type name="extend_or_chord_2_extend_or_truncate_or_round">
         <enumeration items="truncate chord_2_extend round extend"/>
      </type>
      <type name="extend_or_truncate">
         <enumeration items="truncate extend"/>
      </type>
      <type name="square_or_round">
         <enumeration items="square round"/>
      </type>
      <type name="styled_area_representation">
         <select extensible="YES" genericentity="YES" selectitems="Styled_curve_occurrence"/>
      </type>
      <entity name="Curve_style">
         <explicit name="curve_font" optional="YES">
            <typename name="curve_font_or_scaled_curve_font_select"/>
         </explicit>
      </entity>
      <entity name="Curve_style_font"/>
      <entity name="Curve_style_font_and_scaling">
         <explicit name="name">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="curve_font">
            <typename name="curve_style_font_select"/>
         </explicit>
         <explicit name="curve_font_scaling">
            <builtintype type="INTEGER"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute entity-ref="Curve_style_font_and_scaling" attribute="name"/>
         </unique>
      </entity>
      <entity name="Curve_style_parameters">
         <explicit name="name">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="corner_style">
            <typename name="extend_or_chord_2_extend_or_truncate_or_round"/>
         </explicit>
         <explicit name="curve_width">
            <typename name="Length_data_element"/>
         </explicit>
         <explicit name="end_extension">
            <typename name="extend_or_truncate"/>
         </explicit>
         <explicit name="end_style">
            <typename name="square_or_round"/>
         </explicit>
         <explicit name="width_uncertainty">
            <typename name="Length_data_element"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute entity-ref="Curve_style_parameters" attribute="name"/>
         </unique>
      </entity>
      <entity name="Externally_defined_curve_font"/>
      <entity name="Solid_curve"/>
      <entity name="Styled_curve_occurrence">
         <explicit name="styled_curve">
            <typename name="Curve"/>
         </explicit>
         <explicit name="of_curve_style">
            <typename name="Curve_style"/>
         </explicit>
      </entity>
   </schema>
</express>
