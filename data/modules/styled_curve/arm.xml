<?xml version="1.0" encoding="utf-8"?><?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express
  SYSTEM "../../../dtd/express.dtd">
<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="">
   <application name="JSDAI" owner="LKSoft" source="styled_curve_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Styled_curve_arm">
      <interface kind="use" schema="Colour_arm"/>
      <interface kind="use" schema="Elemental_geometric_shape_arm"/>
      <entity name="Curve_font" super.expression="ONEOF (Externally_defined_curve_font, Predefined_curve_font, User_defined_curve_font)">
         <explicit name="curve_font_name">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="Curve_font_pattern">
         <explicit name="on_segment">
            <builtintype type="REAL"/>
         </explicit>
         <explicit name="off_segment">
            <builtintype type="REAL"/>
         </explicit>
      </entity>
      <entity name="Externally_defined_curve_font" supertypes="Curve_font">
         <explicit name="curve_font_reference">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="Predefined_curve_font" supertypes="Curve_font">
         <where expression="SELF.curve_font_name IN ['continuous', 'chain', 'chain double dash', 'dashed', 'dotted']" label="WR1"/>
      </entity>
      <entity name="Solid_curve_font" supertypes="Predefined_curve_font">
         <where expression="SELF.curve_font_name IN ['continuous']" label="WR1"/>
      </entity>
      <entity name="User_defined_curve_font" supertypes="Curve_font">
         <explicit name="pattern">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="Curve_font_pattern"/>
         </explicit>
         <explicit name="segment_symbol">
            <typename name="Geometric_model"/>
         </explicit>
      </entity>
   </schema>
</express>