<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: 2002/07/08 07:04:14 $"
  rcs.revision="$Revision: 1.7 $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.16"
    source="../data/modules/swept_solid/arm.exp"/>

  <schema name="Swept_solid_arm">
    <interface
      kind="use"
      schema="Topologically_bounded_surface_arm">
    </interface>
 <interface
       kind="use"
       schema="Elemental_geometric_shape_arm">
     </interface>

 
     <entity
       name="Axis1_placement"
       supertypes="Detailed_geometric_model_element">
     <explicit
          name="position">
          <typename
           name="Cartesian_point"/>
            </explicit> 
     <explicit
       name="axis">
       <typename
        name="Direction"/>
       </explicit> 
     </entity>
    
    <entity
      name="Swept_solid"
      super.expression="(ONEOF(Extruded_solid, Solid_of_revolution))"
      supertypes="Detailed_geometric_model_element">
     <explicit
        name="swept_face">
        <typename
          name="Advanced_face"/>
      </explicit> 
      
    </entity>
    
    <entity
          name="Extruded_solid"
          supertypes="Swept_solid">
             
       <explicit
        name="extruded_direction">
        <typename
          name="Direction"/>
       </explicit> 
      
     <explicit
        name="depth">
        <typename
          name="length_measure"/>
      </explicit>  
    </entity>
       
     <entity
          name="Solid_of_revolution"
          supertypes="Swept_solid">
        
       <explicit
        name="axis">
        <typename
          name="axis1_placement"/>
       </explicit> 
      
     <explicit
        name="angle">
        <typename
          name="plane_angle_measure"/>
      </explicit>  
      
       </entity>
  </schema>

</express>
