<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="swept_solid_arm schema_instance"/>
   <schema name="Swept_solid_arm">
      <interface kind="use" schema="Topologically_bounded_surface_arm"/>
      <entity name="Axis1_placement" supertypes="Detailed_geometric_model_element">
         <explicit name="axis">
            <typename name="Direction"/>
         </explicit>
         <explicit name="position">
            <typename name="Cartesian_point"/>
         </explicit>
      </entity>
      <entity name="Extruded_solid" supertypes="Swept_solid">
         <explicit name="depth">
            <typename name="length_measure"/>
         </explicit>
         <explicit name="extruded_direction">
            <typename name="Direction"/>
         </explicit>
      </entity>
      <entity name="Solid_of_revolution" supertypes="Swept_solid">
         <explicit name="angle">
            <typename name="plane_angle_measure"/>
         </explicit>
         <explicit name="axis">
            <typename name="Axis1_placement"/>
         </explicit>
      </entity>
      <entity name="Swept_solid" supertypes="Detailed_geometric_model_element" super.expression="ONEOF (Extruded_solid, Solid_of_revolution)">
         <explicit name="swept_face">
            <typename name="Advanced_face"/>
         </explicit>
      </entity>
   </schema>
</express>
