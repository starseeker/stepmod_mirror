	format_string:generic_expression:=
			SELF\binary_generic_expression.operands[2];
WHERE
	WR1: (('SYSTEM_BEHAVIOUR_MIM_LF.NUMERIC_EXPRESSION') 
					IN TYPEOF(value_to_format)) 
			AND (('SYSTEM_BEHAVIOUR_MIM_LF.STRING_EXPRESSION') 
