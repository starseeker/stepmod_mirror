(* USED FROM (ISO13584_expressions_schema); *)
ENTITY index_expression
SUBTYPE OF (string_expression, binary_generic_expression);
DERIVE
	operand:generic_expression:=
SELF\binary_generic_expression.operands[1];
	index:generic_expression:= 
SELF\binary_generic_expression.operands[2];
WHERE
	WR1: ('SYSTEM_BEHAVIOUR_MIM_LF.STRING_EXPRESSION' 
			IN TYPEOF(operand))
		AND ('SYSTEM_BEHAVIOUR_MIM_LF.NUMERIC_EXPRESSION' 
			IN TYPEOF(index));
	WR2: is_int_expr (index);
END_ENTITY;
