  WR1: ('SYSTEM_MODELLING_ARM_LF.STRING_EXPRESSION' IN TYPEOF(operand)) AND ('SYSTEM_MODELLING_ARM_LF.NUMERIC_EXPRESSION' IN TYPEOF(index1)) AND ('SYSTEM_MODELLING_ARM_LF.NUMERIC_EXPRESSION' IN TYPEOF(index2));
