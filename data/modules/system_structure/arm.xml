      </explicit>
    </entity>

    <entity name="Analytical_model"
      supertypes="Software"
    >
    </entity>

    <entity name="Analytical_model_application"
      supertypes="Representation"
    >
      <explicit
        name="model_parameter_assignment">
        <aggregate type="SET"
          lower="1"
          upper="?"
        />
        <typename name="Parameter_assignment"/>
      </explicit>
      <explicit
        name="model_representation">
        <typename name="Analytical_model_definition"/>
      </explicit>
      <explicit
        name="analytical_model_application_name">
        <builtintype type="STRING"
        />
      </explicit>
