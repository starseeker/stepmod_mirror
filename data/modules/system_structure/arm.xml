    <type name="product_item">
      <select
        selectitems="Product_view_definition View_definition_usage"
      />
    </type>

    <type name="system_element_realization_property_assignment_select">
      <select
        selectitems="Assigned_property System_element_realization"
      />
    </type>

    <type name="system_item">
      <select
        selectitems="System_element_definition System_element_usage"
      />
    </type>

    <type name="characteristic">
      <select
        selectitems="Value_list Value_set String_representation_item Numerical_item_with_unit"
      />
    </type>

    <type name="c_documented_element_select">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="documented_element_select"
        selectitems="Characteristic_representation Property_value_representation"
      />
    </type>

    <type name="coordinated_characteristic">
      <select
        selectitems="Characteristic_representation Property_value_representation"
      />
    </type>

    <type name="list_characteristic">
      <aggregate type="LIST"
        lower="1"
        upper="?"
      />
      <typename name="characteristic"/>
    </type>

    <type name="range_class">
      <enumeration
        items="closed lower_open upper_open open"
      />
    </type>

    <type name="scalar_or_coordinated_characteristics">
      <select
        selectitems="coordinated_characteristic characteristic"
      />
    </type>

    <type name="value_qualifier">
      <select
        selectitems="Precision_qualifier Type_qualifier Uncertainty_qualifier"
      />
    </type>

    <type name="mp_documented_element_select">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="documented_element_select"
        selectitems="Model_parameter"
      />
    </type>

    <type name="parameter_assignment_or_rule_general_clause">
      <select
        selectitems="Parameter_assignment Rule_general_clause"
      />
    </type>

    <type name="rule_complex_or_simple">
      <select
        selectitems="Rule_complex_clause Rule_simple_clause"
      />
    </type>

