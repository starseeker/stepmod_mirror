</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: System_structure_arm  Type: syss_security_classification_item 
   <express_ref linkend="system_structure:arm:System_structure_arm.syss_security_classification_item"/>
