UNIQUE
  UR1: SELF\Assigned_property.described_element;
WHERE
  WR1: SIZEOF(['SYSTEM_STRUCTURE_ARM_LF.DOCUMENT_DEFINITION', 'SYSTEM_STRUCTURE_ARM_LF.FILE']*TYPEOF(SELF\Assigned_property.described_element)) = 1;
