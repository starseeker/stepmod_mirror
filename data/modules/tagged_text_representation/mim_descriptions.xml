<!DOCTYPE ext_descriptions SYSTEM "../../../dtd/description.dtd">
<ext_descriptions module_directory="Tagged_text_representation" schema_file="mim.xml" describe.selects="YES" describe.subtype_constraints="YES" rcs.date="$Date: 2005/12/05 10:15:23 $" rcs.revision="$Revision: 1.4 $">
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: Tagged_text_representation_mim
   <express_ref linkend="Tagged_text_representation:mim:Tagged_text_representation_mim"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<ext_description linkend="Tagged_text_representation_mim"/>
	
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Tagged_text_representation_mim  Entity: encode_text_representation_item  
   <express_ref linkend="Tagged_text_representation:mim:Tagged_text_representation_mim.tagged_text_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Tagged_text_representation_mim.tagged_text_item">
		An <b>tagged_text_item</b> is a type of <express_ref linkend="qualified_measure_schema:ir_express:qualified_measure_schema.descriptive_representation_item"/> that is used to hold encoded string data.
	</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Tagged_text_representation_mim  Entity: included_text_block  
   <express_ref linkend="Tagged_text_representation:mim:Tagged_text_representation_mim.included_text_block"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Tagged_text_representation_mim.included_text_block">
		An <b>included_text_block</b> is a type of 
		<express_ref linkend="representation_schema:ir_express:representation_schema.mapped_item"/>.
	</ext_description>
	
	<!-- WR1 -->
	<ext_description linkend="Tagged_text_representation_mim.included_text_block.wr:WR1">
		The <express_ref linkend="representation_schema:ir_express:representation_schema.representation_map.mapped_representation"/>
		of <express_ref linkend="representation_schema:ir_express:representation_schema.mapped_item.mapping_source"/>
		shall be of type <express_ref linkend="Tagged_text_representation:mim:Tagged_text_representation_mim.structured_text_representation"/>.
	</ext_description>
	
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Tagged_text_representation_mim  Entity: structured_text_composition  
   <express_ref linkend="Tagged_text_representation:mim:Tagged_text_representation_mim.structured_text_composition"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Tagged_text_representation_mim.structured_text_composition">
		An <b>structured_text_composition</b> is a type of <express_ref linkend="representation_schema:ir_express:representation_schema.compound_representation_item"/>.
	</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Tagged_text_representation_mim  Entity: structured_text_representation  
   <express_ref linkend="Tagged_text_representation:mim:Tagged_text_representation_mim.structured_text_representation"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Tagged_text_representation_mim.structured_text_representation">
		An <b>structured_text_representation</b> is a type of <express_ref linkend="representation_schema:ir_express:representation_schema.representation"/>.
	</ext_description>	
	<ext_description linkend="Tagged_text_representation_mim.structured_text_representation.items">
	specifies a role of the <express_ref linkend="Tagged_text_representation:mim:Tagged_text_representation_mim.string_representation_item_select"/> for the <b>structured_text_representation</b>.
	</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Tagged_text_representation_mim  Entity: tagged_text_format  
   <express_ref linkend="Tagged_text_representation:mim:Tagged_text_representation_mim.tagged_text_format"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Tagged_text_representation_mim.tagged_text_format">
		An <b>tagged_text_format</b> is a type of <express_ref linkend="classification_schema:ir_express:classification_schema.class"/> or <express_ref linkend="representation_schema:ir_express:representation_schema.representation_context"/>.
	</ext_description>	
		<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Tagged_text_representation_mim  Entity: uniform_resource_indicator  
   <express_ref linkend="Tagged_text_representation:mim:Tagged_text_representation_mim.uniform_resource_indicator"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Tagged_text_representation_mim.uniform_resource_indicator">
		A <b>uniform_resource_indicator</b> is a type of <express_ref linkend="qualified_measure_schema:ir_express:qualified_measure_schema.descriptive_representation_item"/> that identifies a resource in the web.
	</ext_description>

</ext_descriptions>
