<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Task_specification_mim  Entity: advisory_task_method  
   <express_ref linkend="task_specification:mim:Task_specification_mim.advisory_task_method"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Task_specification_mim.advisory_task_method">
	A task_method that provides advice to those carrying out a parent task.</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Task_specification_mim  Entity: concurrent_task_method  
   <express_ref linkend="task_specification:mim:Task_specification_mim.concurrent_task_method"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
  <ext_description linkend="Task_specification_mim.concurrent_task_method">
	  A task_method that involves carrying out multiple <b>task_method</b>s during the time taken to perform the longest of the <b>task_method</b>s.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Task_specification_mim  Entity: end_task  
   <express_ref linkend="task_specification:mim:Task_specification_mim.end_task"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
  <ext_description linkend="Task_specification_mim.end_task">
	  A <b>task_method</b> that indicates that execution of the parent task is to end. 
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Task_specification_mim  Entity: exit_loop  
   <express_ref linkend="task_specification:mim:Task_specification_mim.exit_loop"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
  <ext_description linkend="Task_specification_mim.exit_loop">
	  A <b>task_method</b> that indicates that execution of the parent looping_method is to end.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Task_specification_mim  Entity: looping_method  
   <express_ref linkend="task_specification:mim:Task_specification_mim.looping_method"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
  <ext_description linkend="Task_specification_mim.looping_method">
	  A task_method that involves repeating another task_method.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Task_specification_mim  Entity: simultaneous_task_method  
   <express_ref linkend="task_specification:mim:Task_specification_mim.simultaneous_task_method"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Task_specification_mim.simultaneous_task_method">
	A <b>concurrent_task_method</b> that requires all the specified <b>task_method</b>s to be carried out in parallel and during the same period.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Task_specification_mim  Entity: task  
   <express_ref linkend="task_specification:mim:Task_specification_mim.task"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
  <ext_description linkend="Task_specification_mim.task">
	  An action method that is specified by one or more task_methods.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Task_specification_mim  Entity: task_invocation  
   <express_ref linkend="task_specification:mim:Task_specification_mim.task_invocation"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
  <ext_description linkend="Task_specification_mim.task_invocation">
	  A task_method that involves carrying out a complete task. The task being invoked shall not be a parent of any task invoking this task_method.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Task_specification_mim  Entity: task_method  
   <express_ref linkend="task_specification:mim:Task_specification_mim.task_method"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
  <ext_description linkend="Task_specification_mim.task_method">
	  An action_method that prescribes how a task or part of a task is to be carried out.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Task_specification_mim  Entity: task_method_sequence  
   <express_ref linkend="task_specification:mim:Task_specification_mim.task_method_sequence"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
  <ext_description linkend="Task_specification_mim.task_method_sequence">
	  A task_method that involves carrying out a sequence of task methods in order.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Task_specification_mim  Entity: task_result  
   <express_ref linkend="task_specification:mim:Task_specification_mim.task_result"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
  <ext_description linkend="Task_specification_mim.task_result">
	  An action_method that is specified by a description of the result to be achieved. 
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Task_specification_mim  Entity: task_step  
   <express_ref linkend="task_specification:mim:Task_specification_mim.task_step"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
  <ext_description linkend="Task_specification_mim.task_step">
	  A <b>task_method</b> that represents one stage within a task and is not further broken down. 
</ext_description>
