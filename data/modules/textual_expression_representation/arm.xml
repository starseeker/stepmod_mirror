<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express language_version="2" rcs.date="$Date: 2003/11/06 20:18:21 $" rcs.revision="$Revision: 1.2 $" description.file="arm_descriptions.xml">
	<application name="GraphicalEXPRESS" owner="Eurostep" url="www.eurostep.com" version="1.3" source="textual_expression_representation_arm_v002.vsd"/>
 <schema name="Textual_expression_representation_arm">
    <interface
      kind="use"
      schema="Foundation_representation_arm">
    </interface>
    <type name="formal_language_item_select"> 
      <select
        selectitems="Included_formal_language_representation Textual_expression_composition Textual_expression_representation_item"/>
      <graphic.element
        page="2"
        xcoord="218"
        ycoord="544"/>
    </type>

    <type name="list_of_formal_language_item"> 
      <aggregate type="LIST"
        lower="1"
        upper="?"/>

      <typename name="Textual_expression_representation_item"/>
      <graphic.element
        page="2"
        xcoord="488"
        ycoord="496"/>
    </type>

    <entity
      name="Formal_language_representation"
      supertypes="Representation"      >

      <explicit
        name="items">
        <aggregate type="SET"
          lower="1"
          upper="1"/>

        <typename name="formal_language_item_select"/>
        <redeclaration
          entity-ref="Representation"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="147"
        ycoord="626"/>
    </entity>

    <entity
      name="Included_formal_language_representation"
      supertypes="Representation_item"      >

      <explicit
        name="source">
        <typename name="Formal_language_representation"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="184"
        ycoord="375"/>
    </entity>

    <entity
      name="Textual_expression_composition"
      supertypes="Representation_item"      >

      <explicit
        name="content">
        <typename name="list_of_formal_language_item"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="425"
        ycoord="375"/>
    </entity>

    <entity
      name="Textual_expression_representation_item"
      supertypes="String_representation_item"      >

      <explicit
        name="language">
        <builtintype
          type="STRING"/>
      </explicit>
      <graphic.element
        page="2"
        xcoord="416"
        ycoord="629"/>
    </entity>

    <graphic.element
      page="1"
      xcoord="120"
      ycoord="744"/>
  </schema>

</express>
