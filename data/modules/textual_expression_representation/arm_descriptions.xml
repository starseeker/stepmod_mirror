  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Textual_expression_representation_arm.Textual_expression_representation_item.description">
		the word or words that characterize the <b>Textual_expression_representation_item</b>.
	</ext_description>
