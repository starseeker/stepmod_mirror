<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express language_version="2" rcs.date="$Date: 2006/03/23 15:28:42 $" rcs.revision="$Revision: 1.4 $" description.file="mim_descriptions.xml">
	<application name="GraphicalEXPRESS" owner="Eurostep" url="www.eurostep.com" version="1.3.14" source="textual_expression_representation_v003.vsd"/>
    <schema name="Textual_expression_representation_mim">
        <interface
            kind="use"
            schema="Classification_assignment_mim">
        </interface>
        <interface
            kind="use"
            schema="Foundation_representation_mim">
        </interface>
        <interface
            kind="use"
            schema="qualified_measure_schema">
            <interfaced.item 
                name="descriptive_representation_item"/>
        </interface>
        <interface
            kind="use"
            schema="representation_schema">
            <interfaced.item 
                name="compound_representation_item"/>
            <interfaced.item 
                name="representation"/>
            <interfaced.item 
                name="representation_context"/>
            <interfaced.item 
                name="representation_item"/>
            <interfaced.item 
                name="representation_map"/>
        </interface>
        <type name="ter_classification_item"> 
            <select
                basedon="classification_item"
                selectitems="representation_context text_based_representation"/>
            <graphic.element
                page="2"
                xcoord="447"
                ycoord="433"/>
        </type>
        
        <type name="text_based_item_select"> 
            <select
                extensible="YES"
                genericentity="YES"
                selectitems="included_text_based_representation textual_expression_composition textual_expression_representation_item"/>
            <graphic.element
                page="2"
                xcoord="135"
                ycoord="255"/>
        </type>
        
        <entity
            name="included_text_based_representation"
            supertypes="representation_item"      >
            
            <graphic.element
                page="2"
                xcoord="135"
                ycoord="128"/>
        </entity>
        
        <entity
            name="text_based_representation"
            supertypes="representation"      >
            
            <explicit
                name="context_of_items">
                <typename name="text_based_representation_context"/>
                <redeclaration
                    entity-ref="representation"/>
            </explicit>
            <explicit
                name="items">
                <typename name="text_based_item_select"/>
                <redeclaration
                    entity-ref="representation"/>
            </explicit>
            <graphic.element
                page="2"
                xcoord="135"
                ycoord="433"/>
        </entity>
        
        <entity
            name="text_based_representation_context"
            supertypes="representation_context"      >
            
            <graphic.element
                page="2"
                xcoord="447"
                ycoord="277"/>
        </entity>
        
        <entity
            name="textual_expression_composition"
            supertypes="compound_representation_item"      >
            
            <graphic.element
                page="2"
                xcoord="135"
                ycoord="57"/>
        </entity>
        
        <entity
            name="textual_expression_representation_item"
            supertypes="descriptive_representation_item"      >
            
            <graphic.element
                page="2"
                xcoord="135"
                ycoord="199"/>
        </entity>
        
        <graphic.element
            page="1"
            xcoord="255"
            ycoord="121"/>
    </schema>
</express>
