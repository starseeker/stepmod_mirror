  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Textual_expression_representation_mim  Entity: included_text_based_representation  
   <express_ref linkend="textual_expression_representation:mim:Textual_expression_representation_mim.included_text_based_representation"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Textual_expression_representation_mim.included_text_based_representation">
        A <b>included_text_based_representation</b> is a type of
    <express_ref linkend="representation_schema:ir_express:representation_schema.representation_item" /> where a 
    <express_ref linkend="textual_expression_representation:mim:Textual_expression_representation_mim.text_based_representation"/> 
    is included as a constituent of a <express_ref linkend="representation_schema:ir_express:representation_schema.representation_item" />.
</ext_description><!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Textual_expression_representation_mim  Entity: text_based_representation  
   <express_ref linkend="textual_expression_representation:mim:Textual_expression_representation_mim.text_based_representation"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Textual_expression_representation_mim.text_based_representation">
    A <b>text_based_representation</b> is a type of <express_ref linkend="representation_schema:ir_express:representation_schema.representation" /> 
    where the representation is textual in character.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Textual_expression_representation_mim  Entity: text_based_representation  Attribute: context_of_items 
   <express_ref linkend="textual_expression_representation:mim:Textual_expression_representation_mim.text_based_representation.context_of_items"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Textual_expression_representation_mim.text_based_representation.context_of_items">
    the <express_ref linkend="textual_expression_representation:mim:Textual_expression_representation_mim.text_based_representation_context"/> 
    denoting the circumstances in which an expression is represented using text. 
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Textual_expression_representation_mim  Entity: text_based_representation  Attribute: items 
   <express_ref linkend="textual_expression_representation:mim:Textual_expression_representation_mim.text_based_representation.items"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Textual_expression_representation_mim.text_based_representation.items">
    the <express_ref linkend="textual_expression_representation:mim:Textual_expression_representation_mim.textual_expression_representation_item"/>, 
    <express_ref linkend="textual_expression_representation:mim:Textual_expression_representation_mim.included_text_based_representation"/>, or 
    <express_ref linkend="textual_expression_representation:mim:Textual_expression_representation_mim.textual_expression_composition"/> objects that compose the 
    <express_ref linkend="textual_expression_representation:mim:Textual_expression_representation_mim.text_based_representation"/>.
</ext_description><!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Textual_expression_representation_mim  Entity: text_based_representation_context  
   <express_ref linkend="textual_expression_representation:mim:Textual_expression_representation_mim.text_based_representation_context"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Textual_expression_representation_mim.text_based_representation_context">
        A <b>text_based_representation_context</b> is a type of
        <express_ref linkend="module:mim:Module_mim.representation_context" />
</ext_description><!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Textual_expression_representation_mim  Entity: textual_expression_composition  
