  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Textual_expression_representation_mim.textual_expression_representation_item">
        A <b>textual_expression_representation_item</b> is a type of
        <express_ref linkend="module:mim:Module_mim.descriptive_representation_item" />
</ext_description>
