   <express_ref linkend="textual_expression_representation:mim:Textual_expression_representation_mim"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<ext_description linkend="Textual_expression_representation_mim"/>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Textual_expression_representation_mim  Entity: parameter_representation_item  
   <express_ref linkend="textual_expression_representation:mim:Textual_expression_representation_mim.parameter_representation_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Textual_expression_representation_mim.parameter_representation_item">
		A <b>parameter_representation_item</b> is a type of <express_ref linkend="textual_expression_representation:mim:Textual_expression_representation_mim.textual_expression_representation_item"/> that represents a parameter (or variable) which may be operated on by a	function in a computer program.</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Textual_expression_representation_mim  Entity: textual_expression_encoding_representation_item  
   <express_ref linkend="textual_expression_representation:mim:Textual_expression_representation_mim.textual_expression_encoding_representation_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Textual_expression_representation_mim.textual_expression_encoding_representation_item">
		A <b>textual_expression_encoding_representation_item</b> is a type of <express_ref linkend="representation_schema:ir_express:representation_schema.representation_item"/> that represents the coding form used for a textual expression. 
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Textual_expression_representation_mim  Entity: textual_expression_representation_item  
   <express_ref linkend="textual_expression_representation:mim:Textual_expression_representation_mim.textual_expression_representation_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Textual_expression_representation_mim.textual_expression_representation_item">
		A <b>textual_expression_representation_item</b> is a type of <express_ref linkend="representation_schema:ir_express:representation_schema.compound_representation_item"/> that is used to represent an expression, algorithm or program as a text string.	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Textual_expression_representation_mim  Entity: textual_function_represnetaion_item  
   <express_ref linkend="textual_expression_representation:mim:Textual_expression_representation_mim.textual_function_represnetaion_item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="Textual_expression_representation_mim.textual_function_representation_item">
		A <b>textual_function_representation_item</b> is a type of <express_ref linkend="textual_expression_representation:mim:Textual_expression_representation_mim.textual_expression_representation_item"/> that represents a function conforming to a specific notation or computer-interpretable code. 
	</ext_description>
</ext_descriptions>
