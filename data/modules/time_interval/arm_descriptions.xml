<ext_descriptions module_directory="time_interval" schema_file="arm.xml" rcs.date="$Date: $" rcs.revision="$Revision: $">


  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Time_interval_arm 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
   <ext_description linkend="Time_interval_arm"/>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Time_interval_arm  Type: time_interval_item 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Time_interval_arm.time_interval_item">
				This select is a place holder for assignments of time interval to elements of product data
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Time_interval_arm  Type: time_interval_organization_or_person_in_organization_item 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Time_interval_arm.time_interval_organization_or_person_in_organization_item">
				This select extends the assignments of an organization or a person in an organization to Time_interval.
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Time_interval_arm  Entity: Time_interval 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Time_interval_arm.Time_interval">
				A <b>Time_interval</b> represents a span of time.
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Time_interval_arm  Entity: Time_interval  Attribute: id 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Time_interval_arm.Time_interval.id">
					the identification of the interval assigned by the organization.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Time_interval_arm  Entity: Time_interval  Attribute: name 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Time_interval_arm.Time_interval.name">
					words by which the interval is known.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Time_interval_arm  Entity: Time_interval  Attribute: description 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Time_interval_arm.Time_interval.description">
					text which describes the interval.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Time_interval_arm  Entity: Time_interval  Attribute: duration 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Time_interval_arm.Time_interval.duration">
					the optional length of the time interval.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Time_interval_arm  Entity: Time_interval  Attribute: start 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Time_interval_arm.Time_interval.start">
					the start date or date and time for the time interval.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Time_interval_arm  Entity: Time_interval  Attribute: end 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Time_interval_arm.Time_interval.end">
					the optional end date or date and time for the time interval.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Time_interval_arm  Entity: Time_interval  Attribute: identifier 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Time_interval_arm.Time_interval.identifier">
					the organization (or person in an organization) that assigned and owns the interval id.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Time_interval_arm  Entity: Time_interval  Where: WR1 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Time_interval_arm.Time_interval.wr:WR1">
					the <b>duration</b> and <b>end_date</b> may not both exist.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Time_interval_arm  Entity: Time_interval_assignment 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Time_interval_arm.Time_interval_assignment">
				A <b>Time_interval_assignment</b> assigns a Time_interval to product data.
			</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Time_interval_arm  Entity: Time_interval_assignment  Attribute: assigned_interval 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Time_interval_arm.Time_interval_assignment.assigned_interval">
					the <b>Time_interval</b> to be assigned to product data.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Time_interval_arm  Entity: Time_interval_assignment  Attribute: role 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Time_interval_arm.Time_interval_assignment.role">
					the purpose of the assignment.
				</ext_description>

  <!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Time_interval_arm  Entity: Time_interval_assignment  Attribute: items 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
   <ext_description linkend="Time_interval_arm.Time_interval_assignment.items">
					the product data to which the <b>Time_interval</b> is assigned.
				</ext_description>
</ext_descriptions>