<?xml version="1.0"?><!-- $Id: mim.xml,v 1.6 2002/02/21 17:29:52 robbod Exp $ --><?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?><!DOCTYPE express SYSTEM "../../../dtd/express.dtd"><express language_version="2" rcs.date="$Date: 2002/02/21 17:29:52 $" rcs.revision="$Revision: 1.6 $">	<application name="express2xml.js" owner="Eurostep Limited" url="http://www.eurostep.com" version="1.6" source="../data/modules/time_interval/mim.exp"/>	<schema name="Time_interval_mim">		<interface kind="use" schema="Event_mim"/>		<interface kind="use" schema="Person_organization_assignment_mim"/>		<interface kind="use" schema="date_time_schema">			<interfaced.item name="time_interval"/>			<interfaced.item name="time_interval_with_bounds"/>			<interfaced.item name="time_interval_role"/>		</interface>		<interface kind="use" schema="management_resources_schema">			<interfaced.item name="time_interval_assignment"/>		</interface>		<type name="time_interval_item">			<description>				A <b>time_interval_item</b> specifies those objects to which a <b>time_interval</b> may be assigned.			</description>			<select extensible="YES"/>		</type>		<type name="time_interval_organization_item">			<description>				A <b>time_interval_organization_item</b> extends organization assignment to <b>time_interval</b> instances. 			</description>			<select basedon="organization_item" selectitems="time_interval"/>		</type>		<type name="time_interval_person_and_organization_item">			<description>				A <b>time_interval_person_and_organization_item</b> extends person and organization assignment to <b>time_interval</b> instances.			</description>			<select basedon="person_and_organization_item" selectitems="time_interval"/>		</type>		<entity name="applied_time_interval_assignment" supertypes="time_interval_assignment">			<explicit name="items">				<aggregate type="SET" lower="1" upper="?"/>				<typename name="time_interval_item"/>			</explicit>		</entity>	</schema></express>