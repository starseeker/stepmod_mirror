<?xml version="1.0"?>
<!-- $Id: mim.xml,v 1.8 2002/04/09 16:13:41 goset1 Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/04/09 16:13:41 $"
  rcs.revision="$Revision: 1.8 $"
  description.file="mim_descriptions.xml">>

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.9"
    source="../data/modules/time_interval/mim.exp"/>

  <schema name="Time_interval_mim">
    <interface
      kind="use"
      schema="Date_time_mim">
    </interface>

    <interface
      kind="use"
      schema="Event_mim">
    </interface>

    <interface
      kind="use"
      schema="Value_with_unit_mim">
    </interface>

    <interface
      kind="use"
      schema="date_time_schema">
      <interfaced.item
        name="time_interval"/>
      <interfaced.item
        name="time_interval_relationship"/>
      <interfaced.item
        name="time_interval_role"/>
      <interfaced.item
        name="time_interval_with_bounds"/>
    </interface>

    <interface
      kind="use"
      schema="management_resources_schema">
      <interfaced.item
        name="time_interval_assignment"/>
    </interface>

    <type name="time_interval_item">
      <select
        extensible="YES"
        selectitems="">
      </select>

    </type>
    <entity
      name="applied_time_interval_assignment"
      supertypes="time_interval_assignment">
      <explicit
        name="items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="time_interval_item"/>
      </explicit>
    </entity>

  </schema>

</express>
