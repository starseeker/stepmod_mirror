<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" 
  href="../../../xsl/express.xsl"
?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express language_version="2" rcs.date="$Date: $" rcs.revision="$Revision: $" description.file="arm_descriptions.xml">
	<application name="GraphicalEXPRESS" owner="Eurostep" url="www.eurostep.com" version="1.3.14" source="Type_of_person_arm_v011.vsd"/>
	<schema name="Type_of_person_arm">
		<interface kind="use" schema="Classification_assignment_arm"/>
		<interface kind="use" schema="Experience_arm"/>
		<interface kind="use" schema="Property_assignment_arm"/>
		<interface kind="use" schema="Qualifications_arm"/>
		<type name="defined_attributes">
			<select extensible="YES" genericentity="YES" selectitems="Experience_type Qualification_type"/>
			<graphic.element page="2" xcoord="425" ycoord="155"/>
		</type>
		<type name="type_of_person_classified_select">
			<select extensible="YES" genericentity="YES" basedon="classification_item" selectitems="Type_of_person_definition_relationship"/>
			<graphic.element page="2" xcoord="127" ycoord="524"/>
		</type>
		<type name="type_of_person_item_select">
			<select extensible="YES" genericentity="YES"/>
			<graphic.element page="2" xcoord="113" ycoord="269"/>
		</type>
		<type name="type_of_person_property_assignment_select">
			<select extensible="YES" genericentity="YES" basedon="property_assignment_select" selectitems="Type_of_person_definition"/>
			<graphic.element page="2" xcoord="425" ycoord="340"/>
		</type>
		<entity name="Type_of_person">
			<explicit name="name">
				<description>
          The label by which the type of person is known.
        </description>
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="description" optional="YES">
				<description>
          The text that characterizes the type of person.
        </description>
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="has">
				<description>
          A same type of person can be described by many different definitions.
<example>has a degree or has x years experience</example>
				</description>
				<aggregate type="SET" lower="0" upper="?"/>
				<typename name="Type_of_person_definition"/>
			</explicit>
			<graphic.element page="2" xcoord="85" ycoord="403"/>
		</entity>
		<entity name="Type_of_person_assignment">
			<explicit name="assigned_type_of_person">
				<description>
          The type of person that is to be assigned.
        </description>
				<typename name="Type_of_person"/>
			</explicit>
			<explicit name="items">
				<description>
          The items to which the type of person is assigned.
        </description>
				<aggregate type="SET" lower="1" upper="?"/>
				<typename name="type_of_person_item_select"/>
			</explicit>
			<explicit name="role">
				<description>
          The role that the type of person undertakes in the assignment.
        </description>
				<builtintype type="STRING"/>
			</explicit>
			<graphic.element page="2" xcoord="85" ycoord="333"/>
		</entity>
		<entity name="Type_of_person_definition">
			<explicit name="name">
				<description>
          The label by which the type of person definition is known.
        </description>
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="description" optional="YES">
				<description>
          The text that characterizes the type of person definition.
        </description>
				<builtintype type="STRING"/>
			</explicit>
			<graphic.element page="2" xcoord="425" ycoord="403"/>
		</entity>
		<entity name="Type_of_person_definition_relationship">
			<explicit name="name">
				<description>
          The label by which the type of person definition relationship is known.
        </description>
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="description" optional="YES">
				<description>
          The text that characterizes the type of person definition relationship.
        </description>
				<builtintype type="STRING"/>
			</explicit>
			<explicit name="relating">
				<description>
          The parent definition of the relationship.
        </description>
				<typename name="Type_of_person_definition"/>
			</explicit>
			<explicit name="related">
				<description>
          The child definition of the relationship.
        </description>
				<typename name="Type_of_person_definition"/>
			</explicit>
			<graphic.element page="2" xcoord="425" ycoord="531"/>
		</entity>
		<entity name="Type_of_person_definition_required_attributes_relationship">
			<explicit name="assigned_required_attributes">
				<description>
          The type of person to which the required attributes apply.
        </description>
				<typename name="Type_of_person_definition"/>
			</explicit>
			<explicit name="required_attributes">
				<description>
          The required defined attributes of a person.
<example>Must have a degree</example>
					<example>Must have x years experience</example>
				</description>
				<aggregate type="SET" lower="0" upper="?"/>
				<typename name="defined_attributes"/>
			</explicit>
			<graphic.element page="2" xcoord="425" ycoord="219"/>
		</entity>
		<graphic.element page="1" xcoord="248" ycoord="121"/>
	</schema>
</express>
