         <where label="WR1" expression="NOT('MEASURE_SCHEMA.SI_UNIT' IN TYPEOF(SELF\measure_with_unit.unit_component)) OR&#10;          (SELF\measure_with_unit.unit_component\si_unit.name = gray)"/>
