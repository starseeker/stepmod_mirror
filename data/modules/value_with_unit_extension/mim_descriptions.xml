<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Schema: value_with_unit_extension_mim
<express_ref linkend="value_with_unit_extension:mim:value_with_unit_extension_mim"/>
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="value_with_unit_extension_mim">
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: dielectric_constant_measure_with_unit
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Value_with_unit_extension_mim.dielectric_constant_measure_with_unit">
	An <b>dielectric_constant_measure_with_unit</b> is a type of 
	<express_ref linkend="measure_schema:ir_express:measure_schema.ratio_measure_with_unit"/> 
	that implements the ARM concept of  
	<express_ref linkend="Value_with_unit_extension:arm:Value_with_unit_extension_arm.Dielectric_constant_data_element"/>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: expression_conversion_based_unit
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Value_with_unit_extension_mim.expression_conversion_based_unit">
<p>
An <b>expression_conversion_based_unit</b> is a type of  
<express_ref linkend="measure_schema:ir_express:measure_schema.context_dependent_unit"/> and of <express_ref linkend="iso13584_generic_expressions_schema:ir_express:ISO13584_generic_expressions_schema.variable_semantics"/>
that represents the unit in an explicitly defined mathematical function.
</p></ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Attribute: expression_conversion_based_unit.associated_variable_environment
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Value_with_unit_extension_mim.expression_conversion_based_unit.associated_variable_environment">
<p>
an inverse relationship that specifies that the existence of the <b>associated_variable_environment</b> is dependent on the existence of the <express_ref linkend="iso13584_generic_expressions_schema:ir_express:ISO13584_generic_expressions_schema.environment"/> that specifies the <b>associated_variable_environment</b> as its <express_ref linkend="iso13584_generic_expressions_schema:ir_express:ISO13584_generic_expressions_schema.environment.semantics"/>.
</p>
</ext_description>


<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: loss_tangent_measure_with_unit
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Value_with_unit_extension_mim.loss_tangent_measure_with_unit">
	An <b>loss_tangent_measure_with_unit</b> is a type of 
	<express_ref linkend="measure_schema:ir_express:measure_schema.ratio_measure_with_unit"/> 
	that implements the ARM concept of  
	<express_ref linkend="Value_with_unit_extension:arm:Value_with_unit_extension_arm.Loss_tangent_data_element"/>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: scattering_parameter
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Value_with_unit_extension_mim.scattering_parameter">
	A <b>scattering_parameter</b> is a type of 
	<express_ref linkend="Value_with_unit_extension:mim:Value_with_unit_extension_mim.polar_complex_number_literal"/> 
	that implements the ARM concept of  
	<express_ref linkend="Value_with_unit_extension:arm:Value_with_unit_extension_arm.Scattering_parameter_data_element"/>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
WR: scattering_parameter.WR1
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Value_with_unit_extension_mim.scattering_parameter.wr:WR1">
The <b>scattering_parameter</b> shall not be any other type of  <express_ref linkend="mathematical_functions_schema:ir_express:mathematical_functions_schema.complex_number_literal"/>.
</ext_description>


<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: polar_complex_number_literal
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Value_with_unit_extension_mim.polar_complex_number_literal">
A <b>polar_complex_number_literal</b> is a type of <express_ref linkend="iso13584_generic_expressions_schema:ir_express:ISO13584_generic_expressions_schema.generic_literal"/>.
</ext_description>
<ext_description linkend="Value_with_unit_extension_mim.polar_complex_number_literal.radius">
specifies real value.
</ext_description>
<ext_description linkend="Value_with_unit_extension_mim.polar_complex_number_literal.angle">
specifies real value.
</ext_description>
<ext_description linkend="Value_with_unit_extension_mim.polar_complex_number_literal.wr:WR1">
<p>
The <b>radius</b> should be equal or greater than zero.
</p>
</ext_description>
<ext_description linkend="Value_with_unit_extension_mim.polar_complex_number_literal.wr:WR2">
<p>
The <b>angle</b> should be equal or less than 2*PI and equal or greater than zero.
</p>
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: thermal_resistance_measure_with_unit
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Value_with_unit_extension_mim.thermal_resistance_measure_with_unit">
	An <b>thermal_resistance_measure_with_unit</b> is a type of 
	<express_ref linkend="measure_schema:ir_express:measure_schema.measure_with_unit"/> 
	that implements the ARM concept of  
	<express_ref linkend="Value_with_unit_extension:arm:Value_with_unit_extension_arm.Thermal_resistance_data_element"/>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Value_with_unit_extension_mim.thermal_resistance_measure_with_unit.wr:WR1">
	The type of the 
	<express_ref linkend="measure_schema:ir_express:measure_schema.measure_with_unit.unit_component"/> 
	inherited from 
	<express_ref linkend="measure_schema:ir_express:measure_schema.measure_with_unit"/> 
	shall include
	<express_ref linkend="Value_with_unit_extension:mim:Value_with_unit_extension_mim.thermal_resistance_unit"/>.
</ext_description>

<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
Entity: thermal_resistance_unit
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="Value_with_unit_extension_mim.thermal_resistance_unit">
	An <b>thermal_resistance_unit</b> is a type of 
	<express_ref linkend="measure_schema:ir_express:measure_schema.derived_unit"/>.
</ext_description>

<!-- WR1 -->
<ext_description linkend="Value_with_unit_extension_mim.thermal_resistance_unit.wr:WR1">
	The 
	<express_ref linkend="measure_schema:ir_express:measure_schema.dimensional_exponents"/> 
	shall be set to represent Kelvin-metres per Watt, where the dash indicates multiplication. 	
</ext_description>

</ext_descriptions>
