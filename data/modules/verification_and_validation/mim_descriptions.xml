<ext_description linkend="Verification_and_validation_mim.verification_evidence_item">
</ext_description><!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Verification_and_validation_mim  Entity: verification  
   <express_ref linkend="verification_and_validation:mim:Verification_and_validation_mim.verification"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Verification_and_validation_mim.verification">
        A <b>verification</b> is a type of
        <express_ref linkend="group_schema:ir_express:group_schema.group"/> 
	that is used to assert that an assigned requirement has been verified. 
</ext_description><!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Verification_and_validation_mim  Entity: verification_evidence  
   <express_ref linkend="verification_and_validation:mim:Verification_and_validation_mim.verification_evidence"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Verification_and_validation_mim.verification_evidence">
        A <b>verification_evidence</b> is a type of
        <express_ref linkend="management_resources_schema:ir_express:management_resources_schema.group_assignment"/>
	which assignes a collection of items to verification as evidence for that verification.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Verification_and_validation_mim  Entity: verification_evidence  Attribute: assigned_group 
   <express_ref linkend="verification_and_validation:mim:Verification_and_validation_mim.verification_evidence.assigned_group"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Verification_and_validation_mim.verification_evidence.assigned_group">
	the 
	<express_ref linkend="verification_and_validation:mim:Verification_and_validation_mim.verification"/> 
	that the <b>verification_evidence_items</b> are assigned to.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Verification_and_validation_mim  Entity: verification_evidence  Attribute: items 
   <express_ref linkend="verification_and_validation:mim:Verification_and_validation_mim.verification_evidence.items"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Verification_and_validation_mim.verification_evidence.items">
	the
	<express_ref linkend="verification_and_validation:mim:Verification_and_validation_mim.verification_evidence_item"/>
	which are assigend as evidence for the 
	<express_ref linkend="verification_and_validation:mim:Verification_and_validation_mim.verification"/>.
</ext_description><!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Verification_and_validation_mim  Entity: verification_relationship  
   <express_ref linkend="verification_and_validation:mim:Verification_and_validation_mim.verification_relationship"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Verification_and_validation_mim.verification_relationship">
        A <b>verification_relationship</b> is a type of
        <express_ref linkend="group_schema:ir_express:group_schema.group_relationship"/>
	that identifies a relationship between two verifications.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Verification_and_validation_mim  Entity: verification_relationship  Attribute: relating_group 
   <express_ref linkend="verification_and_validation:mim:Verification_and_validation_mim.verification_relationship.relating_group"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Verification_and_validation_mim.verification_relationship.relating_group">
	the
	<express_ref linkend="verification_and_validation:mim:Verification_and_validation_mim.verification"/>
	which is one part of this instance.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Verification_and_validation_mim  Entity: verification_relationship  Attribute: related_group 
   <express_ref linkend="verification_and_validation:mim:Verification_and_validation_mim.verification_relationship.related_group"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Verification_and_validation_mim.verification_relationship.related_group">
	the
	<express_ref linkend="verification_and_validation:mim:Verification_and_validation_mim.verification"/>
	related to the <b>relating_group</b> 
	which is possibly dependant on this relationship.
</ext_description>
