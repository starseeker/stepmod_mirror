<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-08-24T14:27:51" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="via_component_arm schema_instance"/>
   <schema name="Via_component_arm">
      <interface kind="use" schema="Interconnect_module_connection_routing_arm"/>
      <interface kind="use" schema="Layered_interconnect_module_design_arm"/>
      <entity name="Blind_via" abstract.supertype="YES" supertypes="Via" super.expression="ONEOF (Bonded_conductive_base_blind_via, Non_conductive_base_blind_via, Plated_conductive_base_blind_via)"/>
      <entity name="Bonded_conductive_base_blind_via" supertypes="Blind_via">
         <explicit name="features_join">
            <typename name="Stratum_feature_conductive_join"/>
         </explicit>
         <derived name="name" expression="'bonded conductive base blind via'">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="Product_view_definition"/>
         </derived>
         <unique label="UR1">
            <unique.attribute entity-ref="Bonded_conductive_base_blind_via" attribute="features_join"/>
         </unique>
      </entity>
      <entity name="Buried_via" supertypes="Via">
         <derived name="name" expression="'buried via'">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="Product_view_definition"/>
         </derived>
      </entity>
      <entity name="Interfacial_connection" supertypes="Via">
         <derived name="name" expression="'interfacial connection'">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="Product_view_definition"/>
         </derived>
      </entity>
      <entity name="Non_conductive_base_blind_via" supertypes="Blind_via">
         <derived name="name" expression="'non conductive base blind via'">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="Product_view_definition"/>
         </derived>
      </entity>
      <entity name="Plated_conductive_base_blind_via" supertypes="Blind_via">
         <derived name="name" expression="'plated conductive base blind via'">
            <builtintype type="STRING"/>
            <redeclaration entity-ref="Product_view_definition"/>
         </derived>
      </entity>
      <subtype.constraint name="via_subtypes" entity="Via" super.expression="ONEOF (Blind_via, Buried_via, Interfacial_connection)"/>
   </schema>
</express>
