  REPEAT i := 1 to SIZEOF(sda) BY 1;
    IF 'WELD_ARM_LF.SHAPE_DESCRIPTION_ASSOCIATION' IN TYPEOF (sda[i]) THEN
      gm := gm + sda[i]\Property_definition_representation.used_representation;
