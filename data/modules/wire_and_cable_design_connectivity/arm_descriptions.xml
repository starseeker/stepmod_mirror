<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Wire_and_cable_design_connectivity_arm  Enumeration: wacdc_part_category 
   <express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.wacdc_part_category"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Wire_and_cable_design_connectivity_arm.wacdc_part_category">
	The <b>wacdc_part_category</b> is an enumeration that extends the 
	<express_ref linkend="design_product_data_management:arm:Design_product_data_management_arm.part_category"/> 
	enumeration. It provides classification information for wire, cable, and flat cable.
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.wacdc_part_category.wire">
	the part category for a <i>wire</i>;
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.wacdc_part_category.cable">
	the part category for a <i>cable</i>;
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.wacdc_part_category.flat_cable">
	the part category for a <i>flat (multicore) cable</i>.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Wire_and_cable_design_connectivity_arm  Entity: Cable_occurrence  
   <express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Cable_occurrence"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Cable_occurrence">
	A <b>Cable_occurrence</b> is a type of
    <express_ref linkend="product_occurrence:arm:Product_occurrence_arm.Product_occurrence_with_quantity"/>
	that is an occurrence of a  
	<express_ref linkend="part_view_definition:arm:Part_view_definition_arm.Part_view_definition"/> 
	having a specified length.
	This 
	<express_ref linkend="part_view_definition:arm:Part_view_definition_arm.Part_view_definition"/> 
	shall be associated with a
	<express_ref linkend="part_and_version_identification:arm:Part_and_version_identification_arm.Part"/>
	that is a
	<express_ref linkend="design_product_data_management:arm:Design_product_data_management_arm.Part_with_additional_categories"/>
	that is categorized as
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.wacdc_part_category.cable"/> 
	and also as 
	<express_ref linkend="design_product_data_management:arm:Design_product_data_management_arm.part_category.raw_material_by_length"/>.
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Cable_occurrence.ends_a">
	the
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Cable_occurrence_terminal_location_group"/>
	with the 
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element.element_name"/>
	'end a'.
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Cable_occurrence.ends_b">
	the 
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Cable_occurrence_terminal_location_group"/>
	with the 
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element.element_name"/>
	'end b'.
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Cable_occurrence.intermediates">
	the set of
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Cable_occurrence_terminal_location_group"/>
	with the 
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element.element_name"/>
	'intermediate'.
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Cable_occurrence.terminal_location_groups">
	the set of all
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Cable_occurrence_terminal_location_group"/>
	of this <b>Cable_occurrence</b>.
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Cable_occurrence.wr:WR1">
	a <b>Cable_occurrence</b> shall have exactly one
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Cable_occurrence_terminal_location_group"/>
	with the 
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element.element_name"/>
	'end a'.
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Cable_occurrence.wr:WR2">
	a <b>Cable_occurrence</b> shall have exactly one
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Cable_occurrence_terminal_location_group"/>
	with the 
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element.element_name"/>
	'end b'.
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Cable_occurrence.wr:WR3">
	the
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Cable_occurrence_terminal_location_group"/>
	with the 
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element.element_name"/>
	'end a' shall have the same number of 
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Cable_occurrence_terminal"/>s
	as the
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Cable_occurrence_terminal_location_group"/>
	with the 
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element.element_name"/>
	'end b'. 
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Cable_occurrence.wr:WR4">
	a <b>Cable_occurrence</b> shall belong to a 
	<express_ref linkend="part_and_version_identification:arm:Part_and_version_identification_arm.Part"/>
	that is a
	<express_ref linkend="design_product_data_management:arm:Design_product_data_management_arm.Part_with_additional_categories"/>
	and that contains the category
	<express_ref linkend="design_product_data_management:arm:Design_product_data_management_arm.part_category.raw_material_by_length"/>.
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Cable_occurrence.wr:WR5">
	a <b>Cable_occurrence</b> shall belong to a 
	<express_ref linkend="part_and_version_identification:arm:Part_and_version_identification_arm.Part"/>
	that is a
	<express_ref linkend="design_product_data_management:arm:Design_product_data_management_arm.Part_with_additional_categories"/>
	and that contains the category
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.wacdc_part_category.cable"/>.
</ext_description>


<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Wire_and_cable_design_connectivity_arm  Entity: Cable_occurrence_terminal  
   <express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Cable_occurrence_terminal"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Cable_occurrence_terminal">
	A <b>Cable_occurrence_terminal</b> is a type of
	<express_ref linkend="general_design_connectivity:arm:General_design_connectivity_arm.Occurrence_terminal"/>
	that identifies a terminal of a wire on a 
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Cable_occurrence"/>.
	The terminal may be located at one of the cable ends or at an intermediate location along the length of the cable.
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Cable_occurrence_terminal.associated_definition">
	an inherited attribute that shall have a value of type
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Cable_occurrence"/>.
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Cable_occurrence_terminal.associated_transport_feature">
		an inherited attribute that shall have a value of type
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Wire_identification"/>.
      The value of this attribute need not be specified.
    </ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Cable_occurrence_terminal.location_group">
	the
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Cable_occurrence_terminal_location_group"/>
	that specifies the location of the <b>Cable_occurrence_terminal</b>.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Wire_and_cable_design_connectivity_arm  Entity: Cable_occurrence_terminal_location_group  
   <express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Cable_occurrence_terminal_location_group"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Cable_occurrence_terminal_location_group">
    A <b>Cable_occurrence_terminal_location_group</b> is a type of
    <express_ref linkend="general_design_connectivity:arm:General_design_connectivity_arm.Occurrence_terminal"/>
    that represents the grouping of 
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Cable_occurrence_terminal"/>s
	on one of the ends or at intermediate positions between the ends of a 
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Cable_occurrence"/>.
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Cable_occurrence_terminal_location_group.associated_definition">
	an inherited attribute that shall have a value of type
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Cable_occurrence"/>.
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Cable_occurrence_terminal_location_group.terminals">
	the set of 
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Cable_occurrence_terminal"/>s
	at this <b>Cable_occurrence_terminal_location_group</b>.
</ext_description>

<ext_description linkend="Wire_and_cable_design_connectivity_arm.Occurrence_marking_property">
	<p>
	    An <b>Occurrence_marking_property</b> is a type of
    	<express_ref linkend="property_assignment:arm:Property_assignment_arm.Assigned_property"/>
	    that is a visible impression or trace on a  
		<express_ref linkend="product_occurrence:arm:Product_occurrence_arm.Product_occurrence"/>.
	</p>
	<example>
		An occurrence of a wire in an electrical wire harness may be marked with a text string on a printed label.
	</example>
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Occurrence_marking_property.described_element">
	the <b>described_element</b> shall be of type
	<express_ref linkend="product_occurrence:arm:Product_occurrence_arm.Product_occurrence"/>.
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Occurrence_marking_property.name">
	the <b>name</b> shall be 'occurrence marking property'.
</ext_description>


<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Wire_and_cable_design_connectivity_arm  Entity: Wire_colour_based_identification_code  
   <express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Wire_colour_based_identification_code"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Wire_colour_based_identification_code">
        A <b>Wire_colour_based_identification_code</b> is a type of
	<express_ref linkend="independent_property:arm:Independent_property_arm.Independent_property"/>
	that identifies a particular colour code for a wire.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Wire_and_cable_design_connectivity_arm  Entity: Wire_identification  
   <express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Wire_identification"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Wire_identification">
    A <b>Wire_identification</b> is a type of
    <express_ref linkend="general_design_connectivity:arm:General_design_connectivity_arm.Occurrence_transport_feature"/>
	that identifies a particular colour code for a wire.
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Wire_identification.code">
	the
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Wire_colour_based_identification_code"/>
	that provides the colour or colour pattern identification for the <b>Wire_identification</b>.
    The value of this attribute need not be specified.
</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: Wire_and_cable_design_connectivity_arm  Entity: Wire_occurrence  
   <express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Wire_occurrence"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Wire_occurrence">
    A <b>Wire_occurrence</b> is a type of
    <express_ref linkend="product_occurrence:arm:Product_occurrence_arm.Product_occurrence_with_quantity"/>
	that is an occurrence of a  
	<express_ref linkend="part_view_definition:arm:Part_view_definition_arm.Part_view_definition"/> 
	having a specified length.
	This 
	<express_ref linkend="part_view_definition:arm:Part_view_definition_arm.Part_view_definition"/> 
	shall be associated with a
	<express_ref linkend="part_and_version_identification:arm:Part_and_version_identification_arm.Part"/>
	that is a
	<express_ref linkend="design_product_data_management:arm:Design_product_data_management_arm.Part_with_additional_categories"/>
	that is categorized as
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.wacdc_part_category.wire"/> 
	and also as 
	<express_ref linkend="design_product_data_management:arm:Design_product_data_management_arm.part_category.raw_material_by_length"/>.
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Wire_occurrence.ends_a">
	the single
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Wire_occurrence_terminal"/>
	on one end of the <b>Wire_occurrence</b> with 
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element.element_name"/>
	"end a".
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Wire_occurrence.ends_b">
	the single
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Wire_occurrence_terminal"/>
	on the other end of the <b>Wire_occurrence</b> with
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element.element_name"/>
	"end b".
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Wire_occurrence.intermediates">
	the zero, one or more
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Wire_occurrence_terminal"/>s
	at intermediate positions between the ends of the <b>Wire_occurrence</b> with
	<express_ref linkend="shape_property_assignment:arm:Shape_property_assignment_arm.Shape_element.element_name"/>
	starting with the string "intermediate ".	
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Wire_occurrence.terminals">
	the set of all
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Wire_occurrence_terminal"/>s
	of the <b>Wire_occurrence</b>.
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Wire_occurrence.wr:WR1">
	there shall be one
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Wire_occurrence_terminal"/>
	on one end of the <b>Wire_occurrence</b>.
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Wire_occurrence.wr:WR2">
	there shall be one
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Wire_occurrence_terminal"/>
	on the other end of the <b>Wire_occurrence</b>.
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Wire_occurrence.wr:WR3">
	all 
	<express_ref linkend="general_design_connectivity:arm:General_design_connectivity_arm.Occurrence_terminal"/>s
	for a <b>Wire_occurrence</b> shall be of type
	<express_ref linkend="wire_and_cable_design_connectivity:arm:Wire_and_cable_design_connectivity_arm.Wire_occurrence_terminal"/>.
</ext_description>
<ext_description linkend="Wire_and_cable_design_connectivity_arm.Wire_occurrence.wr:WR4">
	a <b>Wire_occurrence</b> shall belong to a 
	<express_ref linkend="part_and_version_identification:arm:Part_and_version_identification_arm.Part"/>
	that is a
	<express_ref linkend="design_product_data_management:arm:Design_product_data_management_arm.Part_with_additional_categories"/>
	and that contains the category
	<express_ref linkend="design_product_data_management:arm:Design_product_data_management_arm.part_category.raw_material_by_length"/>.
</ext_description>
