<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date: 2016/08/02 09:02:44 $" rcs.revision="$Revision: 1.10 $" reference="ISO/TS 10303-nnnn">
   <application name="JSDAI" owner="LKSoft" source="wiring_harness_assembly_design_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Wiring_harness_assembly_design_arm">
      <interface kind="use" schema="Assembly_module_design_arm"/>
      <interface kind="use" schema="Cable_arm"/>
      <interface kind="use" schema="Edge_based_topological_representation_with_length_arm"/>
      <interface kind="use" schema="Extruded_structure_cross_section_arm"/>
      <interface kind="reference" schema="Foundation_representation_arm">
         <interfaced.item name="using_representations"/>
      </interface>
      <type name="whad_additional_application_domain_enumeration">
         <enumeration basedon="additional_application_domain_enumeration" items="wire_and_part_list_with_connectivity wiring_harness_assembly_3d_design wiring_harness_assembly_design_stick_line wiring_harness_formboard_drawing wiring_harness_segment_topology"/>
      </type>
      <type name="whad_assembly_joint_type">
         <enumeration basedon="assembly_joint_type" items="bolted_connection deep_indented_crimped_connection circular_compressed_crimped_connection crimped_connection glued_connection hexagonal_compressed_crimped_connection insulation_displacement_connection insulation_piercing_connection push_connection snap_connection soldered_connection welded_connection wrapped_connection"/>
      </type>
      <type name="whad_axis_placement_mapping_source">
         <select basedon="axis_placement_mapping_source" selectitems="Vertex Edge"/>
      </type>
      <type name="whad_axis_placement_mapping_target">
         <select basedon="axis_placement_mapping_target" selectitems="Curve Cartesian_point"/>
      </type>
      <type name="whad_part_category">
         <enumeration basedon="part_category" items="backshell cable_ferrule cable_fixing_device cable_support_sleeve cavity_plug conduit connector connector_housing connector_insert connector_kit connector_contact contact_member corrugated_conduit electrified_backshell emi_connector_housing end_cap fixing_device grommet insulation_piercing_connector joint_ferrule label overbraid plug protective_covering seal shielded_connector socket strain_relief_accessory shield tape terminal_lug terminal_block shield_connector splice wiring_harness"/>
      </type>
      <type name="whad_terminal_joint_type">
         <enumeration basedon="terminal_joint_type" items="crimp_terminal fusion_fibre_splice insulation_displacement_terminal lug_terminal mantle_terminal mechanical_fibre_splice non_reusable_insulation_displacement_termination optical_fibre_splice pillar_terminal pin_terminal push_on_contact_terminal saddle_terminal screw_terminal snap_on_contact_terminal socket_terminal solder_terminal stud_terminal resilient_contact_terminal tuning_fork_contact_terminal"/>
      </type>
      <entity name="Harness_node" supertypes="Part_shape_element">
         <explicit name="associated_definition">
            <typename name="Wiring_harness_assembly_design"/>
            <redeclaration entity-ref="Shape_element"/>
         </explicit>
         <explicit name="identified_item">
            <aggregate lower="1" type="SET" upper="1"/>
            <typename name="Vertex"/>
            <redeclaration entity-ref="Shape_element"/>
         </explicit>
         <explicit name="attached_feature" optional="YES">
            <typename name="Component_feature"/>
         </explicit>
         <where expression="associated_definition.topology IN using_representations(identified_item)" label="WR1"/>
      </entity>
      <entity name="Harness_segment" supertypes="Part_shape_element">
         <explicit name="associated_definition">
            <typename name="Wiring_harness_assembly_design"/>
            <redeclaration entity-ref="Shape_element"/>
         </explicit>
         <explicit name="identified_item">
            <typename name="Edge"/>
            <redeclaration entity-ref="Shape_element"/>
         </explicit>
         <explicit name="cross_section" optional="YES">
            <typename name="cross_sectional_constituent_element"/>
         </explicit>
         <explicit name="forced_length" optional="YES">
            <typename name="Length_data_element"/>
         </explicit>
         <where expression="associated_definition.topology IN using_representations(identified_item)" label="WR1"/>
      </entity>
      <entity name="Wiring_harness_assembly_design" supertypes="Assembly_module_design_view">
         <explicit name="topology" optional="YES">
            <typename name="Connected_edge_with_length_set_representation"/>
         </explicit>
      </entity>
      <subtype.constraint entity="Part_shape_element" name="whad_part_shape_element_subtypes" super.expression="ONEOF (Harness_segment, Harness_node)"/>
   </schema>
</express>
