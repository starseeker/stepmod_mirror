<application name="GraphicalEXPRESS" owner="Eurostep" url="www.eurostep.com" version="1.3" source="work_order_arm_v10.vsd" />
<schema name="Work_order_arm">
<interface kind="use" schema="Activity_as_directed_arm">
</interface>
<interface kind="use" schema="Activity_method_arm">
</interface>
<interface kind="use" schema="Approval_arm">
</interface>
<interface kind="use" schema="Contract_arm">
</interface>
<interface kind="use" schema="Date_time_assignment_arm">
</interface>
<interface kind="use" schema="Person_organization_assignment_arm">
</interface>
<interface kind="use" schema="Work_request_arm">
</interface>
<type name="work_order_approval_item">
<select basedon="approval_item" selectitems="Work_order" />
<graphic.element page="2" xcoord="325" ycoord="240" />
</type>
<type name="work_order_contract_items">
<select basedon="contract_item" selectitems="Work_order" />
<graphic.element page="2" xcoord="325" ycoord="340" />
</type>
<type name="work_order_date_or_date_time_item">
<select basedon="date_or_date_time_item" selectitems="Work_order" />
<graphic.element page="2" xcoord="346" ycoord="439" />
