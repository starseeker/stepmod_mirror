</type>
<entity name="Work_order">
<explicit name="description" optional="YES">
<builtintype type="STRING" />
</explicit>
<explicit name="directed_activity">
<typename name="Directed_activity" />
</explicit>
<explicit name="in_response_to">
<aggregate type="SET" lower="1" upper="?" />
<typename name="Work_request" />
</explicit>
<explicit name="name">
<builtintype type="STRING" />
</explicit>
<derived name="chosen_method" expression="Activity_as_directed_arm.Directed_activity.chosen_method">
<typename name="Activity_method" />
</derived>
<derived name="status" expression="Directed_activity.status">
<builtintype type="STRING" />
</derived>
<graphic.element page="2" xcoord="190" ycoord="467" />
</entity>
<graphic.element page="1" xcoord="276" ycoord="687" />
</schema>
