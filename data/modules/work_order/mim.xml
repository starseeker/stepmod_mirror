<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/04/19 07:38:42 $"
  rcs.revision="$Revision: 1.10 $"
  description.file="mim_descriptions.xml">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.9"
    source="plcs_mim_ge13v59v5.exp"/>

  <schema name="work_order_mim">
    <interface
      kind="use"
      schema="action_schema">
      <interfaced.item
        name="action"/>
      <interfaced.item
        name="action_directive"/>
      <interfaced.item
        name="action_status"/>
      <interfaced.item
        name="directed_action"/>
      <interfaced.item
        name="executed_action"/>
    </interface>

    <interface
      kind="use"
      schema="approval_mim">
    </interface>

    <interface
      kind="use"
      schema="contract_mim">
    </interface>

    <interface
      kind="use"
      schema="date_time_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="person_organization_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="work_request_mim">
    </interface>

    <type name="action_approval_item">
      <select
        basedon="approval_item"
        selectitems="action">
      </select>

    </type>
    <type name="action_contract_item">
      <select
        basedon="contract_item"
        selectitems="action">
      </select>

    </type>
    <type name="action_date_and_time_item">
      <select
        basedon="date_and_time_item"
        selectitems="action">
      </select>

    </type>
    <type name="action_date_item">
      <select
        basedon="date_item"
        selectitems="action">
      </select>

    </type>
    <type name="action_organization_item">
      <select
        basedon="organization_item"
        selectitems="action">
      </select>

    </type>
    <type name="action_person_and_organization_item">
      <select
        basedon="person_and_organization_item"
        selectitems="action">
      </select>

    </type>
  </schema>


</express>