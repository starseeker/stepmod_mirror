<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.2 2001/11/21 15:38:19 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.2"
    source="../data/modules/work_order/mim.exp"/>

  <schema name="work_order_mim">
    <interface
      kind="use"
      schema="Approval_mim">
    </interface>

    <interface
      kind="use"
      schema="Contract_mim">
    </interface>

    <interface
      kind="use"
      schema="Date_time_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="Person_organisation_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="Work_request_mim">
    </interface>

    <interface
      kind="use"
      schema="action_schema">
      <interfaced.item
        name="action"/>
      <interfaced.item
        name="action_status"/>
      <interfaced.item
        name="action_directive"/>
      <interfaced.item
        name="directed_action"/>
      <interfaced.item
        name="executed_action"/>
    </interface>

    <type name="action_approval_item">
      <select
        basedon="approval_item"
        selectitems="action">
      </select>

    </type>
    <type name="action_date_item">
      <select
        basedon="date_item"
        selectitems="action">
      </select>

    </type>
    <type name="action_date_and_time_item">
      <select
        basedon="date_and_time_item"
        selectitems="action">
      </select>

    </type>
    <type name="action_organization_item">
      <select
        basedon="organization_item"
        selectitems="action">
      </select>

    </type>
    <type name="action_person_and_organization_item">
      <select
        basedon="person_and_organization_item"
        selectitems="action">
      </select>

    </type>
    <type name="action_contract_item">
      <select
        basedon="contract_item"
        selectitems="action">
      </select>

    </type>
  </schema>

</express>
