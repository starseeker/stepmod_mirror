<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.9 2002/02/25 12:10:19 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $"
  description.file="arm_descriptions.xml">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.9"
    source="plcs_arm_ge13v82v5.exp"/>

  <schema name="Work_request_arm">
    <interface
      kind="use"
      schema="Activity_method_arm">
    </interface>

    <interface
      kind="use"
      schema="Approval_arm">
    </interface>

    <interface
      kind="use"
      schema="Configuration_effectivity_arm">
    </interface>

    <interface
      kind="use"
      schema="Date_time_assignment_arm">
    </interface>

    <interface
      kind="use"
      schema="Document_assignment_arm">
    </interface>

    <interface
      kind="use"
      schema="Person_organization_assignment_arm">
    </interface>

    <interface
      kind="use"
      schema="Product_version_arm">
    </interface>

    <interface
      kind="use"
      schema="Product_view_definition_arm">
    </interface>

    <interface
      kind="use"
      schema="Product_view_definition_structure_arm">
    </interface>

    <interface
      kind="use"
      schema="Property_assignment_arm">
    </interface>

    <type name="affected_item_select">
      <select
        extensible="YES"
        selectitems="Configuration_effectivity Element_property Product_version Product_view_definition View_definition_relationship">
      </select>

    </type>
    <type name="work_request_approval_item">
      <select
        basedon="approval_item"
        selectitems="Work_request">
      </select>

    </type>
    <type name="work_request_date_or_date_time_item">
      <select
        basedon="date_or_date_time_item"
        selectitems="Work_request">
      </select>

    </type>
    <type name="work_request_documented_element_select">
      <select
        basedon="documented_element_select"
        selectitems="Work_request">
      </select>

    </type>
    <type name="work_request_organization_or_person_in_organization_item">
      <select
        basedon="organization_or_person_in_organization_item"
        selectitems="Work_request">
      </select>

    </type>
    <entity
      name="Activity_method_assignment">
      <explicit
        name="assigned_method">
        <typename
          name="Activity_method"/>
      </explicit>
      <explicit
        name="associated_request">
        <typename
          name="Work_request"/>
      </explicit>
      <explicit
        name="relation_type">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="Affected_items_assignment">
      <explicit
        name="assigned_work_request">
        <typename
          name="Work_request"/>
      </explicit>
      <explicit
        name="items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="affected_item_select"/>
      </explicit>
    </entity>

    <entity
      name="Work_request">
      <explicit
        name="purpose">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="request_id">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="version_id">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="Work_request_relationship">
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="id">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="related_work_request">
        <typename
          name="Work_request"/>
      </explicit>
      <explicit
        name="relating_work_request">
        <typename
          name="Work_request"/>
      </explicit>
    </entity>

    <entity
      name="Work_request_status">
      <explicit
        name="status">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="work_request">
        <typename
          name="Work_request"/>
      </explicit>
    </entity>

  </schema>


</express>