<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.17 2002/07/09 14:29:45 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file="arm_descriptions.xml"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.17"
    source="../data/modules/work_request/arm.exp"/>

  <schema name="Work_request_arm">
    <interface
      kind="use"
      schema="Activity_method_arm">
    </interface>

    <type name="affected_item_select">
      <select
        extensible="YES">
      </select>

    </type>
    <entity
      name="Activity_method_assignment">
      <explicit
        name="assigned_method">
        <typename
          name="Activity_method"/>
      </explicit>
      <explicit
        name="associated_request">
        <typename
          name="Work_request"/>
      </explicit>
      <explicit
        name="relation_type">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="Affected_items_assignment">
      <explicit
        name="assigned_work_request">
        <typename
          name="Work_request"/>
      </explicit>
      <explicit
        name="items">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="affected_item_select"/>
      </explicit>
    </entity>

    <entity
      name="Work_request">
      <explicit
        name="request_id">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="version_id">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="purpose">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
    </entity>

    <entity
      name="Work_request_status">
      <explicit
        name="status">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="work_request">
        <typename
          name="Work_request"/>
      </explicit>
    </entity>

  </schema>

</express>
