<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="arm_descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO/TS 10303-1042">
   <application name="JSDAI" owner="LKSoft" source="work_request_arm schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="Work_request_arm">
      <interface kind="use" schema="Activity_method_arm"/>
      <type name="affected_item_select">
         <select extensible="YES"/>
      </type>
      <entity name="Activity_method_assignment">
         <explicit name="relation_type">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="assigned_method">
            <typename name="Activity_method"/>
         </explicit>
         <explicit name="associated_request">
            <typename name="Work_request"/>
         </explicit>
      </entity>
      <entity name="Affected_items_assignment">
         <explicit name="assigned_work_request">
            <typename name="Work_request"/>
         </explicit>
         <explicit name="items">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="affected_item_select"/>
         </explicit>
      </entity>
      <entity name="Work_request">
         <explicit name="request_id">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="version_id">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="description" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="purpose">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="Work_request_relationship">
         <explicit name="relation_type">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="description" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="relating">
            <typename name="Work_request"/>
         </explicit>
         <explicit name="related">
            <typename name="Work_request"/>
         </explicit>
      </entity>
      <entity name="Work_request_status">
         <explicit name="status">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="work_request">
            <typename name="Work_request"/>
         </explicit>
      </entity>
   </schema>
</express>
