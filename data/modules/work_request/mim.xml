<?xml version="1.0"?>
<!-- $Id: mim.xml,v 1.4 2002/01/29 15:46:14 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: 2002/01/29 15:46:14 $"
  rcs.revision="$Revision: 1.4 $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.6"
    source="../data/modules/work_request/mim.exp"/>

  <schema name="work_request_mim">
    <interface
      kind="use"
      schema="Approval_mim">
    </interface>

    <interface
      kind="use"
      schema="Date_time_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="Product_version_mim">
    </interface>

    <interface
      kind="use"
      schema="Product_view_definition_structure_mim">
    </interface>

    <interface
      kind="use"
      schema="Person_organization_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="action_schema">
      <interfaced.item
        name="versioned_action_request"/>
      <interfaced.item
        name="action_request_status"/>
      <interfaced.item
        name="action_method"/>
    </interface>

    <type name="versioned_action_request_approval_item">
      <description>
        A <b>versioned_action_request_approval_item</b> extends approval
        assignment to <b>versioned_action_request</b> instances.  
      </description>
      <select
        basedon="approval_item"
        selectitems="versioned_action_request">
      </select>
    </type>
    <type name="versioned_action_request_date_item">
      <description>
        A <b>versioned_action_request_date_item</b> extends date assignment
        to <b>versioned_action_request</b> instances.  
      </description>
      <select
        basedon="date_item"
        selectitems="versioned_action_request">
      </select>
    </type>
    <type name="versioned_action_request_date_and_time_item">
      <description>
        A <b>versioned_action_request_date_and_time_item</b> extends date
        and time assignment to <b>versioned_action_request</b> instances.  
      </description>

      <select
        basedon="date_and_time_item"
        selectitems="versioned_action_request">
      </select>
    </type>
    <type name="versioned_action_request_organization_item">
      <description>
        An <b>versioned_action_request_organization_item</b> extends
      organization assignment to <b>versioned_action_request</b> instances.
      </description>
      <select
        basedon="organization_item"
        selectitems="versioned_action_request">
      </select>

    </type>
    <type name="versioned_action_request_person_and_organization_item">
      <description>
        A <b>versioned_action_request_person_and_organization_item</b>
        extends person and organization assignment to
        <b>versioned_action_request</b> instances.  
      </description>
      <select
        basedon="person_and_organization_item"
        selectitems="versioned_action_request">
      </select>

    </type>
  </schema>

</express>
