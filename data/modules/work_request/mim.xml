<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.0 2001-11-15 11:24:44+00 rob Exp rob $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.0"
    source="../data/modules/work_request/mim.exp"/>

  <schema name="work_request_mim">
    <interface
      kind="use"
      schema="Approval_mim">
    </interface>

    <interface
      kind="use"
      schema="Date_time_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="Product_version_mim">
    </interface>

    <interface
      kind="use"
      schema="Product_view_definition_structure_mim">
    </interface>

    <interface
      kind="use"
      schema="Person_organisation_assignment_mim">
    </interface>

    <interface
      kind="use"
      schema="action_schema">
      <interfaced.item
        name="versioned_action_request"/>
      <interfaced.item
        name="action_request_status"/>
      <interfaced.item
        name="action_method"/>
    </interface>

    <type name="versioned_action_request_approval_item">
      <select
        basedon="approval_item"
        selectitems="versioned_action_request">
      </select>

    </type>
    <type name="versioned_action_request_date_item">
      <select
        basedon="date_item"
        selectitems="versioned_action_request">
      </select>

    </type>
    <type name="versioned_action_request_date_and_time_item">
      <select
        basedon="date_and_time_item"
        selectitems="versioned_action_request">
      </select>

    </type>
    <type name="versioned_action_request_organization_item">
      <select
        basedon="organization_item"
        selectitems="versioned_action_request">
      </select>

    </type>
    <type name="versioned_action_request_person_and_organization_item">
      <select
        basedon="person_and_organization_item"
        selectitems="versioned_action_request">
      </select>

    </type>
  </schema>

</express>
