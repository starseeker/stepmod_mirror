<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<?xml-stylesheet type="text/xsl"
href="../../../xsl/express.xsl" ?>
<express
   language_version="2"
   rcs.date="$Date: 2004/10/25 18:30:36 $"
   rcs.revision="$Revision: 1.1 $"
   description.file="arm_descriptions.xml">
  <schema name="Work_structure_arm">
    <interface
      kind="use"
      schema="Approval_arm">
    </interface>
    <interface
      kind="use"
      schema="Contract_arm">
    </interface>
    <interface
      kind="use"
      schema="Document_assignment_arm">
    </interface>
    <interface
      kind="use"
      schema="File_identification_arm">
    </interface>
    <interface
      kind="use"
      schema="Information_packet_arm">
    </interface>
    <interface
      kind="use"
      schema="Justification_arm">
    </interface>
    <interface
      kind="use"
      schema="Observation_arm">
    </interface>
    <interface
      kind="use"
      schema="Security_classification_arm">
    </interface>
    <interface
      kind="use"
      schema="Work_breakdown_arm">
    </interface>
    <interface
      kind="use"
      schema="Work_order_arm">
    </interface>
    <interface
      kind="use"
      schema="Work_output_arm">
    </interface>
    <interface
      kind="use"
      schema="Work_request_arm">
    </interface>
    <type name="ws_affected_item_select">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="affected_item_select"
        selectitems="Contract File"
      />
    </type>

    <type name="ws_approval_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="approval_item"
        selectitems="Activity_method_assignment Work_order Work_request"
      />
    </type>

    <type name="ws_observed_context">
      <select
        extensible="YES"
        basedon="observed_context"
        selectitems="Work_output Work_order Work_request"
      />
    </type>

    <type name="ws_wbs_deliverable_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="wbs_deliverable_item"
        selectitems="Digital_file File Hardcopy Work_output"
      />
    </type>

    <type name="ws_work_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="work_item"
      />
    </type>

    <type name="ws_work_output_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="work_output_item"
        selectitems="File"
      />
    </type>

    <type name="ws_information_packet_select">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="information_packet_select"
        selectitems="Work_information_packet"
      />
    </type>

    <type name="ws_information_packet_element">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="information_packet_element"
        selectitems="Activity_method_assignment Affected_items_assignment Approval Approval_assignment Approval_relationship Approval_status Approving_person_organization Contract Contract_assignment Digital_file Directed_activity Document_assignment File File_location_identification Hardcopy Justification Justification_assignment Justification_relationship Justification_support_assignment Observation Observation_consequence Observation_item Observation_relationship Partial_document_assignment Security_classification Security_classification_assignment Work_information_packet Work_order Work_output Work_output_assignment Work_output_relationship Work_request Work_request_status"
      />
    </type>

    <type name="ws_information_packet_item">
      <select
        extensible="YES"
        genericentity="YES"
        basedon="information_packet_item"
        selectitems="Approval Contract Digital_file File Justification Observation Work_order Work_output Work_request"
      />
    </type>

    <entity name="Work_information_packet"
      supertypes="Information_packet_definition"
    >
    </entity>
  </schema>
</express>
