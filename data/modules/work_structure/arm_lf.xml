        />
      </explicit>
      <explicit
        name="assigned_address">
        <typename name="Address"/>
      </explicit>
      <explicit
        name="located_person_organizations">
        <aggregate type="SET"
          lower="1"
          upper="?"
        />
        <typename name="organization_or_person_in_organization_select"/>
      </explicit>
