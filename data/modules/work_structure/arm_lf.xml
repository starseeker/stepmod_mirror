      />
    </type>

    <type name="second_in_minute">
      <builtintype type="REAL"
      />
      <where
        label="WR1"
        expression="{0 &lt;= SELF &lt;= 60.000000}">
      </where>
    </type>

    <type name="security_classification_item">
      <select
        selectitems="Aaa_empty_for_ws_testing"
      />
    </type>

    <type name="string_select">
      <select
        selectitems="Approval Approval_relationship Approval_status Certification Contract Date_or_date_time_assignment Identification_assignment Organization_relationship Person_in_organization Project Project_assignment Project_relationship"
      />
