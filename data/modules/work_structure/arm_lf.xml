  <schema name="Work_structure_arm_cc_lf">
    <type name="activity_item">
      <select
        selectitems="Aaa_empty_for_ws_testing"
      />
    </type>

    <type name="activity_method_item">
      <select
        selectitems="Aaa_empty_for_ws_testing"
      />
    </type>

    <type name="affected_item_select">
