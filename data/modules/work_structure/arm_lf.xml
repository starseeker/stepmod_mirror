      />
    </type>

    <type name="minute_in_hour">
      <builtintype type="INTEGER"
      />
      <where
        label="WR1"
        expression="{0 &lt;= SELF &lt;= 59}">
      </where>
    </type>

    <type name="month_in_year_number">
      <builtintype type="INTEGER"
      />
      <where
        label="WR1"
        expression="{1 &lt;= SELF &lt;= 12}">
      </where>
    </type>

    <type name="observation_recorder">
      <select
        selectitems="Product_as_realized"
      />
    </type>

    <type name="observed_context">
