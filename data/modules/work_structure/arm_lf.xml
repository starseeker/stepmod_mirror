      />
    </type>

    <type name="any_string_value">
      <builtintype type="STRING"
      />
    </type>

    <type name="approval_item">
      <select
        selectitems="Certification Contract Observation"
      />
    </type>

    <type name="certification_item">
      <select
        selectitems="Aaa_empty_for_ws_testing"
      />
    </type>

    <type name="classification_item">
      <select
        selectitems="Observation Observation_relationship Work_output Work_output_assignment Work_output_relationship"
      />
    </type>

    <type name="contract_item">
      <select
        selectitems="Aaa_empty_for_ws_testing"
      />
    </type>

    <type name="date_or_date_time_item">
