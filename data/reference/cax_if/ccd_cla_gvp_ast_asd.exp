      v_sense        : BOOLEAN;
    INVERSE
      using_surfaces : BAG [1:?] OF rectangular_composite_surface FOR 
                          segments;
    WHERE
      wr1: (NOT ('CCD_CLA_GVP_AST_ASD.CURVE_BOUNDED_SURFACE' IN TYPEOF(
               parent_surface)));
  END_ENTITY; -- surface_patch

