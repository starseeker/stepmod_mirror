    poly_loop_type := 'CCD_CLA_GVP_AST_ASD.POLY_LOOP' IN TYPEOF(l[1]);
    REPEAT i := 2 TO SIZEOF(l) BY 1;
      IF ('CCD_CLA_GVP_AST_ASD.POLY_LOOP' IN TYPEOF(l[i])) <> 
          poly_loop_type THEN
