  ENTITY surface_curve
    SUPERTYPE OF (ONEOF (intersection_curve,seam_curve))
    SUBTYPE OF (curve);
      curve_3d              : curve;
      associated_geometry   : LIST [1:2] OF pcurve_or_surface;
      master_representation : preferred_surface_curve_representation;
    DERIVE
      basis_surface : SET [1:2] OF surface := get_basis_surface(SELF);
    WHERE
      wr1: (curve_3d.dim = 3);
      wr2: (('CCD_CLA_GVP_AST_ASD.PCURVE' IN TYPEOF(associated_geometry[1])) 
               OR (master_representation <> pcurve_s1));
      wr3: (('CCD_CLA_GVP_AST_ASD.PCURVE' IN TYPEOF(associated_geometry[2])) 
               OR (master_representation <> pcurve_s2));
      wr4: (NOT ('CCD_CLA_GVP_AST_ASD.PCURVE' IN TYPEOF(curve_3d)));
  END_ENTITY; -- surface_curve

