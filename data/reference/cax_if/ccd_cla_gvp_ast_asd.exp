  END_ENTITY; -- product_definition_relationship

  ENTITY product_definition_shape
    SUBTYPE OF (property_definition);
    UNIQUE
      ur1 : definition;
    WHERE
      wr1: (NOT ('CCD_CLA_GVP_AST_ASD.SHAPE_DEFINITION' IN TYPEOF(SELF\
               property_definition.definition)));
