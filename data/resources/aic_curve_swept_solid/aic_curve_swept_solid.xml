         <interfaced.item name="trimmed_curve"/>
         <interfaced.item name="toroidal_surface"/>
         <interfaced.item name="uniform_curve"/>
         <interfaced.item name="uniform_surface"/>
         <interfaced.item name="vector"/>
      </interface>
      <interface kind="use" schema="geometric_model_schema">
         <interfaced.item name="extruded_area_solid"/>
         <interfaced.item name="revolved_area_solid"/>
         <interfaced.item name="surface_curve_swept_area_solid"/>
         <interfaced.item name="swept_disk_solid"/>
      </interface>
      <interface kind="use" schema="representation_schema">
         <interfaced.item name="mapped_item"/>
      </interface>
      <interface kind="use" schema="product_property_representation_schema">
         <interfaced.item name="shape_representation"/>
      </interface>
      <entity name="curve_swept_solid_shape_representation" supertypes="shape_representation">
         <where expression="SIZEOF (QUERY (it &lt;* SELF.items |&#xA;          NOT (SIZEOF (['AIC_CURVE_SWEPT_SOLID.SWEPT_AREA_SOLID',&#xA;                'AIC_CURVE_SWEPT_SOLID.SWEPT_DISK_SOLID',&#xA;                'AIC_CURVE_SWEPT_SOLID.MAPPED_ITEM',&#xA;                'AIC_CURVE_SWEPT_SOLID.AXIS2_PLACEMENT_3D'] *&#xA;                   TYPEOF(it)) = 1))) = 0" label="WR1"/>
         <where expression="SIZEOF (QUERY (it &lt;* SELF.items |&#xA;         SIZEOF(['AIC_CURVE_SWEPT_SOLID.SWEPT_AREA_SOLID',&#xA;                'AIC_CURVE_SWEPT_SOLID.SWEPT_DISK_SOLID',&#xA;          'AIC_CURVE_SWEPT_SOLID.MAPPED_ITEM'] * TYPEOF(it)) =1 )) &gt; 0" label="WR2"/>
         <where expression="SIZEOF (QUERY (mi &lt;*  QUERY (it &lt;* items |&#xA;                 'AIC_CURVE_SWEPT_SOLID.MAPPED_ITEM' IN TYPEOF(it)) |&#xA;   NOT ('AIC_CURVE_SWEPT_SOLID.CURVE_SWEPT_SOLID_SHAPE_REPRESENTATION' IN&#xA;             TYPEOF(mi\mapped_item.mapping_source.&#xA;                           mapped_representation)))) = 0" label="WR3"/>
         <where expression="SIZEOF (QUERY (scsas &lt;* QUERY (it &lt;* SELF.items |&#xA;        'AIC_CURVE_SWEPT_SOLID.SURFACE_CURVE_SWEPT_AREA_SOLID' IN&#xA;              TYPEOF(it)) |&#xA;          NOT(('AIC_CURVE_SWEPT_SOLID.SURFACE_CURVE' IN&#xA;                 TYPEOF(scsas\surface_curve_swept_area_solid.directrix)) OR&#xA;                   ('AIC_CURVE_SWEPT_SOLID.PCURVE' IN&#xA;                   TYPEOF(scsas\surface_curve_swept_area_solid.directrix))))) = 0" label="WR4"/>
      </entity>
      <entity name="ruled_surface_swept_area_solid" supertypes="surface_curve_swept_area_solid">
         <where expression="('AIC_CURVE_SWEPT_SOLID.B_SPLINE_SURFACE' IN TYPEOF(SELF.reference_surface)) AND&#xA;         (SELF.reference_surface\b_spline_surface.u_degree = 1)" label="WR1"/>
         <where expression="('AIC_CURVE_SWEPT_SOLID.PCURVE' IN TYPEOF(SELF.directrix)) OR&#xA;        (('AIC_CURVE_SWEPT_SOLID.B_SPLINE_CURVE' IN TYPEOF(SELF.directrix\surface_curve.curve_3d))&#xA;         AND&#xA;         (SELF.directrix\surface_curve.curve_3d\b_spline_curve.degree =&#xA;         SELF.reference_surface\b_spline_surface.v_degree))" label="WR2"/>
      </entity>
   </schema>
