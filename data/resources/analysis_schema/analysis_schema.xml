    </interface>
    <interface
      kind="reference"
      schema="product_property_definition_schema">
      <interfaced.item 
        name="general_property"/>
    </interface>
    <interface
      kind="reference"
      schema="support_resource_schema"/>
      <entity
        name="behavioural_decomposition_of_numerical_model">

        <explicit
