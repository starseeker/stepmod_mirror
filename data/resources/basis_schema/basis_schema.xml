<?xml version='1.0' encoding='UTF-8'?>
<!-- $Id: express2xml.js,v 1.38 2004/08/24 16:58:55 thendrix Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml2.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.38"
    source="basis_schema.exp"/>

  <schema name="basis_schema">

    <interface
      kind="reference"
      schema="analysis_schema">
      <interfaced.item
        name="model_property_distribution"/>
    </interface>
    <interface
      kind="reference"
      schema="conditions_schema">
      <interfaced.item
        name="Riemann_1D_data_name"/>
    </interface>
    <interface
      kind="reference"
      schema="domain_schema">
      <interfaced.item
        name="coordinate_data_name"/>
    </interface>
    <interface
      kind="reference"
      schema="equations_schema">
      <interfaced.item
        name="force_moment_data_name"/>
      <interfaced.item
        name="gas_model_data_name"/>
      <interfaced.item
        name="thermal_conductivity_model_data_name"/>
      <interfaced.item
        name="turbulence_closure_data_name"/>
      <interfaced.item
        name="turbulence_model_data_name"/>
      <interfaced.item
        name="viscosity_model_data_name"/>
    </interface>
    <interface
      kind="reference"
      schema="external_reference_schema">
      <interfaced.item
        name="externally_defined_item"/>
    </interface>
    <interface
      kind="reference"
      schema="mathematical_context_schema">
      <interfaced.item
        name="value_context_select"/>
    </interface>
    <interface
      kind="reference"
      schema="mathematical_description_of_distribution_schema">
      <interfaced.item
        name="property_distribution_select"/>
    </interface>
    <interface
      kind="reference"
      schema="product_property_definition_schema">
      <interfaced.item
        name="general_property"/>
    </interface>
    <interface
      kind="reference"
      schema="product_property_representation_schema">
      <interfaced.item
        name="shape_representation"/>
    </interface>
    <interface
      kind="reference"
      schema="representation_schema">
      <interfaced.item
        name="representation_context"/>
    </interface>
    <interface
      kind="reference"
      schema="results_schema">
      <interfaced.item
        name="flow_solution_data_name"/>
    </interface>
    <interface
      kind="reference"
      schema="support_resource_schema">
      <interfaced.item
        name="text"/>
    </interface>
    <type name="choose_general_property_identifier">
      <select
        extensible="YES"
        selectitems="externally_defined_item defined_data_name">
      </select>
    </type>

    <type name="choose_geometry_location">
      <select
        extensible="YES"
        selectitems="externally_defined_item shape_representation">
      </select>
    </type>

    <type name="choose_representation_context_identifier">
      <select
        extensible="YES"
        selectitems="externally_defined_item defined_data_class">
      </select>
    </type>

    <type name="defined_data_class">
      <enumeration
        extensible="YES"
        items="unspecified application_defined dimensional normalised_by_dimensional normalise_by_unknown_dimensional dimensionless_parameter dimensionless_constant">
      </enumeration>
    </type>

    <type name="defined_data_name">
      <select
        extensible="YES"
        selectitems="coordinate_data_name fd_defined_data_name">
      </select>
    </type>

    <type name="fd_defined_data_name">
      <select
        extensible="YES"
        selectitems="fd_nondimensional_parameter_name Riemann_1D_data_name force_moment_data_name gas_model_data_name viscosity_model_data_name thermal_conductivity_model_data_name turbulence_closure_data_name turbulence_model_data_name flow_solution_data_name">
      </select>
    </type>

    <type name="fd_nondimensional_parameter_name">
      <enumeration
        extensible="YES"
        items="Mach Mach_velocity Mach_velocity_sound Reynolds Reynolds_velocity Reynolds_length Reynolds_viscosity_kinematic Prandtl Prandtl_thermal_conductivity Prandtl_viscosity_molecular Prandtl_specific_heat_pressure specific_heat_ratio specific_heat_ratio_pressure specific_heat_ratio_volume coef_pressure coef_skin_friction_x coef_skin_friction_y coef_skin_friction_z coef_pressure_dynamic coef_pressure_reference length_reference">
      </enumeration>
    </type>

    <type name="mbna_property_distribution_select">
      <select
        extensible="YES"
        basedon="property_distribution_select"
        selectitems="model_property_distribution">
      </select>
    </type>

    <type name="mbna_value_context_select">
      <select
        extensible="YES"
        basedon="value_context_select"
        selectitems="choose_representation_context_identifier">
      </select>
    </type>

    <entity
      name="geometry_reference">
      <explicit
        name="description">
        <typename
          name="text"/>
      </explicit>
      <explicit
        name="data">
        <typename
          name="choose_geometry_location"/>
      </explicit>
    </entity>

    <entity
      name="specified_general_property"
      supertypes="general_property">
      <explicit
        name="name_specifier">
        <typename
          name="choose_general_property_identifier"/>
      </explicit>
    </entity>

    <entity
      name="specified_representation_context"
      supertypes="representation_context">
      <explicit
        name="class_specifier">
        <typename
          name="choose_representation_context_identifier"/>
      </explicit>
    </entity>

  </schema>
</express>
