		<entity name="certification_type">
			<explicit name="description">
				<typename name="label"/>
			</explicit>
			<graphic.element page="2" xcoord="99" ycoord="35"/>
		</entity>
		<graphic.element page="1" xcoord="99" ycoord="36"/>
	</schema>
</express>
