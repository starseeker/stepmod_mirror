<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?><!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express description.file="descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO 10303-113">
   <application name="JSDAI" owner="LKSoft" source="counterbore_countersink_schema schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="counterbore_countersink_schema">
      <interface kind="reference" schema="geometry_schema">
         <interfaced.item name="axis2_placement_3d"/>
      </interface>
      <interface kind="reference" schema="measure_schema">
         <interfaced.item name="length_measure_with_unit"/>
         <interfaced.item name="positive_length_measure_with_unit"/>
         <interfaced.item name="plane_angle_measure_with_unit"/>
         <interfaced.item name="positive_plane_angle_measure_with_unit"/>
      </interface>
      <interface kind="reference" schema="product_definition_schema">
         <interfaced.item name="product_definition"/>
      </interface>
      <interface kind="reference" schema="product_structure_schema">
         <interfaced.item name="multi_level_reference_designator"/>
      </interface>
      <interface kind="reference" schema="product_property_definition_schema">
         <interfaced.item name="characterized_object"/>
         <interfaced.item name="property_definition"/>
         <interfaced.item name="shape_aspect"/>
      </interface>
      <interface kind="reference" schema="product_property_representation_schema">
         <interfaced.item name="shape_representation"/>
      </interface>
      <interface kind="reference" schema="shape_tolerance_schema">
         <interfaced.item name="length_tolerance_value"/>
         <interfaced.item name="limits_and_fits"/>
         <interfaced.item name="plane_angle_tolerance_value"/>
      </interface>
      <type name="tolerance_value_or_limits_and_fits">
         <select selectitems="length_tolerance_value limits_and_fits"/>
      </type>
      <entity name="counterbore_definition" supertypes="characterized_object">
         <explicit name="counterbore_diameter">
            <typename name="hole_diameter"/>
         </explicit>
         <explicit name="counterbore_depth">
            <typename name="hole_depth"/>
         </explicit>
      </entity>
      <entity name="counterbore_hole_definition" supertypes="characterized_object">
         <explicit name="counterbore_definition">
            <aggregate lower="1" type="LIST" unique="YES" upper="?"/>
            <typename name="counterbore_definition"/>
         </explicit>
         <explicit name="drill_diameter">
            <typename name="hole_diameter"/>
         </explicit>
         <explicit name="drill_depth" optional="YES">
            <typename name="hole_depth"/>
         </explicit>
         <explicit name="through_hole">
            <builtintype type="BOOLEAN"/>
         </explicit>
         <where expression="through_hole XOR EXISTS(drill_depth)" label="WR1"/>
      </entity>
      <entity name="counterbore_hole_occurrence" supertypes="shape_aspect">
         <explicit name="definition">
            <typename name="counterbore_hole_definition"/>
         </explicit>
         <where expression="'PRODUCT_DEFINITION_SCHEMA.PRODUCT_DEFINITION' IN TYPEOF(SELF\shape_aspect.of_shape\property_definition.definition)" label="WR1"/>
      </entity>
      <entity name="counterbore_hole_occurrence_in_assembly" supertypes="counterbore_hole_occurrence">
         <explicit name="modified_components">
            <typename name="multi_level_reference_designator"/>
         </explicit>
      </entity>
      <entity name="counterdrill_hole_definition" supertypes="characterized_object">
         <explicit name="counterbore_definition">
            <typename name="counterbore_definition"/>
         </explicit>
         <explicit name="counterdrill_angle">
            <typename name="positive_plane_angle_measure_with_unit"/>
         </explicit>
         <explicit name="counterdrill_angle_tolerance">
            <typename name="plane_angle_tolerance_value"/>
         </explicit>
         <explicit name="drill_diameter">
            <typename name="hole_diameter"/>
         </explicit>
         <explicit name="drill_depth" optional="YES">
            <typename name="hole_depth"/>
         </explicit>
         <explicit name="through_hole">
            <builtintype type="BOOLEAN"/>
         </explicit>
         <where expression="through_hole XOR EXISTS(drill_depth)" label="WR1"/>
      </entity>
      <entity name="counterdrill_hole_occurrence" supertypes="shape_aspect">
         <explicit name="definition">
            <typename name="counterdrill_hole_definition"/>
         </explicit>
         <where expression="'PRODUCT_DEFINITION_SCHEMA.PRODUCT_DEFINITION' IN TYPEOF(SELF\shape_aspect.of_shape\property_definition.definition)" label="WR1"/>
      </entity>
      <entity name="counterdrill_hole_occurrence_in_assembly" supertypes="counterdrill_hole_occurrence">
         <explicit name="modified_components">
            <typename name="multi_level_reference_designator"/>
         </explicit>
      </entity>
      <entity name="countersink_hole_definition" supertypes="characterized_object">
         <explicit name="countersink_diameter">
            <typename name="hole_diameter"/>
         </explicit>
         <explicit name="countersink_angle">
            <typename name="positive_plane_angle_measure_with_unit"/>
         </explicit>
         <explicit name="countersink_angle_tolerance">
            <typename name="plane_angle_tolerance_value"/>
         </explicit>
         <explicit name="drill_diameter">
            <typename name="hole_diameter"/>
         </explicit>
         <explicit name="drill_depth" optional="YES">
            <typename name="hole_depth"/>
         </explicit>
         <explicit name="through_hole">
            <builtintype type="BOOLEAN"/>
         </explicit>
         <where expression="through_hole XOR EXISTS(drill_depth)" label="WR1"/>
      </entity>
      <entity name="countersink_hole_occurrence" supertypes="shape_aspect">
         <explicit name="definition">
            <typename name="countersink_hole_definition"/>
         </explicit>
         <where expression="'PRODUCT_DEFINITION_SCHEMA.PRODUCT_DEFINITION' IN TYPEOF(SELF\shape_aspect.of_shape\property_definition.definition)" label="WR1"/>
      </entity>
      <entity name="countersink_hole_occurrence_in_assembly" supertypes="countersink_hole_occurrence">
         <explicit name="modified_components">
            <typename name="multi_level_reference_designator"/>
         </explicit>
      </entity>
      <entity name="hole_depth">
         <explicit name="depth">
            <typename name="positive_length_measure_with_unit"/>
         </explicit>
         <explicit name="depth_tolerance" optional="YES">
            <typename name="length_tolerance_value"/>
         </explicit>
      </entity>
      <entity name="hole_diameter">
         <explicit name="diameter">
            <typename name="positive_length_measure_with_unit"/>
         </explicit>
         <explicit name="diameter_tolerance" optional="YES">
            <typename name="tolerance_value_or_limits_and_fits"/>
         </explicit>
      </entity>
      <entity name="implicit_counterbore_hole_definition" supertypes="counterbore_hole_definition">
         <explicit name="position">
            <typename name="shape_representation"/>
         </explicit>
         <where expression="SIZEOF(position\shape_representation.items) = 1" label="WR1"/>
         <where expression="SIZEOF(QUERY( ri &lt;* position\shape_representation.items | &#10;               NOT ('GEOMETRY_SCHEMA.AXIS2_PLACEMENT_3D' IN TYPEOF (ri))&#10;               )) = 0" label="WR2"/>
      </entity>
      <entity name="implicit_counterdrill_hole_definition" supertypes="counterdrill_hole_definition">
         <explicit name="position">
            <typename name="shape_representation"/>
         </explicit>
         <where expression="SIZEOF(position\shape_representation.items) = 1" label="WR1"/>
         <where expression="SIZEOF(QUERY( ri &lt;* position\shape_representation.items | &#10;               NOT ('GEOMETRY_SCHEMA.AXIS2_PLACEMENT_3D' IN TYPEOF (ri))&#10;               )) = 0" label="WR2"/>
      </entity>
      <entity name="implicit_countersink_hole_definition" supertypes="countersink_hole_definition">
         <explicit name="position">
            <typename name="shape_representation"/>
         </explicit>
         <where expression="SIZEOF(position\shape_representation.items) = 1" label="WR1"/>
         <where expression="SIZEOF(QUERY( ri &lt;* position\shape_representation.items | &#10;               NOT ('GEOMETRY_SCHEMA.AXIS2_PLACEMENT_3D' IN TYPEOF (ri))&#10;               )) = 0" label="WR2"/>
      </entity>
      <entity name="implicit_spotface_definition" supertypes="spotface_hole_definition implicit_counterbore_hole_definition"/>
      <entity name="spotface_definition" supertypes="counterbore_definition">
         <explicit name="spotface_radius">
            <typename name="positive_length_measure_with_unit"/>
         </explicit>
         <explicit name="spotface_radius_tolerance" optional="YES">
            <typename name="length_tolerance_value"/>
         </explicit>
      </entity>
      <entity name="spotface_hole_definition" supertypes="counterbore_hole_definition">
         <explicit name="spotface_definition">
            <aggregate lower="1" type="LIST" unique="YES" upper="?"/>
            <typename name="spotface_definition"/>
         </explicit>
      </entity>
      <entity name="spotface_occurrence" supertypes="counterbore_hole_occurrence">
         <explicit name="definition">
            <typename name="spotface_hole_definition"/>
            <redeclaration entity-ref="counterbore_hole_occurrence"/>
         </explicit>
      </entity>
      <entity name="spotface_occurrence_in_assembly" supertypes="counterbore_hole_occurrence_in_assembly">
         <explicit name="definition">
            <typename name="spotface_hole_definition"/>
            <redeclaration entity-ref="counterbore_hole_occurrence"/>
         </explicit>
      </entity>
   </schema>
</express>
