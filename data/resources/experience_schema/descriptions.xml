   <express_ref linkend="experience_schema::experience_schema.experience.id"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="experience_schema.experience.id">
		the string of characters that identify the <b>experience</b>.
	</ext_description>
