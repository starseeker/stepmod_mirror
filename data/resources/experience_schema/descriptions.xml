	<ext_description linkend="experience_schema"/>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: experience_schema  Entity: experience  
   <express_ref linkend="experience_schema::experience_schema.experience"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="experience_schema.experience">
		An experience is an episode of practice, undertaken by a person or organization, that contributes to the accumulation of knowledge or a skill.
		<example>100 flying hours in a Tornado jet undertaken by a particular pilot in a particular year.</example>
	</ext_description>
