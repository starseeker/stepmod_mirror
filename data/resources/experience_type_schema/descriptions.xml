<!DOCTYPE ext_descriptions SYSTEM "../../../dtd/description.dtd">
<ext_descriptions module_directory="experience_type_schema" schema_file="experience_type_schema.xml" describe.selects="YES" rcs.date="$Date: 2003/03/07 16:26:51 $" rcs.revision="$Revision: 1.2 $">
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: experience_type_schema
   <express_ref linkend="experience_type_schema::experience_type_schema"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<ext_description linkend="experience_type_schema"/>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: experience_type_schema  Entity: experience_type  
   <express_ref linkend="experience_type_schema::experience_type_schema.experience_type"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="experience_type_schema.experience_type">
		An <b>experience_type</b> represents a kind of experience. 
		<example>
			<i>Computing experience</i> and <i>military experience</i> are examples of types of experience.	
		</example>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: experience_type_schema  Entity: experience_type  Attribute: id 
   <express_ref linkend="experience_type_schema::experience_type_schema.experience_type.id"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="experience_type_schema.experience_type.id">
		the identifying string that distinguishes the <b>experience_type</b>.
		<note>
			The identification will always be relative to a particular context.
		</note>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: experience_type_schema  Entity: experience_type  Attribute: name 
   <express_ref linkend="experience_type_schema::experience_type_schema.experience_type.name"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="experience_type_schema.experience_type.name">
		a word or group of words by which the <b>experience_type</b> is known. The value of this attribute need not be specified.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: experience_type_schema  Entity: experience_type  Attribute: description 
   <express_ref linkend="experience_type_schema::experience_type_schema.experience_type.description"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="experience_type_schema.experience_type.description">
		a word or group of words that characterizes the <b>experience_type</b>. The value of this attribute need not be specified.
	</ext_description>
</ext_descriptions>
