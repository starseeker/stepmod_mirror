<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO 10303-57">
   <application name="JSDAI" owner="LKSoft" source="expression_extensions_schema schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="expression_extensions_schema">
      <interface kind="reference" schema="iso13584_expressions_schema">
         <interfaced.item name="defined_function"/>
         <interfaced.item name="expression"/>
      </interface>
      <interface kind="reference" schema="iso13584_generic_expressions_schema">
         <interfaced.item name="generic_literal"/>
         <interfaced.item name="generic_variable"/>
      </interface>
      <interface kind="reference" schema="iso13584_iec61360_dictionary_schema">
         <interfaced.item name="entity_instance_type"/>
      </interface>
      <type name="assignment_target_select">
         <select extensible="YES"/>
      </type>
      <type name="entity_instance_select">
         <select extensible="YES"/>
      </type>
      <entity name="entity_expression" supertypes="entity_instance_type expression">
         <explicit name="entity_name">
            <aggregate lower="1" type="SET" upper="1"/>
            <builtintype type="STRING"/>
            <redeclaration entity-ref="entity_instance_type" old_name="type_name"/>
         </explicit>
      </entity>
      <entity name="entity_instance" supertypes="generic_literal">
         <explicit name="entity_definition">
            <typename name="entity_expression"/>
         </explicit>
         <explicit name="the_instance">
            <typename name="entity_instance_select"/>
         </explicit>
      </entity>
      <entity name="expression_assignment">
         <explicit name="assigned_expression">
            <typename name="expression"/>
         </explicit>
         <explicit name="assigned_to_object">
            <typename name="assignment_target_select"/>
         </explicit>
         <explicit name="role">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="expression_relationship">
         <explicit name="description" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="name" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="related">
            <typename name="expression"/>
         </explicit>
         <explicit name="relating">
            <typename name="expression"/>
         </explicit>
         <explicit name="role">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="literal_assignment">
         <explicit name="base">
            <typename name="expression"/>
         </explicit>
         <explicit name="literal_value">
            <typename name="generic_literal"/>
         </explicit>
         <explicit name="value_context" optional="YES">
            <typename name="entity_instance"/>
         </explicit>
      </entity>
      <entity name="named_function" supertypes="defined_function">
         <explicit name="name">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="named_variable" supertypes="generic_variable">
         <explicit name="name">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
      <entity name="variable_assignment">
         <explicit name="the_function">
            <typename name="defined_function"/>
         </explicit>
         <explicit name="the_variable">
            <typename name="generic_variable"/>
         </explicit>
      </entity>
      <entity name="variable_entity_type" supertypes="entity_expression generic_variable"/>
   </schema>
</express>
