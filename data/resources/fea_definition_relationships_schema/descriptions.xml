  -->
<ext_description linkend="fea_definition_relationships_schema.element_topological_relationship.wr:WR1">
the <express_ref linkend="topology_schema:ir_express:topology_schema.topological_representation_item"/> shall be referenced.
</ext_description>
<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
