<?xml version="1.0"?>
<!-- $Id: express2xml.js,v 1.17 2002/07/09 14:29:45 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  reference="ISO 10303-104"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.17"
    source="../data/resources/fea_scalar_vector_tensor_schema/fea_scalar_vector_tensor_schema.exp"/>

  <schema name="fea_scalar_vector_tensor_schema">
    <interface
      kind="reference"
      schema="measure_schema">
      <interfaced.item
        name="context_dependent_measure"/>
    </interface>

    <interface
      kind="reference"
      schema="representation_schema">
      <interfaced.item
        name="representation_item"/>
    </interface>

    <type name="angular_value">
      <typename
        name="context_dependent_measure"/>

    </type>
    <type name="scalar">
      <typename
        name="context_dependent_measure"/>

    </type>
    <type name="tensor1">
      <select
        selectitems="tensor1_2d tensor1_3d">
      </select>

    </type>
    <type name="tensor1_2d">
      <aggregate
        type="ARRAY"
        lower="1"
        upper="2"/>
      <typename
        name="context_dependent_measure"/>

    </type>
    <type name="tensor1_3d">
      <aggregate
        type="ARRAY"
        lower="1"
        upper="3"/>
      <typename
        name="context_dependent_measure"/>

    </type>
    <type name="symmetric_tensor2_2d">
      <select
        selectitems="anisotropic_symmetric_tensor2_2d">
      </select>

    </type>
    <type name="anisotropic_symmetric_tensor2_2d">
      <aggregate
        type="ARRAY"
        lower="1"
        upper="3"/>
      <typename
        name="context_dependent_measure"/>

    </type>
    <type name="symmetric_tensor2_3d">
      <select
        selectitems="isotropic_symmetric_tensor2_3d orthotropic_symmetric_tensor2_3d anisotropic_symmetric_tensor2_3d">
      </select>

    </type>
    <type name="isotropic_symmetric_tensor2_3d">
      <typename
        name="context_dependent_measure"/>

    </type>
    <type name="orthotropic_symmetric_tensor2_3d">
      <aggregate
        type="ARRAY"
        lower="1"
        upper="3"/>
      <typename
        name="context_dependent_measure"/>

    </type>
    <type name="anisotropic_symmetric_tensor2_3d">
      <aggregate
        type="ARRAY"
        lower="1"
        upper="6"/>
      <typename
        name="context_dependent_measure"/>

    </type>
    <type name="symmetric_tensor4_2d">
      <select
        selectitems="anisotropic_symmetric_tensor4_2d">
      </select>

    </type>
    <type name="anisotropic_symmetric_tensor4_2d">
      <aggregate
        type="ARRAY"
        lower="1"
        upper="6"/>
      <typename
        name="context_dependent_measure"/>

    </type>
    <type name="tensor_type">
      <select
        selectitems="scalar angular_value tensor1_2d tensor1_3d anisotropic_symmetric_tensor2_2d isotropic_symmetric_tensor2_3d orthotropic_symmetric_tensor2_3d anisotropic_symmetric_tensor2_3d anisotropic_symmetric_tensor4_2d anisotropic_symmetric_tensor4_3d fea_isotropic_symmetric_tensor4_3d fea_iso_orthotropic_symmetric_tensor4_3d fea_transverse_isotropic_symmetric_tensor4_3d fea_column_normalised_orthotropic_symmetric_tensor4_3d fea_column_normalised_monoclinic_symmetric_tensor4_3d">
      </select>

    </type>
    <type name="symmetric_tensor4_3d">
      <select
        selectitems="anisotropic_symmetric_tensor4_3d fea_isotropic_symmetric_tensor4_3d fea_iso_orthotropic_symmetric_tensor4_3d fea_transverse_isotropic_symmetric_tensor4_3d fea_column_normalised_orthotropic_symmetric_tensor4_3d fea_column_normalised_monoclinic_symmetric_tensor4_3d">
      </select>

    </type>
    <type name="anisotropic_symmetric_tensor4_3d">
      <aggregate
        type="ARRAY"
        lower="1"
        upper="21"/>
      <typename
        name="context_dependent_measure"/>

    </type>
    <type name="fea_isotropic_symmetric_tensor4_3d">
      <aggregate
        type="ARRAY"
        lower="1"
        upper="2"/>
      <typename
        name="context_dependent_measure"/>

    </type>
    <type name="fea_iso_orthotropic_symmetric_tensor4_3d">
      <aggregate
        type="ARRAY"
        lower="1"
        upper="3"/>
      <typename
        name="context_dependent_measure"/>

    </type>
    <type name="fea_transverse_isotropic_symmetric_tensor4_3d">
      <aggregate
        type="ARRAY"
        lower="1"
        upper="5"/>
      <typename
        name="context_dependent_measure"/>

    </type>
    <type name="fea_column_normalised_orthotropic_symmetric_tensor4_3d">
      <aggregate
        type="ARRAY"
        lower="1"
        upper="9"/>
      <typename
        name="context_dependent_measure"/>

    </type>
    <type name="fea_column_normalised_monoclinic_symmetric_tensor4_3d">
      <aggregate
        type="ARRAY"
        lower="1"
        upper="13"/>
      <typename
        name="context_dependent_measure"/>

    </type>
    <entity
      name="tensor_representation_item"
      supertypes="representation_item">
      <explicit
        name="tensor_value">
        <typename
          name="tensor_type"/>
      </explicit>
    </entity>

  </schema>

</express>
