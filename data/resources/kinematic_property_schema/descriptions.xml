  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="kinematic_property_schema.kinematic_property_mechanism_representation_relation_with_base.used_representation">
	an attribute inherited from the
	<express_ref linkend="product_property_representation_schema:ir_express:product_property_representation_schema.property_definition_representation" />
	shall be redeclared as the
	<express_ref linkend="kinematic_property_schema:ir_express:kinematic_property_schema.mechanism_representation_select" />
