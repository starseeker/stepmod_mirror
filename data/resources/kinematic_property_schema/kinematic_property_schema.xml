<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO 10303-105">
   <application name="JSDAI" owner="LKSoft" source="kinematic_property_schema schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="kinematic_property_schema">
      <interface kind="reference" schema="kinematic_structure_schema">
         <interfaced.item name="kinematic_link_representation"/>
         <interfaced.item name="mechanism_representation"/>
      </interface>
      <interface kind="reference" schema="kinematic_topology_schema">
         <interfaced.item name="kinematic_link"/>
         <interfaced.item name="kinematic_topology_representation"/>
      </interface>
      <interface kind="reference" schema="product_property_definition_schema">
         <interfaced.item name="property_definition"/>
         <interfaced.item name="product_definition_shape"/>
      </interface>
      <interface kind="reference" schema="product_property_representation_schema">
         <interfaced.item name="property_definition_representation"/>
      </interface>
      <entity abstract.supertype="YES" name="kinematic_property_definition_representation" super.expression="ONEOF (kinematic_property_topology_representation, kinematic_property_mechanism_representation_relation)" supertypes="property_definition_representation">
         <explicit name="definition">
            <typename name="product_definition_kinematics"/>
            <redeclaration entity-ref="property_definition_representation"/>
         </explicit>
      </entity>
      <entity name="kinematic_property_mechanism_representation_relation" supertypes="kinematic_property_definition_representation">
         <explicit name="used_representation">
            <typename name="mechanism_representation"/>
            <redeclaration entity-ref="property_definition_representation"/>
         </explicit>
         <explicit name="base">
            <typename name="kinematic_link_representation"/>
         </explicit>
      </entity>
      <entity name="kinematic_property_topology_representation" supertypes="kinematic_property_definition_representation">
         <explicit name="used_representation">
            <typename name="kinematic_topology_representation"/>
            <redeclaration entity-ref="property_definition_representation"/>
         </explicit>
         <explicit name="base">
            <typename name="kinematic_link"/>
         </explicit>
      </entity>
      <entity name="product_definition_kinematics" supertypes="property_definition">
         <explicit name="definition">
            <typename name="product_definition_shape"/>
            <redeclaration entity-ref="property_definition"/>
         </explicit>
         <inverse attribute="definition" entity="kinematic_property_definition_representation" name="representation_relation"/>
         <unique label="UR1">
            <unique.attribute attribute="definition"/>
         </unique>
      </entity>
   </schema>
</express>
