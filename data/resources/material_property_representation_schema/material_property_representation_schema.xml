<?xml version="1.0"?>
<!-- $Id: material_property_representation_schema.xml,v 1.3 2002/07/14 16:43:49 robbod Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  reference="ISO 10303-45"
  rcs.date="$Date: 2002/07/14 16:43:49 $"
  rcs.revision="$Revision: 1.3 $"
  description.file="descriptions.xml">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.17"
    source="material_property_representation_schema schema_instance"/>

  <schema name="material_property_representation_schema">
    <interface
      kind="reference"
      schema="product_property_representation_schema">
      <interfaced.item
        name="property_definition_representation"/>
    </interface>

    <interface
      kind="reference"
      schema="support_resource_schema">
      <interfaced.item
        name="label"/>
      <interfaced.item
        name="text"/>
    </interface>

    <entity
      name="material_property_representation"
      supertypes="property_definition_representation">
      <explicit
        name="dependent_environment">
        <typename
          name="data_environment"/>
      </explicit>
    </entity>

    <entity
      name="data_environment">
      <explicit
        name="name">
        <typename
          name="label"/>
      </explicit>
      <explicit
        name="description">
        <typename
          name="text"/>
      </explicit>
      <explicit
        name="elements">
        <aggregate
          type="SET"
          lower="1"
          upper="?"/>
        <typename
          name="property_definition_representation"/>
      </explicit>
    </entity>

    <entity
      name="data_environment_relationship">
      <explicit
        name="name">
        <typename
          name="label"/>
      </explicit>
      <explicit
        name="description">
        <typename
          name="text"/>
      </explicit>
      <explicit
        name="relating_data_environment">
        <typename
          name="data_environment"/>
      </explicit>
      <explicit
        name="related_data_environment">
        <typename
          name="data_environment"/>
      </explicit>
    </entity>

  </schema>

</express>
