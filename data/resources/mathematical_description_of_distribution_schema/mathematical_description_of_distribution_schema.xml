<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO 10303-51">
   <application name="JSDAI" owner="LKSoft" source="mathematical_description_of_distribution_schema schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="mathematical_description_of_distribution_schema">
      <interface kind="reference" schema="mathematical_context_schema">
         <interfaced.item name="maths_space_context"/>
      </interface>
      <interface kind="reference" schema="mathematical_functions_schema">
         <interfaced.item name="maths_function"/>
      </interface>
      <interface kind="reference" schema="process_property_schema">
         <interfaced.item name="action_property"/>
         <interfaced.item name="resource_property"/>
      </interface>
      <interface kind="reference" schema="product_property_definition_schema">
         <interfaced.item name="property_definition"/>
         <interfaced.item name="general_property_relationship"/>
      </interface>
      <interface kind="reference" schema="support_resource_schema">
         <interfaced.item name="identifier"/>
         <interfaced.item name="label"/>
         <interfaced.item name="text"/>
      </interface>
      <type name="property_distribution_select">
         <select extensible="YES" selectitems="action_property general_property_relationship property_definition resource_property"/>
      </type>
      <entity name="maths_space_context_relationship">
         <explicit name="id">
            <typename name="identifier"/>
         </explicit>
         <explicit name="name">
            <typename name="label"/>
         </explicit>
         <explicit name="description" optional="YES">
            <typename name="text"/>
         </explicit>
         <explicit name="relating_context">
            <typename name="maths_space_context"/>
         </explicit>
         <explicit name="related_context">
            <typename name="maths_space_context"/>
         </explicit>
      </entity>
      <entity name="maths_space_context_relationship_description">
         <explicit name="id">
            <typename name="identifier"/>
         </explicit>
         <explicit name="name">
            <typename name="label"/>
         </explicit>
         <explicit name="description" optional="YES">
            <typename name="text"/>
         </explicit>
         <explicit name="described_relationship">
            <typename name="maths_space_context_relationship"/>
         </explicit>
         <explicit name="describing_relationship">
            <typename name="maths_function"/>
         </explicit>
      </entity>
      <entity name="normalized_property_distribution_description">
         <explicit name="id">
            <typename name="identifier"/>
         </explicit>
         <explicit name="name">
            <typename name="label"/>
         </explicit>
         <explicit name="description" optional="YES">
            <typename name="text"/>
         </explicit>
         <explicit name="abstract_function">
            <typename name="maths_function"/>
         </explicit>
         <explicit name="domain_context">
            <typename name="maths_space_context"/>
         </explicit>
         <explicit name="normalization_basis">
            <typename name="property_distribution_select"/>
         </explicit>
         <explicit name="physical_function">
            <typename name="property_distribution_select"/>
         </explicit>
      </entity>
      <entity name="property_distribution_description">
         <explicit name="id">
            <typename name="identifier"/>
         </explicit>
         <explicit name="name">
            <typename name="label"/>
         </explicit>
         <explicit name="description" optional="YES">
            <typename name="text"/>
         </explicit>
         <explicit name="abstract_function">
            <typename name="maths_function"/>
         </explicit>
         <explicit name="domain_context">
            <typename name="maths_space_context"/>
         </explicit>
         <explicit name="physical_function">
            <typename name="property_distribution_select"/>
         </explicit>
         <explicit name="range_context">
            <typename name="maths_space_context"/>
         </explicit>
      </entity>
   </schema>
</express>
