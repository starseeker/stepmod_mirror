   <express_ref linkend="mechanical_design_schema::mechanical_design_schema.annotation_placeholder_occurrence.item"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<ext_description linkend="mechanical_design_schema.annotation_placeholder_occurrence.item">
