<?xml version='1.0' encoding='UTF-8'?>
<!-- $Id: express2xml.js,v 1.38 2004/08/24 16:58:55 thendrix Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  reference="10303-52"
  rcs.date="$Date: $"
  rcs.revision="$Revision: $">

  <application
    name="express2xml2.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.38"
    source="../data/resources/mesh_connectivity_schema/mesh_connectivity_schema.exp"/>

  <schema name="mesh_connectivity_schema">

    <interface
      kind="reference"
      schema="mesh_topology_schema">
      <interfaced.item
        name="mesh"/>
      <interfaced.item
        name="unstructured_mesh"/>
      <interfaced.item
        name="structured_mesh"/>
      <interfaced.item
        name="mesh_location"/>
      <interfaced.item
        name="indices_group"/>
      <interfaced.item
        name="indices_range"/>
    </interface>
    <interface
      kind="reference"
      schema="mathematical_description_of_distribution_schema">
      <interfaced.item
        name="property_distribution_description"/>
    </interface>
    <interface
      kind="reference"
      schema="mathematical_functions_schema">
      <interfaced.item
        name="listed_real_data"/>
    </interface>
    <interface
      kind="reference"
      schema="support_resource_schema">
      <interfaced.item
        name="identifier"/>
      <interfaced.item
        name="label"/>
      <interfaced.item
        name="text"/>
    </interface>
    <type name="mismatched_region_type">
      <enumeration
        extensible="YES"
        items="abutting overset">
      </enumeration>
    </type>

    <entity
      name="matched_mesh_connection"
      supertypes="mesh_connectivity">
      <explicit
        name="current">
        <typename
          name="structured_mesh"/>
        <redeclaration
          entity-ref="mesh_connectivity"/>
        </explicit>
        <explicit
          name="range">
          <typename
            name="indices_range"/>
        </explicit>
        <explicit
          name="donor">
          <typename
            name="structured_mesh"/>
        </explicit>
        <explicit
          name="donor_range">
          <typename
            name="indices_range"/>
        </explicit>
        <explicit
          name="transform">
          <aggregate
            type="ARRAY"
            lower="1"
            upper="index_count"/>
          <builtintype
            type="INTEGER"/>
        </explicit>
        <where
          label="wr1"
          expression="current :&lt;&gt;: donor">
        </where>
        <where
          label="wr2"
          expression="donor.index_count = index_count">
        </where>
        <where
          label="wr3"
          expression="range.nindices = index_count">
        </where>
        <where
          label="wr4"
          expression="donor_range.nindices = index_count">
        </where>
      </entity>

      <entity
        name="mesh_connectivity">
        <explicit
          name="name">
          <typename
            name="label"/>
        </explicit>
        <explicit
          name="description">
          <typename
            name="text"/>
        </explicit>
        <explicit
          name="id">
          <typename
            name="identifier"/>
        </explicit>
        <explicit
          name="current">
          <typename
            name="mesh"/>
        </explicit>
        <derived
          name="index_count"
          expression="current.index_count">
          <builtintype
            type="INTEGER"/>
        </derived>
      </entity>

      <subtype.constraint name="sc1_mesh_connectivity"
        entity="mesh_connectivity"
        abstract.supertype="YES"
        super.expression="ONEOF(matched_mesh_connection, mismatched_mesh_connection)">
      </subtype.constraint>
      <entity
        name="mesh_overset_hole"
        supertypes="mismatched_mesh_connection">
      </entity>

      <entity
        name="mismatched_donor_mesh">
        <inverse
          name="connect"
          entity="mismatched_mesh_region"
          attribute="donor">
        </inverse>
      </entity>

      <subtype.constraint name="sc1_mismatched_donor_mesh"
        entity="mismatched_donor_mesh"
        abstract.supertype="YES"
        super.expression="ONEOF(structured_donor_mesh, unstructured_donor_mesh)">
      </subtype.constraint>
      <entity
        name="mismatched_mesh_connection"
        supertypes="mesh_connectivity">
        <explicit
          name="points">
          <typename
            name="indices_group"/>
        </explicit>
        <explicit
          name="gridloc">
          <typename
            name="mesh_location"/>
        </explicit>
      </entity>

      <subtype.constraint name="sc1_mismatched_mesh_connection"
        entity="mismatched_mesh_connection"
        abstract.supertype="YES"
        super.expression="ONEOF(mismatched_mesh_region, mesh_overset_hole)">
      </subtype.constraint>
      <entity
        name="mismatched_mesh_region"
        supertypes="mismatched_mesh_connection">
        <explicit
          name="donor">
          <typename
            name="mismatched_donor_mesh"/>
        </explicit>
        <explicit
          name="kind">
          <typename
            name="mismatched_region_type"/>
        </explicit>
        <where
          label="wr1"
          expression="donor :&lt;&gt;: SELF\mesh_connectivity.current">
        </where>
      </entity>

      <entity
        name="multiple_mesh_block">
        <explicit
          name="name">
          <typename
            name="label"/>
        </explicit>
        <explicit
          name="description">
          <typename
            name="text"/>
        </explicit>
        <explicit
          name="id">
          <typename
            name="identifier"/>
        </explicit>
        <explicit
          name="connectivities">
          <aggregate
            type="LIST"/>
          <typename
            name="mesh_connectivity"/>
        </explicit>
      </entity>

      <entity
        name="structured_donor_mesh"
        supertypes="mismatched_donor_mesh">
        <explicit
          name="donor">
          <typename
            name="structured_mesh"/>
        </explicit>
        <explicit
          name="points">
          <typename
            name="listed_real_data"/>
        </explicit>
        <explicit
          name="vsize">
          <builtintype
            type="INTEGER"/>
        </explicit>
        <derived
          name="index_count"
          expression="donor.index_count">
          <builtintype
            type="INTEGER"/>
        </derived>
      </entity>

      <entity
        name="unstructured_donor_mesh"
        supertypes="mismatched_donor_mesh">
        <explicit
          name="donor">
          <typename
            name="unstructured_mesh"/>
        </explicit>
        <explicit
          name="cells">
          <typename
            name="indices_group"/>
        </explicit>
        <explicit
          name="interpolant">
          <typename
            name="property_distribution_description"/>
        </explicit>
        <explicit
          name="vsize">
          <builtintype
            type="INTEGER"/>
        </explicit>
        <derived
          name="index_count"
          expression="donor.index_count">
          <builtintype
            type="INTEGER"/>
        </derived>
        <derived
          name="cell_dim"
          expression="donor.cell_dim">
          <builtintype
            type="INTEGER"/>
        </derived>
      </entity>

    </schema>
  </express>
