<!DOCTYPE ext_descriptions SYSTEM "../../../dtd/description.dtd">
<ext_descriptions module_directory="organization_type_schema" schema_file="organization_type_schema.xml" describe.selects="YES" rcs.date="$Date: 2003/01/09 12:10:39 $" rcs.revision="$Revision: 1.10 $">
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: organization_type_schema
   <express_ref linkend="organization_type_schema::organization_type_schema"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<ext_description linkend="organization_type_schema"/>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: organization_type_schema  Entity: organization_type  
   <express_ref linkend="organization_type_schema::organization_type_schema.organization_type"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="organization_type_schema.organization_type">
		An <b>organization_type</b> represents a kind of organization. 
		<example>
			<i>Standards body</i> and <i>private company</i> are examples of types of organization.	
		</example>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: organization_type_schema  Entity: organization_type  Attribute: id 
   <express_ref linkend="organization_type_schema::organization_type_schema.organization_type.id"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="organization_type_schema.organization_type.id">
		the identifying string that distinguishes the <b>organization_type</b>.
		<note>
			The identification will always be relative to a particular context.
		</note>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: organization_type_schema  Entity: organization_type  Attribute: name 
   <express_ref linkend="organization_type_schema::organization_type_schema.organization_type.name"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="organization_type_schema.organization_type.name">
		a word or group of words by which the <b>organization_type</b> is known. The value of this attribute need not be specified.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: organization_type_schema  Entity: organization_type  Attribute: description 
   <express_ref linkend="organization_type_schema::organization_type_schema.organization_type.description"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="organization_type_schema.organization_type.description">
		a word or group of words that characterizes the <b>organization_type</b>. The value of this attribute need not be specified.
	</ext_description>
</ext_descriptions>
