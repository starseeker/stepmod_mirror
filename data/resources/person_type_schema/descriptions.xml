	<ext_description linkend="person_type_schema.person_type_definition_relationship.name">
		a word or group of words by which the <b>person_type_definition_relationship</b> is known. The value of this attribute need not be specified.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: person_type_schema  Entity: person_type_definition_relationship  Attribute: related 
   <express_ref linkend="person_type_schema::person_type_schema.person_type_definition_relationship.related"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="person_type_schema.person_type_definition_relationship.related">
		the other instance of <b>person_type_definition</b> that is a part of the relationship.  If one element of the relationship is dependent upon the other, this attribute shall be the dependent one.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: person_type_schema  Entity: person_type_definition_relationship  Attribute: relating 
   <express_ref linkend="person_type_schema::person_type_schema.person_type_definition_relationship.relating"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="person_type_schema.person_type_definition_relationship.relating">
		one of the instances of <b>person_type_definition</b> that is a part of the relationship.
	</ext_description>
</ext_descriptions>
