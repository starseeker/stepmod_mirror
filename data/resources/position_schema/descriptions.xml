<!DOCTYPE ext_descriptions SYSTEM "../../../dtd/description.dtd">
<ext_descriptions module_directory="position_schema" schema_file="position_schema.xml" describe.selects="YES" rcs.date="$Date: 2003/01/09 12:10:39 $" rcs.revision="$Revision: 1.10 $">
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: position_schema
   <express_ref linkend="position_schema::position_schema"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<ext_description linkend="position_schema"/>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: position_schema  Entity: position  
   <express_ref linkend="position_schema::position_schema.position"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="position_schema.position">
		A <b>position</b> represents a position of a person in an organization.
		<example>
			The <i>chairperson of ISO 10303 SC4</i> is a position.
		</example>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: position_schema  Entity: position  Attribute: description 
   <express_ref linkend="position_schema::position_schema.position.description"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="position_schema.position.description">
		a word or group of words that characterizes the <b>position</b>. The value of this attribute need not be specified.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: position_schema  Entity: position  Attribute: id 
   <express_ref linkend="position_schema::position_schema.position.id"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="position_schema.position.id">
		the identifying string that distinguishes the <b>position</b>.
		<note>
			The identification will always be relative to a particular context.
		</note>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: position_schema  Entity: position  Attribute: name 
   <express_ref linkend="position_schema::position_schema.position.name"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="position_schema.position.name">
		a word or group of words by which the <b>position</b> is known. The value of this attribute need not be specified.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: position_schema  Entity: position_assignment_relationship  
   <express_ref linkend="position_schema::position_schema.position_assignment_relationship"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="position_schema.position_assignment_relationship">
		A <b>position_assignment_relationship</b> relates two instances of the entity data type <b>position_assignment</b> and provides an identification, name, and description of this relationship.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: position_schema  Entity: position_assignment_relationship  Attribute: description 
   <express_ref linkend="position_schema::position_schema.position_assignment_relationship.description"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="position_schema.position_assignment_relationship.description">
		a word or group of words that characterizes a <b>position_assignment_relationship</b>. The value of this attribute need not be specified.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: position_schema  Entity: position_assignment_relationship  Attribute: id 
   <express_ref linkend="position_schema::position_schema.position_assignment_relationship.id"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
