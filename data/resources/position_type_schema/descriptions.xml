<!DOCTYPE ext_descriptions SYSTEM "../../../dtd/description.dtd">
<ext_descriptions module_directory="position_type_schema" schema_file="position_type_schema.xml" describe.selects="YES" rcs.date="$Date: 2003/01/09 12:10:39 $" rcs.revision="$Revision: 1.10 $">
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: position_type_schema
   <express_ref linkend="position_type_schema::position_type_schema"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<ext_description linkend="position_type_schema"/>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: position_type_schema  Entity: position_type  
   <express_ref linkend="position_type_schema::position_type_schema.position_type"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="position_type_schema.position_type">
		A <b>position_type</b> represents a kind of position of a person in an organization.
		<example>
			A <i>managing director</i> is an example of a type of position.
		</example>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: position_type_schema  Entity: position_type  Attribute: id 
   <express_ref linkend="position_type_schema::position_type_schema.position_type.id"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="position_type_schema.position_type.id">
		the identifying string that distinguishes the <b>position_type</b>.
		<note>
			The identification will always be relative to a particular context.
		</note>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: position_type_schema  Entity: position_type  Attribute: name 
   <express_ref linkend="position_type_schema::position_type_schema.position_type.name"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="position_type_schema.position_type.name">
		a word or group of words by which the <b>position_type</b> is known. The value of this attribute need not be specified.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: position_type_schema  Entity: position_type  Attribute: description 
   <express_ref linkend="position_type_schema::position_type_schema.position_type.description"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="position_type_schema.position_type.description">
		a word or group of words that characterizes the <b>position_type</b>. The value of this attribute need not be specified.
	</ext_description>
</ext_descriptions>
