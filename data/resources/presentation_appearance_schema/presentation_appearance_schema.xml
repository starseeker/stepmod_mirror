            <typename name="label"/>
         </explicit>
         <explicit name="fill_styles">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="fill_style_select"/>
         </explicit>
         <where expression="SIZEOF(QUERY(fill_style &lt;* SELF.fill_styles |&#10;         'PRESENTATION_APPEARANCE_SCHEMA.'+&#10;         'FILL_AREA_STYLE_COLOUR' IN&#10;         TYPEOF(fill_style)&#10;         )) &lt;= 1" label="WR1"/>
