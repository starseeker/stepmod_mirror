         <explicit name="pattern_list" optional="NO">
            
            <aggregate lower="1" optional="NO" type="LIST" unique="NO" upper="?"/>
            
            <typename name="curve_style_font_pattern"/>
         
         </explicit>
      
         <where label="IP1"/>
         <where label="IP2"/>
