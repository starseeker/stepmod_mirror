      <entity name="approximation_tolerance" supertypes="founded_item">
         <explicit name="tolerance">
            <typename name="tolerance_select"/>
         </explicit>
      </entity>
      <entity name="approximation_tolerance_deviation" supertypes="founded_item">
         <explicit name="tessellation_type">
            <typename name="approximation_method"/>
         </explicit>
         <explicit name="tolerances">
            <aggregate lower="1" type="SET" upper="2"/>
            <typename name="tolerance_deviation_select"/>
         </explicit>
         <explicit name="definition_space">
            <typename name="product_or_presentation_space"/>
         </explicit>
         <where expression="(HIINDEX(SELF.tolerances) = 1)&#10;            XOR&#10;        (TYPEOF(SELF.tolerances[1]) &lt;&gt; TYPEOF(SELF.tolerances[2]))" label="WR1"/>
      </entity>
      <entity name="approximation_tolerance_parameter" supertypes="founded_item">
         <explicit name="tolerances">
            <aggregate lower="1" type="SET" upper="2"/>
            <typename name="tolerance_parameter_select"/>
         </explicit>
         <where expression="(HIINDEX (SELF.tolerances) = 1 )&#10;             XOR&#10;        (TYPEOF (SELF.tolerances[1]) &lt;&gt; TYPEOF (SELF.tolerances[2]))" label="WR1"/>
      </entity>
      <entity name="character_glyph_style_outline" supertypes="founded_item">
