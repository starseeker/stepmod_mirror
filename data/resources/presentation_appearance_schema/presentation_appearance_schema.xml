      <entity name="context_dependent_over_riding_styled_item" supertypes="over_riding_styled_item">
         <explicit name="style_context">
            <aggregate lower="1" type="LIST" upper="?"/>
            <typename name="style_context_select"/>
         </explicit>
         <where expression="(SIZEOF(QUERY( sc &lt;* SELF.style_context |&#10;       'REPRESENTATION_SCHEMA.REPRESENTATION_ITEM' IN&#10;         TYPEOF(sc))) = 1) OR&#10;        (SIZEOF(QUERY( sc &lt;* SELF.style_context |&#10;       'REPRESENTATION_SCHEMA.MAPPED_ITEM' IN&#10;         TYPEOF(sc))) = SIZEOF(style_context)) OR&#10;         (SIZEOF(QUERY( sc &lt;* SELF.style_context |&#10;       'PRODUCT_PROPERTY_REPRESENTATION_SCHEMA.SHAPE_REPRESENTATION_RELATIONSHIP' IN&#10;         TYPEOF(sc))) = SIZEOF(style_context))" label="WR1"/>
      </entity>
