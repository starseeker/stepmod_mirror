      </type>
      <type name="u_direction_count">
         <builtintype type="INTEGER"/>
         <where expression="SELF &gt; 1" label="WR1"/>
      </type>
      <type name="v_direction_count">
         <builtintype type="INTEGER"/>
         <where expression="SELF &gt; 1" label="WR1"/>
