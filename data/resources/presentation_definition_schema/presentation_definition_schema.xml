      </where>
      <where
        label="WR2"
        expression="'PRESENTATION_DEFINITION_SCHEMA.SYMBOL_TARGET' IN         TYPEOF (SELF\mapped_item.mapping_target)">
      </where>
      <where
        label="WR3"
        expression="'GEOMETRY_SCHEMA.GEOMETRIC_REPRESENTATION_ITEM' IN         TYPEOF (SELF)">
