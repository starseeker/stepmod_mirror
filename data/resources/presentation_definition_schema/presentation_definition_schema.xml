      <interfaced.item
        name="label"/>
      <interfaced.item
        name="text"/>
      <interfaced.item
        name="bag_to_set"/>
    </interface>

    <type name="text_delineation">
      <typename
        name="label"/>

    </type>
    <type name="defined_symbol_select">
      <select
