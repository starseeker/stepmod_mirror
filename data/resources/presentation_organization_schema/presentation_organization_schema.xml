         </explicit>
         <explicit name="concentration_exponent" optional="NO">
            <builtintype fixed="NO" type="REAL"/>
         </explicit>
         <explicit name="constant_attenuation" optional="NO">
            <builtintype fixed="NO" type="REAL"/>
         </explicit>
         <explicit name="distance_attenuation" optional="NO">
            <builtintype fixed="NO" type="REAL"/>
         </explicit>
         <explicit name="spread_angle" optional="NO">
