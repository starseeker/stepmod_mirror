      <entity abstract.entity="NO" abstract.supertype="NO" name="camera_usage" supertypes="representation_map">
         <explicit name="mapping_origin" optional="NO">
            <typename name="camera_model"/>
            <redeclaration entity-ref="representation_map"/>
         </explicit>
         <where expression="NOT ('PRESENTATION_ORGANIZATION_SCHEMA.PRESENTATION_REPRESENTATION'&#10;          IN TYPEOF(SELF\representation_map.mapped_representation))" label="WR1"/>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="graphical_transformation" supertypes="item_defined_transformation">
