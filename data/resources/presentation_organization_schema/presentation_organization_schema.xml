      <entity abstract.entity="NO" abstract.supertype="NO" name="camera_model_d3" supertypes="camera_model">
         <explicit name="view_reference_system" optional="NO">
            <typename name="axis2_placement_3d"/>
         </explicit>
         <explicit name="perspective_of_volume" optional="NO">
            <typename name="view_volume"/>
         </explicit>
         <where expression="SELF\geometric_representation_item.dim = 3" label="WR1"/>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="camera_model_d3_multi_clipping" supertypes="camera_model_d3">
         <explicit name="shape_clipping" optional="NO">
            <aggregate lower="1" optional="NO" type="SET" unique="NO" upper="?"/>
            <typename name="camera_model_d3_multi_clipping_interection_select"/>
         </explicit>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="camera_model_d3_multi_clipping_intersection" supertypes="geometric_representation_item">
         <explicit name="shape_clipping" optional="NO">
            <aggregate lower="2" optional="NO" type="SET" unique="NO" upper="?"/>
            <typename name="camera_model_d3_multi_clipping_interection_select"/>
         </explicit>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="camera_model_d3_multi_clipping_union" supertypes="geometric_representation_item">
         <explicit name="shape_clipping" optional="NO">
            <aggregate lower="2" optional="NO" type="SET" unique="NO" upper="?"/>
            <typename name="camera_model_d3_multi_clipping_union_select"/>
         </explicit>
      </entity>
      <entity abstract.entity="NO" abstract.supertype="NO" name="camera_model_d3_with_hlhsr" supertypes="camera_model_d3">
