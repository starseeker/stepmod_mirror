      <entity abstract.entity="NO" abstract.supertype="NO" name="presentation_layer_usage">
         <explicit name="assignment" optional="NO">
            <typename name="presentation_layer_assignment"/>
         </explicit>
         <explicit name="presentation" optional="NO">
            <typename name="presentation_representation"/>
         </explicit>
         <unique label="UR1">
            <unique.attribute attribute="assignment"/>
            <unique.attribute attribute="presentation"/>
