<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: product_analysis_schema  Entity: temporal_spatial_domain.name  
   <express_ref linkend="product_analysis_schema:ir_express:product_analysis_schema.temporal_spatial_domain.name"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="product_analysis_schema.temporal_spatial_domain.name">
user-specified instance identifier.</ext_description>

<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: product_analysis_schema  Entity: temporal_spatial_domain.wr 
   <express_ref linkend="product_analysis_schema:ir_express:product_analysis_schema.temporal_spatial_domain.wr"/>
