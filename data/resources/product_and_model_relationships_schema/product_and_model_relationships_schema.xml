<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO 10303-107">
   <application name="JSDAI" owner="LKSoft" source="product_and_model_relationships_schema schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="product_and_model_relationships_schema">
      <interface kind="reference" schema="analysis_schema">
         <interfaced.item name="model_product_domain"/>
      </interface>
      <interface kind="reference" schema="product_analysis_schema">
         <interfaced.item name="physical_product_domain"/>
      </interface>
      <interface kind="reference" schema="product_definition_schema">
         <interfaced.item name="product_definition"/>
      </interface>
      <interface kind="reference" schema="structural_response_representation_schema">
         <interfaced.item name="fea_model"/>
      </interface>
      <interface kind="reference" schema="support_resource_schema">
         <interfaced.item name="identifier"/>
         <interfaced.item name="label"/>
         <interfaced.item name="text"/>
      </interface>
      <type name="discretised_product_model_select">
         <select extensible="YES" selectitems="fea_model model_product_domain"/>
      </type>
      <type name="idealised_product_select">
         <select extensible="YES" selectitems="physical_product_domain product_definition"/>
      </type>
      <entity name="product_view_relationship">
         <explicit name="id">
            <typename name="identifier"/>
         </explicit>
         <explicit name="name">
            <typename name="label"/>
         </explicit>
         <explicit name="description" optional="YES">
            <typename name="text"/>
         </explicit>
         <explicit name="discretised_model">
            <typename name="discretised_product_model_select"/>
         </explicit>
         <explicit name="idealised_product">
            <typename name="idealised_product_select"/>
         </explicit>
      </entity>
   </schema>
</express>
