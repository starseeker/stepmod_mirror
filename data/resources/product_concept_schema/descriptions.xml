  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: product_concept_schema  Entity: product_concept  Attribute: id 
   <express_ref linkend="product_concept_schema:ir_express:product_concept_schema.product_concept.id"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
<ext_description linkend="product_concept_schema.product_concept.id">
	<p>
		the 
		<express_ref linkend="support_resource_schema:ir_express:support_resource_schema.identifier"/> 
		that distinguishes the <b>product_concept</b>.
	</p> 
	<example number="2">
		The <b>id</b> could be a sales model number.
	</example>       
</ext_description>
