<!DOCTYPE ext_descriptions SYSTEM "../../../dtd/description.dtd">
<ext_descriptions module_directory="qualification_type_schema" schema_file="qualification_type_schema.xml" describe.selects="YES" rcs.date="$Date: 2003/03/07 14:02:34 $" rcs.revision="$Revision: 1.2 $">
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: qualification_type_schema
   <express_ref linkend="qualification_type_schema::qualification_type_schema"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<ext_description linkend="qualification_type_schema"/>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: qualification_type_schema  Entity: qualification_type  
   <express_ref linkend="qualification_type_schema::qualification_type_schema.qualification_type"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualification_type_schema.qualification_type">
		A <b>qualification_type</b> represents a kind of qualification.
		<example>
			A <i>university degree</i> is a type of qualification.
		</example>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: qualification_type_schema  Entity: qualification_type  Attribute: id 
   <express_ref linkend="qualification_type_schema::qualification_type_schema.qualification_type.id"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualification_type_schema.qualification_type.id">
		the identifying string that distinguishes the <b>qualification_type</b>.
		<note>
			The identification will always be relative to a particular context.
		</note>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: qualification_type_schema  Entity: qualification_type  Attribute: name 
   <express_ref linkend="qualification_type_schema::qualification_type_schema.qualification_type.name"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualification_type_schema.qualification_type.name">
		 a word or group of words by which the <b>qualification_type</b> is known. The value of this attribute need not be specified.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: qualification_type_schema  Entity: qualification_type  Attribute: description 
   <express_ref linkend="qualification_type_schema::qualification_type_schema.qualification_type.description"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualification_type_schema.qualification_type.description">
		a word or group of words that characterizes the <b>qualification_type</b>. The value of this attribute need not be specified.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: qualification_type_schema  Entity: qualification_type_relationship  
   <express_ref linkend="qualification_type_schema::qualification_type_schema.qualification_type_relationship"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualification_type_schema.qualification_type_relationship">
		A <b>qualification_type_relationship</b> relates two instances of the entity data type  <b>qualification_type</b> and provides an identification, name, and description of this relationship. 
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: qualification_type_schema  Entity: qualification_type_relationship  Attribute: id 
   <express_ref linkend="qualification_type_schema::qualification_type_schema.qualification_type_relationship.id"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualification_type_schema.qualification_type_relationship.id">
		the identifying string that distinguishes the <b>qualification_type_relationship</b>.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: qualification_type_schema  Entity: qualification_type_relationship  Attribute: name 
   <express_ref linkend="qualification_type_schema::qualification_type_schema.qualification_type_relationship.name"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualification_type_schema.qualification_type_relationship.name">
		 a word or group of words by which the  <b>qualification_type_relationship</b> is known. The value of this attribute need not be specified.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: qualification_type_schema  Entity: qualification_type_relationship  Attribute: description 
   <express_ref linkend="qualification_type_schema::qualification_type_schema.qualification_type_relationship.description"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualification_type_schema.qualification_type_relationship.description">
		a word or group of words that characterizes the <b>qualification_type_relationship</b>. The value of this attribute need not be specified.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: qualification_type_schema  Entity: qualification_type_relationship  Attribute: related 
   <express_ref linkend="qualification_type_schema::qualification_type_schema.qualification_type_relationship.related"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualification_type_schema.qualification_type_relationship.related">
		the other instance of <b>qualification_type</b> that is a part of the relationship. If one element of the relationship is dependent upon the other, this attribute shall be the dependent one.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: qualification_type_schema  Entity: qualification_type_relationship  Attribute: relating 
   <express_ref linkend="qualification_type_schema::qualification_type_schema.qualification_type_relationship.relating"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualification_type_schema.qualification_type_relationship.relating">
		one of the instances of <b>qualification_type</b> that is a part of the relationship.
	</ext_description>
</ext_descriptions>
