<ext_descriptions module_directory="qualifications_schema" schema_file="qualifications_schema.xml" describe.selects="YES" rcs.date="$Date: 2003/04/11 19:30:48 $" rcs.revision="$Revision: 1.1 $">
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  Schema: qualifications_schema
   <express_ref linkend="qualifications_schema::qualifications_schema"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<ext_description linkend="qualifications_schema"/>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: qualifications_schema  Entity: qualification_type  
   <express_ref linkend="qualifications_schema::qualifications_schema.qualification_type"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualifications_schema.qualification_type">
		An <b>qualification_type</b> is the formal identification of a capability or aptitude.
		<example number="1">A driving licence.</example>
		<example number="2">A university degree.</example>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: qualifications_schema  Entity: qualification_type  Attribute: id 
   <express_ref linkend="qualifications_schema::qualifications_schema.qualification_type.id"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualifications_schema.qualification_type.id">
		the string of characters that identify the <b>qualification_type</b>.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: qualifications_schema  Entity: qualification_type  Attribute: name 
   <express_ref linkend="qualifications_schema::qualifications_schema.qualification_type.name"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualifications_schema.qualification_type.name">
		the word or words by which the <b>qualification_type</b> is known.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: qualifications_schema  Entity: qualification_type  Attribute: description 
   <express_ref linkend="qualifications_schema::qualifications_schema.qualification_type.description"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualifications_schema.qualification_type.description">
		he word or words that characterize the <b>qualification_type</b>.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: qualifications_schema  Entity: qualification_type_relationship  
   <express_ref linkend="qualifications_schema::qualifications_schema.qualification_type_relationship"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualifications_schema.qualification_type_relationship">
		A <b>qualification_type_relationship</b> relates two instances of the <b>qualification_type</b> entity data type and provides for the identification, naming, and description of that relationship.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: qualifications_schema  Entity: qualification_type_relationship  Attribute: id 
   <express_ref linkend="qualifications_schema::qualifications_schema.qualification_type_relationship.id"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualifications_schema.qualification_type_relationship.id">
		the string of characters that identify the <b>qualification_type_relationship</b>.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: qualifications_schema  Entity: qualification_type_relationship  Attribute: name 
   <express_ref linkend="qualifications_schema::qualifications_schema.qualification_type_relationship.name"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualifications_schema.qualification_type_relationship.name">
		the word or words by which the <b>qualification_type_relationship</b> is known.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: qualifications_schema  Entity: qualification_type_relationship  Attribute: description 
   <express_ref linkend="qualifications_schema::qualifications_schema.qualification_type_relationship.description"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualifications_schema.qualification_type_relationship.description">
		the word or words that characterize the <b>qualification_type_relationship</b>.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: qualifications_schema  Entity: qualification_type_relationship  Attribute: related 
   <express_ref linkend="qualifications_schema::qualifications_schema.qualification_type_relationship.related"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualifications_schema.qualification_type_relationship.related_qualification_type">
		the other instance of <b>qualification_type</b> that is a part of the relationship.  If one element of the relationship is dependent upon the other, attribute shall be the dependent one.
		<note number="2">
			The meaning of this attribute can be defined in the annotated EXPRESS schemas that use or specialize this entity, or in an agreement of common understanding between the partners sharing this information.
		</note>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: qualifications_schema  Entity: qualification_type_relationship  Attribute: relating 
   <express_ref linkend="qualifications_schema::qualifications_schema.qualification_type_relationship.relating"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualifications_schema.qualification_type_relationship.relating_qualification_type">
		one of the instances of <b>qualification_type</b> that is a part of the relationship.
		<note number="1">
			The meaning of this attribute can be defined in the annotated EXPRESS schemas that use or specialize this entity, or in an agreement of common 	understanding between the partners sharing this information.
		</note>
	</ext_description>
