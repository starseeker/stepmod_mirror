	<ext_description linkend="qualifications_schema"/>
	<!--
	 ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: qualifications_schema  Entity: qualification  
   <express_ref linkend="qualifications_schema::qualifications_schema.qualification"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualifications_schema.qualification">
		A qualification is a particular instance of the formal identification of a capability or aptitude in a specific person or organization.
		<example number="1">John Smith's driving licence.</example>
		<example number="2">Mary Brown's doctorate entitled "Datamodelling Using EXPRESS2".</example>
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: qualifications_schema  Entity: qualification  Attribute: id 
   <express_ref linkend="qualifications_schema::qualifications_schema.qualification.id"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualifications_schema.qualification.id">
		the <b>identifier</b> that distinguishes the <b>qualification</b>.
	</ext_description>
	<!-- 
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
   Schema: qualifications_schema  Entity: qualification  Attribute: name 
   <express_ref linkend="qualifications_schema::qualifications_schema.qualification.name"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualifications_schema.qualification.name">
		the <b>label</b> by which the <b>qualification</b> is known.
	</ext_description>
