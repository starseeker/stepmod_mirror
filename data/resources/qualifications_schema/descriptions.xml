   <express_ref linkend="qualifications_schema::qualifications_schema.qualification.id"/>
  ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ 
  -->
	<ext_description linkend="qualifications_schema.qualification.id">
		the 
		<express_ref linkend="support_resource_schema:ir_express:support_resource_schema.identifier"/> 
		that distinguishes the <b>qualification</b>.
	</ext_description>
