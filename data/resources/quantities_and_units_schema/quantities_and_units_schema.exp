ENTITY qus -- stands for "quantity units", but need a very short name
  q : physical_quantitiy;
  us : SET[1:?} OF physical_unit;
UNIQUE
  q;  
END_ENTITY;  

ENTITY general_quantitiy_value
  ABSTRACT SUPERTYPE;
END_ENTITY;

ENTITY iso80000_quantity_value
  SUBTYPE OF (general_quantitiy_value);
  kind_component : iso80000_quantity;
  value_component : REAL;
  unit_component : iso80000_unit;
  multiplier_component : OPTIONAL unit_multiplier;  
WHERE;
  wr1: TRUE; -- nu
END_ENTITY;

END_SCHEMA; -- quantities_and_units_schema
 
 