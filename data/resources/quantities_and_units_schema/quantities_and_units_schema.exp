SCHEMA quantities_and_units_schema;

-- REFERENCE FROM measure_schema(si_prefix);

CONSTANT
valid_quantity_units : SET[0:?] OF qus := [ 
qus(mass,(kilogram,tonne))
];
END_CONSTANT;

