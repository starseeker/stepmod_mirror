<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO 10303-58">
   <application name="JSDAI" owner="LKSoft" source="risk_schema schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="risk_schema">
      <interface kind="reference" schema="action_schema">
         <interfaced.item name="action"/>
         <interfaced.item name="action_method"/>
         <interfaced.item name="action_relationship"/>
      </interface>
      <interface kind="reference" schema="application_context_schema">
         <interfaced.item name="product_definition_context"/>
      </interface>
      <interface kind="reference" schema="date_time_schema">
         <interfaced.item name="event_occurrence"/>
      </interface>
      <interface kind="reference" schema="management_resources_schema">
         <interfaced.item name="event_occurrence_assignment"/>
      </interface>
      <interface kind="reference" schema="product_definition_schema">
         <interfaced.item name="product"/>
         <interfaced.item name="product_definition"/>
         <interfaced.item name="product_definition_formation"/>
         <interfaced.item name="product_definition_relationship"/>
         <interfaced.item name="product_relationship"/>
      </interface>
      <interface kind="reference" schema="product_property_definition_schema">
         <interfaced.item name="characterized_object"/>
         <interfaced.item name="property_definition"/>
      </interface>
      <interface kind="reference" schema="product_property_representation_schema">
         <interfaced.item name="property_definition_representation"/>
      </interface>
      <interface kind="reference" schema="representation_schema">
         <interfaced.item name="representation"/>
      </interface>
      <type name="risk_action">
         <select selectitems="risk_acceptance risk_analysis risk_assessment risk_communication risk_control risk_estimation risk_evaluation risk_identification risk_treatment"/>
      </type>
      <type name="risk_impact_item">
         <select extensible="YES" genericentity="YES" selectitems="product product_definition_formation"/>
      </type>
      <type name="risk_perception_source_item">
         <select extensible="YES" genericentity="YES" selectitems="action action_method product"/>
      </type>
      <entity name="causal_consequence" supertypes="product_definition_relationship">
         <explicit name="relating_product_definition">
            <typename name="risk_consequence"/>
            <redeclaration entity-ref="product_definition_relationship"/>
         </explicit>
         <explicit name="related_product_definition">
            <typename name="risk_consequence"/>
            <redeclaration entity-ref="product_definition_relationship"/>
         </explicit>
      </entity>
      <entity name="contained_acceptance" supertypes="risk_activity_structure">
         <explicit name="parent">
            <typename name="risk_control"/>
            <redeclaration entity-ref="risk_activity_structure"/>
         </explicit>
         <explicit name="child">
            <typename name="risk_acceptance"/>
            <redeclaration entity-ref="risk_activity_structure"/>
         </explicit>
      </entity>
      <entity name="contained_analysis" supertypes="risk_activity_structure">
         <explicit name="parent">
            <typename name="risk_assessment"/>
            <redeclaration entity-ref="risk_activity_structure"/>
         </explicit>
         <explicit name="child">
            <typename name="risk_analysis"/>
            <redeclaration entity-ref="risk_activity_structure"/>
         </explicit>
      </entity>
      <entity name="contained_communication" supertypes="risk_activity_structure">
         <explicit name="parent">
            <typename name="risk_control"/>
            <redeclaration entity-ref="risk_activity_structure"/>
         </explicit>
         <explicit name="child">
            <typename name="risk_communication"/>
            <redeclaration entity-ref="risk_activity_structure"/>
         </explicit>
      </entity>
      <entity name="contained_estimation" supertypes="risk_activity_structure">
         <explicit name="parent">
            <typename name="risk_analysis"/>
            <redeclaration entity-ref="risk_activity_structure"/>
         </explicit>
         <explicit name="child">
            <typename name="risk_estimation"/>
            <redeclaration entity-ref="risk_activity_structure"/>
         </explicit>
      </entity>
      <entity name="contained_evaluation" supertypes="risk_activity_structure">
         <explicit name="parent">
            <typename name="risk_assessment"/>
            <redeclaration entity-ref="risk_activity_structure"/>
         </explicit>
         <explicit name="child">
            <typename name="risk_evaluation"/>
            <redeclaration entity-ref="risk_activity_structure"/>
         </explicit>
      </entity>
      <entity name="contained_identification" supertypes="risk_activity_structure">
         <explicit name="parent">
            <typename name="risk_analysis"/>
            <redeclaration entity-ref="risk_activity_structure"/>
         </explicit>
         <explicit name="child">
            <typename name="risk_identification"/>
            <redeclaration entity-ref="risk_activity_structure"/>
         </explicit>
      </entity>
      <entity name="contained_treatments" supertypes="risk_activity_structure">
         <explicit name="parent">
            <typename name="risk_control"/>
            <redeclaration entity-ref="risk_activity_structure"/>
         </explicit>
         <explicit name="child">
            <typename name="risk_treatment"/>
            <redeclaration entity-ref="risk_activity_structure"/>
         </explicit>
      </entity>
      <entity name="event_occurrence_characterized_object" supertypes="characterized_object event_occurrence"/>
      <entity name="event_probability" supertypes="property_definition">
         <explicit name="definition">
            <typename name="event_occurrence_characterized_object"/>
            <redeclaration entity-ref="property_definition"/>
         </explicit>
      </entity>
      <entity name="monitor" supertypes="risk_activity_structure">
         <explicit name="parent">
            <typename name="risk_control"/>
            <redeclaration entity-ref="risk_activity_structure"/>
         </explicit>
         <explicit name="child">
            <typename name="risk_assessment"/>
            <redeclaration entity-ref="risk_activity_structure"/>
         </explicit>
      </entity>
      <entity name="probability_representation" supertypes="property_definition_representation">
         <explicit name="definition">
            <typename name="event_probability"/>
            <redeclaration entity-ref="property_definition_representation"/>
         </explicit>
      </entity>
      <entity name="related_consequence" supertypes="product_definition_relationship">
         <explicit name="relating_product_definition">
            <typename name="risk_perception"/>
            <redeclaration entity-ref="product_definition_relationship"/>
         </explicit>
         <explicit name="related_product_definition">
            <typename name="risk_consequence"/>
            <redeclaration entity-ref="product_definition_relationship"/>
         </explicit>
      </entity>
      <entity name="risk" supertypes="product"/>
      <entity name="risk_acceptance" supertypes="action"/>
      <entity name="risk_activity_structure" supertypes="action_relationship">
         <explicit name="parent">
            <typename name="risk_action"/>
            <redeclaration entity-ref="action_relationship" old_name="relating_action"/>
         </explicit>
         <explicit name="child">
            <typename name="risk_action"/>
            <redeclaration entity-ref="action_relationship" old_name="related_action"/>
         </explicit>
      </entity>
      <entity name="risk_analysis" supertypes="action"/>
      <entity name="risk_assessment" supertypes="action"/>
      <entity name="risk_attitude" supertypes="property_definition_representation">
         <explicit name="definition">
            <typename name="risk_level"/>
            <redeclaration entity-ref="property_definition_representation"/>
         </explicit>
         <explicit name="criticality_factor">
            <typename name="representation"/>
            <redeclaration entity-ref="property_definition_representation" old_name="used_representation"/>
         </explicit>
      </entity>
      <entity name="risk_communication" supertypes="action"/>
      <entity name="risk_consequence" supertypes="product_definition">
         <explicit name="formation">
            <typename name="risk_perception_formation"/>
            <redeclaration entity-ref="product_definition"/>
         </explicit>
      </entity>
      <entity name="risk_control" supertypes="action"/>
      <entity name="risk_estimation" supertypes="action"/>
      <entity name="risk_evaluation" supertypes="action"/>
      <entity name="risk_identification" supertypes="action"/>
      <entity name="risk_impact_assignment">
         <explicit name="assigned_risk_consequence">
            <typename name="risk_consequence"/>
         </explicit>
         <explicit name="items">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="risk_impact_item"/>
         </explicit>
      </entity>
      <entity name="risk_level" supertypes="property_definition">
         <explicit name="definition">
            <typename name="risk_perception_characterized_object"/>
            <redeclaration entity-ref="property_definition"/>
         </explicit>
      </entity>
      <entity name="risk_measure" supertypes="action_method"/>
      <entity name="risk_perception" supertypes="product_definition">
         <explicit name="formation">
            <typename name="risk_perception_formation"/>
            <redeclaration entity-ref="product_definition"/>
         </explicit>
         <explicit name="risk_perception_context">
            <typename name="risk_perception_context"/>
            <redeclaration entity-ref="product_definition" old_name="frame_of_reference"/>
         </explicit>
      </entity>
      <entity name="risk_perception_characterized_object" supertypes="characterized_object risk_perception"/>
      <entity name="risk_perception_context" supertypes="product_definition_context"/>
      <entity name="risk_perception_formation" supertypes="product_definition_formation">
         <explicit name="of_risk">
            <typename name="risk"/>
            <redeclaration entity-ref="product_definition_formation" old_name="of_product"/>
         </explicit>
      </entity>
      <entity name="risk_perception_relationship" supertypes="product_definition_relationship">
         <explicit name="relating_product_definition">
            <typename name="risk_perception"/>
            <redeclaration entity-ref="product_definition_relationship"/>
         </explicit>
         <explicit name="related_product_definition">
            <typename name="risk_perception"/>
            <redeclaration entity-ref="product_definition_relationship"/>
         </explicit>
      </entity>
      <entity name="risk_perception_source_assignment">
         <explicit name="assigned_risk">
            <typename name="risk_perception"/>
         </explicit>
         <explicit name="items">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="risk_perception_source_item"/>
         </explicit>
      </entity>
      <entity name="risk_relationship" supertypes="product_relationship">
         <explicit name="relating_risk">
            <typename name="risk"/>
            <redeclaration entity-ref="product_relationship" old_name="relating_product"/>
         </explicit>
         <explicit name="related_risk">
            <typename name="risk"/>
            <redeclaration entity-ref="product_relationship" old_name="related_product"/>
         </explicit>
      </entity>
      <entity name="risk_treatment" supertypes="action">
         <explicit name="chosen_method">
            <typename name="risk_measure"/>
            <redeclaration entity-ref="action"/>
         </explicit>
      </entity>
   </schema>
</express>
