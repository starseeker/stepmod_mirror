<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO 10303-56">
   <application name="JSDAI" owner="LKSoft" source="state_observed_schema schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="state_observed_schema">
      <interface kind="reference" schema="state_type_schema">
         <interfaced.item name="state_type"/>
      </interface>
      <interface kind="reference" schema="support_resource_schema">
         <interfaced.item name="label"/>
         <interfaced.item name="text"/>
      </interface>
      <entity name="ascribable_state">
         <explicit name="name">
            <typename name="label"/>
         </explicit>
         <explicit name="description" optional="YES">
            <typename name="text"/>
         </explicit>
         <explicit name="pertaining_state_type">
            <typename name="state_type"/>
         </explicit>
         <explicit name="ascribed_state_observed">
            <typename name="state_observed"/>
         </explicit>
      </entity>
      <entity name="ascribable_state_relationship">
         <explicit name="name">
            <typename name="label"/>
         </explicit>
         <explicit name="description" optional="YES">
            <typename name="text"/>
         </explicit>
         <explicit name="relating_ascribable_state">
            <typename name="ascribable_state"/>
         </explicit>
         <explicit name="related_ascribable_state">
            <typename name="ascribable_state"/>
         </explicit>
      </entity>
      <entity name="state_observed">
         <explicit name="name">
            <typename name="label"/>
         </explicit>
         <explicit name="description" optional="YES">
            <typename name="text"/>
         </explicit>
      </entity>
      <entity abstract.supertype="YES" name="state_observed_assignment">
         <explicit name="assigned_state_observed">
            <typename name="state_observed"/>
         </explicit>
         <explicit name="role">
            <typename name="state_observed_role"/>
         </explicit>
      </entity>
      <entity name="state_observed_relationship">
         <explicit name="name">
            <typename name="label"/>
         </explicit>
         <explicit name="description" optional="YES">
            <typename name="text"/>
         </explicit>
         <explicit name="relating_state_observed">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="state_observed"/>
         </explicit>
         <explicit name="related_state_observed">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="state_observed"/>
         </explicit>
      </entity>
      <entity name="state_observed_role">
         <explicit name="name">
            <typename name="label"/>
         </explicit>
         <explicit name="description" optional="YES">
            <typename name="text"/>
         </explicit>
      </entity>
   </schema>
</express>
