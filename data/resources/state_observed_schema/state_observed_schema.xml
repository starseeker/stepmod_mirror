<?xml version="1.0"?>
<!-- $Id: state_observed_schema.xml,v 1.10 2003/01/24 21:40:23 magnevs Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file = "descriptions.xml" 
  reference="ISO 10303-56" 
  rcs.date="$Date: 2003/01/24 21:40:23 $"
  rcs.revision="$Revision: 1.10 $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.26"
    source="../data/resources/state_observed_schema/state_observed_schema.exp"/>

  <schema name="state_observed_schema">
    <interface
      kind="use"
      schema="state_type_schema">
    </interface>

    <type name="state_of_item">
      <select
        extensible="YES"
        genericentity="YES">
      </select>
      <graphic.element page="1"/>
    </type>
    <entity
      name="applied_state_assignment">
      <explicit
        name="assigned_to">
        <typename
          name="state_of_item"/>
      </explicit>
      <explicit
        name="described_state">
        <typename
          name="state_observed"/>
      </explicit>
      <explicit
        name="role">
        <typename
          name="state_role"/>
      </explicit>
      <graphic.element page="1"/>
    </entity>

    <entity
      name="defined_state_relationship">
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="defined_state">
        <typename
          name="state_assessment"/>
      </explicit>
      <explicit
        name="definitive_state">
        <typename
          name="state_assertion"/>
      </explicit>
      <graphic.element page="1"/>
    </entity>

    <entity
      name="state_assertion">
      <explicit
        name="asserted_state">
        <typename
          name="state_observed"/>
      </explicit>
      <explicit
        name="conformance_state">
        <typename
          name="state_type"/>
      </explicit>
      <explicit
        name="assertion_result"
        optional="YES">
        <builtintype
          type="LOGICAL"/>
      </explicit>
      <graphic.element page="1"/>
    </entity>

    <entity
      name="state_assessment">
      <explicit
        name="assessed_state">
        <typename
          name="state_observed"/>
      </explicit>
      <explicit
        name="comparable_state">
        <typename
          name="state_type"/>
      </explicit>
      <explicit
        name="definitional_comparison">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="assessment_result"
        optional="YES">
        <builtintype
          type="LOGICAL"/>
      </explicit>
      <graphic.element page="1"/>    
</entity>

    <entity
      name="state_observed">
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description">
        <builtintype
          type="STRING"/>
      </explicit>
      <graphic.element page="1"/>    
</entity>

    <entity
      name="state_observed_relationship">
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="relating">
        <typename
          name="state_observed"/>
      </explicit>
      <explicit
        name="related">
        <typename
          name="state_observed"/>
      </explicit>
      <graphic.element page="1"/>
    </entity>

    <entity
      name="state_role">
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description">
        <builtintype
          type="STRING"/>
      </explicit>
      <graphic.element page="1"/>
    </entity>

  </schema>
</express>
