<?xml version="1.0"?>
<!-- $Id: state_type_schema.xml,v 1.8 2003/01/24 21:34:36 magnevs Exp $ -->
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">
<express
  language_version="2"
  description.file = "descriptions.xml" 
  reference="ISO 10303-56" 
  rcs.date="$Date: 2003/01/24 21:34:36 $"
  rcs.revision="$Revision: 1.8 $">

  <application
    name="express2xml.js"
    owner="Eurostep Limited"
    url="http://www.eurostep.com"
    version="1.26"
    source="../data/resources/state_type_schema/state_type_schema.exp"/>

  <schema name="state_type_schema">
    <type name="state_type_of_item">
      <select
        extensible="YES"
        genericentity="YES">
      </select>
      <graphic.element page="1"/>
    </type>
    <entity
      name="applied_state_type_assignment">
      <explicit
        name="assigned_to">
        <typename
          name="state_type_of_item"/>
      </explicit>
      <explicit
        name="described_state_type">
        <typename
          name="state_type"/>
      </explicit>
      <explicit
        name="role">
        <typename
          name="state_type_role"/>
      </explicit>
      <graphic.element page="1"/>
    </entity>

    <entity
      name="state_type">
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description">
        <builtintype
          type="STRING"/>
      </explicit>
      <graphic.element page="1"/>
    </entity>

    <entity
      name="state_type_relationship">
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description"
        optional="YES">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="relating">
        <typename
          name="state_type"/>
      </explicit>
      <explicit
        name="related">
        <typename
          name="state_type"/>
      </explicit>
      <graphic.element page="1"/>
    </entity>

    <entity
      name="state_type_role">
      <explicit
        name="name">
        <builtintype
          type="STRING"/>
      </explicit>
      <explicit
        name="description">
        <builtintype
          type="STRING"/>
      </explicit>
      <graphic.element page="1"/>
    </entity>

  </schema>

</express>
