<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express description.file="descriptions.xml" language_version="2" rcs.date="$Date$" rcs.revision="$Revision$" reference="ISO 10303-61">
   <application name="JSDAI" owner="LKSoft" source="systems_engineering_representation_schema schema_instance" url="www.lksoft.com" version="4.0 beta"/>
   <schema name="systems_engineering_representation_schema">
      <interface kind="reference" schema="action_schema">
         <interfaced.item name="action_method"/>
      </interface>
      <interface kind="reference" schema="iso13584_expressions_schema">
         <interfaced.item name="expression"/>
      </interface>
      <interface kind="reference" schema="management_resources_schema">
         <interfaced.item name="action_method_assignment"/>
      </interface>
      <interface kind="reference" schema="product_analysis_schema"/>
      <interface kind="reference" schema="representation_schema">
         <interfaced.item name="representation"/>
         <interfaced.item name="representation_context"/>
         <interfaced.item name="representation_item"/>
      </interface>
      <interface kind="reference" schema="state_type_schema"/>
      <interface kind="reference" schema="support_resource_schema">
         <interfaced.item name="identifier"/>
         <interfaced.item name="label"/>
         <interfaced.item name="text"/>
      </interface>
      <type name="analysis_model_idealisation_item">
         <select extensible="YES" genericentity="YES" selectitems="physical_product_domain"/>
      </type>
      <type name="expression_element">
         <select selectitems="expression"/>
      </type>
      <type name="representation_proxy_select">
         <select extensible="YES" genericentity="YES"/>
      </type>
      <type name="se_representation_proxy_select">
         <select basedon="representation_proxy_select" selectitems="action_method state_type state_type_relationship"/>
      </type>
      <type name="state_based_behaviour_element">
         <select selectitems="state_type state_type_relationship"/>
      </type>
      <entity name="action_method_assignment_relationship">
         <explicit name="relating_assignment">
            <typename name="action_method_assignment"/>
         </explicit>
         <explicit name="related_assignment">
            <typename name="action_method_assignment"/>
         </explicit>
      </entity>
      <entity name="analysis_model" supertypes="representation">
         <explicit name="context_of_items">
            <typename name="analysis_representation_context"/>
            <redeclaration entity-ref="representation"/>
         </explicit>
      </entity>
      <entity name="analysis_model_relationship">
         <explicit name="id">
            <typename name="identifier"/>
         </explicit>
         <explicit name="name">
            <typename name="label"/>
         </explicit>
         <explicit name="description">
            <typename name="text"/>
         </explicit>
         <explicit name="idealised">
            <typename name="analysis_model"/>
         </explicit>
         <explicit name="idealisation">
            <typename name="analysis_model_idealisation_item"/>
         </explicit>
      </entity>
      <entity name="analysis_representation_context" supertypes="representation_context"/>
      <entity name="description_text">
         <explicit name="description">
            <typename name="text"/>
         </explicit>
      </entity>
      <entity abstract.supertype="YES" name="description_text_assignment">
         <explicit name="description">
            <typename name="description_text"/>
         </explicit>
      </entity>
      <entity abstract.supertype="YES" name="expression_assignment">
         <explicit name="assigned_expression">
            <typename name="expression"/>
         </explicit>
         <explicit name="role">
            <typename name="expression_role"/>
         </explicit>
      </entity>
      <entity name="expression_item_representation_item" supertypes="representation_item">
         <explicit name="item">
            <typename name="expression_element"/>
         </explicit>
      </entity>
      <entity name="expression_representation" supertypes="representation">
         <explicit name="context_of_items">
            <typename name="expression_representation_context"/>
            <redeclaration entity-ref="representation"/>
         </explicit>
         <explicit name="items">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="expression_item_representation_item"/>
            <redeclaration entity-ref="representation"/>
         </explicit>
      </entity>
      <entity name="expression_representation_context" supertypes="representation_context"/>
      <entity name="expression_role">
         <explicit name="name">
            <typename name="label"/>
         </explicit>
         <explicit name="description" optional="YES">
            <typename name="text"/>
         </explicit>
      </entity>
      <entity name="functional_model" supertypes="representation">
         <explicit name="context_of_items">
            <typename name="functional_representation_context"/>
            <redeclaration entity-ref="representation"/>
         </explicit>
         <explicit name="items">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="functional_representation_item"/>
            <redeclaration entity-ref="representation"/>
         </explicit>
      </entity>
      <entity name="functional_representation_context" supertypes="representation_context"/>
      <entity name="functional_representation_item" supertypes="representation_proxy_item">
         <explicit name="item">
            <typename name="action_method"/>
            <redeclaration entity-ref="representation_proxy_item"/>
         </explicit>
      </entity>
      <entity name="representation_proxy_item" supertypes="representation_item">
         <explicit name="item">
            <typename name="representation_proxy_select"/>
         </explicit>
      </entity>
      <entity name="state_based_behaviour_model" supertypes="representation">
         <explicit name="context_of_items">
            <typename name="state_based_behaviour_representation_context"/>
            <redeclaration entity-ref="representation"/>
         </explicit>
         <explicit name="items">
            <aggregate lower="1" type="SET" upper="?"/>
            <typename name="state_based_behaviour_representation_item"/>
            <redeclaration entity-ref="representation"/>
         </explicit>
      </entity>
      <entity name="state_based_behaviour_representation_context" supertypes="representation_context"/>
      <entity name="state_based_behaviour_representation_item" supertypes="representation_proxy_item">
         <explicit name="item">
            <typename name="state_based_behaviour_element"/>
            <redeclaration entity-ref="representation_proxy_item"/>
         </explicit>
      </entity>
   </schema>
</express>
