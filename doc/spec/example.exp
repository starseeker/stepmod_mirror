SCHEMA example;

ENTITY drawing;
   name : STRING;
   elements : SET [1:?] OF shape;
END_ENTITY;

ENTITY shape;
   label : STRING;
END_ENTITY; 

ENTITY point SUBTYPE OF (shape);
   x : REAL;
   y : REAL;
END_ENTITY; 

ENTITY line SUBTYPE OF (shape);
   end1 : point;
   end2 : point;
END_ENTITY;

END_SCHEMA;
