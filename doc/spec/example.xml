<?xml version="1.0"?>

<express language_version="1" rcs.date="$Date: $" rcs.revision="$Revision: $">

  <application name="GraphicalEXPRESS" owner="Eurostep" url="www.eurostep.com"
               version="1.3.15" source="example-expg.vsd"/>

  <schema name="example">

    <entity name="drawing">
      <explicit name="name">
        <builtintype type="STRING"/>
      </explicit>
      <explicit name="elements">
	<aggregate type="SET" lower="1" upper="?"/>
        <typename name="shape"/>
      </explicit>
    </entity>

    <entity name="line" supertypes="shape">
      <explicit name="end1">
        <typename name="point"/>
      </explicit>
      <explicit name="end2">
        <typename name="point"/>
      </explicit>
    </entity>

    <entity name="point" supertypes="shape">
      <explicit name="x">
        <builtintype type="REAL"/>
      </explicit>
      <explicit name="y">
        <builtintype type="REAL"/>
      </explicit>
    </entity>

    <entity name="shape">
      <explicit name="label">
        <builtintype type="STRING"/>
      </explicit>
    </entity>

  </schema>

</express>
