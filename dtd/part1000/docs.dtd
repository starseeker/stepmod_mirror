<!ELEMENT docs (doc+)>

<!ATTLIST docs
    part.version.number CDATA #REQUIRED>


<!ELEMENT doc (tc*)>
<!ATTLIST doc
	name CDATA #REQUIRED
	part CDATA #REQUIRED
	pub_type (TS | IS) #REQUIRED
	series (IGR | IAR | AIC | AM | AP | ATS | PLIB | IM |BO_M) #REQUIRED
	pub_year_mo CDATA #REQUIRED
	format (html-stepmod | html-general | pdf) #REQUIRED
	filename CDATA #IMPLIED
	edition CDATA #REQUIRED
>
<!ELEMENT tc EMPTY>
<!ATTLIST tc
	cor CDATA #REQUIRED
	pub_year_mo CDATA #REQUIRED
	format (html-stepmod | html-general | pdf) #REQUIRED
	filename CDATA #IMPLIED
>