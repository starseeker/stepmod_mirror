<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../../xsl/express.xsl"?>
<!DOCTYPE express SYSTEM "../../../dtd/express.dtd">

<express language_version="2" rcs.date="2005-03-30T17:23:52" rcs.revision="1.0" description.file="arm_descriptions.xml">
   <application name="JSDAI" owner="LKSoft" url="www.lksoft.com" version="4.0 beta" source="annotated_presentation_arm schema_instance"/>
   <schema name="Annotated_presentation_arm">
      <interface kind="use" schema="Appearance_assignment_arm"/>
      <interface kind="use" schema="Associative_text_arm"/>
      <interface kind="use" schema="Contextual_shape_positioning_arm"/>
      <interface kind="use" schema="Curve_appearance_arm"/>
      <interface kind="use" schema="Elemental_geometric_shape_arm"/>
      <interface kind="use" schema="Foundation_representation_arm"/>
      <interface kind="use" schema="General_surface_appearance_arm"/>
      <interface kind="use" schema="Group_arm"/>
      <interface kind="use" schema="Layer_assignment_arm"/>
      <interface kind="use" schema="Fill_area_arm"/>
      <type name="annotated_appearance_context">
         <select basedon="appearance_context" selectitems="Group Layer View View_area"/>
      </type>
      <type name="annotation_select">
         <select extensible="YES" genericentity="YES" selectitems="Annotation_element Draughting_callout"/>
      </type>
      <type name="area_select">
         <select extensible="YES" genericentity="YES" selectitems="View_area"/>
      </type>
      <type name="draughting_callout_content_select">
         <select selectitems="Annotation_curve Annotation_subfigure Annotation_symbol Text"/>
      </type>
      <type name="presentation_association_select">
         <select extensible="YES" genericentity="YES"/>
      </type>
      <type name="symbol_definition_select">
         <select selectitems="Dimension_symbol General_symbol Geometric_tolerance_symbol Marker Surface_condition_symbol Terminator"/>
      </type>
      <type name="view_placement_select">
         <select extensible="YES" genericentity="YES" selectitems="Axis_placement_mapping Cartesian_transformation_2d Mapping_based_template_instance"/>
      </type>
      <entity name="Annotation_subfigure" supertypes="Annotation_element"/>
      <entity name="Annotation_symbol" supertypes="Annotation_element">
         <explicit name="symbol_definition">
            <typename name="symbol_definition_select"/>
         </explicit>
      </entity>
      <entity name="Dimension_symbol"/>
      <entity name="Draughting_callout" supertypes="Representation_item">
         <explicit name="contents">
            <aggregate type="LIST" lower="1" upper="?"/>
            <typename name="draughting_callout_content_select"/>
         </explicit>
      </entity>
      <entity name="General_symbol"/>
      <entity name="Geometric_tolerance_symbol"/>
      <entity name="Styled_annotation_model" supertypes="Styled_model">
         <explicit name="is_defined_in">
            <typename name="Geometric_coordinate_space"/>
         </explicit>
      </entity>
      <entity name="Surface_condition_symbol"/>
      <entity name="View">
         <explicit name="id" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="name">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="description" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="layout">
            <typename name="area_select"/>
         </explicit>
         <explicit name="view_placement">
            <typename name="view_placement_select"/>
         </explicit>
         <explicit name="view_space">
            <typename name="Geometric_coordinate_space"/>
         </explicit>
      </entity>
      <entity name="View_area">
         <explicit name="name" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="description" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="size">
            <builtintype type="REAL"/>
         </explicit>
      </entity>
      <entity name="View_area_assignment">
         <explicit name="associated_view_area">
            <typename name="View_area"/>
         </explicit>
         <explicit name="is_describing">
            <typename name="presentation_association_select"/>
         </explicit>
      </entity>
      <entity name="View_area_relationship">
         <explicit name="description" optional="YES">
            <builtintype type="STRING"/>
         </explicit>
         <explicit name="related">
            <typename name="View_area"/>
         </explicit>
         <explicit name="relating">
            <typename name="View_area"/>
         </explicit>
         <explicit name="relationship_type">
            <builtintype type="STRING"/>
         </explicit>
      </entity>
   </schema>
</express>
