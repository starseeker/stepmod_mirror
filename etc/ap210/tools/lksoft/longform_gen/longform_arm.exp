      WR7 : NOT ('PACKAGE_ARM.PACKAGE_TERMINAL' IN TYPEOF(SELF));
      WR8 : NOT ('PACKAGE_ARM.PACKAGE_BODY_SURFACE' IN TYPEOF(SELF));
