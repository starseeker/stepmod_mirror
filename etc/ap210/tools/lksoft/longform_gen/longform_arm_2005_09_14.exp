    SUBTYPE OF (Edge_shape);
      minimum_radius_length : Length_data_element;
      maximum_radius_length : Length_data_element;
    DERIVE
      SELF\Representation.name : STRING := 'radius edge feature shape';
  END_ENTITY;

  ENTITY Step_edge_feature_shape
