   WHERE
     WR1 : (SIZEOF(USEDIN(SELF,'FABRICATION_TECHNOLOGY_ARM.PART_TEMPLATE_PLANAR_SHAPE_MODEL.' + 'SHAPE_CHARACTERIZED_PART_TEMPLATE')) = 0) AND 
            (SIZEOF(USEDIN(SELF,'FABRICATION_TECHNOLOGY_ARM.PART_TEMPLATE_3D_SHAPE_MODEL.' + 'SHAPE_CHARACTERIZED_PART_TEMPLATE')) = 0) AND 
            (SIZEOF(USEDIN(SELF,'FABRICATION_TECHNOLOGY_ARM.' + 'PART_TEMPLATE_NON_PLANAR_2D_SHAPE_MODEL.' + 'SHAPE_CHARACTERIZED_PART_TEMPLATE')) = 0);
