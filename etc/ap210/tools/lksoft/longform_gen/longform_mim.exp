  WR3: SIZEOF (QUERY (pd <* USEDIN (SELF, 
       'PACKAGE_MIM.' +
       'PROPERTY_DEFINITION.DEFINITION') |
       (SIZEOF (QUERY (sa <* USEDIN (pd,
       'PACKAGE_MIM.' +
       'SHAPE_ASPECT.OF_SHAPE') |
