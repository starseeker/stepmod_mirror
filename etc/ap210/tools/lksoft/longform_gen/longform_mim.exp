 WR19: NOT(is_interconnect_module_usage(SELF))
       OR
       (SIZEOF (QUERY (pd <* USEDIN (SELF,
       'PHYSICAL_UNIT_USAGE_VIEW_MIM.' +
       'PROPERTY_DEFINITION.DEFINITION') |
       (pd\property_definition.name = 
