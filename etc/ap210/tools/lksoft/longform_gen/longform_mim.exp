  WR11: (NOT(SELF\shape_aspect.description IN
       ['termination constraint'])) OR
               (SIZEOF( QUERY ( pd <* USEDIN(SELF,
            'NON_FEATURE_SHAPE_ELEMENT_MIM.' +
                         'PROPERTY_DEFINITION.DEFINITION')
