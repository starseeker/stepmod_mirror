       (pd\property_definition.description = 'positional boundary member property')
       AND NOT(('EXTENDED_GEOMETRIC_TOLERANCE_MIM.'
       + 'POSITIONAL_BOUNDARY_MEMBER' IN TYPEOF(pd.definition))))) = 0; 
