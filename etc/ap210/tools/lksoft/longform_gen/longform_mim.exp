SCHEMA Styled_text_mim;
	USE FROM Document_assignment_mim;	-- ISO/TS 10303-1122
	USE FROM Elemental_geometric_shape_mim;	-- ISO/TS 10303-1004
	USE FROM Specification_document_mim;	-- ISO/TS 10303-1747
	
