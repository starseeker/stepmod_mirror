 SUBTYPE OF (product_definition);
WHERE
  WR1: (NOT ('PHYSICAL_CONNECTIVITY_DEFINITION_MIM.' +
       'PRODUCT_DEFINITION_WITH_ASSOCIATED_DOCUMENTS' IN 
