       OR (SIZEOF(QUERY(a2p2d <* sr.items |
       ('LAYERED_INTERCONNECT_MODULE_2D_DESIGN_MIM.'
       + 'AXIS2_PLACEMENT_2D' IN TYPEOF(a2p2d))
       )) >= 0)))) = 0;
