menubar =
  element menubar {
    attlist.menubar, (menuitem | menubreak | menuspace)+
  }
attlist.menubar &= empty
menuitem = element menuitem { attlist.menuitem, empty }
attlist.menuitem &=
  attribute item { text },
  attribute absolute.url { text }?,
  attribute relative.url { text }?,
  attribute module.url { text }?,
  attribute img { text }?
menubreak = element menubreak { attlist.menubreak, empty }
attlist.menubreak &= empty
menuspace = element menuspace { attlist.menuspace, text }
attlist.menuspace &= empty
start = menubar
