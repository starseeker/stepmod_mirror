<?xml version="1.0"?>

 <!DOCTYPE rdf:RDF [
   <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
   <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
   <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
 ]>

 <!--DOCTYPE rdf:RDF [<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">]-->

 <rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#">
 
  <owl:Ontology rdf:about=""/>


<!-- basic types from core p41 schema -->

<owl:DatatypeProperty rdf:ID="label">
  <rdfs:range rdf:resource="&xsd;string"/>
  <rdfs:comment>Some special text from Lothar.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:ID="text">
  <rdfs:range rdf:resource="&xsd;string"/>
  <rdfs:comment>Some special text from Lothar.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:ID="identifier">
  <rdfs:range rdf:resource="&xsd;string"/>
  <rdfs:comment>Some special text from Lothar.</rdfs:comment>
</owl:DatatypeProperty>


<!-- and now specific usages within STEP Express schemas-->
 <owl:DatatypeProperty rdf:ID="id">
   <rdfs:subPropertyOf rdf:resource="#identifier"/>
 </owl:DatatypeProperty>

 <owl:DatatypeProperty rdf:ID="version_id">
   <rdfs:subPropertyOf rdf:resource="#identifier"/>
 </owl:DatatypeProperty>

 <owl:DatatypeProperty rdf:ID="name">
   <rdfs:subPropertyOf rdf:resource="#label"/>
 </owl:DatatypeProperty>

 <owl:DatatypeProperty rdf:ID="role">
   <rdfs:subPropertyOf rdf:resource="#label"/>
 </owl:DatatypeProperty>

 <owl:DatatypeProperty rdf:about="#description">
   <rdfs:subPropertyOf rdf:resource="#text"/>
 </owl:DatatypeProperty>
 
 <!-- Ontology on meta level 3 -->
 
  <owl:Class rdf:about="#Class_of_class_of_physical_object">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </owl:Class>
 
 <!-- Ontology on level 2 -->
 
  <owl:Class rdf:about="#Class_of_physical_object">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdf:type rdf:resource="#Class_of_class_of_physical_object"/>
  </owl:Class>
 
  <owl:Class rdf:about="#Class_of_physical_object_by_property_restriction">
    <rdfs:subClassOf rdf:resource="#Class_of_physical_object"/>
    <rdf:type rdf:resource="#Class_of_class_of_physical_object"/>
  </owl:Class>
 
  <owl:Class rdf:about="#Part">
    <rdfs:subClassOf rdf:resource="#Class_of_physical_object"/>
    <rdf:type rdf:resource="#Class_of_class_of_physical_object"/>
  </owl:Class>
 
  <owl:Class rdf:about="#Part_version">
    <rdfs:subClassOf rdf:resource="#Part"/>
    <rdf:type rdf:resource="#Class_of_class_of_physical_object"/>
  </owl:Class>
 
  <owl:ObjectProperty rdf:about='Part_version_predecessor'>
    <rdfs:domain rdf:resource='#Part_version'/>
    <rdfs:range rdf:resource='#Part_version'/>
  </owl:ObjectProperty>
 
  <owl:ObjectProperty rdf:about='Part_version_successor'>
    <owl:inverseOf rdf:resource='#Part_version_predecessor'/>
    <rdfs:domain rdf:resource='#Part_version'/>
    <rdfs:range rdf:resource='#Part_version'/>
  </owl:ObjectProperty>
 
  <owl:Class rdf:about="#Product_specification">
    <rdfs:subClassOf rdf:resource="#Product_class"/>
    <rdf:type rdf:resource="#Class_of_class_of_physical_object"/>
  </owl:Class>
 
  <owl:Class rdf:about="#Product_class">
    <rdfs:subClassOf rdf:resource="#Class_of_physical_object"/>
    <rdf:type rdf:resource="#Class_of_class_of_physical_object"/>
  </owl:Class>
 
 <!-- Ontology on meta level 1 -->
 
  <owl:Class rdf:about="#Physical_object">
    <rdf:type rdf:resource="#Class_of_physical_object"/>
  </owl:Class> 
   
  <owl:Class rdf:about="#Product_as_individual">
    <rdfs:subClassOf rdf:resource="#Physical_object"/>
    <rdf:type rdf:resource="#Class_of_physical_object"/>
  </owl:Class>
  
 </rdf:RDF>