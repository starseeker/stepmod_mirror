<!-- 3D Display Filters definition -->
<xsd:complexType name="MigDisplayFiltersType">
  <xsd:annotation>
    <xsd:documentation>
    Attributes:

        workplaneVisible     - Visibility of the workplane
        wireframeVisible     - Visibility of wireframe geometry
        partVisible          - Visibility of part geometry
        assemblyVisible      - Visibility of assembly geometry
        drawingLayoutVisible - Visibility of drawing layout geometry
        NCVisible            - Visibility of NC geometry
        sheetMetalVisible    - Visibility of sheet metal geometry
        harnessVisible       - Visibility of harness geometry

    Elements:
        PartFilter     - Visibility settings for Parts
        AssemblyFilter - Visibility settings for Assemblies
   </xsd:documentation>
   </xsd:annotation>
   <xsd:complexContent>
      <xsd:extension base="xsd:anyType">
         <xsd:sequence>
            <xsd:element name="PartFilter"     type="plm:MigPartDisplayFilterType"
                         minOccurs="0" maxOccurs="1"/>
            <xsd:element name="AssemblyFilter" type="plm:MigAssemblyDisplayFilterType"
                         minOccurs="0" maxOccurs="1"/>
         </xsd:sequence>
      <xsd:attribute name="workplaneVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="wireframeVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="partVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="assemblyVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="drawingLayoutVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="ncVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="sheetMetalVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="harnessVisible" type="xsd:boolean" default="true"/>
      </xsd:extension>
   </xsd:complexContent>
</xsd:complexType>


