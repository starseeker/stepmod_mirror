      <xsd:attribute name="keyDimensionVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="keyDimensionLabelVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="referenceDimensionVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="referenceDimensionLabelVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="noteVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="noteLabelVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="lineWeldVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="lineWeldLabelVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="spotWeldVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="spotWeldLabelVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="surfaceFinishVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="surfaceFinishLabelVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="locatorVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="locatorLabelVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="measurementPointVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="measurementPointLabelVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="balloonVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="balloonLabelVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="fastenerVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="fastenerLabelVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="userDefinedSymbolVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="userDefinedSymbolLabelVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="circleCentreVisible" type="xsd:boolean" default="true"/>
