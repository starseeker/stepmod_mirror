<!-- *** Part Display Filters  -->
<!-- -->
<xsd:complexType name="MigPartDisplayFilterType">
  <xsd:annotation>
    <xsd:documentation>

    Attributes:

    nameVisible                 - Visibility of entity name
    localOriginTriadVisible     - Visibility of local origin triad
    principleAxesVisible        - Visibility of principle axes
    systemReferenceTriadVisible - Visibility of the system reference triad
    NCReferenceTriadVisible     - Visibility of NC reference triad
    FEMReferenceTriadVisible    - Visibility of FEM reference triad
    2DConstraintVisible         - Visibility of 2D constraints
    2DConstraintLabelVisible    - Visibility of 3D constraint labels
    3DConstraintVisible         - Visibility of 3D constraints
    3DConstraintLabelVisible    - Visibility of 3D constraint labels
    massPropertiesVisible       - Visibility of mass properties

    Elements:

    AnnotationVisibility        - Visibility of annotation
    SolidGeometryVisibility     - Visibility of solid geometry
    WireframeVisibility         - Visibility of wireframe geometry
    ReferenceGeometryVisibility - Visibility of reference geometry
    CentreGeometryVisibility    - Visibility of centre geometry
    HarnessVisibility           - Visibility of harness geometry

    </xsd:documentation>
  </xsd:annotation>
  <xsd:complexContent>
    <xsd:extension base="xsd:anyType">
      <xsd:sequence>
         <xsd:element name="AnnotationVisibility"        type="plm:MigPMIDisplayFilterType" 
                      minOccurs="0" maxOccurs="1"/>
         <xsd:element name="SolidGeometryVisibility"     type="plm:MigSolidGeometryDisplayFilterType"
                      minOccurs="0" maxOccurs="1"/>
         <xsd:element name="WireframeVisibility"         type="plm:MigWireframeDisplayFilterType"
                      minOccurs="0" maxOccurs="1"/>
         <xsd:element name="ReferenceGeometryVisibility" type="plm:MigReferenceGeometryDisplayFilterType"
                      minOccurs="0" maxOccurs="1"/>
         <xsd:element name="CentreGeometryVisibility"    type="plm:MigCentreGeometryDisplayFilterType"
                      minOccurs="0" maxOccurs="1"/>
         <xsd:element name="HarnessVisibility"           type="plm:MigPartHarnessDisplayFilterType"
                      minOccurs="0" maxOccurs="1"/>
      </xsd:sequence>
      <xsd:attribute name="nameVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="localOriginTriadVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="principleAxesVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="systemReferenceTriadVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="ncReferenceTriadVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="femReferenceTriadVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="constraint2DVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="constraint2DLabelVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="constraint3DVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="constraint3DLabelVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="massPropertiesVisible" type="xsd:boolean" default="true"/>
    </xsd:extension>
