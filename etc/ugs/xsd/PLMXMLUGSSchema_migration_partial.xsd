<xsd:schema targetNamespace="http://www.plmxml.org/Schemas/PLMXMLSchema" 
            xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
            xmlns:plm="http://www.plmxml.org/Schemas/PLMXMLSchema" 
            elementFormDefault="qualified" attributeFormDefault="unqualified" xml:lang="en">

  <xsd:include schemaLocation="PLMXMLSchema.xsd" />
  <xsd:include schemaLocation="PLMXMLAnnotationSchema.xsd" />

  <!-- Element for the migration of user and system attributes -->
  <xsd:complexType name="MigUserDataElementType">
    <xsd:annotation>
     <xsd:documentation>
     In addition to the UserValue elements
     Attributes:

       systemTitle - System defined name for the attribute.

     Elements:
         EntityRef - Reference to associated entity that determines the value of this 
                     user data element. The value of the user data element reflects a
                     cached value of the refrenced entity.

     </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="plm:UserDataElementType">
        <xsd:sequence>
          <xsd:element ref="plm:EntityRef" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
        <xsd:attribute name="systemTitle" type="xsd:string" use="optional"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="MigUserDataType">
   <xsd:annotation>
    <xsd:documentation>
    Similar to UserValue elements except that the element includes MigUserValue elements.
    </xsd:documentation>
   </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="plm:AttributeBase">
        <xsd:choice>
          <xsd:element name="MigUserValue" type="plm:MigUserDataElementType" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:choice>
        <xsd:attribute name="type" type="xsd:string" use="optional"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="MigUserData" type="plm:MigUserDataType" substitutionGroup="plm:Attribute"/>


  <!-- Group element for migration of groups -->
  <xsd:complexType name="DesignGroupType">
   <xsd:annotation>
     <xsd:documentation>
     DesignGroup definition for migration
 
     Attributes:
       isSignificant - User defined field to indicate importance of design group
           isVisible - True if design group is made available to user
     highlightColour - Colour to highlight group when selected.       
             isValid - True if all the elements in the DesignGroup are present
              number - User assigned number to group.

     Elements:
         EntityRef - Reference to associated entities for the group.

    </xsd:documentation>
   </xsd:annotation>
     <xsd:complexContent>
      <xsd:extension base="plm:AttribOwnerBase">
        <xsd:choice minOccurs="0" maxOccurs="unbounded">
          <xsd:element ref="plm:EntityRef"/>
        </xsd:choice>
        <xsd:attribute name="isSignificant"     type="xsd:boolean" default="false"/>
        <xsd:attribute name="isVisible"         type="xsd:boolean" default="true"/>
        <xsd:attribute name="highlightColour"   type="plm:RGBType" default="1.0 0.0 0.0"/>
        <xsd:attribute name="isValid"           type="xsd:boolean" default="true"/>
        <xsd:attribute name="number"            type="xsd:int"     use="optional"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="DesignGroup" type="plm:DesignGroupType" 
               substitutionGroup="plm:AttribOwner"/>


