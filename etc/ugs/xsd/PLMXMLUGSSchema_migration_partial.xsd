<xsd:simpleType name="MigHiddenLineStyle">
  <xsd:restriction base="xsd:NMTOKEN">
    <xsd:enumeration value="removed"/>
    <xsd:enumeration value="solid"/>
    <xsd:enumeration value="dashed"/>
    <xsd:enumeration value="visible"/>
  </xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="MigHiddenLineMethod">
  <xsd:restriction base="xsd:NMTOKEN">
    <xsd:enumeration value="normal"/>
    <xsd:enumeration value="fixed"/>
    <xsd:enumeration value="faded"/>
  </xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="MigLineWeight">
  <xsd:restriction base="xsd:NMTOKEN">
    <xsd:enumeration value="thin"/>
    <xsd:enumeration value="medium"/>
    <xsd:enumeration value="thick"/>
  </xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="MigFiniteElementCull">
  <xsd:restriction base="xsd:NMTOKEN">
    <xsd:enumeration value="noFacesRemoved"/>
    <xsd:enumeration value="solidFEFacesRemoved"/>
    <xsd:enumeration value="allFEFacesRemoved"/>
  </xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="MigLineQuality">
  <xsd:restriction base="xsd:NMTOKEN">
    <xsd:enumeration value="sketch"/>
    <xsd:enumeration value="coarse"/>
    <xsd:enumeration value="normal"/>
    <xsd:enumeration value="fine"/>
  </xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="MigInterferenceStyle">
  <xsd:restriction base="xsd:NMTOKEN">
    <xsd:enumeration value="wireframe"/>
    <xsd:enumeration value="solid"/>
  </xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="MigClipDisplay">
  <xsd:restriction base="xsd:NMTOKEN">
    <xsd:enumeration value="transparent"/>
    <xsd:enumeration value="translucent"/>
    <xsd:enumeration value="wireframe"/>
  </xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="MigClipMotionSpace">
  <xsd:restriction base="xsd:NMTOKEN">
    <xsd:enumeration value="screenSpace"/>
    <xsd:enumeration value="modelSpace"/>
  </xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="MigAntiAliasingLevel">
  <xsd:restriction base="xsd:NMTOKEN">
    <xsd:enumeration value="onePerPixelNoAntiAliasing"/>
    <xsd:enumeration value="fourPerPixelFixedAntiAlias"/>
    <xsd:enumeration value="adaptiveAntiAliasLimit40"/>
    <xsd:enumeration value="adaptiveAntiAliasLimit128"/>
  </xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="MigLocalShading">
  <xsd:restriction base="xsd:NMTOKEN">
    <xsd:enumeration value="deviceHardware"/>
    <xsd:enumeration value="watkins"/>
    <xsd:enumeration value="pathTrace"/>
    <xsd:enumeration value="preciseHiddenLine"/>
    <xsd:enumeration value="fastHiddenLine"/>
    <xsd:enumeration value="hiddenDisplayGeneratorDescriptorFile"/>
  </xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="MigCurrentDisplayType">
  <xsd:restriction base="xsd:NMTOKEN">
    <xsd:enumeration value="draw"/>
    <xsd:enumeration value="wireframe"/>
    <xsd:enumeration value="hiddenLine"/>
    <xsd:enumeration value="shadedImage"/>
    <xsd:enumeration value="plot"/>
    <xsd:enumeration value="applicationSpecific"/>
  </xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="MigDepthCueCoefficientType">
  <xsd:restriction base="xsd:double">
    <xsd:minInclusive value="0.0"/>
    <xsd:maxInclusive value="1.0"/>
  </xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="MigNumberOfReflectionsType">
  <xsd:restriction base="xsd:integer">
    <xsd:minInclusive value="1"/>
    <xsd:maxInclusive value="9"/>
  </xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="MigAmbientLightCoefficientType">
  <xsd:restriction base="xsd:double">
    <xsd:minInclusive value="0.0"/>
    <xsd:maxInclusive value="1.0"/>
  </xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="MigImageLightMultiplierType">
  <xsd:restriction base="xsd:double">
    <xsd:minInclusive value="0.0"/>
    <xsd:maxInclusive value="1.000"/>
  </xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="MigModelSizeRelativeScaleType">
  <xsd:restriction base="xsd:double">
    <xsd:minInclusive value="0.0"/>
    <xsd:maxInclusive value="1.0"/>
  </xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="MigLabelSizeMultiplierType">
  <xsd:restriction base="xsd:double">
    <xsd:minInclusive value="0.1"/>
    <xsd:maxInclusive value="10.0"/>
  </xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="MigDitheringLevelType">
  <xsd:restriction base="xsd:integer">
    <xsd:minInclusive value="1"/>
    <xsd:maxInclusive value="4"/>
  </xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="MigPixelGridType">
  <xsd:restriction base="xsd:integer">
    <xsd:minInclusive value="1"/>
    <xsd:maxInclusive value="128"/>
  </xsd:restriction>
</xsd:simpleType>


<!-- *** Display Options  -->
<!-- -->
<xsd:complexType name="MigDisplayOptionsType">
  <xsd:annotation>
    <xsd:documentation>

    Attributes:

    ------------------------
    Multi-purpose parameters
    ------------------------
    outline                       - Flag indicating whether or not outlines are displayed for
                                    shaded displays.
    outlineColour                 - Colour used to for outlines in a shaded display.
                                    Default white.
    backgroundColour              - Colour used to draw the background.
                                    Default black.
    showViewportOutline           - Flag indicating whether or not viewport outlines are shown
    showViewportNumber            - Flag indicating whether or not viewport numbers are shown
    showCoordinateSystem          - Flag indicating whether or not coordinate systems are shown
    showOrigin                    - Flag indicating whether or not the world coordinate origin is shown
    backfaceCull                  - Flag indicating whether or not backface culling is performed.
                                    Applies also to polygons. Backfacing polygons are removed.
    cullFacingReverse             - Flag indicating whether or not the sense of the backfaceCull
                                    flag is reversed. true would mean that front facing polygons
                                    are culled instead of backwards facing polygons.
    lineQuality                   - Indicates the level of curve discretization for hidden line
                                    display
    partTranslucency              - Flag indicating whether or not part translucency is enabled.
                                    Translucency controls the amount of light an object allows to pass
                                    through it.
    partPrecedence                - Flag indicating whether or not part precedence is enabled.
                                    Precedence controls X-Ray effects for an object.
    assemblyTranslucency          - Flag indicating whether or not assembly translucency is enabled
                                    Translucency controls the amount of light an object allows to pass
                                    through it.
    assemblyPrecedence            - Flag indicating whether or not assembly precedence is enabled
                                    Precedence controls X-Ray effects for an object.
    enableContour                 - Flag indicating whether or not the display of contour lines is enabled
    contourLabelDisplay           - Flag indicating whether of not the display of labels for contour
                                    lines is enabled
    enableFringe                  - Flag indicating whether or not fringe display is enabled
    discreteFringeColour          - Flag indicating whether or not vertices within a contour band
                                    are displayed with the start colour of the contour band.
    fringeIntensity               - Flag indicating whether or not fringe intensity is enabled. If
                                    enabled, fringe colours are displayed at full intensity and all
                                    lighting effects are ignored.
    silhouetteVisible             - Flag indicating whether or not silhouettes are visible.
                                    Only applicable to wireframe displays.
    surfaceLighting               - Flag indicating whether or not surface lighting is enabled
    depthCue                      - Flag indicating whether or not depth cueing is enable
    depthCueCoefficient           - Controls the intensity at the back of an object when depth cueing is on.
                                    0.0 = Maximum depth cueing
                                    1.0 = No depth cueing
    mixedModeDisplay              - Flag indicating whether or not mixed mode display is enabled
    revertToWireframe             - Flag indicating whether or not to revert to wireframe display
                                    during model motion
    hardwareBackfaces             - Flag indicating whether or not hardware backfaces are used if the
                                    platform supports this capability.
    hardwareBacklighting          - Flag indicating whether or not hardware backlighting is used if
                                    the platform supports this capability
    hiddenGeometryStyle           - Line style for hidden geometry
    hiddenGeometryMethod          - Method used to display hidden geometry
    hiddenGeometryFixedColour     - Colour used to display hidden geometry when a fixed colour is specified
    hiddenAnnoSameAsGeometry      - Flag indicating attributes for hidden annotation are the same as
                                    the geometry settings. Overrides settings for hidden line style, method
                                    and colour.
    hiddenAnnotationStlye         - Line style for hidden annotation
    hiddenAnnotationMethod        - Method used to display hidden annotation
    hiddenAnnotationFixedColour   - Colour used to display hidden annotation when a fixed colour is specified
    hiddenGeometryLineWeight      - Line weight for hidden geometry lines
    visibleGeometryLineWeight     - Line weight for visible geometry lines
    boundaryGeometryLineWeight    - Line weight for boundary geometry lines
    hiddenAnnotationLineWeight    - Line weight for hidden annotation lines
    visibleAnnotationLineWeight   - Line weight for visible annotation lines
    textureEnabled                - Flag indicating whether or not texture is enabled
    advancedLighting              - Flag indicating whether or not advanced lighting is enabled
    ambientLightCoefficient       - A coefficient applied for ambient lighting indicating the percent
                                    of the surface colour displayed
    imageLightMultiplier          - Multiplier applied to the total light contribution at the image.
                                    Light contribution includes ambient light and light sources.
    modelSizeRelativeScale        - Scale factor for model size relative graphics
                                    0.0 - All MSR coordinates collpase to MSR origin
                                    1.0 - Full MSR is used
    labelSizeMultiplier           - Scale factor for all 3D text
    localShading                  - Indicates the type of local shading applied
    currentDisplayType            - Indicates the type of display currently in use
    autoDraw                      - Flag indicating whether or not the autodraw facility is enabled.
    tangentEdgesVisible           - Flag indicating whether or not tangent edges are visible
    isolinesVisible               - Flag indicating whether or not isolines are visible
    showUnboundedSurfaces         - Flag indicating whether or not unbounded surfaces are shown
    seamsVisible                  - Flag indicating whether or not seams are visible
    dynamicViewing                - Flag indicating whether or not dynamic viewing is enabled

    -------------------------
    Finite Element Parameters
    -------------------------
    finiteElementCull             - Indicates type of finite element culling performed.
                                    backfaceCull flag must be TRUE for culling to take place.
    internalFace                  - Flag indicating whether or not internal faces are removed from
                                    Finite Element pictures. This gives a free face display.
    featureAngle                  - Flag indicating whether or not FE feature lines are removed from
                                    an FE display. The feature angle is stored elsewhere.
    2DShrink                      - Flag indicating whether or not shrinking is performed for 2D FE elements
    3DShrink                      - Flag indicating whether or not shrinking is performed for 3D FE elements
    enableDeformationDisplay      - Enable or disable deformation display
    imaginaryPartDeformation      - Display real or imaginary part deformation
                                    0 = display real part deformation
                                    1 = display imaginary part deformation
    deformationScaleRealPart      - Multiplied by all deformation to produce deflections for
                                    real valued deformations.
    deformationScaleImaginaryPart - Multiplied by all deformation to produce deflections for
                                    real valued deformations.

    ----------------------
    Ray Tracing Parameters
    ----------------------
    dithering                     - Flag indicating whether or not dithering is enabled
    ditheringLevel                - Level applied during dithering
    shadows                       - Flag indicating whether or not shadows are enabled during ray tracing
                                    display
    shadowsAndReflections         - Flag indicating both shadows and reflections are enabled during ray
                                    tracing display
    numberOfReflections           - Number of reflections
    pixelGrid                     - Size of the image in pixels
    antiAliasingLevel             - Indicates level of anti-aliasing applied during ray tracing
    antiAliasingHardwareLines     - Flag indicating whether or not hardware anti-aliasing lines are
                                    used if they are available

    -------------------
    Clipping Parameters
    -------------------
    clipping                      - Flag indicating whether or not clipping is enabled
    displayPlaneAB                - Flag indicating whether or not clipping plane AB is displayed
    displayPlaneAC                - Flag indicating whether or not clipping plane AC is displayed
    displayPlaneBC                - Flag indicating whether or not clipping plane BC is displayed
    invertPlaneAB                 - Flag indicating whether or not clipping plane AB is inverted
    invertPlaneAC                 - Flag indicating whether or not clipping plane AC is inverted
    invertPlaneBC                 - Flag indicating whether or not clipping plane BC is inverted
    normalTriad                   - Flag indicating whether or not the normal triad is displayed for
                                    clipping
    dynamicClippingDisplayType    - Indicates how clipping is displayed
    dynamicClippingMotion         - Indicates the space for dynamic motion applied during clipping
    clippingPlaneOrigin           - Coordinates for the origin of the clipping plane
    axisA3DClipPoint              - Clip point on axis A. The vector from the clippingPlaneOrigin
                                    to this point defines the 3D world direction vector for the
                                    dynamic clip point on the X axis.
    axisC3DClipPoint              - Clip point on axis C. The vector from the clippingPlaneOrigin
                                    to this point defines the 3D world direction vector for the
                                    dynamic clip point on the Y axis.
    checkInterference             - Flag indicating whether or not interference check is enabled
    interferenceStyle             - Indicates the style of interference check performed
    interferenceColour            - Colour used to show interference
    capping                       - Flag indicating whether or not capping is performed
    cappingColour                 - Colour applied for capping


    Elements:

    </xsd:documentation>
  </xsd:annotation>
  <xsd:complexContent>
    <xsd:extension base="xsd:anyType">
      <xsd:attribute name="outline" type="xsd:boolean" default="false"/>
      <xsd:attribute name="outlineColour" type="plm:RGBAType" use="optional"/>
      <xsd:attribute name="backgroundColour" type="plm:RGBAType" use="optional"/>
      <xsd:attribute name="showViewportOutline" type="xsd:boolean" default="true"/>
      <xsd:attribute name="showViewportNumber" type="xsd:boolean" default="false"/>
      <xsd:attribute name="showCoordinateSystem" type="xsd:boolean" default="true"/>
      <xsd:attribute name="showOrigin" type="xsd:boolean" default="false"/>
      <xsd:attribute name="backfaceCull" type="xsd:boolean" default="false"/>
      <xsd:attribute name="cullFacingReverse" type="xsd:boolean" default="false"/>
      <xsd:attribute name="lineQuality" type="plm:MigLineQuality" default="normal"/>
      <xsd:attribute name="partTranslucency" type="xsd:boolean" default="false"/>
      <xsd:attribute name="partPrecedence" type="xsd:boolean" default="false"/>
      <xsd:attribute name="assemblyTranslucency" type="xsd:boolean" default="false"/>
      <xsd:attribute name="assemblyPrecedence" type="xsd:boolean" default="false"/>
      <xsd:attribute name="enableContour" type="xsd:boolean" default="false"/>
      <xsd:attribute name="contourLabelDisplay" type="xsd:boolean" default="false"/>
      <xsd:attribute name="enableFringe" type="xsd:boolean" default="false"/>
      <xsd:attribute name="discreteFringeColour" type="xsd:boolean" default="false"/>
      <xsd:attribute name="fringeIntensity" type="xsd:boolean" default="false"/>
      <xsd:attribute name="silhouetteVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="surfaceLighting" type="xsd:boolean" default="false"/>
      <xsd:attribute name="depthCue" type="xsd:boolean" default="false"/>
      <xsd:attribute name="depthCueCoefficient" type="plm:MigDepthCueCoefficientType" default="0.5"/>
      <xsd:attribute name="mixedModeDisplay" type="xsd:boolean" default="false"/>
      <xsd:attribute name="revertToWireframe" type="xsd:boolean" default="false"/>
      <xsd:attribute name="hardwareBackfaces" type="xsd:boolean" default="true"/>
      <xsd:attribute name="hardwareBacklighting" type="xsd:boolean" default="false"/>
      <xsd:attribute name="hiddenGeometryStyle" type="plm:MigHiddenLineStyle" default="solid"/>
      <xsd:attribute name="hiddenGeometryMethod" type="plm:MigHiddenLineMethod" default="faded"/>
      <xsd:attribute name="hiddenGeometryFixedColour" type="plm:RGBAType" use="optional"/>
      <xsd:attribute name="hiddenAnnoSameAsGeometry" type="xsd:boolean" default="false"/>
      <xsd:attribute name="hiddenAnnotationStyle" type="plm:MigHiddenLineStyle" default="solid"/>
      <xsd:attribute name="hiddenAnnotationMethod" type="plm:MigHiddenLineMethod" default="faded"/>
      <xsd:attribute name="hiddenAnnotationFixedColour" type="plm:RGBAType" use="optional"/>
      <xsd:attribute name="hiddenGeometryLineWeight" type="plm:MigLineWeight" default="thin"/>
      <xsd:attribute name="visibleGeometryLineWeight" type="plm:MigLineWeight" default="thin"/>
      <xsd:attribute name="boundaryGeometryLineWeight" type="plm:MigLineWeight" default="thin"/>
      <xsd:attribute name="hiddenAnnotationLineWeight" type="plm:MigLineWeight" default="thin"/>
      <xsd:attribute name="visibleAnnotationLineWeight" type="plm:MigLineWeight" default="thin"/>
      <xsd:attribute name="textureEnabled" type="xsd:boolean" default="false"/>
      <xsd:attribute name="advancedLighting" type="xsd:boolean" default="false"/>
      <xsd:attribute name="ambientLightCoefficient" type="plm:MigAmbientLightCoefficientType" default = "0.15"/>
      <xsd:attribute name="imageLightMultiplier" type="plm:MigImageLightMultiplierType" default="1.0"/>
      <xsd:attribute name="modelSizeRelativeScale" type="plm:MigModelSizeRelativeScaleType" default="1.0"/>
      <xsd:attribute name="labelSizeMultiplier" type="plm:MigLabelSizeMultiplierType" default="1.0"/>
      <xsd:attribute name="localShading" type="plm:MigLocalShading" default="deviceHardware"/>
      <xsd:attribute name="currentDisplayType" type="plm:MigCurrentDisplayType" default="hiddenLine"/>
      <xsd:attribute name="autoDraw" type="xsd:boolean" default="true"/>
      <xsd:attribute name="tangentEdgesVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="isolinesVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="showUnboundedSurfaces" type="xsd:boolean" default="true"/>
      <xsd:attribute name="seamsVisible" type="xsd:boolean" default="true"/>
      <xsd:attribute name="dynamicViewing" type="xsd:boolean" default="true"/>

      <xsd:attribute name="finiteElementCull" type="plm:MigFiniteElementCull" default="noFacesRemoved"/>
      <xsd:attribute name="internalFace" type="xsd:boolean" default="false"/>
      <xsd:attribute name="featureAngle" type="xsd:boolean" default="false"/>
      <xsd:attribute name="shrink2D" type="xsd:boolean" default="false"/>
      <xsd:attribute name="shrink3D" type="xsd:boolean" default="false"/>
      <xsd:attribute name="enableDeformationDisplay" type="xsd:boolean" default="false"/>
      <xsd:attribute name="imaginaryPartDeformation" type="xsd:boolean" default="false"/>
      <xsd:attribute name="deformationScaleRealPart" type="xsd:double" default="0.0"/>
      <xsd:attribute name="deformationScaleImaginaryPart" type="xsd:double" default="0.0"/>

      <xsd:attribute name="dithering" type="xsd:boolean" default="true"/>
      <xsd:attribute name="ditheringLevel" type="plm:MigDitheringLevelType" default="1"/>
      <xsd:attribute name="shadows" type="xsd:boolean" default="false"/>
      <xsd:attribute name="shadowsAndReflections" type="xsd:boolean" default="false"/>
      <xsd:attribute name="numberOfReflections" type="plm:MigNumberOfReflectionsType" default="1"/>
      <xsd:attribute name="pixelGrid" type="plm:MigPixelGridType" default="1"/>
      <xsd:attribute name="antiAliasingLevel" type="plm:MigAntiAliasingLevel" default="onePerPixelNoAntiAliasing"/>
      <xsd:attribute name="antiAliasingHardwareLines" type="xsd:boolean" default="false"/>

      <xsd:attribute name="clipping" type="xsd:boolean" default="false"/>
      <xsd:attribute name="displayPlaneAB" type="xsd:boolean" default="false"/>
      <xsd:attribute name="displayPlaneAC" type="xsd:boolean" default="true"/>
      <xsd:attribute name="displayPlaneBC" type="xsd:boolean" default="false"/>
      <xsd:attribute name="invertPlaneAB" type="xsd:boolean" default="false"/>
      <xsd:attribute name="invertPlaneAC" type="xsd:boolean" default="false"/>
      <xsd:attribute name="invertPlaneBC" type="xsd:boolean" default="false"/>
      <xsd:attribute name="normalTriad" type="xsd:boolean" default="true"/>
      <xsd:attribute name="dynamicClippingDisplayType" type="plm:MigClipDisplay" default="transparent"/>
      <xsd:attribute name="dynamicClippingMotion" type="plm:MigClipMotionSpace" default="screenSpace"/>
      <xsd:attribute name="clippingPlaneOrigin" type="plm:VectorType" default="0.0 0.0 0.0"/>
      <xsd:attribute name="axisA3DClipPoint" type="plm:VectorType" default="1.0 0.0 0.0"/>
      <xsd:attribute name="axisC3DClipPoint" type="plm:VectorType" default="0.0 0.0 1.0"/>
      <xsd:attribute name="checkInterference" type="xsd:boolean" default="false"/>
      <xsd:attribute name="interferenceStyle" type="plm:MigInterferenceStyle" default="wireframe"/>
      <xsd:attribute name="interferenceColour" type="plm:RGBAType" use="optional"/>
      <xsd:attribute name="capping" type="xsd:boolean" default="false"/>
      <xsd:attribute name="cappingColour" type="plm:RGBAType" use="optional"/>
    </xsd:extension>
  </xsd:complexContent>
</xsd:complexType>

<!-- Migration display options for Model View Display -->
<xsd:complexType name="MigDisplayConfigurationType">
   <xsd:annotation>
    <xsd:documentation>

    Attributes:

    Elements:
     Options - Display options 
     Filters - All the display filters

    </xsd:documentation>
   </xsd:annotation>
   <xsd:complexContent>
      <xsd:extension base="plm:AttribOwnerBase">
        <xsd:sequence>
          <xsd:element name="Options" type="plm:MigDisplayOptionsType" 
                       minOccurs="0" maxOccurs="1" />
          <xsd:element name="Filters" type="plm:MigDisplayFiltersType"
                       minOccurs="0" maxOccurs="1" />
        </xsd:sequence>
      </xsd:extension>
   </xsd:complexContent>
</xsd:complexType>
<xsd:element name="MigDisplayConfiguration" type="plm:MigDisplayConfigurationType" 
                                            substitutionGroup="plm:AttribOwner" />

<!-- Migration Model View Folder Organisation -->
<xsd:complexType name="MigModelViewFolderType">
   <xsd:annotation>
    <xsd:documentation>

    Attributes:
              name - Name of the Folder
     modelViewRefs - References to all the model views organised in this folder.

    Elements:


    </xsd:documentation>
   </xsd:annotation>
   <xsd:complexContent>
      <xsd:extension base="xsd:anyType">
        <xsd:attribute name="name"           type="xsd:string"  use="required"/>
        <xsd:attribute name="modelViewRefs"  type="plm:uriReferenceListType"  use="optional"/>
      </xsd:extension>
   </xsd:complexContent>
</xsd:complexType>

<!-- Migration Model View Compound Organisation -->
<xsd:complexType name="MigModelViewCompoundType">
   <xsd:annotation>
    <xsd:documentation>
    Collection of ModelViews that were selected by the user for simulataneous display.

    Attributes:
     modelViewRefs - References to all the model views organised in this compound view

    Elements:

    </xsd:documentation>
   </xsd:annotation>
   <xsd:complexContent>
      <xsd:extension base="xsd:anyType">
        <xsd:attribute name="modelViewRefs"  type="plm:uriReferenceListType"  use="optional"/>
      </xsd:extension>
   </xsd:complexContent>
</xsd:complexType>

<xsd:complexType name="MigModelViewContextType">
   <xsd:annotation>
    <xsd:documentation>

    Attributes:
     productViewRef - Reference to the assembly context for this suite of model views.

    Elements:
          Folder - Folder elements into which Model views are organised.
    CompoundView - Collection of ModelViews that were selected by the user
                   for simulataneous display.

    </xsd:documentation>
   </xsd:annotation>
   <xsd:complexContent>
      <xsd:extension base="plm:AttribOwnerBase">
        <xsd:sequence>
          <xsd:element name="Folder"       type="plm:MigModelViewFolderType"
                       minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="CompoundView" type="plm:MigModelViewCompoundType"
                       minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute name="productViewRef"     type="plm:anyURIType"   use="required"/>
      </xsd:extension>
   </xsd:complexContent>
</xsd:complexType>
<xsd:element name="MigModelViewContext" type="plm:MigModelViewContextType" 
                                        substitutionGroup="plm:AttribOwner" />


<!-- Linked annotation group -->
<xsd:complexType name="MigLinkedAnn3DInstanceGroupType">
   <xsd:annotation>
    <xsd:documentation>

    Elements:
         EntityRef - References to Ann3DInstances that are linked tothe model view.

    </xsd:documentation>
   </xsd:annotation>
   <xsd:complexContent>
      <xsd:extension base="xsd:anyType">
        <xsd:choice minOccurs="0" maxOccurs="unbounded">
          <xsd:element ref="plm:EntityRef"/>
        </xsd:choice>
      </xsd:extension>
   </xsd:complexContent>
</xsd:complexType>

<!-- Alternate Ann3DInstance -->
  <xsd:complexType name="MigAlternateAnn3DInstanceType">
    <xsd:annotation>
      <xsd:documentation>
      An alternate Instance of an Annotation3D.

      Attributes:
      
      alternativeOfRef:       Reference to a source Ann3DInstance for which this 
                              instance is an alternative
      displayRef:             A reference to an element, derived from Ann3DDisplayType,
                              which specifies how this instance is to be displayed.
                              
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="plm:GDEInstanceType">
        <xsd:attribute name="alternativeOfRef" type="plm:anyURIType" use="required"/>
        <xsd:attribute name="displayRef"       type="plm:anyURIType" use="optional"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="MigAlternateAnn3DInstance" type="plm:MigAlternateAnn3DInstanceType" 
               substitutionGroup="plm:GDEInstance"/>

<!-- Base Model View definition -->
<xsd:complexType name="MigModelViewBase">
   <xsd:annotation>
    <xsd:documentation>
    Attributes:
        active           - Flag indicating whether or not the Model View is the active View.
        angle            - Rotation angles about the X, Y and Z axes
        eyeDirection     - Eye direction and angle
        eyePosition      - XYZ position for the eye of the viewer
        target           - XYZ on model where the viewer is "looking" on the model
        viewportDiameter - Diameter of the view. Indicates how close to or far from the model
                           the view is.
        clipping         - Flag indicating whether or not clipping is enabled
        clipThickness    - Distance between the front and back clipping planes
        viewIndicator    - Reference to the item that depicts this Model View
                           (PMI reference)
        viewGeometryRefs - References to geometry that provides the associated view 
                           direction information
        viewGeometryUpRefs- References to 'up' vector when view geometry reference is used.
        designGroupRef   - Reference to the Design Group used by this Model View
        displayConfigRef - Reference to the display options and filters

    Elements:
        LinkedAnnotationGroup    - Annotation that is visible in this Model View
        UnlinkedAnnotationGroup  - Annotation that is visible in this Model View and
                              has View specific attributes. Note that this annotation
                              will be written to the PLMXML file with the View specific
                              attributes.
    </xsd:documentation>
   </xsd:annotation>
   <xsd:complexContent>
      <xsd:extension base="plm:AttribOwnerBase">
         <xsd:sequence>
            <xsd:element name="LinkedAnnotationGroup" type="plm:MigLinkedAnn3DInstanceGroupType"
                         minOccurs="0" maxOccurs="1" />
            <xsd:element name="UnlinkedAnnotationGroup" type="plm:Ann3DInstanceGroupType"
                         minOccurs="0" maxOccurs="1" />
         </xsd:sequence>
         <xsd:attribute name="active"            type="xsd:boolean" default="false" />
         <xsd:attribute name="angle"             type="plm:VectorType" use="optional"/>
         <xsd:attribute name="eyeDirection"      type="plm:VectorType" use="optional"/>
         <xsd:attribute name="eyePosition"       type="plm:VectorType" use="optional"/>
         <xsd:attribute name="target"            type="plm:VectorType" use="optional"/>
         <xsd:attribute name="viewportDiameter"  type="xsd:double"     use="optional"/>
         <xsd:attribute name="clipping"          type="xsd:boolean"    default="false" />
         <xsd:attribute name="clipThickness"     type="xsd:double"     use="optional"/>
         <xsd:attribute name="viewIndicatorRef"  type="plm:anyURIType" use="optional"/>
         <xsd:attribute name="viewGeometryRefs"   type="plm:uriReferenceListType" use="optional"/>
         <xsd:attribute name="viewGeometryUpRefs" type="plm:uriReferenceListType" use="optional"/>
         <xsd:attribute name="designGroupRef"   type="plm:anyURIType" use="optional"/>
         <xsd:attribute name="displayConfigRef" type="plm:anyURIType" use="optional"/>
      </xsd:extension>
   </xsd:complexContent>
</xsd:complexType>
<xsd:element name="MigModelView" type="plm:MigModelViewBase" substitutionGroup="plm:AttribOwner" />


<xsd:simpleType name="MigSectionViewResult">
  <xsd:restriction base="xsd:NMTOKEN">
    <xsd:enumeration value="solid"/>
    <xsd:enumeration value="plane"/>
  </xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="MigSectionViewExcludeType">
  <xsd:restriction base="xsd:NMTOKEN">
    <xsd:enumeration value="drafting"/>
    <xsd:enumeration value="cutOnly"/>
  </xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="MigSectionViewResultOfCut">
  <xsd:restriction base="xsd:NMTOKEN">
    <xsd:enumeration value="deleted"/>
    <xsd:enumeration value="fullResult"/>
    <xsd:enumeration value="cutResult"/>
    <xsd:enumeration value="nullResult"/>
  </xsd:restriction>
</xsd:simpleType>

<!-- *** Section View Cut Data -->
<xsd:complexType name="MigSectionViewCutType">
  <xsd:annotation>
    <xsd:documentation>

    Attributes:

       occurrenceRef  - Hierarchy entity within an Assembly that identifies the specific 
                        occurrence being cut. Empty for Parts.
       resultOfCut    - Indicates the geometric result of attempting to cut the instance
                        deleted    - Instance has been deleted from the Assembly
                        fullResult - The full part remained after the cut (missed)
                        cutResult  - The part was cut
                        nullResult - The cut resulted in a null output (nothing left)
       geometryRefs   - References to the cut geometry.

    Elements:

    </xsd:documentation>
  </xsd:annotation>
  <xsd:complexContent>
    <xsd:extension base="xsd:anyType">
      <xsd:attribute name="occurrenceRef"  type="plm:anyURIType"                use="optional"/>
      <xsd:attribute name="resultOfCut"    type="plm:MigSectionViewResultOfCut" default="nullResult"/>
      <xsd:attribute name="geometryRefs"   type="plm:uriReferenceListType"      use="optional"/>
    </xsd:extension>
  </xsd:complexContent>
</xsd:complexType>

<!-- *** Section View Exclusion Data -->
<xsd:complexType name="MigSectionViewExclusionType">
  <xsd:annotation>
    <xsd:documentation>

    Attributes:
       exclusionType   - Indicates the type of exclusion list
       exclusionRefs   - references to instances or occurrences excluded from the Cut

    Elements:

    </xsd:documentation>
  </xsd:annotation>
  <xsd:complexContent>
    <xsd:extension base="xsd:anyType">
      <xsd:attribute name="exclusionType"  type="plm:MigSectionViewExcludeType"   default="drafting"/>
      <xsd:attribute name="exclusionRefs"  type="plm:uriReferenceListType"   use="optional"/>
    </xsd:extension>
  </xsd:complexContent>
</xsd:complexType>


<!-- Dependent annotation group -->
<xsd:complexType name="MigDependentAnnotationGroupType">
   <xsd:annotation>
    <xsd:documentation>

    Elements:
         EntityRef - References to Ann3D elements on which this section view is dependent.

    </xsd:documentation>
   </xsd:annotation>
   <xsd:complexContent>
      <xsd:extension base="xsd:anyType">
        <xsd:choice minOccurs="0" maxOccurs="unbounded">
          <xsd:element ref="plm:EntityRef"/>
        </xsd:choice>
      </xsd:extension>
   </xsd:complexContent>
</xsd:complexType>


<!-- Section View definition -->
<xsd:complexType name="MigSectionViewType">
   <xsd:annotation>
    <xsd:documentation>

    Attributes:

       cuttingPlaneSymbol  - Reference to the Cutting Plane Symbol used to define this Section View
       result              - Indicates the type of the result from the Section
       unfold              - Indicates whether or not the section cut is to be unfolded
       crosshatch          - Indicates whether or not crosshatch is to be applied to the resulting cut
                             The following are the values applied to crossahtches created as a side-effect
                             of the Section View creation.
       rotate              - Indicates whether or not the crosshatch pattern is rotated
       pattern             - The pattern used for the Crosshatch
       spacing             - The spacing between the lines in the Crosshatch
       angle               - The angle of the lines in the crosshatch from the X direction for the symbol
       orientToCut         - Flag indicating whether or not the Section View is oriented to the cut geometry

    Elements:
       CutGeometry         - For each instance with a result of cutResult, the geometry of the resulting
                             part will be output.
       Excluded            - Instances or occurrences to be excluded from section
       DependentAnnotationGroup - Annotation that is dependent on this section.

    </xsd:documentation>
   </xsd:annotation>
   <xsd:complexContent>
      <xsd:extension base="plm:MigModelViewBase">
         <xsd:sequence>
            <xsd:element name="CutGeometry"              type="plm:MigSectionViewCutType" 
                         minOccurs="0" maxOccurs="unbounded" />
            <xsd:element name="Excluded"                 type="plm:MigSectionViewExclusionType"
                         minOccurs="0" maxOccurs="1" />
            <xsd:element name="DependentAnnotationGroup" type="plm:MigDependentAnnotationGroupType"
                         minOccurs="0" maxOccurs="1" />
         </xsd:sequence>

         <xsd:attribute name="cuttingPlaneSymbolRef" type="plm:anyURIType" use="optional"/>
         <xsd:attribute name="result" type="plm:MigSectionViewResult" default="solid"/>
         <xsd:attribute name="unfold"             type="xsd:boolean" default="false" />
         <xsd:attribute name="crosshatch"         type="xsd:boolean" default="false" />
         <xsd:attribute name="crosshatchRotate"   type="xsd:boolean" default="false"/>
         <xsd:attribute name="crosshatchPattern"  type="plm:Ann3DCrosshatchPattern" default="iron"/>
         <xsd:attribute name="crosshatchSpacing"  type="xsd:double"  use="optional"/>
         <xsd:attribute name="crosshatchAngle"    type="xsd:double"  use="optional"/>
         <xsd:attribute name="orientToCut"        type="xsd:boolean" default="false" />
      </xsd:extension>
   </xsd:complexContent>
</xsd:complexType>
<xsd:element name="MigSectionView" type="plm:MigSectionViewType" substitutionGroup="plm:AttribOwner" />

<!-- Explicit Section View definition -->
<xsd:complexType name="MigExplicitSectionViewType">
  <xsd:annotation>
    <xsd:documentation>

    Attributes:
       parentViewRef  - Reference to the Parent of this Explicit Section VIew

    Elements:

    </xsd:documentation>
  </xsd:annotation>
   <xsd:complexContent>
      <xsd:extension base="plm:MigModelViewBase">
         <xsd:attribute name="parentViewRef" type="plm:anyURIType" use="required"/>
      </xsd:extension>
   </xsd:complexContent>
</xsd:complexType>
<xsd:element name="MigExplicitSectionView" type="plm:MigExplicitSectionViewType" substitutionGroup="plm:AttribOwner" />

</xsd:schema>
